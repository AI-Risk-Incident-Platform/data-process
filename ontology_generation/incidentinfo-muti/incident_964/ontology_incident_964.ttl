@base <https://w3id.org/airo#> .

ex:AICEODeepfake rdf:type airo:AISystem ;
    rdfs:label "AI CEO Deepfake"@en ;
    rdfs:label "AI CEO深度伪造"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasStakeholder ex:FerrariCompany .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:isRiskSourceFor ex:DeepfakeTechnology ;
    airo:hasConsequence ex:PotentialFinancialLoss .

ex:DeepfakeTechnology rdf:type airo:RiskSource ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PotentialFinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Potential Financial Loss"@en ;
    rdfs:label "潜在财务损失"@zh ;
    airo:hasImpactOnStakeholder ex:FerrariCompany .

ex:FerrariCompany rdf:type airo:Stakeholder ;
    rdfs:label "Ferrari Company"@en ;
    rdfs:label "法拉利公司"@zh .