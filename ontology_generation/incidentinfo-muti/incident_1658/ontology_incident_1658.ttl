@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceDepixelizer rdf:type airo:AISystem ;
    rdfs:label "Face Depixelizer"@en ;
    rdfs:label "面部去像素化工具"@zh ;
    airo:hasRisk ex:RacialBiasInImageReconstruction ;
    airo:usesTechnique ex:ImageSuperResolution .

ex:RacialBiasInImageReconstruction rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Image Reconstruction Risk"@en ;
    rdfs:label "图像重建中的种族偏见风险"@zh ;
    airo:hasConsequence ex:MisrepresentationOfEthnicFeatures ;
    airo:isRiskSourceFor ex:BiasedTrainingDataset .

ex:MisrepresentationOfEthnicFeatures rdf:type airo:Consequence ;
    rdfs:label "Misrepresentation of Ethnic Features Consequence"@en ;
    rdfs:label "种族特征错误呈现后果"@zh ;
    airo:hasImpact ex:ReinforcementOfRacialStereotypes .

ex:ImageSuperResolution rdf:type airo:AITechnique ;
    rdfs:label "Image Super-Resolution Technique"@en ;
    rdfs:label "图像超分辨率技术"@zh .

ex:ReinforcementOfRacialStereotypes rdf:type airo:Impact ;
    rdfs:label "Reinforcement of Racial Stereotypes Impact"@en ;
    rdfs:label "强化种族刻板印象的影响"@zh .

ex:BiasedTrainingDataset rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Dataset Risk Source"@en ;
    rdfs:label "有偏见的训练数据集风险源"@zh .