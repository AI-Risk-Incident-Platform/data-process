ex:Midjourney rdf:type airo:AISystem ;
    rdfs:label "Midjourney"@en ;
    rdfs:label "Midjourney"@zh ;
    airo:hasRisk ex:ArtIndustryDisruptionRisk ;
    airo:hasCapability ex:ImageGeneration ;
    airo:hasStakeholder ex:Artists ;
    airo:isAppliedWithinDomain ex:ArtIndustry .

ex:ArtIndustryDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Art Industry Disruption Risk"@en ;
    rdfs:label "艺术行业颠覆风险"@zh .

ex:ArtIndustry rdf:type airo:Domain ;
    rdfs:label "Art Industry"@en ;
    rdfs:label "艺术行业"@zh .

ex:ImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Image Generation"@en ;
    rdfs:label "图像生成"@zh .

ex:Artists rdf:type airo:Stakeholder ;
    rdfs:label "Artists"@en ;
    rdfs:label "艺术家"@zh .