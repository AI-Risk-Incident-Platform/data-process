@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:usesTechnique ex:DeepLearningBasedVideoManipulation ;
    airo:hasPurpose ex:PoliticalDisinformation ;
    airo:isAppliedWithinDomain ex:PoliticalCommunication .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisperception .

ex:PublicMisperception rdf:type airo:Consequence ;
    rdfs:label "Public Misperception Consequence"@en ;
    rdfs:label "公众误解后果"@zh ;
    airo:hasImpact ex:DamageToPoliticalReputation .

ex:DamageToPoliticalReputation rdf:type airo:Impact ;
    rdfs:label "Damage to Political Reputation Impact"@en ;
    rdfs:label "政治声誉损害影响"@zh .

ex:DeepLearningBasedVideoManipulation rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Video Manipulation Technique"@en ;
    rdfs:label "基于深度学习的视频操纵技术"@zh .

ex:PoliticalDisinformation rdf:type airo:Purpose ;
    rdfs:label "Political Disinformation Purpose"@en ;
    rdfs:label "政治虚假信息目的"@zh .

ex:PoliticalCommunication rdf:type airo:Domain ;
    rdfs:label "Political Communication Domain"@en ;
    rdfs:label "政治传播领域"@zh .