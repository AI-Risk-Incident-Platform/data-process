@base <https://w3id.org/airo#> .

ex:PublicSectorAITools rdf:type airo:AISystem ;
    rdfs:label "Public Sector AI Tools"@en ;
    rdfs:label "公共部门AI工具"@zh ;
    airo:hasRisk ex:BiasRiskInPublicSectorAI ;
    airo:hasStakeholder ex:UKPublicSector ;
    airo:isAppliedWithinDomain ex:PublicSector .

ex:BiasRiskInPublicSectorAI rdf:type airo:Risk ;
    rdfs:label "Bias Risk in Public Sector AI"@en ;
    rdfs:label "公共部门AI中的偏见风险"@zh ;
    airo:hasImpact ex:PublicScandalImpact .

ex:UKPublicSector rdf:type airo:Stakeholder ;
    rdfs:label "UK Public Sector"@en ;
    rdfs:label "英国公共部门"@zh .

ex:PublicScandalImpact rdf:type airo:Impact ;
    rdfs:label "Public Scandal Impact"@en ;
    rdfs:label "公共丑闻影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKPublicSector .

ex:PublicSector rdf:type airo:Domain ;
    rdfs:label "Public Sector Domain"@en ;
    rdfs:label "公共部门领域"@zh .