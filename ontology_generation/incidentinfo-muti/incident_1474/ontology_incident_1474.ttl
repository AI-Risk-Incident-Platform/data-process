@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAISystem rdf:type airo:AISystem ;
    rdfs:label "Google AI System"@en ;
    rdfs:label "谷歌AI系统"@zh ;
    airo:hasRisk ex:RegulatoryComplianceRisk ;
    airo:isDevelopedBy ex:GoogleLLC ;
    airo:hasStakeholder ex:FrenchCompetitionAuthority .

ex:RegulatoryComplianceRisk rdf:type airo:Risk ;
    rdfs:label "Regulatory Compliance Risk"@en ;
    rdfs:label "监管合规风险"@zh ;
    airo:hasConsequence ex:FinancialPenalty .

ex:FrenchCompetitionAuthority rdf:type airo:Stakeholder ;
    rdfs:label "French Competition Authority"@en ;
    rdfs:label "法国竞争管理局"@zh .

ex:FinancialPenalty rdf:type airo:Consequence ;
    rdfs:label "Financial Penalty Consequence"@en ;
    rdfs:label "经济处罚后果"@zh ;
    airo:hasImpactOnStakeholder ex:GoogleLLC .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌有限责任公司"@zh .