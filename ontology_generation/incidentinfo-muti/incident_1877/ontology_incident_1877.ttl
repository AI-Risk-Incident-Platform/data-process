ex:AIVideoGenerator rdf:type airo:AISystem ;
    rdfs:label "AI Video Generator"@en ;
    rdfs:label "AI视频生成器"@zh ;
    airo:hasRisk ex:PornContentGenerationRisk ;
    airo:hasAIUser ex:UsersJailbreakingAI ;
