    airo:isRiskSourceFor ex:JailbreakForPornGeneration .

ex:JailbreakForPornGeneration rdf:type airo:Misuse ;
    rdfs:label "Jailbreak for Porn Generation"@en ;
    rdfs:label "破解用于色情内容生成"@zh .

ex:UsersJailbreakingAI rdf:type airo:AIUser ;
    rdfs:label "Users Jailbreaking AI"@en ;
    rdfs:label "破解AI的用户"@zh .

ex:UnauthorizedPornContent rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Porn Content"@en ;
