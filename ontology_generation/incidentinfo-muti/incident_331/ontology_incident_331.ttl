@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GreyballSoftware rdf:type airo:AISystem ;
    rdfs:label "Greyball Software"@en ;
    rdfs:label "Greyball软件"@zh ;
    airo:hasRisk ex:RegulatoryEvasionRisk ;
    airo:hasPurpose ex:AvoidRegulatoryEnforcement ;
    airo:isDeployedBy ex:UberTechnologies ;
    airo:isAppliedWithinDomain ex:RideSharingIndustry ;
    airo:hasStakeholder ex:TransportationRegulators .

ex:RegulatoryEvasionRisk rdf:type airo:Risk ;
    rdfs:label "Regulatory Evasion Risk"@en ;
    rdfs:label "规避监管风险"@zh ;
    airo:isRiskSourceFor ex:SecretSoftwareTool ;
    airo:hasConsequence ex:RegulatoryAuthorityDeception .

ex:UberTechnologies rdf:type airo:AIOperator ;
    rdfs:label "Uber Technologies"@en ;
    rdfs:label "优步科技"@zh .

ex:AvoidRegulatoryEnforcement rdf:type airo:Purpose ;
    rdfs:label "Avoid Regulatory Enforcement"@en ;
    rdfs:label "规避监管执法"@zh .

ex:RideSharingIndustry rdf:type airo:Domain ;
    rdfs:label "Ride-Sharing Industry"@en ;
    rdfs:label "共享出行行业"@zh .

ex:SecretSoftwareTool rdf:type airo:RiskSource ;
    rdfs:label "Secret Software Tool"@en ;
    rdfs:label "秘密软件工具"@zh .

ex:RegulatoryAuthorityDeception rdf:type airo:Consequence ;
    rdfs:label "Regulatory Authority Deception"@en ;
    rdfs:label "欺骗监管机构"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:TransportationRegulators .

ex:TransportationRegulators rdf:type airo:Stakeholder ;
    rdfs:label "Transportation Regulators"@en ;
    rdfs:label "交通监管部门"@zh .