@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PoliceRobot rdf:type airo:AISystem ;
    rdfs:label "Police Robot"@en ;
    rdfs:label "警察机器人"@zh ;
    airo:hasRisk ex:IneffectivePublicServiceRisk ;
    airo:hasPurpose ex:PublicSafetyAssistance ;
    airo:isDeployedBy ex:CaliforniaPoliceDepartment ;
    airo:isAppliedWithinDomain ex:LawEnforcement ;
    airo:hasStakeholder ex:CrimeVictim .

ex:IneffectivePublicServiceRisk rdf:type airo:Risk ;
    rdfs:label "Ineffective Public Service Risk"@en ;
    rdfs:label "公共服务失效风险"@zh ;
    airo:hasConsequence ex:FailureToReportCrime ;
    airo:isRiskSourceFor ex:InappropriateBehavior .

ex:FailureToReportCrime rdf:type airo:Consequence ;
    rdfs:label "Failure to Report Crime Consequence"@en ;
    rdfs:label "未能报告犯罪的后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:CrimeVictim rdf:type airo:Stakeholder ;
    rdfs:label "Crime Victim"@en ;
    rdfs:label "犯罪受害者"@zh .

ex:CaliforniaPoliceDepartment rdf:type airo:AIOperator ;
    rdfs:label "California Police Department"@en ;
    rdfs:label "加州警察局"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任度下降的影响"@zh ;
    airo:hasImpactOnStakeholder ex:CrimeVictim .

ex:PublicSafetyAssistance rdf:type airo:Purpose ;
    rdfs:label "Public Safety Assistance Purpose"@en ;
    rdfs:label "公共安全协助的目的"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .

ex:InappropriateBehavior rdf:type airo:Misuse ;
    rdfs:label "Inappropriate Behavior Misuse"@en ;
    rdfs:label "不当行为"@zh .