@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ImageAutocompletionAI rdf:type airo:AISystem ;
    rdfs:label "Image Autocompletion AI"@en ;
    rdfs:label "图像自动补全AI"@zh ;
    airo:hasRisk ex:InappropriateImageGenerationRisk ;
    airo:hasCapability ex:ImageGenerationFromPartialInput ;
    airo:usesTechnique ex:GenerativeAdversarialNetwork .

ex:InappropriateImageGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Image Generation Risk"@en ;
    rdfs:label "不当图像生成风险"@zh ;
    airo:hasConsequence ex:MisrepresentationOfIndividuals ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:MisrepresentationOfIndividuals rdf:type airo:Consequence ;
    rdfs:label "Misrepresentation of Individuals Consequence"@en ;
    rdfs:label "个人形象歪曲后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ImageGenerationFromPartialInput rdf:type airo:AICapability ;
    rdfs:label "Image Generation from Partial Input Capability"@en ;
    rdfs:label "基于部分输入的图像生成能力"@zh .

ex:GenerativeAdversarialNetwork rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Network Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "名誉损害影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .