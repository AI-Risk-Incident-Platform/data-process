@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UnderArmourAICommercial rdf:type airo:AISystem ;
    rdfs:label "Under Armour AI Commercial"@en ;
    rdfs:label "Under Armour AI广告"@zh ;
    airo:hasRisk ex:PlagiarismRisk ;
    airo:hasStakeholder ex:ContentCreators ;
    airo:isAppliedWithinDomain ex:Advertising .

ex:PlagiarismRisk rdf:type airo:Risk ;
    rdfs:label "Plagiarism Risk"@en ;
    rdfs:label "抄袭风险"@zh ;
    airo:hasConsequence ex:ReputationDamage .

ex:ContentCreators rdf:type airo:Stakeholder ;
    rdfs:label "Content Creators"@en ;
    rdfs:label "内容创作者"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage"@en ;
    rdfs:label "声誉损害"@zh ;
    airo:hasImpactOnStakeholder ex:ContentCreators .

ex:Advertising rdf:type airo:Domain ;
    rdfs:label "Advertising"@en ;
    rdfs:label "广告"@zh .