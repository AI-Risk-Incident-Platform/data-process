@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CruiseSelfDrivingCar rdf:type airo:AISystem ;
    rdfs:label "Cruise Self-Driving Car"@en ;
    rdfs:label "Cruise自动驾驶汽车"@zh ;
    airo:hasRisk ex:TrafficCollisionRisk ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTransportation ;
    airo:isDeployedBy ex:GeneralMotors .

ex:TrafficCollisionRisk rdf:type airo:Risk ;
    rdfs:label "Traffic Collision Risk"@en ;
    rdfs:label "交通事故风险"@zh ;
    airo:hasConsequence ex:MultipleInjuries .

ex:MultipleInjuries rdf:type airo:Consequence ;
    rdfs:label "Multiple Injuries Consequence"@en ;
    rdfs:label "多人受伤后果"@zh ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:NHTSA rdf:type airo:Stakeholder ;
    rdfs:label "National Highway Traffic Safety Administration"@en ;
    rdfs:label "美国国家公路交通安全管理局"@zh .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Public Safety Threat Impact"@en ;
    rdfs:label "公共安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:NHTSA .

ex:AutonomousVehicleTransportation rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Transportation Domain"@en ;
    rdfs:label "自动驾驶汽车交通领域"@zh .

ex:GeneralMotors rdf:type airo:AIOperator ;
    rdfs:label "General Motors"@en ;
    rdfs:label "通用汽车公司"@zh .