@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Self-Driving System"@en ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:hasAISubject ex:TeslaVehicleOccupants ;
    airo:hasStakeholder ex:TeslaInc ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTransportation ;
    airo:usesTechnique ex:ComputerVisionBasedAutonomousDriving ;
    airo:hasAIOperator ex:TeslaDriver .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    airo:hasConsequence ex:HumanInjuryFromAutonomousCrash .

ex:HumanInjuryFromAutonomousCrash rdf:type airo:Consequence ;
    rdfs:label "自动驾驶碰撞导致人员受伤的后果"@zh ;
    rdfs:label "Human Injury from Autonomous Crash Consequence"@en ;
    airo:hasImpact ex:PhysicalHarmToVehicleOccupants .

ex:TeslaVehicleOccupants rdf:type airo:AISubject ;
    rdfs:label "特斯拉车辆乘员"@zh ;
    rdfs:label "Tesla Vehicle Occupants"@en .

ex:TeslaInc rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:TeslaDriver rdf:type airo:AIOperator ;
    rdfs:label "特斯拉驾驶员"@zh ;
    rdfs:label "Tesla Driver"@en .

ex:PhysicalHarmToVehicleOccupants rdf:type airo:Impact ;
    rdfs:label "车辆乘员身体伤害的影响"@zh ;
    rdfs:label "Physical Harm to Vehicle Occupants Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaVehicleOccupants .

ex:AutonomousVehicleTransportation rdf:type airo:Domain ;
    rdfs:label "自动驾驶车辆运输领域"@zh ;
    rdfs:label "Autonomous Vehicle Transportation Domain"@en .

ex:ComputerVisionBasedAutonomousDriving rdf:type airo:AITechnique ;
    rdfs:label "基于计算机视觉的自动驾驶技术"@zh ;
    rdfs:label "Computer Vision Based Autonomous Driving Technique"@en .