@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TrueCrimeChannelAI rdf:type airo:AISystem ;
    rdfs:label "True Crime Channel AI"@en ;
    rdfs:label "真实犯罪频道AI"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasPurpose ex:ContentGeneration ;
    airo:hasStakeholder ex:Viewers .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicConfusion .

ex:Viewers rdf:type airo:Stakeholder ;
    rdfs:label "Viewers"@en ;
    rdfs:label "观众"@zh .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion"@en ;
    rdfs:label "公众混淆"@zh ;
    airo:hasImpactOnStakeholder ex:Viewers .

ex:ContentGeneration rdf:type airo:Purpose ;
    rdfs:label "Content Generation"@en ;
    rdfs:label "内容生成"@zh .