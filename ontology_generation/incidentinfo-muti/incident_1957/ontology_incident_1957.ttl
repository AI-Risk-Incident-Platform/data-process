@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TikTokAlgorithm rdf:type airo:AISystem ;
    rdfs:label "TikTok Algorithm"@en ;
    rdfs:label "TikTok算法"@zh ;
    airo:hasRisk ex:ExposureToExtremistContentRisk ;
    airo:hasAISubject ex:Teenagers ;
    airo:hasCapability ex:ContentRecommendation ;
    airo:hasPurpose ex:UserEngagement ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatform ;
    airo:hasStakeholder ex:TikTokCompany .

ex:ExposureToExtremistContentRisk rdf:type airo:Risk ;
    rdfs:label "Exposure to Extremist Content Risk"@en ;
    rdfs:label "接触极端内容风险"@zh ;
    airo:hasConsequence ex:PsychologicalHarm ;
    airo:isRiskSourceFor ex:AlgorithmicRecommendationSystem .

ex:Teenagers rdf:type airo:AISubject ;
    rdfs:label "Teenagers"@en ;
    rdfs:label "青少年"@zh .

ex:PsychologicalHarm rdf:type airo:Consequence ;
    rdfs:label "Psychological Harm Consequence"@en ;
    rdfs:label "心理伤害后果"@zh ;
    airo:hasImpact ex:NegativeMentalHealthEffects .

ex:NegativeMentalHealthEffects rdf:type airo:Impact ;
    rdfs:label "Negative Mental Health Effects Impact"@en ;
    rdfs:label "负面心理健康影响"@zh ;
    airo:hasImpactOnStakeholder ex:Teenagers .

ex:AlgorithmicRecommendationSystem rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Recommendation System Risk Source"@en ;
    rdfs:label "算法推荐系统风险源"@zh .

ex:TikTokCompany rdf:type airo:Stakeholder ;
    rdfs:label "TikTok Company"@en ;
    rdfs:label "TikTok公司"@zh .

ex:ContentRecommendation rdf:type airo:AICapability ;
    rdfs:label "Content Recommendation Capability"@en ;
    rdfs:label "内容推荐能力"@zh .

ex:UserEngagement rdf:type airo:Purpose ;
    rdfs:label "User Engagement Purpose"@en ;
    rdfs:label "用户参与目的"@zh .

ex:SocialMediaPlatform rdf:type airo:Domain ;
    rdfs:label "Social Media Platform Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .