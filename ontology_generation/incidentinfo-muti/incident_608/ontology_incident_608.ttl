@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PublicSectorAITools rdf:type airo:AISystem ;
    rdfs:label "Public Sector AI Tools"@en ;
    rdfs:label "公共部门AI工具"@zh ;
    airo:hasRisk ex:AlgorithmicBiasRisk ;
    airo:hasStakeholder ex:UKGovernment ;
    airo:isAppliedWithinDomain ex:PublicSector .

ex:AlgorithmicBiasRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias Risk"@en ;
    rdfs:label "算法偏见风险"@zh ;
    airo:hasConsequence ex:BenefitSuspensions .

ex:UKGovernment rdf:type airo:Stakeholder ;
    rdfs:label "UK Government"@en ;
    rdfs:label "英国政府"@zh .

ex:PublicSector rdf:type airo:Domain ;
    rdfs:label "Public Sector"@en ;
    rdfs:label "公共部门"@zh .

ex:BenefitSuspensions rdf:type airo:Impact ;
    rdfs:label "Benefit Suspensions Impact"@en ;
    rdfs:label "福利暂停影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKGovernment .