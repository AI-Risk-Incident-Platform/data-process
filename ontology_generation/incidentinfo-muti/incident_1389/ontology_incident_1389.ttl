@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh ;
    airo:hasRisk ex:RacialBiasInFacialRecognition ;
    airo:hasCapability ex:FaceDetectionAndRecognition ;
    airo:usesTechnique ex:DeepLearningForFaceRecognition ;
    airo:isAppliedWithinDomain ex:BiometricsAndSecurity .

ex:RacialBiasInFacialRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Facial Recognition Risk"@en ;
    rdfs:label "面部识别中的种族偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:DiscriminatoryOutcomes .

ex:FaceDetectionAndRecognition rdf:type airo:AICapability ;
    rdfs:label "Face Detection and Recognition Capability"@en ;
    rdfs:label "面部检测与识别能力"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .

ex:DiscriminatoryOutcomes rdf:type airo:Impact ;
    rdfs:label "Discriminatory Outcomes Impact"@en ;
    rdfs:label "歧视性结果的影响"@zh .

ex:DeepLearningForFaceRecognition rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning for Face Recognition Technique"@en ;
    rdfs:label "用于面部识别的深度学习技术"@zh .

ex:BiometricsAndSecurity rdf:type airo:Domain ;
    rdfs:label "Biometrics and Security Domain"@en ;
    rdfs:label "生物识别与安全领域"@zh .