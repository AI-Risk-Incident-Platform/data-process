@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlexaVoiceAssistant rdf:type airo:AISystem ;
    rdfs:label "Alexa Voice Assistant"@en ;
    rdfs:label "Alexa语音助手"@zh ;
    airo:hasRisk ex:FakeNewsDisseminationRisk ;
    airo:hasAICapability ex:VoiceResponseGeneration ;
    airo:isDeployedBy ex:Amazon .

ex:FakeNewsDisseminationRisk rdf:type airo:Risk ;
    rdfs:label "Fake News Dissemination Risk"@en ;
    rdfs:label "虚假新闻传播风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:VoiceAssistantResponseSystem .

ex:VoiceAssistantResponseSystem rdf:type airo:RiskSource ;
    rdfs:label "Voice Assistant Response System"@en ;
    rdfs:label "语音助手响应系统"@zh .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误信虚假信息后果"@zh ;
    airo:hasImpact ex:SocialTrustErosion .

ex:SocialTrustErosion rdf:type airo:Impact ;
    rdfs:label "Social Trust Erosion Impact"@en ;
    rdfs:label "社会信任度下降影响"@zh ;
    airo:hasImpactOnStakeholder ex:Amazon .

ex:VoiceResponseGeneration rdf:type airo:AICapability ;
    rdfs:label "Voice Response Generation Capability"@en ;
    rdfs:label "语音响应生成能力"@zh .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .