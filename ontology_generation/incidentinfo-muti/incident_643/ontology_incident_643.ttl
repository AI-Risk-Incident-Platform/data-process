@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video System"@en ;
    rdfs:label "深度伪造视频系统"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:hasAISubject ex:PublicFigures ;
    airo:hasStakeholder ex:SocialMediaPlatformX .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PublicConfusion ;
    airo:isRiskSourceFor ex:MaliciousContentCreators .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion Consequence"@en ;
    rdfs:label "公众困惑后果"@zh ;
    airo:hasImpact ex:PoliticalTension .

ex:PoliticalTension rdf:type airo:Impact ;
    rdfs:label "Political Tension Impact"@en ;
    rdfs:label "政治紧张影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaPlatformX .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:SocialMediaPlatformX rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Platform X"@en ;
    rdfs:label "社交媒体平台X"@zh .

ex:PublicFigures rdf:type airo:AISubject ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:MaliciousContentCreators rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creators Risk Source"@en ;
    rdfs:label "恶意内容创作者"@zh .