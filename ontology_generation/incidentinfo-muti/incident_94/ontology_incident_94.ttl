@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:StanfordVaccineDistributionSystem rdf:type airo:AISystem ;
    rdfs:label "Stanford Vaccine Distribution System"@en ;
    rdfs:label "斯坦福疫苗分发系统"@zh ;
    airo:hasRisk ex:UnfairVaccineAllocationRisk ;
    airo:hasStakeholder ex:FrontlineMedicalWorkers .

ex:UnfairVaccineAllocationRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Vaccine Allocation Risk"@en ;
    rdfs:label "疫苗分配不公风险"@zh ;
    airo:hasConsequence ex:HealthcareWorkerProtests .

ex:FrontlineMedicalWorkers rdf:type airo:Stakeholder ;
    rdfs:label "Frontline Medical Workers"@en ;
    rdfs:label "一线医护人员"@zh .

ex:HealthcareWorkerProtests rdf:type airo:Consequence ;
    rdfs:label "Healthcare Worker Protests Consequence"@en ;
    rdfs:label "医护人员抗议后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:FrontlineMedicalWorkers .