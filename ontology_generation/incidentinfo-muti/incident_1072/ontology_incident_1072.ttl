@base <https://w3id.org/airo#> .

ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    rdfs:label "Deepfake Video Generation System"@en ;
    airo:hasRisk ex:PoliticalDisinformationRisk ;
    airo:hasAISubject ex:MauricioMacri ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:isAppliedWithinDomain ex:PoliticalCampaign ;
    airo:hasStakeholder ex:ProMileiTrolls .

ex:PoliticalDisinformationRisk rdf:type airo:Risk ;
    rdfs:label "政治虚假信息风险"@zh ;
    rdfs:label "Political Disinformation Risk"@en ;
    airo:hasConsequence ex:ElectionInterference .

ex:ProMileiTrolls rdf:type airo:Stakeholder ;
    rdfs:label "支持Milei的网络水军"@zh ;
    rdfs:label "Pro-Milei Trolls"@en .

ex:MauricioMacri rdf:type airo:AISubject ;
    rdfs:label "Mauricio Macri"@zh ;
    rdfs:label "Mauricio Macri"@en .

ex:ElectionInterference rdf:type airo:Consequence ;
    rdfs:label "选举干预后果"@zh ;
    rdfs:label "Election Interference Consequence"@en ;
    airo:hasImpact ex:PublicTrustErosion .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en .

ex:PoliticalCampaign rdf:type airo:Domain ;
    rdfs:label "政治竞选领域"@zh ;
    rdfs:label "Political Campaign Domain"@en .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:MauricioMacri .