ex:StableDiffusion3 rdf:type airo:AISystem ;
    rdfs:label "Stable Diffusion 3"@en ;
    rdfs:label "Stable Diffusion 3"@zh ;
    airo:hasRisk ex:DisastrousLaunchRisk ;
    airo:isDevelopedBy ex:StabilityAI ;
    airo:isAppliedWithinDomain ex:GenerativeAI .

ex:DisastrousLaunchRisk rdf:type airo:Risk ;
    rdfs:label "Disastrous Launch Risk"@en ;
    rdfs:label "灾难性发布风险"@zh ;
    airo:hasImpact ex:ChangeAILandscape .

ex:ChangeAILandscape rdf:type airo:Impact ;
    rdfs:label "Change AI Landscape Impact"@en ;
    rdfs:label "改变AI格局的影响"@zh .

ex:StabilityAI rdf:type airo:AIDeveloper ;
    rdfs:label "Stability AI"@en ;
    rdfs:label "Stability AI"@zh .

ex:GenerativeAI rdf:type airo:Domain ;
    rdfs:label "Generative AI Domain"@en ;
    rdfs:label "生成式AI领域"@zh .