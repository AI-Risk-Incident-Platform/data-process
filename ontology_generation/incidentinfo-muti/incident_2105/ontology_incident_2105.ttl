@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:ReinforceRacistPolicingRisk ;
    airo:hasCapability ex:EmotionAndGenderPrediction ;
    airo:hasStakeholder ex:NGO ;
    airo:isAppliedWithinDomain ex:PublicTransportationSecurity .

ex:ReinforceRacistPolicingRisk rdf:type airo:Risk ;
    rdfs:label "Reinforce Racist Policing Risk"@en ;
    rdfs:label "强化种族主义警务风险"@zh ;
    airo:hasImpact ex:RacialDiscrimination .

ex:EmotionAndGenderPrediction rdf:type airo:AICapability ;
    rdfs:label "Emotion and Gender Prediction Capability"@en ;
    rdfs:label "情绪和性别预测能力"@zh .

ex:NGO rdf:type airo:Stakeholder ;
    rdfs:label "NGO"@en ;
    rdfs:label "非政府组织"@zh .

ex:RacialDiscrimination rdf:type airo:Impact ;
    rdfs:label "Racial Discrimination Impact"@en ;
    rdfs:label "种族歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:NGO .

ex:PublicTransportationSecurity rdf:type airo:Domain ;
    rdfs:label "Public Transportation Security Domain"@en ;
    rdfs:label "公共交通安保领域"@zh .