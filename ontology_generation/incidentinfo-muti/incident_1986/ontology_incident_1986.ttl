@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OkCupidDatingPlatform rdf:type airo:AISystem ;
    rdfs:label "OkCupid Dating Platform"@en ;
    rdfs:label "OkCupid约会平台"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:OkCupidUsers ;
    airo:hasStakeholder ex:FTC .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:DataSharingWithThirdParties ;
    airo:hasConsequence ex:LegalActionAgainstAICompany .

ex:FTC rdf:type airo:Stakeholder ;
    rdfs:label "Federal Trade Commission"@en ;
    rdfs:label "联邦贸易委员会"@zh .

ex:OkCupidUsers rdf:type airo:AISubject ;
    rdfs:label "OkCupid Users"@en ;
    rdfs:label "OkCupid用户"@zh .

ex:UnauthorizedUseOfPersonalData rdf:type airo:Impact ;
    rdfs:label "Unauthorized Use of Personal Data Impact"@en ;
    rdfs:label "个人数据未经授权使用的影响"@zh ;
    airo:hasImpactOnStakeholder ex:OkCupidUsers .

ex:DataSharingWithThirdParties rdf:type airo:RiskSource ;
    rdfs:label "Data Sharing with Third Parties Risk Source"@en ;
    rdfs:label "与第三方共享数据的风险源"@zh .

ex:LegalActionAgainstAICompany rdf:type airo:Consequence ;
    rdfs:label "Legal Action Against AI Company Consequence"@en ;
    rdfs:label "对AI公司采取法律行动的后果"@zh ;
    airo:hasImpact ex:UnauthorizedUseOfPersonalData .