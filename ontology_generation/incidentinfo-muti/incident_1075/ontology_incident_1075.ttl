ex:LiteratureTranslationAI rdf:type airo:AISystem ;
    rdfs:label "Literature Translation AI"@en ;
    rdfs:label "文学翻译AI"@zh ;
    airo:hasRisk ex:InaccurateLiteraryTranslationRisk ;
    airo:hasCapability ex:LiteraryTextTranslation ;
