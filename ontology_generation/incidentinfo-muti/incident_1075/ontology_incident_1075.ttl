    rdfs:label "Publishing Industry"@en ;
    rdfs:label "出版业"@zh .

ex:LiteraryTextTranslation rdf:type airo:AICapability ;
    rdfs:label "Literary Text Translation Capability"@en ;
    rdfs:label "文学文本翻译能力"@zh .