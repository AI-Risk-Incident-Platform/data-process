@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MaterDeiMedicineRobots rdf:type airo:AISystem ;
    rdfs:label "Mater Dei医疗机器人"@zh ;
    rdfs:label "Mater Dei Medicine Robots"@en ;
    airo:hasRisk ex:OperationalRiskInMedicalRobots ;
    airo:isAppliedWithinDomain ex:Healthcare ;
    airo:hasStakeholder ex:MaterDeiHospital .

ex:OperationalRiskInMedicalRobots rdf:type airo:Risk ;
    rdfs:label "医疗机器人操作风险"@zh ;
    rdfs:label "Operational Risk in Medical Robots"@en ;
    airo:hasConsequence ex:ServiceDisruptionInHealthcare .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "医疗保健领域"@zh ;
    rdfs:label "Healthcare Domain"@en .

ex:MaterDeiHospital rdf:type airo:Stakeholder ;
    rdfs:label "Mater Dei医院"@zh ;
    rdfs:label "Mater Dei Hospital"@en .

ex:ServiceDisruptionInHealthcare rdf:type airo:Consequence ;
    rdfs:label "医疗服务中断后果"@zh ;
    rdfs:label "Service Disruption in Healthcare Consequence"@en .