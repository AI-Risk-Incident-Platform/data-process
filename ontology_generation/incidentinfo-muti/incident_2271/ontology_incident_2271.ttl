@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:IdentityFraudRisk ;
    airo:hasCapability ex:RealisticMediaGeneration ;
    airo:hasStakeholder ex:GeneralPublic .

ex:IdentityFraudRisk rdf:type airo:Risk ;
    rdfs:label "Identity Fraud Risk"@en ;
    rdfs:label "身份欺诈风险"@zh ;
    airo:hasConsequence ex:SocialTrustErosion .

ex:RealisticMediaGeneration rdf:type airo:AICapability ;
    rdfs:label "Realistic Media Generation Capability"@en ;
    rdfs:label "逼真媒体生成能力"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "普通公众"@zh .

ex:SocialTrustErosion rdf:type airo:Consequence ;
    rdfs:label "Social Trust Erosion Consequence"@en ;
    rdfs:label "社会信任侵蚀后果"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .