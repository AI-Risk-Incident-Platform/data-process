@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DisneyEmployeeAITool rdf:type airo:AISystem ;
    rdfs:label "Disney Employee AI Tool"@en ;
    rdfs:label "迪士尼员工AI工具"@zh ;
    airo:hasRisk ex:HackRisk ;
    airo:hasStakeholder ex:DisneyEmployee .

ex:HackRisk rdf:type airo:Risk ;
    rdfs:label "Hack Risk"@en ;
    rdfs:label "黑客攻击风险"@zh ;
    airo:hasConsequence ex:LifeRuined .

ex:DisneyEmployee rdf:type airo:Stakeholder ;
    rdfs:label "Disney Employee"@en ;
    rdfs:label "迪士尼员工"@zh .

ex:LifeRuined rdf:type airo:Impact ;
    rdfs:label "Life Ruined"@en ;
    rdfs:label "生活被毁"@zh ;
    airo:hasImpactOnStakeholder ex:DisneyEmployee .