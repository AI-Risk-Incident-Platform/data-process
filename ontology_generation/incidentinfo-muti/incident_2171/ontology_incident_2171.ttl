@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AllstateAutoInsuranceAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Allstate汽车保险算法"@zh ;
    rdfs:label "Allstate Auto Insurance Algorithm"@en ;
    airo:hasRisk ex:UnfairPricingRisk ;
    airo:isDeployedBy ex:AllstateCorporation ;
    airo:hasStakeholder ex:AutoInsuranceCustomers ;
    airo:isAppliedWithinDomain ex:AutoInsuranceIndustry .

ex:UnfairPricingRisk rdf:type airo:Risk ;
    rdfs:label "不公平定价风险"@zh ;
    rdfs:label "Unfair Pricing Risk"@en ;
    airo:hasConsequence ex:HigherInsurancePremiums .

ex:AutoInsuranceCustomers rdf:type airo:Stakeholder ;
    rdfs:label "汽车保险客户"@zh ;
    rdfs:label "Auto Insurance Customers"@en .

ex:AllstateCorporation rdf:type airo:AIOperator ;
    rdfs:label "Allstate公司"@zh ;
    rdfs:label "Allstate Corporation"@en .

ex:AutoInsuranceIndustry rdf:type airo:Domain ;
    rdfs:label "汽车保险行业"@zh ;
    rdfs:label "Auto Insurance Industry"@en .

ex:HigherInsurancePremiums rdf:type airo:Consequence ;
    rdfs:label "更高保险费后果"@zh ;
    rdfs:label "Higher Insurance Premiums Consequence"@en ;
    airo:hasImpact ex:FinancialBurdenOnCustomers .

ex:FinancialBurdenOnCustomers rdf:type airo:Impact ;
    rdfs:label "客户财务负担影响"@zh ;
    rdfs:label "Financial Burden on Customers Impact"@en ;
    airo:hasImpactOnStakeholder ex:AutoInsuranceCustomers .