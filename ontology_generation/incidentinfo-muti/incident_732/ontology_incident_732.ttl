@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAIModels rdf:type airo:AISystem ;
    rdfs:label "生成式AI模型"@zh ;
    rdfs:label "Generative AI Models"@en ;
    airo:hasRisk ex:RussianDisinformationSpreadRisk ;
    airo:hasCapability ex:ContentGeneration ;
    airo:usesTechnique ex:NaturalLanguageGeneration ;
    airo:hasStakeholder ex:RussianGovernment .

ex:RussianDisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "俄罗斯虚假信息传播风险"@zh ;
    rdfs:label "Russian Disinformation Spread Risk"@en ;
    airo:isRiskSourceFor ex:FakeNewsSites ;
    airo:hasConsequence ex:PropagandaDissemination .

ex:RussianGovernment rdf:type airo:Stakeholder ;
    rdfs:label "俄罗斯政府"@zh ;
    rdfs:label "Russian Government"@en .

ex:PropagandaDissemination rdf:type airo:Consequence ;
    rdfs:label "宣传传播后果"@zh ;
    rdfs:label "Propaganda Dissemination Consequence"@en ;
    airo:hasImpact ex:PublicMisinformation .

ex:PublicMisinformation rdf:type airo:Impact ;
    rdfs:label "公众误导影响"@zh ;
    rdfs:label "Public Misinformation Impact"@en ;
    airo:hasImpactOnStakeholder ex:RussianGovernment .

ex:ContentGeneration rdf:type airo:AICapability ;
    rdfs:label "内容生成能力"@zh ;
    rdfs:label "Content Generation Capability"@en .

ex:FakeNewsSites rdf:type airo:RiskSource ;
    rdfs:label "虚假新闻网站风险源"@zh ;
    rdfs:label "Fake News Sites Risk Source"@en .

ex:NaturalLanguageGeneration rdf:type airo:AITechnique ;
    rdfs:label "自然语言生成技术"@zh ;
    rdfs:label "Natural Language Generation Technique"@en .