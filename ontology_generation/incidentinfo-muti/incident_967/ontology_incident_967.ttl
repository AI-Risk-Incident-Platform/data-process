@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:PoliticalDisinformationRisk ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasPurpose ex:PoliticalManipulation ;
    airo:hasStakeholder ex:PoliticalFigures ;
    airo:isAppliedWithinDomain ex:PoliticalCommunication .

ex:PoliticalDisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Disinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicTrustErosion ;
    airo:isRiskSourceFor ex:UnreliableInformationSources ;
    airo:hasRiskControl ex:PoliticalSmearCampaign .

ex:PublicTrustErosion rdf:type airo:Consequence ;
    rdfs:label "Public Trust Erosion Consequence"@en ;
    rdfs:label "公众信任侵蚀后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:PoliticalFigures rdf:type airo:Stakeholder ;
    rdfs:label "Political Figures"@en ;
    rdfs:label "政治人物"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:PoliticalFigures .

ex:PoliticalManipulation rdf:type airo:Purpose ;
    rdfs:label "Political Manipulation Purpose"@en ;
    rdfs:label "政治操纵目的"@zh .

ex:PoliticalCommunication rdf:type airo:Domain ;
    rdfs:label "Political Communication Domain"@en ;
    rdfs:label "政治传播领域"@zh .

ex:UnreliableInformationSources rdf:type airo:RiskSource ;
    rdfs:label "Unreliable Information Sources Risk Source"@en ;
    rdfs:label "不可靠信息来源风险源"@zh .

ex:PoliticalSmearCampaign rdf:type airo:Misuse ;
    rdfs:label "Political Smear Campaign Misuse"@en ;
    rdfs:label "政治诽谤活动滥用"@zh .