@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GirlsOnlyAppWithGenderScreeningAI rdf:type airo:AISystem ;
    rdfs:label "Girls-Only App with Gender Screening AI"@en ;
    rdfs:label "仅限女孩的性别筛查AI应用"@zh ;
    airo:hasRisk ex:GenderDiscriminationRisk ;
    airo:hasCapability ex:GenderRecognitionCapability ;
    airo:hasPurpose ex:CreateFemaleOnlySocialSpace .

ex:GenderDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Gender Discrimination Risk"@en ;
    rdfs:label "性别歧视风险"@zh ;
    airo:isRiskSourceFor ex:GenderScreeningAlgorithm .

ex:GenderRecognitionCapability rdf:type airo:AICapability ;
    rdfs:label "Gender Recognition Capability"@en ;
    rdfs:label "性别识别能力"@zh .

ex:CreateFemaleOnlySocialSpace rdf:type airo:Purpose ;
    rdfs:label "Create Female-Only Social Space"@en ;
    rdfs:label "创建女性专属社交空间"@zh .

ex:GenderScreeningAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Gender Screening Algorithm"@en ;
    rdfs:label "性别筛查算法"@zh .