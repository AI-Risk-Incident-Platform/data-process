@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:InternetCensorshipSystem rdf:type airo:AISystem ;
    rdfs:label "Internet Censorship System"@en ;
    rdfs:label "互联网审查系统"@zh ;
    airo:hasRisk ex:CensorshipCircumventionRisk ;
    airo:usesTechnique ex:ContentFiltering ;
    airo:hasStakeholder ex:ChineseCitizens .

ex:CensorshipCircumventionRisk rdf:type airo:Risk ;
    rdfs:label "Censorship Circumvention Risk"@en ;
    rdfs:label "审查规避风险"@zh ;
    airo:hasConsequence ex:InformationControlBypass .

ex:ChineseCitizens rdf:type airo:Stakeholder ;
    rdfs:label "Chinese Citizens"@en ;
    rdfs:label "中国公民"@zh .

ex:ContentFiltering rdf:type airo:AITechnique ;
    rdfs:label "Content Filtering Technique"@en ;
    rdfs:label "内容过滤技术"@zh .

ex:InformationControlBypass rdf:type airo:Consequence ;
    rdfs:label "Information Control Bypass Consequence"@en ;
    rdfs:label "信息控制绕过后果"@zh .