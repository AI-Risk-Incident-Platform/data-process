@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookContentModerationAI rdf:type airo:AISystem ;
    rdfs:label "Facebook Content Moderation AI"@en ;
    rdfs:label "Facebook内容审核AI"@zh ;
    airo:hasRisk ex:LaborExploitationRisk ;
    airo:isDeployedBy ex:FacebookInc ;
    airo:hasStakeholder ex:AfricanContentModerators ;
    airo:isAppliedWithinDomain ex:SocialMediaContentModeration .

ex:LaborExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Labor Exploitation Risk"@en ;
    rdfs:label "劳工剥削风险"@zh ;
    airo:hasImpact ex:WorkerExploitationImpact .

ex:AfricanContentModerators rdf:type airo:Stakeholder ;
    rdfs:label "African Content Moderators"@en ;
    rdfs:label "非洲内容审核员"@zh .

ex:FacebookInc rdf:type airo:AIOperator ;
    rdfs:label "Facebook Inc."@en ;
    rdfs:label "Facebook公司"@zh .

ex:WorkerExploitationImpact rdf:type airo:Impact ;
    rdfs:label "Worker Exploitation Impact"@en ;
    rdfs:label "工人剥削影响"@zh ;
    airo:hasImpactOnStakeholder ex:AfricanContentModerators .

ex:SocialMediaContentModeration rdf:type airo:Domain ;
    rdfs:label "Social Media Content Moderation Domain"@en ;
    rdfs:label "社交媒体内容审核领域"@zh .