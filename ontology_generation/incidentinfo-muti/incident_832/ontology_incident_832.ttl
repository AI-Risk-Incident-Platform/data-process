@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIRumorGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Rumor Generation System"@en ;
    rdfs:label "AI谣言生成系统"@zh ;
    airo:hasRisk ex:SocialOrderDisruptionRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:PublicSecurityOrgans .

ex:SocialOrderDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Social Order Disruption Risk"@en ;
    rdfs:label "社会秩序破坏风险"@zh ;
    airo:hasConsequence ex:PublicPanic ;
    airo:isRiskSourceFor ex:MaliciousContentCreation .

ex:PublicSecurityOrgans rdf:type airo:Stakeholder ;
    rdfs:label "Public Security Organs"@en ;
    rdfs:label "公安机关"@zh .

ex:PublicPanic rdf:type airo:Consequence ;
    rdfs:label "Public Panic Consequence"@en ;
    rdfs:label "公众恐慌后果"@zh ;
    airo:hasImpact ex:SocialStabilityThreat .

ex:SocialStabilityThreat rdf:type airo:Impact ;
    rdfs:label "Social Stability Threat Impact"@en ;
    rdfs:label "社会稳定威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:PublicSecurityOrgans .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:MaliciousContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creation Risk Source"@en ;
    rdfs:label "恶意内容创作风险源"@zh .