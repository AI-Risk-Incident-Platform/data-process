ex:PublicOutrage rdf:type airo:Consequence ;
    rdfs:label "Public Outrage Consequence"@en ;
    rdfs:label "公众愤怒后果"@zh ;
    airo:hasImpact ex:DamageToRoyalFamilyReputation .

ex:DamageToRoyalFamilyReputation rdf:type airo:Impact ;
    rdfs:label "Damage to Royal Family Reputation Impact"@en ;
    rdfs:label "损害王室声誉影响"@zh ;
