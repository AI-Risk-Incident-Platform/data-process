ex:DeepfakeQueenSpeech rdf:type airo:AISystem ;
    rdfs:label "Deepfake Queen Speech"@en ;
    rdfs:label "女王演讲深度伪造"@zh ;
    airo:hasRisk ex:PublicMisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:ChristmasEntertainment ;
    airo:isAppliedWithinDomain ex:MediaBroadcasting ;
    airo:hasStakeholder ex:UKViewers .

ex:PublicMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Public Misinformation Risk"@en ;
    rdfs:label "公众误导风险"@zh ;
