ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:UKViewers rdf:type airo:Stakeholder ;
    rdfs:label "UK Viewers"@en ;
    rdfs:label "英国观众"@zh .

ex:ChristmasEntertainment rdf:type airo:Purpose ;
    rdfs:label "Christmas Entertainment Purpose"@en ;
    rdfs:label "圣诞娱乐目的"@zh .

ex:MediaBroadcasting rdf:type airo:Domain ;
