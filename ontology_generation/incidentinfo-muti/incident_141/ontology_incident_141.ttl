@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BrandSafetyTech rdf:type airo:AISystem ;
    rdfs:label "Brand Safety Tech"@en ;
    rdfs:label "品牌安全技术"@zh ;
    airo:hasRisk ex:NewsDefundingRisk ;
    airo:hasStakeholder ex:NewsOrganizations .

ex:NewsDefundingRisk rdf:type airo:Risk ;
    rdfs:label "News Defunding Risk"@en ;
    rdfs:label "新闻资金削减风险"@zh ;
    airo:hasConsequence ex:FinancialImpactOnNewsOrganizations ;
    airo:isRiskSourceFor ex:InaccurateContentClassification .

ex:FinancialImpactOnNewsOrganizations rdf:type airo:Impact ;
    rdfs:label "Financial Impact on News Organizations"@en ;
    rdfs:label "对新闻机构的财务影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewsOrganizations .

ex:NewsOrganizations rdf:type airo:Stakeholder ;
    rdfs:label "News Organizations"@en ;
    rdfs:label "新闻机构"@zh .

ex:InaccurateContentClassification rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate Content Classification"@en ;
    rdfs:label "不准确的内容分类"@zh .