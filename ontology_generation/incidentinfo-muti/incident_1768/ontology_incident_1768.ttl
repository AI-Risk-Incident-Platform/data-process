@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookEmojiReactionSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Emoji Reaction System"@en ;
    rdfs:label "Facebook表情反应系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInContentEngagement ;
    airo:hasStakeholder ex:FacebookUsers ;
    airo:isDeployedBy ex:FacebookInc .

ex:AlgorithmicBiasInContentEngagement rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Content Engagement Risk"@en ;
    rdfs:label "内容参与度中的算法偏见风险"@zh ;
    airo:hasConsequence ex:DistortedContentEngagementMetrics .

ex:FacebookUsers rdf:type airo:Stakeholder ;
    rdfs:label "Facebook Users"@en ;
    rdfs:label "Facebook用户"@zh .

ex:FacebookInc rdf:type airo:AIOperator ;
    rdfs:label "Facebook Inc."@en ;
    rdfs:label "Facebook公司"@zh .

ex:DistortedContentEngagementMetrics rdf:type airo:Impact ;
    rdfs:label "Distorted Content Engagement Metrics Impact"@en ;
    rdfs:label "内容参与度指标失真的影响"@zh ;
    airo:hasImpactOnStakeholder ex:FacebookUsers .