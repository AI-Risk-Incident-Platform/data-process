@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:isAppliedWithinDomain ex:AutonomousVehicles ;
    airo:hasStakeholder ex:USRegulatoryAuthorities .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆致命风险"@zh ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    airo:hasConsequence ex:FatalCrashConsequence ;
    airo:isRiskSourceFor ex:AutopilotSystemFailure .

ex:FatalCrashConsequence rdf:type airo:Consequence ;
    rdfs:label "致命碰撞后果"@zh ;
    rdfs:label "Fatal Crash Consequence"@en ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:USRegulatoryAuthorities rdf:type airo:Stakeholder ;
    rdfs:label "美国监管机构"@zh ;
    rdfs:label "US Regulatory Authorities"@en ;
    airo:hasImpactOnStakeholder ex:PublicSafetyThreat .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:AutonomousVehicles rdf:type airo:Domain ;
    rdfs:label "自动驾驶汽车领域"@zh ;
    rdfs:label "Autonomous Vehicles Domain"@en .

ex:AutopilotSystemFailure rdf:type airo:RiskSource ;
    rdfs:label "自动驾驶系统故障风险源"@zh ;
    rdfs:label "Autopilot System Failure Risk Source"@en .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "公共安全威胁影响"@zh ;
    rdfs:label "Public Safety Threat Impact"@en .