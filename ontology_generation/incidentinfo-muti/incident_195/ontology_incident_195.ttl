@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:Flipagram rdf:type airo:AISystem ;
    rdfs:label "Flipagram"@en ;
    rdfs:label "Flipagram"@zh ;
    airo:hasRisk ex:ContentScrapingRisk ;
    airo:hasStakeholder ex:ByteDance ;
    airo:hasAIOperator ex:ByteDance .

ex:ContentScrapingRisk rdf:type airo:Risk ;
    rdfs:label "Content Scraping Risk"@en ;
    rdfs:label "内容抓取风险"@zh ;
    airo:hasConsequence ex:IntellectualPropertyViolation ;
    airo:isRiskSourceFor ex:FakeAccountsCreation ;
    airo:hasMisuse ex:UnauthorizedContentScraping .

ex:ByteDance rdf:type airo:Stakeholder ;
    rdfs:label "ByteDance"@en ;
    rdfs:label "字节跳动"@zh ;
    rdf:type airo:AIOperator .

ex:IntellectualPropertyViolation rdf:type airo:Consequence ;
    rdfs:label "Intellectual Property Violation Consequence"@en ;
    rdfs:label "知识产权侵犯后果"@zh ;
    airo:hasImpact ex:PlatformCompetitionDistortion .

ex:PlatformCompetitionDistortion rdf:type airo:Impact ;
    rdfs:label "Platform Competition Distortion Impact"@en ;
    rdfs:label "平台竞争扭曲影响"@zh ;
    airo:hasImpactOnStakeholder ex:ByteDance .

ex:FakeAccountsCreation rdf:type airo:RiskSource ;
    rdfs:label "Fake Accounts Creation Risk Source"@en ;
    rdfs:label "虚假账户创建风险源"@zh .

ex:UnauthorizedContentScraping rdf:type airo:Misuse ;
    rdfs:label "Unauthorized Content Scraping Misuse"@en ;
    rdfs:label "未经授权内容抓取滥用"@zh .