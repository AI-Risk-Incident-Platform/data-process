@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:StabilityAIArtGenerator rdf:type airo:AISystem ;
    rdfs:label "Stability AI艺术生成器"@zh ;
    rdfs:label "Stability AI Art Generator"@en ;
    airo:hasRisk ex:IntellectualPropertyViolationRisk ;
    airo:isDeployedBy ex:StabilityAI ;
    airo:hasStakeholder ex:GettyImages ;
    airo:hasPurpose ex:AIArtGeneration ;
    airo:isAppliedWithinDomain ex:DigitalArtCreation .

ex:IntellectualPropertyViolationRisk rdf:type airo:Risk ;
    rdfs:label "知识产权侵权风险"@zh ;
    rdfs:label "Intellectual Property Violation Risk"@en ;
    airo:hasConsequence ex:LegalActionConsequence ;
    airo:isRiskSourceFor ex:UnauthorizedDataUse .

ex:GettyImages rdf:type airo:Stakeholder ;
    rdfs:label "盖蒂图片社"@zh ;
    rdfs:label "Getty Images"@en .

ex:StabilityAI rdf:type airo:AIOperator ;
    rdfs:label "Stability AI公司"@zh ;
    rdfs:label "Stability AI"@en .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "法律诉讼后果"@zh ;
    rdfs:label "Legal Action Consequence"@en ;
    airo:hasImpactOnStakeholder ex:GettyImages .

ex:UnauthorizedDataUse rdf:type airo:RiskSource ;
    rdfs:label "未经授权数据使用"@zh ;
    rdfs:label "Unauthorized Data Use"@en .

ex:DigitalArtCreation rdf:type airo:Domain ;
    rdfs:label "数字艺术创作领域"@zh ;
    rdfs:label "Digital Art Creation Domain"@en .

ex:AIArtGeneration rdf:type airo:Purpose ;
    rdfs:label "AI艺术生成目的"@zh ;
    rdfs:label "AI Art Generation Purpose"@en .