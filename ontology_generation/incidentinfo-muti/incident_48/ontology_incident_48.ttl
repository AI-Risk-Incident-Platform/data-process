@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DAOOrganization rdf:type airo:AISystem ;
    rdfs:label "DAO Organization"@en ;
    rdfs:label "DAO组织"@zh ;
    airo:hasRisk ex:DAOAttackRisk ;
    airo:usesTechnique ex:SmartContractTechnology ;
    airo:isAppliedWithinDomain ex:DecentralizedFinanceDomain ;
    airo:hasStakeholder ex:EthereumCommunity .

ex:DAOAttackRisk rdf:type airo:Risk ;
    rdfs:label "DAO Attack Risk"@en ;
    rdfs:label "DAO攻击风险"@zh ;
    airo:hasConsequence ex:FundsLossConsequence ;
    airo:isRiskSourceFor ex:SmartContractVulnerabilityRiskSource .

ex:FundsLossConsequence rdf:type airo:Consequence ;
    rdfs:label "Funds Loss Consequence"@en ;
    rdfs:label "资金损失后果"@zh ;
    airo:hasImpact ex:TrustLossInDecentralizedSystemsImpact .

ex:EthereumCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Ethereum Community"@en ;
    rdfs:label "以太坊社区"@zh .

ex:SmartContractVulnerabilityRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Smart Contract Vulnerability Risk Source"@en ;
    rdfs:label "智能合约漏洞风险源"@zh .

ex:TrustLossInDecentralizedSystemsImpact rdf:type airo:Impact ;
    rdfs:label "Trust Loss in Decentralized Systems Impact"@en ;
    rdfs:label "去中心化系统信任丧失的影响"@zh ;
    airo:hasImpactOnStakeholder ex:EthereumCommunity .

ex:SmartContractTechnology rdf:type airo:AITechnique ;
    rdfs:label "Smart Contract Technology"@en ;
    rdfs:label "智能合约技术"@zh .

ex:DecentralizedFinanceDomain rdf:type airo:Domain ;
    rdfs:label "Decentralized Finance Domain"@en ;
    rdfs:label "去中心化金融领域"@zh .