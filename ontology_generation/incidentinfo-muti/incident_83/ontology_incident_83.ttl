@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:KidneyTransplantAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Kidney Transplant Algorithm"@en ;
    rdfs:label "肾脏移植算法"@zh ;
    airo:hasRisk ex:RacialBiasInMedicalAlgorithm ;
    airo:hasPurpose ex:KidneyFunctionAssessment ;
    airo:hasStakeholder ex:BlackPatients ;
    airo:isAppliedWithinDomain ex:Healthcare .

ex:RacialBiasInMedicalAlgorithm rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Medical Algorithm Risk"@en ;
    rdfs:label "医疗算法中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DeniedKidneyTransplantsForBlackPatients ;
    airo:isRiskSourceFor ex:RaceMultiplierInGFRCalculation .

ex:DeniedKidneyTransplantsForBlackPatients rdf:type airo:Consequence ;
    rdfs:label "Denied Kidney Transplants for Black Patients Consequence"@en ;
    rdfs:label "黑人患者被拒绝肾脏移植的后果"@zh ;
    airo:hasImpact ex:HealthDisparitiesForAfricanAmericans .

ex:HealthDisparitiesForAfricanAmericans rdf:type airo:Impact ;
    rdfs:label "Health Disparities for African Americans Impact"@en ;
    rdfs:label "非裔美国人健康差异的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackPatients .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "Healthcare Domain"@en ;
    rdfs:label "医疗保健领域"@zh .

ex:BlackPatients rdf:type airo:Stakeholder ;
    rdfs:label "Black Patients"@en ;
    rdfs:label "黑人患者"@zh .

ex:RaceMultiplierInGFRCalculation rdf:type airo:RiskSource ;
    rdfs:label "Race Multiplier in GFR Calculation Risk Source"@en ;
    rdfs:label "肾小球滤过率计算中的种族乘数风险源"@zh .

ex:KidneyFunctionAssessment rdf:type airo:Purpose ;
    rdfs:label "Kidney Function Assessment Purpose"@en ;
    rdfs:label "肾功能评估的目的"@zh .