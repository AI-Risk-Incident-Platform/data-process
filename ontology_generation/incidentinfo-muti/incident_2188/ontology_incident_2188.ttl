@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ScottishExamResultsAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Scottish Exam Results Algorithm"@en ;
    rdfs:label "苏格兰考试成绩算法"@zh ;
    airo:hasRisk ex:UnfairGradingRisk ;
    airo:isAppliedWithinDomain ex:EducationSystem .

ex:UnfairGradingRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Grading Risk"@en ;
    rdfs:label "不公平评分风险"@zh ;
    airo:hasConsequence ex:InaccurateExamResults .

ex:InaccurateExamResults rdf:type airo:Consequence ;
    rdfs:label "Inaccurate Exam Results Consequence"@en ;
    rdfs:label "考试成绩不准确后果"@zh ;
    airo:hasImpact ex:StudentOpportunityLoss .

ex:StudentOpportunityLoss rdf:type airo:Impact ;
    rdfs:label "Student Opportunity Loss Impact"@en ;
    rdfs:label "学生机会损失影响"@zh .

ex:EducationSystem rdf:type airo:Domain ;
    rdfs:label "Education System Domain"@en ;
    rdfs:label "教育系统领域"@zh .