@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:Snapchat rdf:type airo:AISystem ;
    rdfs:label "Snapchat"@en ;
    rdfs:label "Snapchat"@zh ;
    airo:hasRisk ex:ChildSafetyRisk ;
    airo:hasStakeholder ex:Children .

ex:ChildSafetyRisk rdf:type airo:Risk ;
    rdfs:label "Child Safety Risk"@en ;
    rdfs:label "儿童安全风险"@zh ;
    airo:hasConsequence ex:PotentialHarmToChildren ;
    airo:isRiskSourceFor ex:ConvictedSexOffenders .

ex:Children rdf:type airo:Stakeholder ;
    rdfs:label "Children"@en ;
    rdfs:label "儿童"@zh .

ex:PotentialHarmToChildren rdf:type airo:Consequence ;
    rdfs:label "Potential Harm to Children"@en ;
    rdfs:label "对儿童的潜在伤害"@zh ;
    airo:hasImpactOnStakeholder ex:Children .

ex:ConvictedSexOffenders rdf:type airo:RiskSource ;
    rdfs:label "Convicted Sex Offenders"@en ;
    rdfs:label "已定罪的性犯罪者"@zh .