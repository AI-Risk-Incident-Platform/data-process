@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video System"@en ;
    rdfs:label "深度伪造视频系统"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:MaliPublic .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:SocialUnrest ;
    airo:isRiskSourceFor ex:UncontrolledDeepfakeRelease .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:MaliPublic rdf:type airo:Stakeholder ;
    rdfs:label "Mali Public"@en ;
    rdfs:label "马里公众"@zh .

ex:SocialUnrest rdf:type airo:Consequence ;
    rdfs:label "Social Unrest Consequence"@en ;
    rdfs:label "社会动荡后果"@zh ;
    airo:hasImpactOnStakeholder ex:MaliPublic .

ex:UncontrolledDeepfakeRelease rdf:type airo:RiskSource ;
    rdfs:label "Uncontrolled Deepfake Release Risk Source"@en ;
    rdfs:label "不受控的深度伪造发布风险源"@zh .