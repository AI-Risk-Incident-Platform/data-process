@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VolkswagenFrontAssistSystem rdf:type airo:AISystem ;
    rdfs:label "大众前部辅助系统"@zh ;
    rdfs:label "Volkswagen Front Assist System"@en ;
    airo:hasRisk ex:AutomaticEmergencyBrakingFailureRisk ;
    airo:isDevelopedBy ex:VolkswagenAG ;
    airo:hasStakeholder ex:VolkswagenGroupOfAmerica ;
    airo:hasPurpose ex:CollisionAvoidance ;
    airo:isAppliedWithinDomain ex:AutomotiveSafetySystems .

ex:AutomaticEmergencyBrakingFailureRisk rdf:type airo:Risk ;
    rdfs:label "自动紧急制动失效风险"@zh ;
    rdfs:label "Automatic Emergency Braking Failure Risk"@en ;
    airo:hasConsequence ex:PotentialCollisionAccidents .

ex:VolkswagenGroupOfAmerica rdf:type airo:Stakeholder ;
    rdfs:label "美国大众汽车集团"@zh ;
    rdfs:label "Volkswagen Group of America"@en .

ex:VolkswagenAG rdf:type airo:AIOperator ;
    rdfs:label "大众汽车集团"@zh ;
    rdfs:label "Volkswagen AG"@en .

ex:PotentialCollisionAccidents rdf:type airo:Consequence ;
    rdfs:label "潜在碰撞事故后果"@zh ;
    rdfs:label "Potential Collision Accidents Consequence"@en .

ex:AutomotiveSafetySystems rdf:type airo:Domain ;
    rdfs:label "汽车安全系统领域"@zh ;
    rdfs:label "Automotive Safety Systems Domain"@en .

ex:CollisionAvoidance rdf:type airo:Purpose ;
    rdfs:label "碰撞避免目的"@zh ;
    rdfs:label "Collision Avoidance Purpose"@en .