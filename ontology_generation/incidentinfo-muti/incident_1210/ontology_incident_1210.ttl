@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaAIChatbots rdf:type airo:AISystem ;
    rdfs:label "Meta人工智能聊天机器人"@zh ;
    rdfs:label "Meta AI Chatbots"@en ;
    airo:hasRisk ex:ControversialFigureImitationRisk ;
    airo:hasStakeholder ex:MetaPlatformUsers ;
    airo:isDeployedBy ex:MetaCompany .

ex:ControversialFigureImitationRisk rdf:type airo:Risk ;
    rdfs:label "争议人物模仿风险"@zh ;
    rdfs:label "Controversial Figure Imitation Risk"@en ;
    airo:hasConsequence ex:PublicControversy ;
    airo:isRiskSourceFor ex:HistoricalFigureMisrepresentation .

ex:MetaPlatformUsers rdf:type airo:Stakeholder ;
    rdfs:label "Meta平台用户"@zh ;
    rdfs:label "Meta Platform Users"@en .

ex:MetaCompany rdf:type airo:AIOperator ;
    rdfs:label "Meta公司"@zh ;
    rdfs:label "Meta Company"@en .

ex:PublicControversy rdf:type airo:Consequence ;
    rdfs:label "公众争议后果"@zh ;
    rdfs:label "Public Controversy Consequence"@en ;
    airo:hasImpactOnStakeholder ex:MetaPlatformUsers .

ex:HistoricalFigureMisrepresentation rdf:type airo:Misuse ;
    rdfs:label "历史人物错误表征滥用"@zh ;
    rdfs:label "Historical Figure Misrepresentation Misuse"@en .