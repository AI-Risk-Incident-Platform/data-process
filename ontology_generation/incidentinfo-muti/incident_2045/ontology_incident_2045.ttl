ex:SanTORobot rdf:type airo:AISystem ;
    rdfs:label "Catholic Robot SanTO"@en ;
    rdfs:label "天主教机器人SanTO"@zh ;
    airo:hasRisk ex:EthicalAndReligiousImpactRisk ;
    airo:hasPurpose ex:ProvidingReligiousSupport ;
    airo:hasStakeholder ex:ReligiousCommunities ;
    airo:isAppliedWithinDomain ex:ReligiousServices .

ex:EthicalAndReligiousImpactRisk rdf:type airo:Risk ;
    rdfs:label "Ethical and Religious Impact Risk"@en ;
    rdfs:label "伦理和宗教影响风险"@zh .

ex:ReligiousCommunities rdf:type airo:Stakeholder ;
    rdfs:label "Religious Communities"@en ;
    rdfs:label "宗教社区"@zh .

ex:ReligiousServices rdf:type airo:Domain ;
    rdfs:label "Religious Services"@en ;
    rdfs:label "宗教服务"@zh .

ex:ProvidingReligiousSupport rdf:type airo:Purpose ;
    rdfs:label "Providing Religious Support"@en ;
    rdfs:label "提供宗教支持"@zh .