ex:VoiceverseNFTPlatform rdf:type airo:AISystem ;
    rdfs:label "Voiceverse NFT Platform"@en ;
    rdfs:label "Voiceverse NFT平台"@zh ;
    airo:hasRisk ex:IntellectualPropertyTheftRisk ;
    airo:isDeployedBy ex:Voiceverse ;
    airo:hasStakeholder ex:TroyBaker ;
    airo:isAppliedWithinDomain ex:NFTMarketplaceDomain .

ex:IntellectualPropertyTheftRisk rdf:type airo:Risk ;
    rdfs:label "Intellectual Property Theft Risk"@en ;
    rdfs:label "知识产权盗窃风险"@zh .

ex:TroyBaker rdf:type airo:Stakeholder ;
    rdfs:label "Troy Baker"@en ;
    rdfs:label "特洛伊·贝克"@zh .

ex:Voiceverse rdf:type airo:AIOperator ;
    rdfs:label "Voiceverse"@en ;
    rdfs:label "Voiceverse公司"@zh .

ex:NFTMarketplaceDomain rdf:type airo:Domain ;
    rdfs:label "NFT Marketplace Domain"@en ;
    rdfs:label "NFT市场领域"@zh .