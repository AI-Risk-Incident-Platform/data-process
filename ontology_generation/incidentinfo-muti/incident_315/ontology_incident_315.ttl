@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookAdvertisingSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Advertising System"@en ;
    rdfs:label "Facebook广告系统"@zh ;
    airo:hasRisk ex:AdvertisingFraudRisk ;
    airo:usesTechnique ex:TargetedAdvertisingAlgorithm ;
    airo:hasPurpose ex:DigitalAdvertisingPlatform ;
    airo:hasStakeholder ex:Advertisers ;
    airo:isAppliedWithinDomain ex:DigitalMarketing .

ex:AdvertisingFraudRisk rdf:type airo:Risk ;
    rdfs:label "Advertising Fraud Risk"@en ;
    rdfs:label "广告欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:AlgorithmicManipulation .

ex:Advertisers rdf:type airo:Stakeholder ;
    rdfs:label "Advertisers"@en ;
    rdfs:label "广告商"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:BusinessRevenueLoss .

ex:BusinessRevenueLoss rdf:type airo:Impact ;
    rdfs:label "Business Revenue Loss Impact"@en ;
    rdfs:label "企业收入损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Advertisers .

ex:AlgorithmicManipulation rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Manipulation Risk Source"@en ;
    rdfs:label "算法操纵风险源"@zh .

ex:TargetedAdvertisingAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Targeted Advertising Algorithm Technique"@en ;
    rdfs:label "定向广告算法技术"@zh .

ex:DigitalAdvertisingPlatform rdf:type airo:Purpose ;
    rdfs:label "Digital Advertising Platform Purpose"@en ;
    rdfs:label "数字广告平台目的"@zh .

ex:DigitalMarketing rdf:type airo:Domain ;
    rdfs:label "Digital Marketing Domain"@en ;
    rdfs:label "数字营销领域"@zh .