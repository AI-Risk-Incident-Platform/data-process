@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAIChatGPT rdf:type airo:AISystem ;
    rdfs:label "OpenAI ChatGPT"@en ;
    rdfs:label "OpenAI ChatGPT"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDeployedBy ex:Microsoft ;
    airo:isAppliedWithinDomain ex:MediaPublishing ;
    airo:hasPurpose ex:ContentGeneration ;
    airo:hasStakeholder ex:NewYorkTimes .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalActionConsequence .

ex:NewYorkTimes rdf:type airo:Stakeholder ;
    rdfs:label "New York Times"@en ;
    rdfs:label "纽约时报"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Action Consequence"@en ;
    rdfs:label "法律诉讼后果"@zh ;
    airo:hasImpactOnStakeholder ex:NewYorkTimes .

ex:MediaPublishing rdf:type airo:Domain ;
    rdfs:label "Media Publishing Domain"@en ;
    rdfs:label "媒体出版领域"@zh .

ex:ContentGeneration rdf:type airo:Purpose ;
    rdfs:label "Content Generation Purpose"@en ;
    rdfs:label "内容生成目的"@zh .