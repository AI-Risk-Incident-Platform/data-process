@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousDrivingFatalityRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasPurpose ex:AutonomousDrivingAssistance ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionAndMachineLearning ;
    airo:hasStakeholder ex:GovernmentRegulators .

ex:AutonomousDrivingFatalityRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶致命风险"@zh ;
    rdfs:label "Autonomous Driving Fatality Risk"@en ;
    airo:hasConsequence ex:DriverFatality ;
    airo:isRiskSourceFor ex:SystemLimitationsInPartialAutonomy .

ex:DriverFatality rdf:type airo:Consequence ;
    rdfs:label "驾驶员死亡后果"@zh ;
    rdfs:label "Driver Fatality Consequence"@en ;
    airo:hasImpact ex:PublicSafetyConcerns .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:GovernmentRegulators rdf:type airo:Stakeholder ;
    rdfs:label "政府监管机构"@zh ;
    rdfs:label "Government Regulators"@en .

ex:PublicSafetyConcerns rdf:type airo:Impact ;
    rdfs:label "公共安全担忧影响"@zh ;
    rdfs:label "Public Safety Concerns Impact"@en ;
    airo:hasImpactOnStakeholder ex:GovernmentRegulators .

ex:AutonomousDrivingAssistance rdf:type airo:Purpose ;
    rdfs:label "自动驾驶辅助目的"@zh ;
    rdfs:label "Autonomous Driving Assistance Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVisionAndMachineLearning rdf:type airo:AITechnique ;
    rdfs:label "计算机视觉与机器学习技术"@zh ;
    rdfs:label "Computer Vision and Machine Learning Technique"@en .

ex:SystemLimitationsInPartialAutonomy rdf:type airo:RiskSource ;
    rdfs:label "部分自主性系统限制风险源"@zh ;
    rdfs:label "System Limitations in Partial Autonomy Risk Source"@en .