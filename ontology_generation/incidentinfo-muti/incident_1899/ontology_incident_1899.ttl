@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DominoAIOrderSystem rdf:type airo:AISystem ;
    rdfs:label "达美乐AI电话订餐系统"@zh ;
    rdfs:label "Domino's AI Phone-Order System"@en ;
    airo:hasRisk ex:VoicePrintCollectionRisk ;
    airo:hasAISubject ex:PhoneOrderCustomers ;
    airo:hasStakeholder ex:DominoPizza ;
    airo:hasCapability ex:VoiceRecognitionForOrderProcessing ;
    airo:isAppliedWithinDomain ex:FoodDeliveryService .

ex:VoicePrintCollectionRisk rdf:type airo:Risk ;
    rdfs:label "声纹采集风险"@zh ;
    rdfs:label "Voice Print Collection Risk"@en ;
    airo:hasConsequence ex:PotentialPrivacyViolation ;
    airo:isRiskSourceFor ex:VoicePrintDataCollection .

ex:DominoPizza rdf:type airo:Stakeholder ;
    rdfs:label "达美乐披萨"@zh ;
    rdfs:label "Domino's Pizza"@en .

ex:PhoneOrderCustomers rdf:type airo:AISubject ;
    rdfs:label "电话订餐顾客"@zh ;
    rdfs:label "Phone Order Customers"@en .

ex:PotentialPrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "潜在隐私侵犯后果"@zh ;
    rdfs:label "Potential Privacy Violation Consequence"@en ;
    airo:hasImpactOnStakeholder ex:PhoneOrderCustomers .

ex:VoicePrintDataCollection rdf:type airo:RiskSource ;
    rdfs:label "声纹数据采集风险源"@zh ;
    rdfs:label "Voice Print Data Collection Risk Source"@en .

ex:VoiceRecognitionForOrderProcessing rdf:type airo:AICapability ;
    rdfs:label "用于订单处理的语音识别能力"@zh ;
    rdfs:label "Voice Recognition for Order Processing Capability"@en .

ex:FoodDeliveryService rdf:type airo:Domain ;
    rdfs:label "食品配送服务领域"@zh ;
    rdfs:label "Food Delivery Service Domain"@en .