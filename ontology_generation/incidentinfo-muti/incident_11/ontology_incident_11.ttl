@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleImageSearch rdf:type airo:AISystem ;
    rdfs:label "Google Image Search"@en ;
    rdfs:label "谷歌图片搜索"@zh ;
    airo:hasRisk ex:GenderBiasInSearchResults ;
    airo:hasCapability ex:ImageRecognitionAndRanking ;
    airo:hasStakeholder ex:FemaleProfessionals .

ex:GenderBiasInSearchResults rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Search Results Risk"@en ;
    rdfs:label "搜索结果中的性别偏见风险"@zh ;
    airo:hasConsequence ex:ReinforcementOfGenderStereotypes ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:ReinforcementOfGenderStereotypes rdf:type airo:Consequence ;
    rdfs:label "Reinforcement of Gender Stereotypes Consequence"@en ;
    rdfs:label "强化性别刻板印象的后果"@zh ;
    airo:hasImpact ex:UnderrepresentationOfWomenInLeadership .

ex:ImageRecognitionAndRanking rdf:type airo:AICapability ;
    rdfs:label "Image Recognition and Ranking Capability"@en ;
    rdfs:label "图像识别与排名能力"@zh .

ex:FemaleProfessionals rdf:type airo:Stakeholder ;
    rdfs:label "Female Professionals"@en ;
    rdfs:label "女性专业人士"@zh .

ex:UnderrepresentationOfWomenInLeadership rdf:type airo:Impact ;
    rdfs:label "Underrepresentation of Women in Leadership Impact"@en ;
    rdfs:label "女性在领导层中代表性不足的影响"@zh ;
    airo:hasImpactOnStakeholder ex:FemaleProfessionals .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .