@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TuSimpleSelfDrivingTruck rdf:type airo:AISystem ;
    rdfs:label "TuSimple Self-Driving Truck"@en ;
    rdfs:label "TuSimple自动驾驶卡车"@zh ;
    airo:hasRisk ex:SafetyRiskInAutonomousTruckOperation ;
    airo:usesTechnique ex:ComputerVisionAndMachineLearningForAutonomousDriving ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTransportation ;
    airo:hasStakeholder ex:TuSimpleCompany .

ex:SafetyRiskInAutonomousTruckOperation rdf:type airo:Risk ;
    rdfs:label "Safety Risk in Autonomous Truck Operation"@en ;
    rdfs:label "自动驾驶卡车运营中的安全风险"@zh ;
    airo:hasConsequence ex:VehicleCrash ;
    airo:isRiskSourceFor ex:SystemFailureOrHumanErrorMisrepresentation .

ex:VehicleCrash rdf:type airo:Consequence ;
    rdfs:label "Vehicle Crash Consequence"@en ;
    rdfs:label "车辆碰撞后果"@zh ;
    airo:hasImpact ex:PublicSafetyConcerns .

ex:SystemFailureOrHumanErrorMisrepresentation rdf:type airo:RiskSource ;
    rdfs:label "System Failure or Human Error Misrepresentation Risk Source"@en ;
    rdfs:label "系统故障或人为错误误报风险源"@zh .

ex:TuSimpleCompany rdf:type airo:Stakeholder ;
    rdfs:label "TuSimple Company"@en ;
    rdfs:label "TuSimple公司"@zh .

ex:PublicSafetyConcerns rdf:type airo:Impact ;
    rdfs:label "Public Safety Concerns Impact"@en ;
    rdfs:label "公共安全担忧影响"@zh ;
    airo:hasImpactOnStakeholder ex:TuSimpleCompany .

ex:AutonomousVehicleTransportation rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Transportation Domain"@en ;
    rdfs:label "自动驾驶交通运输领域"@zh .

ex:ComputerVisionAndMachineLearningForAutonomousDriving rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision and Machine Learning for Autonomous Driving Technique"@en ;
    rdfs:label "用于自动驾驶的计算机视觉和机器学习技术"@zh .