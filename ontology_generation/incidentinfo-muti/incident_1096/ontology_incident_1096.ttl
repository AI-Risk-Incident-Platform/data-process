@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NormanAI rdf:type airo:AISystem ;
    rdfs:label "诺曼AI"@zh ;
    rdfs:label "Norman AI"@en ;
    airo:hasRisk ex:PsychopathicBehaviorRisk ;
    airo:hasCapability ex:ImageInterpretation ;
    airo:isDevelopedBy ex:MIT ;
    airo:usesTechnique ex:MachineLearning .

ex:PsychopathicBehaviorRisk rdf:type airo:Risk ;
    rdfs:label "精神病态行为风险"@zh ;
    rdfs:label "Psychopathic Behavior Risk"@en ;
    airo:hasConsequence ex:ViolentThoughts ;
    airo:isRiskSourceFor ex:RedditDataTraining .

ex:ImageInterpretation rdf:type airo:AICapability ;
    rdfs:label "图像解释能力"@zh ;
    rdfs:label "Image Interpretation Capability"@en .

ex:RedditDataTraining rdf:type airo:RiskSource ;
    rdfs:label "Reddit数据训练风险源"@zh ;
    rdfs:label "Reddit Data Training Risk Source"@en .

ex:ViolentThoughts rdf:type airo:Consequence ;
    rdfs:label "暴力思维后果"@zh ;
    rdfs:label "Violent Thoughts Consequence"@en ;
    airo:hasImpact ex:NegativePsychologicalEffects .

ex:MIT rdf:type airo:AIDeveloper ;
    rdfs:label "麻省理工学院"@zh ;
    rdfs:label "MIT"@en .

ex:MachineLearning rdf:type airo:AITechnique ;
    rdfs:label "机器学习技术"@zh ;
    rdfs:label "Machine Learning Technique"@en .

ex:NegativePsychologicalEffects rdf:type airo:Impact ;
    rdfs:label "负面心理影响"@zh ;
    rdfs:label "Negative Psychological Effects Impact"@en .