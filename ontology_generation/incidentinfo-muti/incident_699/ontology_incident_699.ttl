@base <https://w3id.org/airo#> .

ex:AIChatbots rdf:type airo:AISystem ;
    rdfs:label "AI Chatbots"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:HumanConnectionDisruptionRisk ;
    airo:hasStakeholder ex:OnlineCommunityMembers .

ex:HumanConnectionDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Human Connection Disruption Risk"@en ;
    rdfs:label "人类社交连接中断风险"@zh ;
    airo:hasImpact ex:ReducedAuthenticHumanInteraction .

ex:ReducedAuthenticHumanInteraction rdf:type airo:Impact ;
    rdfs:label "Reduced Authentic Human Interaction Impact"@en ;
    rdfs:label "真实人类互动减少的影响"@zh ;
    airo:hasImpactOnStakeholder ex:OnlineCommunityMembers .

ex:OnlineCommunityMembers rdf:type airo:Stakeholder ;
    rdfs:label "Online Community Members"@en ;
    rdfs:label "在线社区成员"@zh .