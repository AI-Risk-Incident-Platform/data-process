@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ScamAI rdf:type airo:AISystem ;
    rdfs:label "Scam AI"@en ;
    rdfs:label "诈骗AI"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:SocialEngineeringAI ;
    airo:isAppliedWithinDomain ex:FinancialServices ;
    airo:hasStakeholder ex:BankCustomers .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:AIEnhancedScammingTechniques .

ex:BankCustomers rdf:type airo:Stakeholder ;
    rdfs:label "Bank Customers"@en ;
    rdfs:label "银行客户"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpact ex:PersonalFinancialSecurityThreat .

ex:PersonalFinancialSecurityThreat rdf:type airo:Impact ;
    rdfs:label "Personal Financial Security Threat"@en ;
    rdfs:label "个人财务安全威胁"@zh ;
    airo:hasImpactOnStakeholder ex:BankCustomers .

ex:SocialEngineeringAI rdf:type airo:AITechnique ;
    rdfs:label "Social Engineering AI Technique"@en ;
    rdfs:label "社会工程学AI技术"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:AIEnhancedScammingTechniques rdf:type airo:RiskSource ;
    rdfs:label "AI-Enhanced Scamming Techniques"@en ;
    rdfs:label "AI增强的诈骗技术"@zh .