@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceID rdf:type airo:AISystem ;
    rdfs:label "Face ID"@en ;
    rdfs:label "面容ID"@zh ;
    airo:hasRisk ex:FaceIDHackingRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasPurpose ex:SecureAuthentication ;
    airo:hasStakeholder ex:iPhoneXUsers ;
    airo:isDeployedBy ex:Apple .

ex:FaceIDHackingRisk rdf:type airo:Risk ;
    rdfs:label "Face ID Hacking Risk"@en ;
    rdfs:label "面容ID被黑客攻击风险"@zh ;
    airo:isRiskSourceFor ex:PotentialSecurityVulnerabilities ;
    airo:hasConsequence ex:UnauthorizedAccess .

ex:iPhoneXUsers rdf:type airo:Stakeholder ;
    rdfs:label "iPhone X Users"@en ;
    rdfs:label "iPhone X用户"@zh .

ex:Apple rdf:type airo:AIOperator ;
    rdfs:label "Apple"@en ;
    rdfs:label "苹果公司"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:SecureAuthentication rdf:type airo:Purpose ;
    rdfs:label "Secure Authentication"@en ;
    rdfs:label "安全认证"@zh .

ex:PotentialSecurityVulnerabilities rdf:type airo:RiskSource ;
    rdfs:label "Potential Security Vulnerabilities"@en ;
    rdfs:label "潜在安全漏洞"@zh .

ex:UnauthorizedAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Access"@en ;
    rdfs:label "未经授权访问"@zh ;
    airo:hasImpact ex:PrivacyConcerns .

ex:PrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "Privacy Concerns"@en ;
    rdfs:label "隐私问题"@zh ;
    airo:hasImpactOnStakeholder ex:iPhoneXUsers .