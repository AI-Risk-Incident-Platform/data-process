@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIWeaponScanner rdf:type airo:AISystem ;
    rdfs:label "AI Weapon Scanner"@en ;
    rdfs:label "AI武器扫描仪"@zh ;
    airo:hasRisk ex:SecurityFailureRisk ;
    airo:hasPurpose ex:WeaponDetection ;
    airo:hasCapability ex:ObjectRecognition ;
    airo:isAppliedWithinDomain ex:EducationSecurity ;
    airo:hasStakeholder ex:SchoolAdministrations .

ex:SecurityFailureRisk rdf:type airo:Risk ;
    rdfs:label "Security Failure Risk"@en ;
    rdfs:label "安全失效风险"@zh ;
    airo:hasConsequence ex:SchoolSecurityCompromise ;
    airo:isRiskSourceFor ex:AlgorithmicLimitations .

ex:SchoolSecurityCompromise rdf:type airo:Consequence ;
    rdfs:label "School Security Compromise Consequence"@en ;
    rdfs:label "学校安全受损后果"@zh ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:EducationSecurity rdf:type airo:Domain ;
    rdfs:label "Education Security Domain"@en ;
    rdfs:label "教育安全领域"@zh .

ex:WeaponDetection rdf:type airo:Purpose ;
    rdfs:label "Weapon Detection Purpose"@en ;
    rdfs:label "武器检测目的"@zh .

ex:ObjectRecognition rdf:type airo:AICapability ;
    rdfs:label "Object Recognition Capability"@en ;
    rdfs:label "物体识别能力"@zh .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Public Safety Threat Impact"@en ;
    rdfs:label "公共安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:SchoolAdministrations .

ex:AlgorithmicLimitations rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Limitations Risk Source"@en ;
    rdfs:label "算法限制风险源"@zh .

ex:SchoolAdministrations rdf:type airo:Stakeholder ;
    rdfs:label "School Administrations"@en ;
    rdfs:label "学校管理部门"@zh .