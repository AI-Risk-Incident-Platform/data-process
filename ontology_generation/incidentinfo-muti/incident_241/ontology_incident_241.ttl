@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChildAbusePredictionTool rdf:type airo:AISystem ;
    rdfs:label "Child Abuse Prediction Tool"@en ;
    rdfs:label "儿童虐待预测工具"@zh ;
    airo:hasRisk ex:InaccurateChildAbusePredictionRisk ;
    airo:isAppliedWithinDomain ex:ChildProtectionServices ;
    airo:hasPurpose ex:AssistingChildAbuseCaseAssessments ;
    airo:hasStakeholder ex:OregonChildWelfareDepartment .

ex:InaccurateChildAbusePredictionRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Child Abuse Prediction Risk"@en ;
    rdfs:label "儿童虐待预测不准确风险"@zh ;
    airo:hasConsequence ex:PotentialMisclassificationOfCases .

ex:ChildProtectionServices rdf:type airo:Domain ;
    rdfs:label "Child Protection Services Domain"@en ;
    rdfs:label "儿童保护服务领域"@zh .

ex:AssistingChildAbuseCaseAssessments rdf:type airo:Purpose ;
    rdfs:label "Assisting Child Abuse Case Assessments Purpose"@en ;
    rdfs:label "辅助儿童虐待案件评估的目的"@zh .

ex:OregonChildWelfareDepartment rdf:type airo:Stakeholder ;
    rdfs:label "Oregon Child Welfare Department"@en ;
    rdfs:label "俄勒冈州儿童福利部门"@zh .

ex:PotentialMisclassificationOfCases rdf:type airo:Consequence ;
    rdfs:label "Potential Misclassification of Cases Consequence"@en ;
    rdfs:label "案件潜在错误分类后果"@zh .