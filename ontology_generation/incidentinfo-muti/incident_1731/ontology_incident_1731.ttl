ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:PrivacyViolationAndReputationDamageRisk ;
    airo:hasAICapability ex:FacialRecognitionAndManipulation ;
    airo:isDeployedBy ex:XiaoYu ;
    airo:hasStakeholder ex:InternetCelebrities ;
    airo:hasAISubject ex:VictimsOfDeepfakePornography ;
    airo:hasPurpose ex:CreatingFakePornographicContent ;
    airo:isAppliedWithinDomain ex:OnlineEntertainmentAndSocialMedia ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:PrivacyViolationAndReputationDamageRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Reputation Damage Risk"@en ;
    rdfs:label "隐私侵犯和名誉损害风险"@zh ;
