    airo:isRiskSourceFor ex:MaliciousContentCreators ;
    airo:isRiskSourceFor ex:NonConsensualIntimateImageAbuse ;
    airo:isRiskSourceFor ex:EasyAccessToDeepfakeTools .

ex:LegalPenaltiesForCreators rdf:type airo:Consequence ;
    rdfs:label "Legal Penalties for Creators Consequence"@en ;
    rdfs:label "创作者法律处罚后果"@zh ;
