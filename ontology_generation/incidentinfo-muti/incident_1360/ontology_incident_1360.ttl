@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIScreeningToolForChildWelfare rdf:type airo:AISystem ;
    rdfs:label "AI Screening Tool for Child Welfare"@en ;
    rdfs:label "儿童福利AI筛查工具"@zh ;
    airo:hasRisk ex:BiasAgainstFamiliesWithDisabilities ;
    airo:hasStakeholder ex:DepartmentOfJustice ;
    airo:isDeployedBy ex:PennsylvaniaChildWelfareAgency ;
    airo:isAppliedWithinDomain ex:ChildWelfareServices .

ex:BiasAgainstFamiliesWithDisabilities rdf:type airo:Risk ;
    rdfs:label "Bias Against Families with Disabilities Risk"@en ;
    rdfs:label "对残疾家庭存在偏见的风险"@zh ;
    airo:hasConsequence ex:DiscriminationAgainstDisabledFamilies .

ex:DepartmentOfJustice rdf:type airo:Stakeholder ;
    rdfs:label "Department of Justice"@en ;
    rdfs:label "司法部"@zh .

ex:PennsylvaniaChildWelfareAgency rdf:type airo:AIOperator ;
    rdfs:label "Pennsylvania Child Welfare Agency"@en ;
    rdfs:label "宾夕法尼亚州儿童福利机构"@zh .

ex:DiscriminationAgainstDisabledFamilies rdf:type airo:Impact ;
    rdfs:label "Discrimination Against Disabled Families Impact"@en ;
    rdfs:label "对残疾家庭的歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:DepartmentOfJustice .

ex:ChildWelfareServices rdf:type airo:Domain ;
    rdfs:label "Child Welfare Services Domain"@en ;
    rdfs:label "儿童福利服务领域"@zh .