@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialImageSearchEngine rdf:type airo:AISystem ;
    rdfs:label "Facial Image Search Engine"@en ;
    rdfs:label "面部图像搜索引擎"@zh ;
    airo:hasRisk ex:BIPAViolationRisk ;
    airo:hasStakeholder ex:IllinoisResidents ;
    airo:isAppliedWithinDomain ex:BiometricDataCollection .

ex:BIPAViolationRisk rdf:type airo:Risk ;
    rdfs:label "BIPA Violation Risk"@en ;
    rdfs:label "BIPA违规风险"@zh ;
    airo:hasImpact ex:PrivacyViolationImpact .

ex:IllinoisResidents rdf:type airo:Stakeholder ;
    rdfs:label "Illinois Residents"@en ;
    rdfs:label "伊利诺伊州居民"@zh .

ex:PrivacyViolationImpact rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:IllinoisResidents .

ex:BiometricDataCollection rdf:type airo:Domain ;
    rdfs:label "Biometric Data Collection Domain"@en ;
    rdfs:label "生物特征数据收集领域"@zh .