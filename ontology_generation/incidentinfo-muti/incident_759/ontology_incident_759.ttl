@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TikTokAlgorithm rdf:type airo:AISystem ;
    rdfs:label "TikTok Algorithm"@en ;
    rdfs:label "TikTok算法"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:hasCapability ex:ContentRecommendationAlgorithm ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatformDomain ;
    airo:hasStakeholder ex:AmericanPublic .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicOpinionManipulationConsequence .

ex:AmericanPublic rdf:type airo:Stakeholder ;
    rdfs:label "American Public"@en ;
    rdfs:label "美国公众"@zh .

ex:PublicOpinionManipulationConsequence rdf:type airo:Consequence ;
    rdfs:label "Public Opinion Manipulation Consequence"@en ;
    rdfs:label "舆论操纵后果"@zh ;
    airo:hasImpactOnStakeholder ex:AmericanPublic .

ex:ContentRecommendationAlgorithmCapability rdf:type airo:AICapability ;
    rdfs:label "Content Recommendation Algorithm Capability"@en ;
    rdfs:label "内容推荐算法能力"@zh .

ex:SocialMediaPlatformDomain rdf:type airo:Domain ;
    rdfs:label "Social Media Platform Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .