@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Full Self-Driving System"@en ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:hasStakeholder ex:TeslaEmployees .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    rdfs:label "自动驾驶车辆致命风险"@zh ;
    airo:hasConsequence ex:HumanFatality .

ex:HumanFatality rdf:type airo:Consequence ;
    rdfs:label "Human Fatality Consequence"@en ;
    rdfs:label "人员死亡后果"@zh ;
    airo:hasImpact ex:LossOfLife .

ex:TeslaEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Tesla Employees"@en ;
    rdfs:label "特斯拉员工"@zh .

ex:LossOfLife rdf:type airo:Impact ;
    rdfs:label "Loss of Life Impact"@en ;
    rdfs:label "生命损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:TeslaEmployees .