@base <https://w3id.org/airo#> .

ex:NetflixDocumentaryAI rdf:type airo:AISystem ;
    rdfs:label "Netflix纪录片AI"@zh ;
    rdfs:label "Netflix Documentary AI"@en ;
    airo:hasRisk ex:FalseHistoricalRecordCreation ;
    airo:usesTechnique ex:AIImages ;
    airo:hasStakeholder ex:Netflix .

ex:FalseHistoricalRecordCreation rdf:type airo:Risk ;
    rdfs:label "虚假历史记录创建风险"@zh ;
    rdfs:label "False Historical Record Creation Risk"@en ;
    airo:hasConsequence ex:MisrepresentationOfHistoricalEvents .

ex:MisrepresentationOfHistoricalEvents rdf:type airo:Consequence ;
    rdfs:label "历史事件误传后果"@zh ;
    rdfs:label "Misrepresentation of Historical Events Consequence"@en ;
    airo:hasImpact ex:PublicMisinformation .

ex:Netflix rdf:type airo:Stakeholder ;
    rdfs:label "Netflix"@zh ;
    rdfs:label "Netflix"@en .

ex:AIImages rdf:type airo:AITechnique ;
    rdfs:label "AI图像"@zh ;
    rdfs:label "AI Images"@en .

ex:PublicMisinformation rdf:type airo:Impact ;
    rdfs:label "公众误导影响"@zh ;
    rdfs:label "Public Misinformation Impact"@en ;
    airo:hasImpactOnStakeholder ex:Netflix .