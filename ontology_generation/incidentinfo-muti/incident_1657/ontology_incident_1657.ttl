@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceDepixelizer rdf:type airo:AISystem ;
    rdfs:label "Face Depixelizer"@en ;
    rdfs:label "面部去像素化工具"@zh ;
    airo:hasRisk ex:RacialBiasInImageReconstruction ;
    airo:usesTechnique ex:ImageReconstructionAlgorithm .

ex:RacialBiasInImageReconstruction rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Image Reconstruction Risk"@en ;
    rdfs:label "图像重建中的种族偏见风险"@zh ;
    airo:hasConsequence ex:MisrepresentationOfRace ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:MisrepresentationOfRace rdf:type airo:Consequence ;
    rdfs:label "Misrepresentation of Race Consequence"@en ;
    rdfs:label "种族误表征后果"@zh ;
    airo:hasImpact ex:PerpetuationOfRacialStereotypes .

ex:ImageReconstructionAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Image Reconstruction Algorithm"@en ;
    rdfs:label "图像重建算法"@zh .

ex:PerpetuationOfRacialStereotypes rdf:type airo:Impact ;
    rdfs:label "Perpetuation of Racial Stereotypes Impact"@en ;
    rdfs:label "种族刻板印象延续的影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .