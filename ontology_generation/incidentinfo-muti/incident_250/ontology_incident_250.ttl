@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LicensePlateSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "License Plate Surveillance System"@en ;
    rdfs:label "车牌监控系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:CoCoSheriffsDepartment ;
    airo:isAppliedWithinDomain ex:LawEnforcement ;
    airo:hasStakeholder ex:PrivacyAdvocate .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:FalseIdentificationConsequence .

ex:PrivacyAdvocate rdf:type airo:Stakeholder ;
    rdfs:label "Privacy Advocate"@en ;
    rdfs:label "隐私倡导者"@zh .

ex:CoCoSheriffsDepartment rdf:type airo:AIOperator ;
    rdfs:label "CoCo Sheriff's Department"@en ;
    rdfs:label "科科县警长办公室"@zh .

ex:FalseIdentificationConsequence rdf:type airo:Consequence ;
    rdfs:label "False Identification Consequence"@en ;
    rdfs:label "错误识别后果"@zh ;
    airo:hasImpactOnStakeholder ex:PrivacyAdvocate .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .