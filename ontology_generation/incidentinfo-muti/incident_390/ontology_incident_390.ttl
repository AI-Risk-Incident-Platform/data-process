@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberFaceRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Uber Face Recognition System"@en ;
    rdfs:label "优步面部识别系统"@zh ;
    airo:hasRisk ex:TransgenderDiscriminationRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasAISubject ex:TransgenderDrivers ;
    airo:hasStakeholder ex:UberCompany .

ex:TransgenderDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Transgender Discrimination Risk"@en ;
    rdfs:label "跨性别歧视风险"@zh ;
    airo:hasConsequence ex:UnfairAccountDeactivation ;
    airo:isRiskSourceFor ex:BiasedFacialRecognitionAlgorithm .

ex:TransgenderDrivers rdf:type airo:AISubject ;
    rdfs:label "Transgender Drivers"@en ;
    rdfs:label "跨性别司机"@zh .

ex:UnfairAccountDeactivation rdf:type airo:Consequence ;
    rdfs:label "Unfair Account Deactivation Consequence"@en ;
    rdfs:label "不公平账户停用后果"@zh ;
    airo:hasImpact ex:DiscriminationAgainstTransgenderIndividuals .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:UberCompany rdf:type airo:Stakeholder ;
    rdfs:label "Uber Company"@en ;
    rdfs:label "优步公司"@zh .

ex:DiscriminationAgainstTransgenderIndividuals rdf:type airo:Impact ;
    rdfs:label "Discrimination Against Transgender Individuals Impact"@en ;
    rdfs:label "对跨性别个体的歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:TransgenderDrivers .

ex:BiasedFacialRecognitionAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Biased Facial Recognition Algorithm Risk Source"@en ;
    rdfs:label "有偏见的面部识别算法风险源"@zh .