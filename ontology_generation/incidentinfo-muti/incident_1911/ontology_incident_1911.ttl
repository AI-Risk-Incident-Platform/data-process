@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:Palworld rdf:type airo:AISystem ;
    rdfs:label "Palworld"@en ;
    rdfs:label "Palworld"@zh ;
    airo:hasRisk ex:PlagiarismControversyRisk ;
    airo:hasStakeholder ex:PokemonCompany .

ex:PlagiarismControversyRisk rdf:type airo:Risk ;
    rdfs:label "Plagiarism Controversy Risk"@en ;
    rdfs:label "抄袭争议风险"@zh ;
    airo:isRiskSourceFor ex:AICharacterDesignSimilarityRiskSource ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        airo:hasImpact ex:IntellectualPropertyDisputeImpact
    ] .

ex:PokemonCompany rdf:type airo:Stakeholder ;
    rdfs:label "Pokemon Company"@en ;
    rdfs:label "宝可梦公司"@zh .

ex:AICharacterDesignSimilarityRiskSource rdf:type airo:RiskSource ;
    rdfs:label "AI Character Design Similarity Risk Source"@en ;
    rdfs:label "AI角色设计相似性风险源"@zh .

ex:IntellectualPropertyDisputeImpact rdf:type airo:Impact ;
    rdfs:label "Intellectual Property Dispute Impact"@en ;
    rdfs:label "知识产权纠纷影响"@zh ;
    airo:hasImpactOnStakeholder ex:PokemonCompany .