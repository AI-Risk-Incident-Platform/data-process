@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedImageSystem rdf:type airo:AISystem ;
    rdfs:label "AI生成图像系统"@zh ;
    rdfs:label "AI Generated Image System"@en ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasStakeholder ex:PoliticalFigure .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "虚假信息风险"@zh ;
    rdfs:label "Misinformation Risk"@en ;
    airo:hasConsequence ex:PoliticalMisinformation ;
    airo:hasImpact ex:PublicDeception .

ex:PoliticalFigure rdf:type airo:Stakeholder ;
    rdfs:label "政治人物"@zh ;
    rdfs:label "Political Figure"@en .

ex:PoliticalMisinformation rdf:type airo:Misuse ;
    rdfs:label "政治虚假信息滥用"@zh ;
    rdfs:label "Political Misinformation Misuse"@en .

ex:PublicDeception rdf:type airo:Impact ;
    rdfs:label "公众欺骗影响"@zh ;
    rdfs:label "Public Deception Impact"@en ;
    airo:hasImpactOnStakeholder ex:PoliticalFigure .