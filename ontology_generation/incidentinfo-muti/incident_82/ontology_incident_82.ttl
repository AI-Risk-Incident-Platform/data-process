@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CollegeAdmissionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "College Admission Algorithm"@en ;
    rdfs:label "大学录取算法"@zh ;
    airo:hasRisk ex:UnfairAdmissionPracticesRisk ;
    airo:hasStakeholder ex:CollegeApplicants ;
    airo:isAppliedWithinDomain ex:HigherEducationAdmissions .

ex:UnfairAdmissionPracticesRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Admission Practices Risk"@en ;
    rdfs:label "不公平录取实践风险"@zh ;
    airo:hasImpact ex:EducationalOpportunityLoss .

ex:CollegeApplicants rdf:type airo:Stakeholder ;
    rdfs:label "College Applicants"@en ;
    rdfs:label "大学申请者"@zh .

ex:HigherEducationAdmissions rdf:type airo:Domain ;
    rdfs:label "Higher Education Admissions Domain"@en ;
    rdfs:label "高等教育录取领域"@zh .

ex:EducationalOpportunityLoss rdf:type airo:Impact ;
    rdfs:label "Educational Opportunity Loss Impact"@en ;
    rdfs:label "教育机会丧失影响"@zh ;
    airo:hasImpactOnStakeholder ex:CollegeApplicants .