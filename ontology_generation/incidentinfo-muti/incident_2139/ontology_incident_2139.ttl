ex:SpotifyEmotionalSpeechRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Spotify Emotional Speech Recognition System"@en ;
    rdfs:label "Spotify情感语音识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:Spotify ;
    airo:hasPurpose ex:MusicRecommendation ;
    airo:hasStakeholder ex:Musicians ;
    airo:isAppliedWithinDomain ex:MusicStreamingService .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:Musicians rdf:type airo:Stakeholder ;
    rdfs:label "Musicians"@en ;
    rdfs:label "音乐人"@zh .

ex:Spotify rdf:type airo:AIOperator ;
    rdfs:label "Spotify"@en ;
    rdfs:label "Spotify"@zh .

ex:MusicRecommendation rdf:type airo:Purpose ;
    rdfs:label "Music Recommendation"@en ;
    rdfs:label "音乐推荐"@zh .

ex:MusicStreamingService rdf:type airo:Domain ;
    rdfs:label "Music Streaming Service"@en ;
    rdfs:label "音乐流媒体服务"@zh .