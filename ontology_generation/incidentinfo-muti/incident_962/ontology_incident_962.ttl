@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MAGANewspaperAIBot rdf:type airo:AISystem ;
    rdfs:label "MAGA Newspaper AI Bot"@en ;
    rdfs:label "MAGA报纸AI机器人"@zh ;
    airo:hasRisk ex:RacistContentPromotionRisk ;
    airo:hasStakeholder ex:MAGANewspaperOwner .

ex:RacistContentPromotionRisk rdf:type airo:Risk ;
    rdfs:label "Racist Content Promotion Risk"@en ;
    rdfs:label "种族主义内容推广风险"@zh ;
    airo:hasConsequence ex:RacialDiscriminationPromotion .

ex:MAGANewspaperOwner rdf:type airo:Stakeholder ;
    rdfs:label "MAGA Newspaper Owner"@en ;
    rdfs:label "MAGA报纸所有者"@zh .

ex:RacialDiscriminationPromotion rdf:type airo:Consequence ;
    rdfs:label "Racial Discrimination Promotion Consequence"@en ;
    rdfs:label "种族歧视推广后果"@zh ;
    airo:hasImpact ex:SocialHarm .

ex:SocialHarm rdf:type airo:Impact ;
    rdfs:label "Social Harm Impact"@en ;
    rdfs:label "社会危害影响"@zh ;
    airo:hasImpactOnStakeholder ex:MAGANewspaperOwner .