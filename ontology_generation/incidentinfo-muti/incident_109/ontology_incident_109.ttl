@base <https://w3id.org/airo#> .

ex:AIChatbot rdf:type airo:AISystem ;
    rdfs:label "AI Chatbot"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:DataCollectionLegalDisputeRisk ;
    airo:hasStakeholder ex:LegalAuthorities .

ex:DataCollectionLegalDisputeRisk rdf:type airo:Risk ;
    rdfs:label "Data Collection Legal Dispute Risk"@en ;
    rdfs:label "数据收集法律纠纷风险"@zh ;
    airo:isRiskSourceFor ex:DataCollectionPractices .

ex:LegalAuthorities rdf:type airo:Stakeholder ;
    rdfs:label "Legal Authorities"@en ;
    rdfs:label "法律机构"@zh .

ex:DataCollectionPractices rdf:type airo:RiskSource ;
    rdfs:label "Data Collection Practices"@en ;
    rdfs:label "数据收集行为"@zh .