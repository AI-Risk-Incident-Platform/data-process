@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedImages rdf:type airo:AISystem ;
    rdfs:label "AI Generated Images"@en ;
    rdfs:label "AI生成图像"@zh ;
    airo:hasRisk ex:MisinformationSpread ;
    airo:usesTechnique ex:ImageGeneration ;
    airo:hasStakeholder ex:SocialMediaUsers .

ex:MisinformationSpread rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PublicConfusion .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion Consequence"@en ;
    rdfs:label "公众困惑后果"@zh ;
    airo:hasImpact ex:CredibilityLoss .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:ImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Image Generation Technique"@en ;
    rdfs:label "图像生成技术"@zh .

ex:CredibilityLoss rdf:type airo:Impact ;
    rdfs:label "Credibility Loss Impact"@en ;
    rdfs:label "可信度损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .