@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:WrongfulDeathRisk ;
    airo:hasPurpose ex:AutonomousDriving ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVision ;
    airo:hasStakeholder ex:TeslaCompany ;
    airo:hasAIOperator ex:TeslaDriver .

ex:WrongfulDeathRisk rdf:type airo:Risk ;
    rdfs:label "过失致死风险"@zh ;
    rdfs:label "Wrongful Death Risk"@en ;
    airo:hasConsequence ex:DeadlyCrash ;
    airo:isRiskSourceFor ex:SystemFailure ;
    airo:isRiskSourceFor ex:SensorMalfunction .

ex:DeadlyCrash rdf:type airo:Consequence ;
    rdfs:label "致命车祸后果"@zh ;
    rdfs:label "Deadly Crash Consequence"@en ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:TeslaCompany rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Company"@en .

ex:TeslaDriver rdf:type airo:AIOperator ;
    rdfs:label "特斯拉驾驶员"@zh ;
    rdfs:label "Tesla Driver"@en .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "公共安全威胁"@zh ;
    rdfs:label "Public Safety Threat"@en ;
    airo:hasImpactOnStakeholder ex:TeslaCompany ;
    airo:hasImpactOnStakeholder ex:TeslaDriver .

ex:AutonomousDriving rdf:type airo:Purpose ;
    rdfs:label "自动驾驶目的"@zh ;
    rdfs:label "Autonomous Driving Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVision rdf:type airo:AITechnique ;
    rdfs:label "计算机视觉技术"@zh ;
    rdfs:label "Computer Vision Technique"@en .

ex:SystemFailure rdf:type airo:RiskSource ;
    rdfs:label "系统故障风险源"@zh ;
    rdfs:label "System Failure Risk Source"@en ;
    airo:exploitsVulnerability ex:HumanOverreliance .

ex:SensorMalfunction rdf:type airo:Hazard ;
    rdfs:label "传感器故障危害"@zh ;
    rdfs:label "Sensor Malfunction Hazard"@en ;
    airo:exploitsVulnerability ex:HumanOverreliance .

ex:HumanOverreliance rdf:type airo:Vulnerability ;
    rdfs:label "人类过度依赖脆弱性"@zh ;
    rdfs:label "Human Overreliance Vulnerability"@en .