@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialAnalysisJobScreeningService rdf:type airo:AISystem ;
    rdfs:label "Facial Analysis Job Screening Service"@en ;
    rdfs:label "面部分析求职筛选服务"@zh ;
    airo:hasRisk ex:DiscriminationInHiringProcess ;
    airo:usesTechnique ex:FacialRecognitionAlgorithm ;
    airo:hasStakeholder ex:JobApplicants .

ex:DiscriminationInHiringProcess rdf:type airo:Risk ;
    rdfs:label "Discrimination in Hiring Process Risk"@en ;
    rdfs:label "招聘过程中的歧视风险"@zh ;
    airo:hasConsequence ex:UnfairEmploymentOpportunities .

ex:JobApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Job Applicants"@en ;
    rdfs:label "求职者"@zh .

ex:FacialRecognitionAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Algorithm"@en ;
    rdfs:label "面部识别算法"@zh .

ex:UnfairEmploymentOpportunities rdf:type airo:Impact ;
    rdfs:label "Unfair Employment Opportunities Impact"@en ;
    rdfs:label "不公平就业机会的影响"@zh ;
    airo:hasImpactOnStakeholder ex:JobApplicants .