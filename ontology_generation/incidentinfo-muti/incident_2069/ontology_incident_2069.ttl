@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlgorithmicAdministrationSystem rdf:type airo:AISystem ;
    rdfs:label "Algorithmic Administration System"@en ;
    rdfs:label "算法行政管理系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInPublicAdministration ;
    airo:isAppliedWithinDomain ex:PublicAdministration ;
    airo:hasStakeholder ex:DanishCentralAgency .

ex:AlgorithmicBiasInPublicAdministration rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Public Administration Risk"@en ;
    rdfs:label "公共行政中的算法偏见风险"@zh .

ex:PublicAdministration rdf:type airo:Domain ;
    rdfs:label "Public Administration Domain"@en ;
    rdfs:label "公共行政领域"@zh .

ex:DanishCentralAgency rdf:type airo:Stakeholder ;
    rdfs:label "Danish Central Agency"@en ;
    rdfs:label "丹麦中央机构"@zh .