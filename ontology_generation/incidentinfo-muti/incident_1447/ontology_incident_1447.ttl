@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BorderControlLieDetector rdf:type airo:AISystem ;
    rdfs:label "Border Control Lie Detector"@en ;
    rdfs:label "边境控制测谎仪"@zh ;
    airo:hasRisk ex:LackOfTransparencyInLieDetectorTech ;
    airo:hasStakeholder ex:EuropeanCommission .

ex:LackOfTransparencyInLieDetectorTech rdf:type airo:Risk ;
    rdfs:label "Lack of Transparency in Lie Detector Tech Risk"@en ;
    rdfs:label "测谎技术缺乏透明度风险"@zh ;
    airo:hasConsequence ex:PublicDistrustInAITechnology ;
    airo:isRiskSourceFor ex:SecrecyInResearchProject .

ex:EuropeanCommission rdf:type airo:Stakeholder ;
    rdfs:label "European Commission"@en ;
    rdfs:label "欧盟委员会"@zh .

ex:SecrecyInResearchProject rdf:type airo:RiskSource ;
    rdfs:label "Secrecy in Research Project Risk Source"@en ;
    rdfs:label "研究项目保密性风险源"@zh .

ex:PublicDistrustInAITechnology rdf:type airo:Impact ;
    rdfs:label "Public Distrust in AI Technology Impact"@en ;
    rdfs:label "公众对AI技术不信任的影响"@zh ;
    airo:hasImpactOnStakeholder ex:EuropeanCommission .