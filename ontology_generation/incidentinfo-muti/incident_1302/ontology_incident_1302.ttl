@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:LanguageProcessingErrorRisk ;
    airo:hasAIUser ex:EnglishSpeakingUsers ;
    airo:hasCapability ex:MultilingualResponseGeneration .

ex:LanguageProcessingErrorRisk rdf:type airo:Risk ;
    rdfs:label "Language Processing Error Risk"@en ;
    rdfs:label "语言处理错误风险"@zh ;
    airo:hasConsequence ex:UserConfusion ;
    airo:isRiskSourceFor ex:LanguageModelBug .

ex:UserConfusion rdf:type airo:Consequence ;
    rdfs:label "User Confusion"@en ;
    rdfs:label "用户困惑"@zh .

ex:MultilingualResponseGeneration rdf:type airo:AICapability ;
    rdfs:label "Multilingual Response Generation"@en ;
    rdfs:label "多语言响应生成"@zh .

ex:EnglishSpeakingUsers rdf:type airo:AIUser ;
    rdfs:label "English Speaking Users"@en ;
    rdfs:label "英语使用者"@zh .

ex:LanguageModelBug rdf:type airo:RiskSource ;
    rdfs:label "Language Model Bug"@en ;
    rdfs:label "语言模型错误"@zh .