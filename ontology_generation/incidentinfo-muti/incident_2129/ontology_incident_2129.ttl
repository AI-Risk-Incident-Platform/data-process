ex:DelhiGovtSchoolsFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Delhi Govt Schools Facial Recognition System"@en ;
    rdfs:label "德里政府学校面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:DelhiGovernment ;
    airo:hasAISubject ex:SchoolStudents ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:usesTechnique ex:FacialRecognition ;
    airo:hasPurpose ex:StudentIdentification .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:StudentPrivacyConcerns .

ex:DelhiGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Delhi Government"@en ;
    rdfs:label "德里政府"@zh .

ex:SchoolStudents rdf:type airo:AISubject ;
    rdfs:label "School Students"@en ;
    rdfs:label "学校学生"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .

ex:FacialRecognition rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technique"@en ;
    rdfs:label "面部识别技术"@zh .

ex:StudentIdentification rdf:type airo:Purpose ;
    rdfs:label "Student Identification Purpose"@en ;
    rdfs:label "学生识别目的"@zh .

ex:StudentPrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "Student Privacy Concerns Impact"@en ;
    rdfs:label "学生隐私担忧影响"@zh ;
    airo:hasImpactOnStakeholder ex:SchoolStudents .