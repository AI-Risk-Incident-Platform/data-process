@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HumanoidRobot rdf:type airo:AISystem ;
    rdfs:label "Humanoid Robot"@en ;
    rdfs:label "人形机器人"@zh ;
    airo:hasRisk ex:JobPerformanceFailureRisk ;
    airo:isDevelopedBy ex:Softbank ;
    airo:hasPurpose ex:AutomatedWorkforce ;
    airo:hasStakeholder ex:Softbank .

ex:JobPerformanceFailureRisk rdf:type airo:Risk ;
    rdfs:label "Job Performance Failure Risk"@en ;
    rdfs:label "工作表现失败风险"@zh ;
    airo:hasConsequence ex:EmploymentTermination ;
    airo:isRiskSourceFor ex:TechnicalLimitations .

ex:Softbank rdf:type airo:AIOperator ;
    rdfs:label "Softbank"@en ;
    rdfs:label "软银"@zh .

ex:EmploymentTermination rdf:type airo:Consequence ;
    rdfs:label "Employment Termination Consequence"@en ;
    rdfs:label "雇佣终止后果"@zh ;
    airo:hasImpact ex:FinancialLossForOperator .

ex:FinancialLossForOperator rdf:type airo:Impact ;
    rdfs:label "Financial Loss for Operator Impact"@en ;
    rdfs:label "运营商财务损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Softbank .

ex:AutomatedWorkforce rdf:type airo:Purpose ;
    rdfs:label "Automated Workforce Purpose"@en ;
    rdfs:label "自动化劳动力目的"@zh .

ex:TechnicalLimitations rdf:type airo:RiskSource ;
    rdfs:label "Technical Limitations Risk Source"@en ;
    rdfs:label "技术限制风险源"@zh .