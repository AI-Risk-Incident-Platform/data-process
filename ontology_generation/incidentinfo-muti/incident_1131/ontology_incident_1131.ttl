@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlgorithmicBusRoutingSystem rdf:type airo:AISystem ;
    rdfs:label "Algorithmic Bus Routing System"@en ;
    rdfs:label "算法公交路线系统"@zh ;
    airo:hasRisk ex:PotentialRouteOptimizationBias ;
    airo:hasCapability ex:DynamicRouteOptimization ;
    airo:isAppliedWithinDomain ex:PublicTransportation ;
    airo:hasPurpose ex:ImproveBusEfficiency .

ex:PotentialRouteOptimizationBias rdf:type airo:Risk ;
    rdfs:label "Potential Route Optimization Bias Risk"@en ;
    rdfs:label "潜在路线优化偏见风险"@zh .

ex:DynamicRouteOptimization rdf:type airo:AICapability ;
    rdfs:label "Dynamic Route Optimization Capability"@en ;
    rdfs:label "动态路线优化能力"@zh .

ex:PublicTransportation rdf:type airo:Domain ;
    rdfs:label "Public Transportation Domain"@en ;
    rdfs:label "公共交通领域"@zh .

ex:ImproveBusEfficiency rdf:type airo:Purpose ;
    rdfs:label "Improve Bus Efficiency Purpose"@en ;
    rdfs:label "提高公交效率的目的"@zh .