@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PimEyesFacialSearchEngine rdf:type airo:AISystem ;
    rdfs:label "PimEyes Facial Search Engine"@en ;
    rdfs:label "PimEyes面部搜索引擎"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:DeceasedIndividuals ;
    airo:hasStakeholder ex:UKRegulatoryAuthorities .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:WebCrawlingOfPersonalPhotos ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Privacy Violation Consequence"@en ;
        rdfs:label "隐私侵犯后果"@zh ;
        airo:hasImpact ex:StalkingPotential
    ] .

ex:UKRegulatoryAuthorities rdf:type airo:Stakeholder ;
    rdfs:label "UK Regulatory Authorities"@en ;
    rdfs:label "英国监管机构"@zh .

ex:DeceasedIndividuals rdf:type airo:AISubject ;
    rdfs:label "Deceased Individuals"@en ;
    rdfs:label "已故人士"@zh .

ex:WebCrawlingOfPersonalPhotos rdf:type airo:RiskSource ;
    rdfs:label "Web Crawling of Personal Photos"@en ;
    rdfs:label "个人照片的网络爬取"@zh .

ex:StalkingPotential rdf:type airo:Impact ;
    rdfs:label "Stalking Potential Impact"@en ;
    rdfs:label "潜在跟踪影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKRegulatoryAuthorities ;
    airo:hasImpactOnStakeholder ex:DeceasedIndividuals .