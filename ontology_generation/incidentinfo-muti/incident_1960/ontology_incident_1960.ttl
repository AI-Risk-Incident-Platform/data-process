@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedMusicSystem rdf:type airo:AISystem ;
    rdfs:label "AI生成音乐系统"@zh ;
    rdfs:label "AI Generated Music System"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:usesTechnique ex:VoiceSynthesisTechnology ;
    airo:hasStakeholder ex:MusicArtists ;
    airo:isDeployedBy ex:Ghostwriter .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:hasConsequence ex:LegalChallenges .

ex:VoiceSynthesisTechnology rdf:type airo:AITechnique ;
    rdfs:label "语音合成技术"@zh ;
    rdfs:label "Voice Synthesis Technology"@en .

ex:MusicArtists rdf:type airo:Stakeholder ;
    rdfs:label "音乐艺术家"@zh ;
    rdfs:label "Music Artists"@en .

ex:LegalChallenges rdf:type airo:Consequence ;
    rdfs:label "法律挑战后果"@zh ;
    rdfs:label "Legal Challenges Consequence"@en ;
    airo:hasImpactOnStakeholder ex:MusicArtists .

ex:Ghostwriter rdf:type airo:AIOperator ;
    rdfs:label "Ghostwriter"@zh ;
    rdfs:label "Ghostwriter"@en .