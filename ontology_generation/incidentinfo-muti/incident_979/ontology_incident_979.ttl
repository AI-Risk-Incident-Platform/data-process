@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AppleAI rdf:type airo:AISystem ;
    rdfs:label "Apple AI System"@en ;
    rdfs:label "苹果AI系统"@zh ;
    airo:hasRisk ex:InappropriateContentRisk ;
    airo:hasAISubject ex:ElderlyUser .

ex:InappropriateContentRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Risk"@en ;
    rdfs:label "不当内容风险"@zh ;
    airo:hasConsequence ex:ExposureToExplicitMaterial .

ex:ExposureToExplicitMaterial rdf:type airo:Consequence ;
    rdfs:label "Exposure to Explicit Material"@en ;
    rdfs:label "接触不当材料"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:ElderlyUser rdf:type airo:AISubject ;
    rdfs:label "Elderly User"@en ;
    rdfs:label "老年用户"@zh .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress"@en ;
    rdfs:label "心理困扰"@zh ;
    airo:hasImpactOnStakeholder ex:ElderlyUser .