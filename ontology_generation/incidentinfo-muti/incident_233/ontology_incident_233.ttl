@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WazeNavigationApp rdf:type airo:AISystem ;
    rdfs:label "Waze Navigation App"@en ;
    rdfs:label "Waze导航应用"@zh ;
    airo:hasRisk ex:NavigationMisguidanceRisk ;
    airo:hasCapability ex:RealTimeRouteOptimization ;
    airo:isAppliedWithinDomain ex:TransportationNavigation .

ex:NavigationMisguidanceRisk rdf:type airo:Risk ;
    rdfs:label "Navigation Misguidance Risk"@en ;
    rdfs:label "导航误导风险"@zh ;
    airo:hasConsequence ex:VehicleAccidents ;
    airo:isRiskSourceFor ex:InadequateWeatherConditionConsideration .

ex:VehicleAccidents rdf:type airo:Consequence ;
    rdfs:label "Vehicle Accidents Consequence"@en ;
    rdfs:label "车辆事故后果"@zh ;
    airo:hasImpact ex:RoadSafetyCompromise .

ex:TransportationNavigation rdf:type airo:Domain ;
    rdfs:label "Transportation Navigation Domain"@en ;
    rdfs:label "交通导航领域"@zh .

ex:RealTimeRouteOptimization rdf:type airo:AICapability ;
    rdfs:label "Real-Time Route Optimization Capability"@en ;
    rdfs:label "实时路线优化能力"@zh .

ex:InadequateWeatherConditionConsideration rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Weather Condition Consideration Risk Source"@en ;
    rdfs:label "天气条件考虑不足风险源"@zh .

ex:RoadSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Road Safety Compromise Impact"@en ;
    rdfs:label "道路安全受损影响"@zh .