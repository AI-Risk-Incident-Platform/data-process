@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RyanairFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "瑞安航空面部识别系统"@zh ;
    rdfs:label "Ryanair Facial Recognition System"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:Ryanair ;
    airo:hasStakeholder ex:PrivacyRightsGroup .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:hasConsequence ex:PersonalDataExposure ;
    airo:isRiskSourceFor ex:InvasiveVerificationProcess .

ex:PrivacyRightsGroup rdf:type airo:Stakeholder ;
    rdfs:label "隐私权组织"@zh ;
    rdfs:label "Privacy Rights Group"@en .

ex:Ryanair rdf:type airo:AIOperator ;
    rdfs:label "瑞安航空"@zh ;
    rdfs:label "Ryanair"@en .

ex:PersonalDataExposure rdf:type airo:Impact ;
    rdfs:label "个人数据暴露"@zh ;
    rdfs:label "Personal Data Exposure"@en ;
    airo:hasImpactOnStakeholder ex:PrivacyRightsGroup .

ex:InvasiveVerificationProcess rdf:type airo:RiskSource ;
    rdfs:label "侵入性验证流程"@zh ;
    rdfs:label "Invasive Verification Process"@en ;
    airo:isRiskSourceFor ex:PrivacyViolationRisk .