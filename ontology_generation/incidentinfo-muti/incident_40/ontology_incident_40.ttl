@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIInJudicialSystem rdf:type airo:AISystem ;
    rdfs:label "AI in Judicial System"@en ;
    rdfs:label "司法系统中的AI"@zh ;
    airo:hasRisk ex:RacialBiasInAI ;
    airo:hasStakeholder ex:MarginalizedCommunities ;
    airo:isAppliedWithinDomain ex:JudicialSystem .

ex:RacialBiasInAI rdf:type airo:Risk ;
    rdfs:label "Racial Bias in AI Risk"@en ;
    rdfs:label "AI中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryOutcomes ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:DiscriminatoryOutcomes rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Outcomes Consequence"@en ;
    rdfs:label "歧视性结果后果"@zh ;
    airo:hasImpact ex:SocialInequality .

ex:SocialInequality rdf:type airo:Impact ;
    rdfs:label "Social Inequality Impact"@en ;
    rdfs:label "社会不平等影响"@zh ;
    airo:hasImpactOnStakeholder ex:MarginalizedCommunities .

ex:MarginalizedCommunities rdf:type airo:Stakeholder ;
    rdfs:label "Marginalized Communities"@en ;
    rdfs:label "边缘化群体"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:JudicialSystem rdf:type airo:Domain ;
    rdfs:label "Judicial System Domain"@en ;
    rdfs:label "司法系统领域"@zh .