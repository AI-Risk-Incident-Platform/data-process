@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CruiseDriverlessCars rdf:type airo:AISystem ;
    rdfs:label "Cruise无人驾驶汽车"@zh ;
    rdfs:label "Cruise Driverless Cars"@en ;
    airo:hasRisk ex:EndangeringChildrenRisk ;
    airo:hasCapability ex:ChildDetection ;
    airo:isDeployedBy ex:Cruise ;
    airo:isAppliedWithinDomain ex:AutonomousVehicles .

ex:EndangeringChildrenRisk rdf:type airo:Risk ;
    rdfs:label "危及儿童风险"@zh ;
    rdfs:label "Endangering Children Risk"@en ;
    airo:hasConsequence ex:ChildEndangerment ;
    airo:isRiskSourceFor ex:DetectionIssues .

ex:DetectionIssues rdf:type airo:RiskSource ;
    rdfs:label "检测问题"@zh ;
    rdfs:label "Detection Issues"@en .

ex:ChildEndangerment rdf:type airo:Consequence ;
    rdfs:label "儿童危险"@zh ;
    rdfs:label "Child Endangerment"@en .

ex:Cruise rdf:type airo:AIOperator ;
    rdfs:label "Cruise公司"@zh ;
    rdfs:label "Cruise"@en .

ex:ChildDetection rdf:type airo:AICapability ;
    rdfs:label "儿童检测能力"@zh ;
    rdfs:label "Child Detection Capability"@en .

ex:AutonomousVehicles rdf:type airo:Domain ;
    rdfs:label "自动驾驶汽车领域"@zh ;
    rdfs:label "Autonomous Vehicles Domain"@en .