ex:AIChatbotsForStalking rdf:type airo:AISystem ;
    rdfs:label "用于跟踪的AI聊天机器人"@zh ;
    rdfs:label "AI Chatbots for Stalking"@en ;
    airo:hasRisk ex:PrivacyViolationAndPhysicalThreat ;
    airo:hasAISubject ex:StalkingVictim ;
    airo:hasStakeholder ex:Professor ;
    airo:hasMisuse ex:AIForCriminalActivities .

ex:PrivacyViolationAndPhysicalThreat rdf:type airo:Risk ;
    rdfs:label "隐私侵犯和人身威胁风险"@zh ;
    rdfs:label "Privacy Violation and Physical Threat Risk"@en ;
    airo:hasConsequence ex:PsychologicalDistress .

ex:PsychologicalDistress rdf:type airo:Consequence ;
    rdfs:label "心理困扰后果"@zh ;
    rdfs:label "Psychological Distress Consequence"@en ;
    airo:hasImpactOnStakeholder ex:Professor .

ex:StalkingVictim rdf:type airo:AISubject ;
    rdfs:label "跟踪受害者"@zh ;
    rdfs:label "Stalking Victim"@en .

ex:Professor rdf:type airo:Stakeholder ;
    rdfs:label "教授"@zh ;
    rdfs:label "Professor"@en .

ex:AIForCriminalActivities rdf:type airo:Misuse ;
    rdfs:label "用于犯罪活动的AI滥用"@zh ;
    rdfs:label "AI for Criminal Activities Misuse"@en .