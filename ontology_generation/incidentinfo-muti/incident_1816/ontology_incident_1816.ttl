@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CohereAI rdf:type airo:AISystem ;
    rdfs:label "Cohere AI"@en ;
    rdfs:label "Cohere AI"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:Publishers ;
    airo:isDeployedBy ex:Cohere .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:isRiskSourceFor ex:AllegedCopyrightInfringement ;
    airo:hasImpactOnStakeholder ex:Publishers .

ex:Publishers rdf:type airo:Stakeholder ;
    rdfs:label "Publishers"@en ;
    rdfs:label "出版商"@zh .

ex:Cohere rdf:type airo:AIOperator ;
    rdfs:label "Cohere"@en ;
    rdfs:label "Cohere"@zh .

ex:AllegedCopyrightInfringement rdf:type airo:RiskSource ;
    rdfs:label "Alleged Copyright Infringement"@en ;
    rdfs:label "涉嫌版权侵权"@zh .