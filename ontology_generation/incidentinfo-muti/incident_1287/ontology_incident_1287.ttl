ex:ViggleAI rdf:type airo:AISystem ;
    rdfs:label "Viggle AI"@en ;
    rdfs:label "Viggle AI"@zh ;
    airo:hasRisk ex:UncontrollableAICharacterRisk ;
    airo:hasCapability ex:CharacterGeneration ;
    airo:hasPurpose ex:MemeCreation ;
    airo:isAppliedWithinDomain ex:SocialMedia .

ex:UncontrollableAICharacterRisk rdf:type airo:Risk ;
    rdfs:label "Uncontrollable AI Character Risk"@en ;
    rdfs:label "不可控AI角色风险"@zh .

ex:CharacterGeneration rdf:type airo:AICapability ;
    rdfs:label "Character Generation Capability"@en ;
    rdfs:label "角色生成能力"@zh .

ex:MemeCreation rdf:type airo:Purpose ;
    rdfs:label "Meme Creation Purpose"@en ;
    rdfs:label "表情包创作目的"@zh .

ex:SocialMedia rdf:type airo:Domain ;
    rdfs:label "Social Media Domain"@en ;
    rdfs:label "社交媒体领域"@zh .