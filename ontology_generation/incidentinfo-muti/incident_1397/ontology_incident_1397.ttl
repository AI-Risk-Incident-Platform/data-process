@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAI rdf:type airo:AISystem ;
    rdfs:label "Generative AI"@en ;
    rdfs:label "生成式AI"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:usesTechnique ex:TextGeneration ;
    airo:hasStakeholder ex:Authors .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:UnauthorizedUseOfCopyrightedMaterial ;
    airo:isRiskSourceFor ex:PiratedBooks .

ex:TextGeneration rdf:type airo:AITechnique ;
    rdfs:label "Text Generation Technique"@en ;
    rdfs:label "文本生成技术"@zh .

ex:Authors rdf:type airo:Stakeholder ;
    rdfs:label "Authors"@en ;
    rdfs:label "作者"@zh .

ex:PiratedBooks rdf:type airo:RiskSource ;
    rdfs:label "Pirated Books"@en ;
    rdfs:label "盗版书籍"@zh .

ex:UnauthorizedUseOfCopyrightedMaterial rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Use of Copyrighted Material"@en ;
    rdfs:label "未经授权使用受版权保护材料"@zh ;
    airo:hasImpact ex:EconomicLossForAuthors .

ex:EconomicLossForAuthors rdf:type airo:Impact ;
    rdfs:label "Economic Loss for Authors"@en ;
    rdfs:label "作者经济损失"@zh ;
    airo:hasImpactOnStakeholder ex:Authors .