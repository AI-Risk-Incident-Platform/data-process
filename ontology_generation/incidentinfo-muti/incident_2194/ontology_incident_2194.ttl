@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DataSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Data Surveillance System"@en ;
    rdfs:label "数据监控系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasPurpose ex:CareProvision ;
    airo:hasStakeholder ex:IndividualsUnderSurveillance .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LossOfPersonalPrivacy .

ex:IndividualsUnderSurveillance rdf:type airo:Stakeholder ;
    rdfs:label "Individuals Under Surveillance"@en ;
    rdfs:label "被监控个体"@zh .

ex:LossOfPersonalPrivacy rdf:type airo:Impact ;
    rdfs:label "Loss of Personal Privacy"@en ;
    rdfs:label "个人隐私丧失"@zh ;
    airo:hasImpactOnStakeholder ex:IndividualsUnderSurveillance .

ex:CareProvision rdf:type airo:Purpose ;
    rdfs:label "Care Provision"@en ;
    rdfs:label "护理提供"@zh .