@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedNewsletters rdf:type airo:AISystem ;
    rdfs:label "AI Generated Newsletters"@en ;
    rdfs:label "AI生成新闻通讯"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasStakeholder ex:NewsConsumers .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:AutomatedContentGeneration .

ex:NewsConsumers rdf:type airo:Stakeholder ;
    rdfs:label "News Consumers"@en ;
    rdfs:label "新闻消费者"@zh .

ex:AutomatedContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Automated Content Generation"@en ;
    rdfs:label "自动化内容生成"@zh .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation"@en ;
    rdfs:label "公众误信"@zh ;
    airo:hasImpactOnStakeholder ex:NewsConsumers .