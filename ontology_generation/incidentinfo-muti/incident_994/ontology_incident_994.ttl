@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:FalseInformationRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:NorwegianMan ;
    airo:hasAISubject ex:IndividualsWithChildren .

ex:FalseInformationRisk rdf:type airo:Risk ;
    rdfs:label "False Information Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:EmotionalDistress ;
    airo:isRiskSourceFor ex:InaccurateResponseGeneration .

ex:EmotionalDistress rdf:type airo:Consequence ;
    rdfs:label "Emotional Distress Consequence"@en ;
    rdfs:label "情绪困扰后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:NorwegianMan rdf:type airo:Stakeholder ;
    rdfs:label "Norwegian Man"@en ;
    rdfs:label "挪威男子"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "名誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:NorwegianMan .

ex:InaccurateResponseGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate Response Generation Risk Source"@en ;
    rdfs:label "不准确回答生成风险源"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:IndividualsWithChildren rdf:type airo:AISubject ;
    rdfs:label "Individuals with Children"@en ;
    rdfs:label "有子女的个人"@zh .