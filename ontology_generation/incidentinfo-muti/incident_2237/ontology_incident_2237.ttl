@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:SexualHarassmentRisk ;
    airo:hasStakeholder ex:Coworkers ;
    airo:isDeployedBy ex:FacialRecognitionStartup ;
    airo:hasRisk ex:SexualHarassmentMisuse .

ex:SexualHarassmentRisk rdf:type airo:Risk ;
    rdfs:label "Sexual Harassment Risk"@en ;
    rdfs:label "性骚扰风险"@zh ;
    airo:hasConsequence ex:WorkplaceHarassmentImpact .

ex:Coworkers rdf:type airo:Stakeholder ;
    rdfs:label "Coworkers"@en ;
    rdfs:label "同事"@zh .

ex:FacialRecognitionStartup rdf:type airo:AIOperator ;
    rdfs:label "Facial Recognition Startup"@en ;
    rdfs:label "面部识别初创公司"@zh .

ex:SexualHarassmentMisuse rdf:type airo:Misuse ;
    rdfs:label "Sexual Harassment Misuse"@en ;
    rdfs:label "性骚扰滥用"@zh ;
    airo:isRiskSourceFor ex:SexualHarassmentRisk .

ex:WorkplaceHarassmentImpact rdf:type airo:Impact ;
    rdfs:label "Workplace Harassment Impact"@en ;
    rdfs:label "职场骚扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:Coworkers .