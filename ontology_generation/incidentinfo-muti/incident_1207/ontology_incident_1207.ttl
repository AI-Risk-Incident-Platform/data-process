@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NaverAISystem rdf:type airo:AISystem ;
    rdfs:label "Naver AI System"@en ;
    rdfs:label "Naver人工智能系统"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDeployedBy ex:NaverCorporation ;
    airo:hasStakeholder ex:KoreanBroadcasters ;
    airo:isAppliedWithinDomain ex:MediaAndEntertainmentDomain .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalActionConsequence ;
    airo:isRiskSourceFor ex:UnauthorizedContentUse .

ex:KoreanBroadcasters rdf:type airo:Stakeholder ;
    rdfs:label "Korean Broadcasters"@en ;
    rdfs:label "韩国广播公司"@zh .

ex:NaverCorporation rdf:type airo:AIOperator ;
    rdfs:label "Naver Corporation"@en ;
    rdfs:label "Naver公司"@zh .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Action Consequence"@en ;
    rdfs:label "法律诉讼后果"@zh ;
    airo:hasImpactOnStakeholder ex:KoreanBroadcasters .

ex:UnauthorizedContentUse rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Content Use"@en ;
    rdfs:label "未经授权的内容使用"@zh .

ex:MediaAndEntertainmentDomain rdf:type airo:Domain ;
    rdfs:label "Media and Entertainment Domain"@en ;
    rdfs:label "媒体与娱乐领域"@zh .