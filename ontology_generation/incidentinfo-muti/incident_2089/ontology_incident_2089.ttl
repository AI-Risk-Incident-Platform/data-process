@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HealthcareAlgorithms rdf:type airo:AISystem ;
    rdfs:label "Healthcare Algorithms"@en ;
    rdfs:label "医疗保健算法"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInHealthcare ;
    airo:isAppliedWithinDomain ex:HealthcareIndustry .

ex:AlgorithmicBiasInHealthcare rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Healthcare Risk"@en ;
    rdfs:label "医疗保健中的算法偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:HealthcareDisparities .

ex:HealthcareIndustry rdf:type airo:Domain ;
    rdfs:label "Healthcare Industry"@en ;
    rdfs:label "医疗保健行业"@zh .

ex:HealthcareDisparities rdf:type airo:Impact ;
    rdfs:label "Healthcare Disparities Impact"@en ;
    rdfs:label "医疗保健差异的影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .