@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasCapability ex:FaceAndVoiceSynthesis ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "金融欺诈风险"@zh ;
    rdfs:label "Financial Fraud Risk"@en ;
    airo:hasConsequence ex:ScammingThroughIdentityImpersonation .

ex:FaceAndVoiceSynthesis rdf:type airo:AICapability ;
    rdfs:label "面部和语音合成能力"@zh ;
    rdfs:label "Face and Voice Synthesis Capability"@en .

ex:ScammingThroughIdentityImpersonation rdf:type airo:Misuse ;
    rdfs:label "通过身份冒充进行诈骗"@zh ;
    rdfs:label "Scamming Through Identity Impersonation"@en ;
    airo:hasImpact ex:FinancialLossForVictims .

ex:FinancialLossForVictims rdf:type airo:Impact ;
    rdfs:label "受害者经济损失"@zh ;
    rdfs:label "Financial Loss for Victims"@en .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "金融服务领域"@zh ;
    rdfs:label "Financial Services Domain"@en .