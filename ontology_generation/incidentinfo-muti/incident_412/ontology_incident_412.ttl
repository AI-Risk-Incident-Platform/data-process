@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Uber Facial Recognition System"@en ;
    rdfs:label "Uber面部识别系统"@zh ;
    airo:hasRisk ex:DriverAccountLockoutRisk ;
    airo:hasAISubject ex:IndianDrivers ;
    airo:hasStakeholder ex:UberCompany .

ex:DriverAccountLockoutRisk rdf:type airo:Risk ;
    rdfs:label "Driver Account Lockout Risk"@en ;
    rdfs:label "司机账户锁定风险"@zh ;
    airo:hasConsequence ex:DriverAccountAccessDenial ;
    airo:isRiskSourceFor ex:FacialRecognitionSystemFailure .

ex:DriverAccountAccessDenial rdf:type airo:Consequence ;
    rdfs:label "Driver Account Access Denial Consequence"@en ;
    rdfs:label "司机账户访问被拒后果"@zh ;
    airo:hasImpact ex:DriverIncomeLoss .

ex:IndianDrivers rdf:type airo:AISubject ;
    rdfs:label "Indian Drivers"@en ;
    rdfs:label "印度司机"@zh .

ex:UberCompany rdf:type airo:Stakeholder ;
    rdfs:label "Uber Company"@en ;
    rdfs:label "Uber公司"@zh .

ex:FacialRecognitionSystemFailure rdf:type airo:RiskSource ;
    rdfs:label "Facial Recognition System Failure Risk Source"@en ;
    rdfs:label "面部识别系统故障风险源"@zh .

ex:DriverIncomeLoss rdf:type airo:Impact ;
    rdfs:label "Driver Income Loss Impact"@en ;
    rdfs:label "司机收入损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:IndianDrivers .