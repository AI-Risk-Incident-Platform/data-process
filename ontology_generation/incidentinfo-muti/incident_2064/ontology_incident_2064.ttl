@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NioPilotAutonomousDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "Nio Pilot Autonomous Driving System"@en ;
    rdfs:label "Nio Pilot自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:hasStakeholder ex:NioUsers ;
    airo:isDeployedBy ex:Nio .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    rdfs:label "自动驾驶致命事故风险"@zh ;
    airo:hasConsequence ex:ReputationDamage ;
    airo:hasRiskControl ex:UserTestRequirement .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "声誉损害后果"@zh ;
    airo:hasImpact ex:StockPriceDrop .

ex:Nio rdf:type airo:AIOperator ;
    rdfs:label "Nio"@en ;
    rdfs:label "蔚来汽车"@zh .

ex:NioUsers rdf:type airo:Stakeholder ;
    rdfs:label "Nio Users"@en ;
    rdfs:label "蔚来用户"@zh .

ex:UserTestRequirement rdf:type airo:RiskControl ;
    rdfs:label "User Test Requirement Control"@en ;
    rdfs:label "用户测试要求管控"@zh .

ex:StockPriceDrop rdf:type airo:Impact ;
    rdfs:label "Stock Price Drop Impact"@en ;
    rdfs:label "股价下跌影响"@zh ;
    airo:hasImpactOnStakeholder ex:NioUsers .