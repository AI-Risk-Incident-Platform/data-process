@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedSpamSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Spam System"@en ;
    rdfs:label "AI生成垃圾信息系统"@zh ;
    airo:hasRisk ex:SocialMediaSpamRisk ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:usesTechnique ex:ContentGenerationAI .

ex:SocialMediaSpamRisk rdf:type airo:Risk ;
    rdfs:label "Social Media Spam Risk"@en ;
    rdfs:label "社交媒体垃圾信息风险"@zh ;
    airo:hasImpact ex:UserExperienceDegradation .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:UserExperienceDegradation rdf:type airo:Impact ;
    rdfs:label "User Experience Degradation Impact"@en ;
    rdfs:label "用户体验下降影响"@zh .

ex:ContentGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Content Generation AI Technique"@en ;
    rdfs:label "内容生成AI技术"@zh .