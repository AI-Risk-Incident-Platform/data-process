@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HoodlineAINewsSystem rdf:type airo:AISystem ;
    rdfs:label "Hoodline人工智能新闻系统"@zh ;
    rdfs:label "Hoodline AI News System"@en ;
    airo:hasRisk ex:FalseAccusationRisk ;
    airo:usesTechnique ex:AutomatedContentGeneration ;
    airo:hasStakeholder ex:SanMateoDistrictAttorney .

ex:FalseAccusationRisk rdf:type airo:Risk ;
    rdfs:label "虚假指控风险"@zh ;
    rdfs:label "False Accusation Risk"@en ;
    airo:hasConsequence ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "声誉损害后果"@zh ;
    rdfs:label "Reputation Damage Consequence"@en ;
    airo:hasImpact ex:LegalAndProfessionalConsequences .

ex:SanMateoDistrictAttorney rdf:type airo:Stakeholder ;
    rdfs:label "圣马特奥地区检察官"@zh ;
    rdfs:label "San Mateo District Attorney"@en .

ex:AutomatedContentGeneration rdf:type airo:AITechnique ;
    rdfs:label "自动内容生成技术"@zh ;
    rdfs:label "Automated Content Generation Technique"@en .

ex:LegalAndProfessionalConsequences rdf:type airo:Impact ;
    rdfs:label "法律和职业影响"@zh ;
    rdfs:label "Legal and Professional Consequences Impact"@en ;
    airo:hasImpactOnStakeholder ex:SanMateoDistrictAttorney .