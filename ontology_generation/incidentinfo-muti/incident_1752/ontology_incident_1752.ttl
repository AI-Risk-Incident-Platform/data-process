@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonGoFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "亚马逊Go人脸识别系统"@zh ;
    rdfs:label "Amazon Go Facial Recognition System"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:Amazon ;
    airo:hasAISubject ex:StoreCustomers ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:isAppliedWithinDomain ex:RetailSurveillance .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:isRiskSourceFor ex:LackOfTransparencyInDataCollection ;
    airo:hasConsequence ex:PersonalPrivacyCompromise .

ex:Amazon rdf:type airo:Stakeholder ;
    rdfs:label "亚马逊"@zh ;
    rdfs:label "Amazon"@en .

ex:StoreCustomers rdf:type airo:AISubject ;
    rdfs:label "商店顾客"@zh ;
    rdfs:label "Store Customers"@en .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "人脸识别技术"@zh ;
    rdfs:label "Facial Recognition Technology"@en .

ex:PersonalPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "个人隐私泄露影响"@zh ;
    rdfs:label "Personal Privacy Compromise Impact"@en ;
    airo:hasImpactOnStakeholder ex:StoreCustomers .

ex:LackOfTransparencyInDataCollection rdf:type airo:RiskSource ;
    rdfs:label "数据收集缺乏透明度风险源"@zh ;
    rdfs:label "Lack of Transparency in Data Collection Risk Source"@en .

ex:RetailSurveillance rdf:type airo:Domain ;
    rdfs:label "零售监控领域"@zh ;
    rdfs:label "Retail Surveillance Domain"@en .