@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlgorithmicAudioSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Algorithmic Audio Surveillance System"@en ;
    rdfs:label "算法音频监控系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:CityOfOrleans ;
    airo:isAppliedWithinDomain ex:PublicSafety .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LegalCondemnation .

ex:CityOfOrleans rdf:type airo:Stakeholder ;
    rdfs:label "City of Orleans"@en ;
    rdfs:label "奥尔良市"@zh .

ex:LegalCondemnation rdf:type airo:Impact ;
    rdfs:label "Legal Condemnation"@en ;
    rdfs:label "法律谴责"@zh ;
    airo:hasImpactOnStakeholder ex:CityOfOrleans .

ex:PublicSafety rdf:type airo:Domain ;
    rdfs:label "Public Safety"@en ;
    rdfs:label "公共安全"@zh .