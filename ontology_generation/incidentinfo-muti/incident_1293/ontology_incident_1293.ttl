@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AustralianFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Australian Facial Recognition System"@en ;
    rdfs:label "澳大利亚面部识别系统"@zh ;
    airo:hasRisk ex:BiometricDataBreachRisk ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasStakeholder ex:AustralianPublic .

ex:BiometricDataBreachRisk rdf:type airo:Risk ;
    rdfs:label "Biometric Data Breach Risk"@en ;
    rdfs:label "生物识别数据泄露风险"@zh ;
    airo:hasConsequence ex:PersonalDataExposure ;
    airo:isRiskSourceFor ex:SystemVulnerability .

ex:PersonalDataExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Data Exposure Consequence"@en ;
    rdfs:label "个人数据暴露后果"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:AustralianPublic rdf:type airo:Stakeholder ;
    rdfs:label "Australian Public"@en ;
    rdfs:label "澳大利亚公众"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:AustralianPublic .

ex:SystemVulnerability rdf:type airo:RiskSource ;
    rdfs:label "System Vulnerability Risk Source"@en ;
    rdfs:label "系统漏洞风险源"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .