@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedPhotoSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Photo System"@en ;
    rdfs:label "AI生成照片系统"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasCapability ex:ImageGenerationCapability ;
    airo:hasPurpose ex:ContentCreationPurpose ;
    airo:isAppliedWithinDomain ex:PublicSectorDomain ;
    airo:usesTechnique ex:GenerativeAdversarialNetwork ;
    airo:hasStakeholder ex:TanzaniaTaxAgency .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisperception ;
    airo:isRiskSourceFor ex:UnverifiedContentCreation .

ex:PublicMisperception rdf:type airo:Consequence ;
    rdfs:label "Public Misperception Consequence"@en ;
    rdfs:label "公众误解后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ImageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Image Generation Capability"@en ;
    rdfs:label "图像生成能力"@zh .

ex:TanzaniaTaxAgency rdf:type airo:Stakeholder ;
    rdfs:label "Tanzania Tax Agency"@en ;
    rdfs:label "坦桑尼亚税务局"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:TanzaniaTaxAgency .

ex:ContentCreationPurpose rdf:type airo:Purpose ;
    rdfs:label "Content Creation Purpose"@en ;
    rdfs:label "内容创作目的"@zh .

ex:PublicSectorDomain rdf:type airo:Domain ;
    rdfs:label "Public Sector Domain"@en ;
    rdfs:label "公共部门领域"@zh .

ex:GenerativeAdversarialNetwork rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Network Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:UnverifiedContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Unverified Content Creation Risk Source"@en ;
    rdfs:label "未经核实的内容创作风险源"@zh .