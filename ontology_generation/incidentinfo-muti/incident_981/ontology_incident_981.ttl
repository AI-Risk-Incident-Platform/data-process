ex:DeepfakeVoiceCloneSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Voice Clone System"@en ;
    rdfs:label "深度伪造语音克隆系统"@zh ;
    airo:hasRisk ex:DeepfakeScamRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasStakeholder ex:WPPCEO .

ex:DeepfakeScamRisk rdf:type airo:Risk ;
    rdfs:label "Deepfake Scam Risk"@en ;
    rdfs:label "深度伪造诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialFraud ;
    airo:isRiskSourceFor ex:MaliciousDeepfakeUse .

ex:WPPCEO rdf:type airo:Stakeholder ;
    rdfs:label "WPP CEO"@en ;
    rdfs:label "WPP首席执行官"@zh .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:FinancialFraud rdf:type airo:Consequence ;
    rdfs:label "Financial Fraud Consequence"@en ;
    rdfs:label "金融诈骗后果"@zh ;
    airo:hasImpactOnStakeholder ex:WPPCEO .

ex:MaliciousDeepfakeUse rdf:type airo:RiskSource ;
    rdfs:label "Malicious Deepfake Use Risk Source"@en ;
    rdfs:label "恶意使用深度伪造的风险源"@zh .