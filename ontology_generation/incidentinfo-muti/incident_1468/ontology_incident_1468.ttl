@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BBCAIVoiceoverSystem rdf:type airo:AISystem ;
    rdfs:label "BBC AI语音系统"@zh ;
    rdfs:label "BBC AI Voiceover System"@en ;
    airo:hasRisk ex:JobDisplacementRisk ;
    airo:hasStakeholder ex:SaraPoyzer ;
    airo:isDeployedBy ex:BBC .

ex:JobDisplacementRisk rdf:type airo:Risk ;
    rdfs:label "工作替代风险"@zh ;
    rdfs:label "Job Displacement Risk"@en ;
    airo:hasImpact ex:PublicOutrage .

ex:SaraPoyzer rdf:type airo:Stakeholder ;
    rdfs:label "莎拉·波伊泽"@zh ;
    rdfs:label "Sara Poyzer"@en .

ex:PublicOutrage rdf:type airo:Impact ;
    rdfs:label "公众愤怒"@zh ;
    rdfs:label "Public Outrage"@en ;
    airo:hasImpactOnStakeholder ex:SaraPoyzer .

ex:BBC rdf:type airo:AIOperator ;
    rdfs:label "英国广播公司"@zh ;
    rdfs:label "BBC"@en .