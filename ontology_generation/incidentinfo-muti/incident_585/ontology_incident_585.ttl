@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AustralianTerrorismPredictionTool rdf:type airo:AISystem ;
    rdfs:label "Australian Terrorism Prediction Tool"@en ;
    rdfs:label "澳大利亚恐怖主义预测工具"@zh ;
    airo:hasRisk ex:DiscriminatoryRisk ;
    airo:hasAISubject ex:AutisticIndividuals ;
    airo:isAppliedWithinDomain ex:CounterTerrorism .

ex:DiscriminatoryRisk rdf:type airo:Risk ;
    rdfs:label "Discriminatory Risk"@en ;
    rdfs:label "歧视性风险"@zh ;
    airo:hasConsequence ex:UnjustCriminalization ;
    airo:isRiskSourceFor ex:BiasedAlgorithmDesign .

ex:UnjustCriminalization rdf:type airo:Consequence ;
    rdfs:label "Unjust Criminalization Consequence"@en ;
    rdfs:label "不公正刑事定罪后果"@zh ;
    airo:hasImpact ex:HarmToAutisticIndividuals .

ex:HarmToAutisticIndividuals rdf:type airo:Impact ;
    rdfs:label "Harm to Autistic Individuals Impact"@en ;
    rdfs:label "对自闭症患者的伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:AutisticIndividuals .

ex:BiasedAlgorithmDesign rdf:type airo:RiskSource ;
    rdfs:label "Biased Algorithm Design Risk Source"@en ;
    rdfs:label "有偏见的算法设计风险源"@zh .

ex:AutisticIndividuals rdf:type airo:AISubject ;
    rdfs:label "Autistic Individuals"@en ;
    rdfs:label "自闭症患者"@zh .

ex:CounterTerrorism rdf:type airo:Domain ;
    rdfs:label "Counter-Terrorism Domain"@en ;
    rdfs:label "反恐领域"@zh .