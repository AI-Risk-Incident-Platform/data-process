ex:SexFantasyChatbots rdf:type airo:AISystem ;
    rdfs:label "性幻想聊天机器人"@zh ;
    rdfs:label "Sex Fantasy Chatbots"@en ;
    airo:hasRisk ex:ExplicitMessagesLeakRisk ;
    airo:hasCapability ex:ConversationalAIForAdultContent .

ex:ExplicitMessagesLeakRisk rdf:type airo:Risk ;
    rdfs:label "露骨信息泄露风险"@zh ;
    rdfs:label "Explicit Messages Leak Risk"@en ;
    airo:hasConsequence ex:PrivacyViolation ;
    airo:isRiskSourceFor ex:InadequateContentFiltering .

ex:ConversationalAIForAdultContent rdf:type airo:AICapability ;
    rdfs:label "成人内容对话AI能力"@zh ;
    rdfs:label "Conversational AI for Adult Content Capability"@en .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "隐私侵犯后果"@zh ;
    rdfs:label "Privacy Violation Consequence"@en ;
    airo:hasImpact ex:UserReputationDamage .

ex:UserReputationDamage rdf:type airo:Impact ;
    rdfs:label "用户声誉损害影响"@zh ;
    rdfs:label "User Reputation Damage Impact"@en .

ex:InadequateContentFiltering rdf:type airo:RiskSource ;
    rdfs:label "内容过滤不足风险源"@zh ;
    rdfs:label "Inadequate Content Filtering Risk Source"@en .