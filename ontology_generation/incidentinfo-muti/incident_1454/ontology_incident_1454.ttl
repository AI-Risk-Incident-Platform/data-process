ex:AIPeerReviewAssistance rdf:type airo:AISystem ;
    rdfs:label "AI Peer-Review Assistance System"@en ;
    rdfs:label "AI同行评审辅助系统"@zh ;
    airo:hasRisk ex:PeerReviewQualityRisk ;
    airo:hasStakeholder ex:AIResearchers ;
    airo:isAppliedWithinDomain ex:AcademicResearch ;
    airo:hasPurpose ex:AutomatingPeerReviewProcess .

ex:PeerReviewQualityRisk rdf:type airo:Risk ;
    rdfs:label "Peer Review Quality Risk"@en ;
    rdfs:label "同行评审质量风险"@zh .

ex:AIResearchers rdf:type airo:Stakeholder ;
    rdfs:label "AI Researchers"@en ;
    rdfs:label "AI研究人员"@zh .

ex:AcademicResearch rdf:type airo:Domain ;
    rdfs:label "Academic Research Domain"@en ;
    rdfs:label "学术研究领域"@zh .

ex:AutomatingPeerReviewProcess rdf:type airo:Purpose ;
    rdfs:label "Automating Peer Review Process Purpose"@en ;
    rdfs:label "自动化同行评审流程的目的"@zh .