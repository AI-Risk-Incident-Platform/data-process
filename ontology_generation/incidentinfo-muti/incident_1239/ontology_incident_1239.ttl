@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAI rdf:type airo:AISystem ;
    rdfs:label "生成式AI"@zh ;
    rdfs:label "Generative AI"@en ;
    airo:hasRisk ex:CulturalNarrativeEndangerment ;
    airo:hasCapability ex:ImageGeneration ;
    airo:hasAISubject ex:CulturalCommunities .

ex:CulturalNarrativeEndangerment rdf:type airo:Risk ;
    rdfs:label "文化叙事危害风险"@zh ;
    rdfs:label "Cultural Narrative Endangerment Risk"@en ;
    airo:hasConsequence ex:CulturalRepresentationBias ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:ImageGeneration rdf:type airo:AICapability ;
    rdfs:label "图像生成能力"@zh ;
    rdfs:label "Image Generation Capability"@en .

ex:CulturalRepresentationBias rdf:type airo:Consequence ;
    rdfs:label "文化代表性偏见后果"@zh ;
    rdfs:label "Cultural Representation Bias Consequence"@en ;
    airo:hasImpact ex:StereotypeReinforcement .

ex:StereotypeReinforcement rdf:type airo:Impact ;
    rdfs:label "刻板印象强化影响"@zh ;
    rdfs:label "Stereotype Reinforcement Impact"@en ;
    airo:hasImpactOnStakeholder ex:CulturalCommunities .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "偏见训练数据风险源"@zh ;
    rdfs:label "Biased Training Data Risk Source"@en .

ex:CulturalCommunities rdf:type airo:AISubject ;
    rdfs:label "文化社群"@zh ;
    rdfs:label "Cultural Communities"@en .