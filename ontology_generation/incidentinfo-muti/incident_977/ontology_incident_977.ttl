@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVirtualImageSystem rdf:type airo:AISystem ;
    rdfs:label "AI Virtual Image System"@en ;
    rdfs:label "AI虚拟形象系统"@zh ;
    airo:hasRisk ex:UncannyValleyEffectRisk ;
    airo:hasStakeholder ex:ArtGalleryVisitors .

ex:UncannyValleyEffectRisk rdf:type airo:Risk ;
    rdfs:label "Uncanny Valley Effect Risk"@en ;
    rdfs:label "恐怖谷效应风险"@zh ;
    airo:hasConsequence ex:PublicBacklashConsequence .

ex:PublicBacklashConsequence rdf:type airo:Consequence ;
    rdfs:label "Public Backlash Consequence"@en ;
    rdfs:label "公众反弹后果"@zh ;
    airo:hasImpact ex:NegativePublicPerceptionImpact .

ex:NegativePublicPerceptionImpact rdf:type airo:Impact ;
    rdfs:label "Negative Public Perception Impact"@en ;
    rdfs:label "负面公众认知影响"@zh ;
    airo:hasImpactOnStakeholder ex:ArtGalleryVisitors .

ex:ArtGalleryVisitors rdf:type airo:Stakeholder ;
    rdfs:label "Art Gallery Visitors"@en ;
    rdfs:label "美术馆参观者"@zh .