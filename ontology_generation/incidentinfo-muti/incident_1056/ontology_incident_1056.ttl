@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVideoDeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "AI Video Deepfake Technology"@en ;
    rdfs:label "AI视频深度伪造技术"@zh ;
    airo:hasRisk ex:PublicFraudRisk ;
    airo:usesTechnique ex:DeepfakeVideoGeneration ;
    airo:hasPurpose ex:FraudulentInvestmentScheme ;
    airo:hasStakeholder ex:CyprusPublic .

ex:PublicFraudRisk rdf:type airo:Risk ;
    rdfs:label "Public Fraud Risk"@en ;
    rdfs:label "公众欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:MaliciousUseOfAITechnology .

ex:DeepfakeVideoGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Video Generation Technique"@en ;
    rdfs:label "深度伪造视频生成技术"@zh .

ex:CyprusPublic rdf:type airo:Stakeholder ;
    rdfs:label "Cyprus Public"@en ;
    rdfs:label "塞浦路斯公众"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:CyprusPublic .

ex:MaliciousUseOfAITechnology rdf:type airo:RiskSource ;
    rdfs:label "Malicious Use of AI Technology Risk Source"@en ;
    rdfs:label "AI技术的恶意使用风险源"@zh .

ex:FraudulentInvestmentScheme rdf:type airo:Purpose ;
    rdfs:label "Fraudulent Investment Scheme Purpose"@en ;
    rdfs:label "欺诈性投资计划目的"@zh .