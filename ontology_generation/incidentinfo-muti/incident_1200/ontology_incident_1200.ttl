@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepSeekAIChatbot rdf:type airo:AISystem ;
    rdfs:label "DeepSeek AI Chatbot"@en ;
    rdfs:label "DeepSeek AI聊天机器人"@zh ;
    airo:hasRisk ex:SecurityTestFailureRisk ;
    airo:hasStakeholder ex:SecurityResearchers .

ex:SecurityTestFailureRisk rdf:type airo:Risk ;
    rdfs:label "Security Test Failure Risk"@en ;
    rdfs:label "安全测试失败风险"@zh ;
    airo:isRiskSourceFor ex:InadequateSafetyGuardrails ;
    airo:hasConsequence ex:PotentialSecurityBreaches .

ex:SecurityResearchers rdf:type airo:Stakeholder ;
    rdfs:label "Security Researchers"@en ;
    rdfs:label "安全研究人员"@zh .

ex:InadequateSafetyGuardrails rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Safety Guardrails"@en ;
    rdfs:label "安全防护不足"@zh .

ex:PotentialSecurityBreaches rdf:type airo:Consequence ;
    rdfs:label "Potential Security Breaches"@en ;
    rdfs:label "潜在安全漏洞"@zh .