@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GrabDriverAllocationSystem rdf:type airo:AISystem ;
    rdfs:label "Grab Driver Allocation System"@en ;
    rdfs:label "Grab司机分配系统"@zh ;
    airo:hasRisk ex:DriverDiscriminationRisk ;
    airo:hasStakeholder ex:IndonesianCompetitionWatchdog ;
    airo:isAppliedWithinDomain ex:RideHailingService ;
    airo:isDeployedBy ex:Grab .

ex:DriverDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Driver Discrimination Risk"@en ;
    rdfs:label "司机歧视风险"@zh ;
    airo:hasConsequence ex:UnfairMarketCompetition .

ex:IndonesianCompetitionWatchdog rdf:type airo:Stakeholder ;
    rdfs:label "Indonesian Competition Watchdog"@en ;
    rdfs:label "印尼竞争监管机构"@zh .

ex:UnfairMarketCompetition rdf:type airo:Consequence ;
    rdfs:label "Unfair Market Competition Consequence"@en ;
    rdfs:label "不公平市场竞争后果"@zh ;
    airo:hasImpact ex:MarketDistortion .

ex:MarketDistortion rdf:type airo:Impact ;
    rdfs:label "Market Distortion Impact"@en ;
    rdfs:label "市场扭曲影响"@zh ;
    airo:hasImpactOnStakeholder ex:IndonesianCompetitionWatchdog .

ex:Grab rdf:type airo:AIOperator ;
    rdfs:label "Grab"@en ;
    rdfs:label "Grab公司"@zh .

ex:RideHailingService rdf:type airo:Domain ;
    rdfs:label "Ride-Hailing Service Domain"@en ;
    rdfs:label "网约车服务领域"@zh .