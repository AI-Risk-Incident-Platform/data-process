@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Content Moderation System"@en ;
    rdfs:label "Facebook内容审核系统"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:hasPurpose ex:ContentModerationForPublicSafety ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatform ;
    airo:hasStakeholder ex:NewsPublishers ;
    airo:isDeployedBy ex:FacebookInc .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "错误信息传播风险"@zh ;
    airo:hasConsequence ex:PublicHealthInformationGap ;
    airo:isRiskSourceFor ex:OverzealousContentFiltering .

ex:PublicHealthInformationGap rdf:type airo:Consequence ;
    rdfs:label "Public Health Information Gap Consequence"@en ;
    rdfs:label "公共卫生信息缺口后果"@zh ;
    airo:hasImpact ex:PublicMisinformationImpact .

ex:NewsPublishers rdf:type airo:Stakeholder ;
    rdfs:label "News Publishers"@en ;
    rdfs:label "新闻出版商"@zh .

ex:FacebookInc rdf:type airo:AIOperator ;
    rdfs:label "Facebook Inc."@en ;
    rdfs:label "Facebook公司"@zh .

ex:PublicMisinformationImpact rdf:type airo:Impact ;
    rdfs:label "Public Misinformation Impact"@en ;
    rdfs:label "公众错误信息影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewsPublishers .

ex:ContentModerationForPublicSafety rdf:type airo:Purpose ;
    rdfs:label "Content Moderation for Public Safety Purpose"@en ;
    rdfs:label "为公共安全进行内容审核的目的"@zh .

ex:SocialMediaPlatform rdf:type airo:Domain ;
    rdfs:label "Social Media Platform Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:OverzealousContentFiltering rdf:type airo:RiskSource ;
    rdfs:label "Overzealous Content Filtering Risk Source"@en ;
    rdfs:label "过度内容过滤的风险源"@zh .