ex:ImageBasedAITools rdf:type airo:AISystem ;
    rdfs:label "Image-Based AI Tools"@en ;
    rdfs:label "基于图像的AI工具"@zh ;
    airo:hasRisk ex:ElectionMistrustRisk ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:ElectionMistrustRisk rdf:type airo:Risk ;
    rdfs:label "Election Mistrust Risk"@en ;
    rdfs:label "选举不信任风险"@zh ;
    airo:hasConsequence ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Consequence ;
    rdfs:label "Public Trust Erosion Consequence"@en ;
    rdfs:label "公众信任侵蚀后果"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .