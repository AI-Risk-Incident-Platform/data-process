@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingBeta rdf:type airo:AISystem ;
    rdfs:label "Tesla Full Self-Driving Beta"@en ;
    rdfs:label "特斯拉全自动驾驶测试版"@zh ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:isDevelopedBy ex:TeslaInc ;
    airo:hasStakeholder ex:TeslaVehicleOwners ;
    airo:hasPurpose ex:AutonomousDrivingTechnologyDevelopment ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:MachineLearningForAutonomousDriving .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    airo:hasConsequence ex:VehicleCrashes ;
    airo:isRiskSourceFor ex:SoftwareDefectsInAutonomousDriving .

ex:VehicleCrashes rdf:type airo:Consequence ;
    rdfs:label "Vehicle Crashes Consequence"@en ;
    rdfs:label "车辆碰撞后果"@zh ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:TeslaVehicleOwners rdf:type airo:Stakeholder ;
    rdfs:label "Tesla Vehicle Owners"@en ;
    rdfs:label "特斯拉车主"@zh .

ex:SoftwareDefectsInAutonomousDriving rdf:type airo:RiskSource ;
    rdfs:label "Software Defects in Autonomous Driving Risk Source"@en ;
    rdfs:label "自动驾驶软件缺陷风险源"@zh .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Public Safety Threat Impact"@en ;
    rdfs:label "公共安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:TeslaVehicleOwners .

ex:AutonomousDrivingTechnologyDevelopment rdf:type airo:Purpose ;
    rdfs:label "Autonomous Driving Technology Development Purpose"@en ;
    rdfs:label "自动驾驶技术开发目的"@zh .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "Automotive Industry Domain"@en ;
    rdfs:label "汽车工业领域"@zh .

ex:MachineLearningForAutonomousDriving rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning for Autonomous Driving Technique"@en ;
    rdfs:label "自动驾驶机器学习技术"@zh .