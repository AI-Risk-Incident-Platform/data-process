@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MicrosoftDynamics365 rdf:type airo:AISystem ;
    rdfs:label "Microsoft Dynamics 365"@en ;
    rdfs:label "微软Dynamics 365"@zh ;
    airo:hasRisk ex:WorkerSurveillanceRisk ;
    airo:hasStakeholder ex:Employees ;
    airo:isDevelopedBy ex:Microsoft .

ex:WorkerSurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Worker Surveillance Risk"@en ;
    rdfs:label "员工监控风险"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:Employees rdf:type airo:Stakeholder ;
    rdfs:label "Employees"@en ;
    rdfs:label "员工"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:Employees .