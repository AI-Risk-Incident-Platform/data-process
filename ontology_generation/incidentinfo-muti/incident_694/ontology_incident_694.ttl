ex:HitlerDeepfake rdf:type airo:AISystem ;
    rdfs:label "Hitler Deepfake"@en ;
    rdfs:label "希特勒深度伪造"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
