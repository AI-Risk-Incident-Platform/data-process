@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UpstartAILendingSystem rdf:type airo:AISystem ;
    rdfs:label "Upstart AI Lending System"@en ;
    rdfs:label "Upstart AI贷款系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInLending ;
    airo:hasAICapability ex:CreditRiskAssessment ;
    airo:hasPurpose ex:InclusiveLending ;
    airo:hasStakeholder ex:UpstartInvestors ;
    airo:isDeployedBy ex:UpstartExecutives ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:AlgorithmicBiasInLending rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Lending Risk"@en ;
    rdfs:label "贷款中的算法偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryLendingPractices ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:DiscriminatoryLendingPractices rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Lending Practices Consequence"@en ;
    rdfs:label "歧视性贷款行为后果"@zh ;
    airo:hasImpact ex:FinancialExclusion .

ex:CreditRiskAssessment rdf:type airo:AICapability ;
    rdfs:label "Credit Risk Assessment Capability"@en ;
    rdfs:label "信用风险评估能力"@zh .

ex:UpstartInvestors rdf:type airo:Stakeholder ;
    rdfs:label "Upstart Investors"@en ;
    rdfs:label "Upstart投资者"@zh .

ex:UpstartExecutives rdf:type airo:AIOperator ;
    rdfs:label "Upstart Executives"@en ;
    rdfs:label "Upstart高管"@zh .

ex:FinancialExclusion rdf:type airo:Impact ;
    rdfs:label "Financial Exclusion Impact"@en ;
    rdfs:label "金融排斥影响"@zh ;
    airo:hasImpactOnStakeholder ex:UpstartInvestors .

ex:InclusiveLending rdf:type airo:Purpose ;
    rdfs:label "Inclusive Lending Purpose"@en ;
    rdfs:label "包容性贷款目的"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .