@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en ;
    airo:hasRisk ex:OnlineScamRisk ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasAISubject ex:CelebrityFans ;
    airo:hasStakeholder ex:LeCreusetBrand .

ex:OnlineScamRisk rdf:type airo:Risk ;
    rdfs:label "网络诈骗风险"@zh ;
    rdfs:label "Online Scam Risk"@en ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:CelebrityDeepfakeMisuse .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "经济损失后果"@zh ;
    rdfs:label "Financial Loss Consequence"@en ;
    airo:hasImpact ex:BrandReputationDamage .

ex:CelebrityFans rdf:type airo:AISubject ;
    rdfs:label "名人粉丝"@zh ;
    rdfs:label "Celebrity Fans"@en .

ex:LeCreusetBrand rdf:type airo:Stakeholder ;
    rdfs:label "Le Creuset品牌"@zh ;
    rdfs:label "Le Creuset Brand"@en .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "生成对抗网络"@zh ;
    rdfs:label "Generative Adversarial Networks"@en .

ex:CelebrityDeepfakeMisuse rdf:type airo:Misuse ;
    rdfs:label "名人深度伪造滥用"@zh ;
    rdfs:label "Celebrity Deepfake Misuse"@en .

ex:BrandReputationDamage rdf:type airo:Impact ;
    rdfs:label "品牌声誉损害影响"@zh ;
    rdfs:label "Brand Reputation Damage Impact"@en ;
    airo:hasImpactOnStakeholder ex:LeCreusetBrand .