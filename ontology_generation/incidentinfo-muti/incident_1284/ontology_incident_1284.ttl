@base <https://w3id.org/airo#> .

ex:AIGirlfriendApps rdf:type airo:AISystem ;
    rdfs:label "AI Girlfriend Apps"@en ;
    rdfs:label "AI女友应用"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:PrivacyExperts .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:DataTheft .

ex:DataTheft rdf:type airo:Consequence ;
    rdfs:label "Data Theft"@en ;
    rdfs:label "数据盗窃"@zh ;
    airo:hasImpact ex:EmotionalHarm .

ex:PrivacyExperts rdf:type airo:Stakeholder ;
    rdfs:label "Privacy Experts"@en ;
    rdfs:label "隐私专家"@zh .

ex:EmotionalHarm rdf:type airo:Impact ;
    rdfs:label "Emotional Harm"@en ;
    rdfs:label "情感伤害"@zh ;
    airo:hasImpactOnStakeholder ex:PrivacyExperts .