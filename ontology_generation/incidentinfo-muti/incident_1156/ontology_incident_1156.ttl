@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeAlgorithm rdf:type airo:AISystem ;
    rdfs:label "YouTube Algorithm"@en ;
    rdfs:label "YouTube算法"@zh ;
    airo:hasRisk ex:ChildEndangermentRisk ;
    airo:hasAISubject ex:ChildrenUsers ;
    airo:hasCapability ex:ContentRecommendationAlgorithm .

ex:ChildEndangermentRisk rdf:type airo:Risk ;
    rdfs:label "Child Endangerment Risk"@en ;
    rdfs:label "儿童危害风险"@zh ;
    airo:hasConsequence ex:ExposureToInappropriateContent ;
    airo:isRiskSourceFor ex:AlgorithmicContentSelection .

ex:ContentRecommendationAlgorithm rdf:type airo:AICapability ;
    rdfs:label "Content Recommendation Algorithm"@en ;
    rdfs:label "内容推荐算法"@zh .

ex:ChildrenUsers rdf:type airo:AISubject ;
    rdfs:label "Children Users"@en ;
    rdfs:label "儿童用户"@zh .

ex:ExposureToInappropriateContent rdf:type airo:Consequence ;
    rdfs:label "Exposure to Inappropriate Content"@en ;
    rdfs:label "接触不当内容"@zh ;
    airo:hasImpact ex:PsychologicalHarmToChildren .

ex:PsychologicalHarmToChildren rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Children"@en ;
    rdfs:label "对儿童的心理伤害"@zh ;
    airo:hasImpactOnStakeholder ex:ParentsAndGuardians .

ex:ParentsAndGuardians rdf:type airo:Stakeholder ;
    rdfs:label "Parents and Guardians"@en ;
    rdfs:label "父母和监护人"@zh .

ex:AlgorithmicContentSelection rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Content Selection"@en ;
    rdfs:label "算法内容选择"@zh .