@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AITicketScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI Ticket Scam System"@en ;
    rdfs:label "AI票务诈骗系统"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasStakeholder ex:QueenFans ;
    airo:hasImpactOnStakeholder ex:QueenFans ;
    airo:hasRisk ex:AISystemForTicketFraud .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:QueenFans rdf:type airo:Stakeholder ;
    rdfs:label "Queen Fans"@en ;
    rdfs:label "皇后乐队粉丝"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh .

ex:AISystemForTicketFraud rdf:type airo:Misuse ;
    rdfs:label "AI System for Ticket Fraud Misuse"@en ;
    rdfs:label "用于票务诈骗的AI系统滥用"@zh ;
    airo:isRiskSourceFor ex:FinancialFraudRisk .