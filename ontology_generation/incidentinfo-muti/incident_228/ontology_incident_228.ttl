ex:FourthGradeStudents rdf:type airo:Stakeholder ;
    rdfs:label "Fourth Grade Students"@en ;
    rdfs:label "四年级学生"@zh .

ex:UnauthorizedAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Access"@en ;
    rdfs:label "未经授权访问"@zh .

ex:WeakAuthenticationMechanism rdf:type airo:Vulnerability ;
    rdfs:label "Weak Authentication Mechanism"@en ;
    rdfs:label "弱认证机制"@zh .