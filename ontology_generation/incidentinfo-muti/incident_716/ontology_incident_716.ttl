@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TechGiantsDataHarvestingAI rdf:type airo:AISystem ;
    rdfs:label "Tech Giants Data Harvesting AI"@en ;
    rdfs:label "科技巨头数据采集AI"@zh ;
    airo:hasRisk ex:DataPrivacyViolationRisk ;
    airo:hasStakeholder ex:TechGiants .

ex:DataPrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Data Privacy Violation Risk"@en ;
    rdfs:label "数据隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:UnethicalDataCollectionPractices ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        airo:hasImpact ex:UserPrivacyCompromise
    ] .

ex:TechGiants rdf:type airo:Stakeholder ;
    rdfs:label "Tech Giants"@en ;
    rdfs:label "科技巨头"@zh .

ex:UnethicalDataCollectionPractices rdf:type airo:RiskSource ;
    rdfs:label "Unethical Data Collection Practices"@en ;
    rdfs:label "不道德数据收集实践"@zh .

ex:UserPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "User Privacy Compromise"@en ;
    rdfs:label "用户隐私泄露"@zh .