@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleSearchAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Google Search Algorithm"@en ;
    rdfs:label "谷歌搜索算法"@zh ;
    airo:hasRisk ex:AntiSemiticContentRisk ;
    airo:hasStakeholder ex:JewishCommunity .

ex:AntiSemiticContentRisk rdf:type airo:Risk ;
    rdfs:label "Anti-Semitic Content Risk"@en ;
    rdfs:label "反犹太内容风险"@zh ;
    airo:isRiskSourceFor ex:ExtremistCampaign ;
    airo:hasImpact ex:AntiSemiticPropaganda .

ex:JewishCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Jewish Community"@en ;
    rdfs:label "犹太社区"@zh .

ex:ExtremistCampaign rdf:type airo:RiskSource ;
    rdfs:label "Extremist Campaign"@en ;
    rdfs:label "极端主义活动"@zh .

ex:AntiSemiticPropaganda rdf:type airo:Impact ;
    rdfs:label "Anti-Semitic Propaganda Impact"@en ;
    rdfs:label "反犹太宣传影响"@zh ;
    airo:hasImpactOnStakeholder ex:JewishCommunity .