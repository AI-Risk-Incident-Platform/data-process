@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NetflixAIPosterGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Netflix AI Poster Generation System"@en ;
    rdfs:label "Netflix AI海报生成系统"@zh ;
    airo:hasRisk ex:CreativeIndustryDisplacementRisk ;
    airo:isDeployedBy ex:Netflix ;
    airo:isAppliedWithinDomain ex:EntertainmentIndustry ;
    airo:hasPurpose ex:MarketingMaterialGeneration .

ex:CreativeIndustryDisplacementRisk rdf:type airo:Risk ;
    rdfs:label "Creative Industry Displacement Risk"@en ;
    rdfs:label "创意产业替代风险"@zh ;
    airo:hasImpact ex:ArtistJobDisplacement .

ex:Netflix rdf:type airo:AIOperator ;
    rdfs:label "Netflix"@en ;
    rdfs:label "Netflix"@zh .

ex:EntertainmentIndustry rdf:type airo:Domain ;
    rdfs:label "Entertainment Industry"@en ;
    rdfs:label "娱乐产业"@zh .

ex:MarketingMaterialGeneration rdf:type airo:Purpose ;
    rdfs:label "Marketing Material Generation"@en ;
    rdfs:label "营销材料生成"@zh .

ex:ArtistJobDisplacement rdf:type airo:Impact ;
    rdfs:label "Artist Job Displacement"@en ;
    rdfs:label "艺术家工作替代"@zh ;
    airo:hasImpactOnStakeholder ex:Netflix .