@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IBM_Watson_for_Oncology_System rdf:type airo:AISystem ;
    rdfs:label "IBM Watson肿瘤治疗系统"@zh ;
    rdfs:label "IBM Watson for Oncology System"@en ;
    airo:hasRisk ex:Unsafe_Cancer_Treatment_Recommendations_Risk ;
    airo:hasCapability ex:Cancer_Treatment_Recommendation_Capability ;
    airo:hasPurpose ex:Assisting_Oncologists_in_Treatment_Decisions_Purpose ;
    airo:isAppliedWithinDomain ex:Medical_Oncology_Domain ;
    airo:hasStakeholder ex:Cancer_Patients ;
    airo:isDevelopedBy ex:IBM_Corporation .

ex:Unsafe_Cancer_Treatment_Recommendations_Risk rdf:type airo:Risk ;
    rdfs:label "不安全癌症治疗建议风险"@zh ;
    rdfs:label "Unsafe Cancer Treatment Recommendations Risk"@en ;
    airo:hasConsequence ex:Potential_Harm_to_Cancer_Patients_Consequence ;
    airo:isRiskSourceFor ex:Inaccurate_Training_Data_Risk_Source .

ex:Potential_Harm_to_Cancer_Patients_Consequence rdf:type airo:Consequence ;
    rdfs:label "对癌症患者的潜在伤害后果"@zh ;
    rdfs:label "Potential Harm to Cancer Patients Consequence"@en ;
    airo:hasImpact ex:Patient_Safety_Compromise_Impact .

ex:Cancer_Treatment_Recommendation_Capability rdf:type airo:AICapability ;
    rdfs:label "癌症治疗建议能力"@zh ;
    rdfs:label "Cancer Treatment Recommendation Capability"@en .

ex:IBM_Corporation rdf:type airo:AIOperator ;
    rdfs:label "IBM公司"@zh ;
    rdfs:label "IBM Corporation"@en .

ex:Cancer_Patients rdf:type airo:Stakeholder ;
    rdfs:label "癌症患者"@zh ;
    rdfs:label "Cancer Patients"@en .

ex:Patient_Safety_Compromise_Impact rdf:type airo:Impact ;
    rdfs:label "患者安全受损影响"@zh ;
    rdfs:label "Patient Safety Compromise Impact"@en ;
    airo:hasImpactOnStakeholder ex:Cancer_Patients .

ex:Assisting_Oncologists_in_Treatment_Decisions_Purpose rdf:type airo:Purpose ;
    rdfs:label "辅助肿瘤学家治疗决策目的"@zh ;
    rdfs:label "Assisting Oncologists in Treatment Decisions Purpose"@en .

ex:Medical_Oncology_Domain rdf:type airo:Domain ;
    rdfs:label "医学肿瘤学领域"@zh ;
    rdfs:label "Medical Oncology Domain"@en .

ex:Inaccurate_Training_Data_Risk_Source rdf:type airo:RiskSource ;
    rdfs:label "不准确训练数据风险源"@zh ;
    rdfs:label "Inaccurate Training Data Risk Source"@en .