@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AdobeFireflyImageGenerator rdf:type airo:AISystem ;
    rdfs:label "Adobe Firefly Image Generator"@en ;
    rdfs:label "Adobe Firefly图像生成器"@zh ;
    airo:hasRisk ex:EthicalConcernsInAITrainingData ;
    airo:isDevelopedBy ex:AdobeInc ;
    airo:hasPurpose ex:EthicalAIImageGeneration ;
    airo:isAppliedWithinDomain ex:CreativeSoftwareIndustry .

ex:EthicalConcernsInAITrainingData rdf:type airo:Risk ;
    rdfs:label "Ethical Concerns in AI Training Data Risk"@en ;
    rdfs:label "AI训练数据中的伦理问题风险"@zh ;
    airo:isRiskSourceFor ex:UseOfCompetitorAIGeneratedImages ;
    airo:hasImpact ex:ReputationDamage .

ex:UseOfCompetitorAIGeneratedImages rdf:type airo:RiskSource ;
    rdfs:label "Use of Competitor AI-Generated Images Risk Source"@en ;
    rdfs:label "使用竞争对手AI生成图像的风险源"@zh .

ex:AdobeInc rdf:type airo:AIOperator ;
    rdfs:label "Adobe Inc."@en ;
    rdfs:label "Adobe公司"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:AdobeInc .

ex:EthicalAIImageGeneration rdf:type airo:Purpose ;
    rdfs:label "Ethical AI Image Generation Purpose"@en ;
    rdfs:label "伦理AI图像生成的目的"@zh .

ex:CreativeSoftwareIndustry rdf:type airo:Domain ;
    rdfs:label "Creative Software Industry Domain"@en ;
    rdfs:label "创意软件行业领域"@zh .