@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MicrosoftNewsAI rdf:type airo:AISystem ;
    rdfs:label "Microsoft News AI"@en ;
    rdfs:label "微软新闻AI"@zh ;
    airo:hasRisk ex:RacialBiasInAIIdentification ;
    airo:isDeployedBy ex:MicrosoftCorporation ;
    airo:hasAISubject ex:MixedRaceCelebrities ;
    airo:isAppliedWithinDomain ex:NewsPublishing .

ex:RacialBiasInAIIdentification rdf:type airo:Risk ;
    rdfs:label "Racial Bias in AI Identification Risk"@en ;
    rdfs:label "AI识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:MisidentificationOfMixedRaceIndividuals ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:MisidentificationOfMixedRaceIndividuals rdf:type airo:Consequence ;
    rdfs:label "Misidentification of Mixed-Race Individuals Consequence"@en ;
    rdfs:label "混血个体误识别的后果"@zh ;
    airo:hasImpact ex:HarmToPublicImageAndReputation .

ex:HarmToPublicImageAndReputation rdf:type airo:Impact ;
    rdfs:label "Harm to Public Image and Reputation Impact"@en ;
    rdfs:label "对公众形象和声誉的损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:MicrosoftCorporation .

ex:MicrosoftCorporation rdf:type airo:AIOperator ;
    rdfs:label "Microsoft Corporation"@en ;
    rdfs:label "微软公司"@zh .

ex:MixedRaceCelebrities rdf:type airo:AISubject ;
    rdfs:label "Mixed-Race Celebrities"@en ;
    rdfs:label "混血名人"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:NewsPublishing rdf:type airo:Domain ;
    rdfs:label "News Publishing Domain"@en ;
    rdfs:label "新闻出版领域"@zh .