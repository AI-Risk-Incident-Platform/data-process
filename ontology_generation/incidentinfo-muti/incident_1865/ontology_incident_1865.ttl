@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleBard rdf:type airo:AISystem ;
    rdfs:label "Google Bard"@en ;
    rdfs:label "谷歌Bard"@zh ;
    airo:hasRisk ex:PoliticalBiasRisk ;
    airo:hasStakeholder ex:Google ;
    airo:hasCapability ex:ContentGeneration .

ex:PoliticalBiasRisk rdf:type airo:Risk ;
    rdfs:label "Political Bias Risk"@en ;
    rdfs:label "政治偏见风险"@zh ;
    airo:hasImpact ex:MisinformationSpread .

ex:Google rdf:type airo:Stakeholder ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:ContentGeneration rdf:type airo:AICapability ;
    rdfs:label "Content Generation Capability"@en ;
    rdfs:label "内容生成能力"@zh .

ex:MisinformationSpread rdf:type airo:Impact ;
    rdfs:label "Misinformation Spread Impact"@en ;
    rdfs:label "错误信息传播影响"@zh ;
    airo:hasImpactOnStakeholder ex:Google .