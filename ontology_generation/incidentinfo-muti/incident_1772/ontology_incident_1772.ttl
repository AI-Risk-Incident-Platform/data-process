@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SelfDrivingShuttleBus rdf:type airo:AISystem ;
    rdfs:label "Self-Driving Shuttle Bus"@en ;
    rdfs:label "自动驾驶班车"@zh ;
    airo:hasRisk ex:PhysicalInjuryRisk ;
    airo:hasAISubject ex:VisuallyImpairedAthlete ;
    airo:isDeployedBy ex:Toyota ;
    airo:isAppliedWithinDomain ex:ParalympicGamesTransportation .

ex:PhysicalInjuryRisk rdf:type airo:Risk ;
    rdfs:label "Physical Injury Risk"@en ;
    rdfs:label "身体伤害风险"@zh ;
    airo:hasConsequence ex:AthleteWithdrawalFromCompetition ;
    airo:isRiskSourceFor ex:AutonomousVehicleSystemFailure .

ex:AthleteWithdrawalFromCompetition rdf:type airo:Consequence ;
    rdfs:label "Athlete Withdrawal from Competition Consequence"@en ;
    rdfs:label "运动员退出比赛后果"@zh ;
    airo:hasImpact ex:AthleteCareerDisruption .

ex:VisuallyImpairedAthlete rdf:type airo:AISubject ;
    rdfs:label "Visually Impaired Athlete"@en ;
    rdfs:label "视障运动员"@zh .

ex:Toyota rdf:type airo:AIOperator ;
    rdfs:label "Toyota"@en ;
    rdfs:label "丰田"@zh .

ex:AthleteCareerDisruption rdf:type airo:Impact ;
    rdfs:label "Athlete Career Disruption Impact"@en ;
    rdfs:label "运动员职业生涯中断影响"@zh ;
    airo:hasImpactOnStakeholder ex:VisuallyImpairedAthlete .

ex:ParalympicGamesTransportation rdf:type airo:Domain ;
    rdfs:label "Paralympic Games Transportation Domain"@en ;
    rdfs:label "残奥会交通领域"@zh .

ex:AutonomousVehicleSystemFailure rdf:type airo:RiskSource ;
    rdfs:label "Autonomous Vehicle System Failure Risk Source"@en ;
    rdfs:label "自动驾驶系统故障风险源"@zh .