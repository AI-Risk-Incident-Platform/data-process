@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IRSAuditSystem rdf:type airo:AISystem ;
    rdfs:label "IRS Audit System"@en ;
    rdfs:label "IRS审计系统"@zh ;
    airo:hasRisk ex:RacialBiasInTaxAudits ;
    airo:isAppliedWithinDomain ex:TaxationSystem ;
    airo:hasStakeholder ex:BlackTaxpayers .

ex:RacialBiasInTaxAudits rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Tax Audits Risk"@en ;
    rdfs:label "税务审计中的种族偏见风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInAuditSelection ;
    airo:hasConsequence ex:DisproportionateAuditBurden .

ex:DisproportionateAuditBurden rdf:type airo:Consequence ;
    rdfs:label "Disproportionate Audit Burden Consequence"@en ;
    rdfs:label "不成比例的审计负担后果"@zh ;
    airo:hasImpact ex:FinancialHardshipForMinorities .

ex:FinancialHardshipForMinorities rdf:type airo:Impact ;
    rdfs:label "Financial Hardship for Minorities Impact"@en ;
    rdfs:label "少数族裔财务困难的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackTaxpayers .

ex:BlackTaxpayers rdf:type airo:Stakeholder ;
    rdfs:label "Black Taxpayers"@en ;
    rdfs:label "黑人纳税人"@zh .

ex:TaxationSystem rdf:type airo:Domain ;
    rdfs:label "Taxation System Domain"@en ;
    rdfs:label "税收系统领域"@zh .

ex:AlgorithmicBiasInAuditSelection rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Audit Selection Risk Source"@en ;
    rdfs:label "审计选择中的算法偏见风险源"@zh .