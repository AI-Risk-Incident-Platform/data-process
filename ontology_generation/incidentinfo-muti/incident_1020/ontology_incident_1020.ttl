@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LLM rdf:type airo:AISystem ;
    rdfs:label "Large Language Model"@en ;
    rdfs:label "大型语言模型"@zh ;
    airo:hasRisk ex:HarmfulBehaviorRisk ;
    airo:hasCapability ex:NaturalLanguageGeneration .

ex:HarmfulBehaviorRisk rdf:type airo:Risk ;
    rdfs:label "Harmful Behavior Risk"@en ;
    rdfs:label "有害行为风险"@zh ;
    airo:hasConsequence ex:PotentialHarmToUsers ;
    airo:isRiskSourceFor ex:UnintendedOutputGeneration .

ex:UnintendedOutputGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unintended Output Generation"@en ;
    rdfs:label "意外输出生成"@zh .

ex:PotentialHarmToUsers rdf:type airo:Consequence ;
    rdfs:label "Potential Harm to Users"@en ;
    rdfs:label "对用户的潜在伤害"@zh .

ex:NaturalLanguageGeneration rdf:type airo:AICapability ;
    rdfs:label "Natural Language Generation Capability"@en ;
    rdfs:label "自然语言生成能力"@zh .