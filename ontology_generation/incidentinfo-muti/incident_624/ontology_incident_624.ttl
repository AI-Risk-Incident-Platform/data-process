@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIImageGenerationModels rdf:type airo:AISystem ;
    rdfs:label "AI Image Generation Models"@en ;
    rdfs:label "AI图像生成模型"@zh ;
    airo:hasRisk ex:ChildAbuseContentRisk ;
    airo:usesTechnique ex:DeepLearningImageGeneration ;
    airo:hasAISubject ex:Children ;
    airo:hasStakeholder ex:ChildProtectionOrganizations .

ex:ChildAbuseContentRisk rdf:type airo:Risk ;
    rdfs:label "Child Abuse Content Risk"@en ;
    rdfs:label "儿童虐待内容风险"@zh ;
    airo:hasConsequence ex:DisseminationOfIllegalContent ;
    airo:isRiskSourceFor ex:LAION5BDataset .

ex:LAION5BDataset rdf:type airo:RiskSource ;
    rdfs:label "LAION 5B Dataset"@en ;
    rdfs:label "LAION 5B数据集"@zh .

ex:DisseminationOfIllegalContent rdf:type airo:Consequence ;
    rdfs:label "Dissemination of Illegal Content Consequence"@en ;
    rdfs:label "非法内容传播后果"@zh ;
    airo:hasImpact ex:HarmToChildren .

ex:HarmToChildren rdf:type airo:Impact ;
    rdfs:label "Harm to Children Impact"@en ;
    rdfs:label "对儿童的伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChildProtectionOrganizations .

ex:DeepLearningImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Image Generation Technique"@en ;
    rdfs:label "深度学习图像生成技术"@zh .

ex:ChildProtectionOrganizations rdf:type airo:Stakeholder ;
    rdfs:label "Child Protection Organizations"@en ;
    rdfs:label "儿童保护组织"@zh .

ex:Children rdf:type airo:AISubject ;
    rdfs:label "Children"@en ;
    rdfs:label "儿童"@zh .