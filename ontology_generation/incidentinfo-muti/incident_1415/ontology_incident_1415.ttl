ex:SearchEngineRankingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Search Engine Ranking Algorithm"@en ;
    rdfs:label "搜索引擎排名算法"@zh ;
    airo:hasRisk ex:NonconsensualDeepfakePornDistributionRisk ;
    airo:isDeployedBy ex:GoogleAndBing ;
    airo:usesTechnique ex:DeepfakeTechnology .

ex:NonconsensualDeepfakePornDistributionRisk rdf:type airo:Risk ;
    rdfs:label "Nonconsensual Deepfake Porn Distribution Risk"@en ;
    rdfs:label "非自愿深度伪造色情内容传播风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmicContentRankingBias ;
    airo:hasConsequence ex:PrivacyViolationAndReputationDamage .

ex:GoogleAndBing rdf:type airo:AIOperator ;
    rdfs:label "Google and Bing"@en ;
    rdfs:label "谷歌和必应"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PrivacyViolationAndReputationDamage rdf:type airo:Impact ;
    rdfs:label "Privacy Violation and Reputation Damage"@en ;
    rdfs:label "隐私侵犯和名誉损害"@zh .

ex:AlgorithmicContentRankingBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Content Ranking Bias"@en ;
    rdfs:label "算法内容排名偏见"@zh .