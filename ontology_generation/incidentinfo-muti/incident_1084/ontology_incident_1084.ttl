@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AnyVisionFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "AnyVision Facial Recognition System"@en ;
    rdfs:label "AnyVision面部识别系统"@zh ;
    airo:hasRisk ex:EthicalConcernsInFacialRecognition ;
    airo:hasStakeholder ex:MicrosoftCorporation ;
    airo:hasStakeholder ex:PalestinianCitizens ;
    airo:isDeployedBy ex:AnyVision ;
    airo:hasPurpose ex:BorderControlEfficiency ;
    airo:isAppliedWithinDomain ex:BorderSecurity .

ex:EthicalConcernsInFacialRecognition rdf:type airo:Risk ;
    rdfs:label "Ethical Concerns in Facial Recognition Risk"@en ;
    rdfs:label "面部识别中的伦理问题风险"@zh ;
    airo:hasConsequence ex:PrivacyViolationConcerns .

ex:MicrosoftCorporation rdf:type airo:Stakeholder ;
    rdfs:label "Microsoft Corporation"@en ;
    rdfs:label "微软公司"@zh .

ex:AnyVision rdf:type airo:AIOperator ;
    rdfs:label "AnyVision"@en ;
    rdfs:label "AnyVision公司"@zh .

ex:PalestinianCitizens rdf:type airo:AISubject ;
    rdfs:label "Palestinian Citizens"@en ;
    rdfs:label "巴勒斯坦公民"@zh .

ex:PrivacyViolationConcerns rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Concerns Impact"@en ;
    rdfs:label "隐私侵犯担忧的影响"@zh ;
    airo:hasImpactOnStakeholder ex:PalestinianCitizens .

ex:BorderControlEfficiency rdf:type airo:Purpose ;
    rdfs:label "Border Control Efficiency Purpose"@en ;
    rdfs:label "提高边境控制效率的目的"@zh .

ex:BorderSecurity rdf:type airo:Domain ;
    rdfs:label "Border Security Domain"@en ;
    rdfs:label "边境安全领域"@zh .