@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedDeepfakes rdf:type airo:AISystem ;
    rdfs:label "AI Generated Deepfakes"@en ;
    rdfs:label "AI生成深度伪造"@zh ;
    airo:hasRisk ex:SextortionRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:FBI .

ex:SextortionRisk rdf:type airo:Risk ;
    rdfs:label "Sextortion Risk"@en ;
    rdfs:label "性勒索风险"@zh ;
    airo:isRiskSourceFor ex:MaliciousActors ;
    airo:hasConsequence ex:PrivacyViolation .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Privacy Violation Consequence"@en ;
    rdfs:label "隐私侵犯后果"@zh ;
    airo:hasImpact ex:PsychologicalHarm .

ex:FBI rdf:type airo:Stakeholder ;
    rdfs:label "FBI"@en ;
    rdfs:label "联邦调查局"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:MaliciousActors rdf:type airo:RiskSource ;
    rdfs:label "Malicious Actors"@en ;
    rdfs:label "恶意行为者"@zh .

ex:PsychologicalHarm rdf:type airo:Impact ;
    rdfs:label "Psychological Harm Impact"@en ;
    rdfs:label "心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:FBI .