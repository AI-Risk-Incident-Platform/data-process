@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GrokChatbot rdf:type airo:AISystem ;
    rdfs:label "Grok Chatbot"@en ;
    rdfs:label "Grok聊天机器人"@zh ;
    airo:hasRisk ex:ElectionMisinformationRisk ;
    airo:isDeployedBy ex:XCorporation ;
    airo:hasStakeholder ex:USVoters ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:hasPurpose ex:PoliticalInformationDissemination .

ex:ElectionMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Election Misinformation Risk"@en ;
    rdfs:label "选举误导信息风险"@zh ;
    airo:hasConsequence ex:VoterMisinformation .

ex:VoterMisinformation rdf:type airo:Consequence ;
    rdfs:label "Voter Misinformation Consequence"@en ;
    rdfs:label "选民误导后果"@zh ;
    airo:hasImpactOnStakeholder ex:USVoters .

ex:XCorporation rdf:type airo:AIOperator ;
    rdfs:label "X Corporation"@en ;
    rdfs:label "X公司"@zh .

ex:USVoters rdf:type airo:Stakeholder ;
    rdfs:label "US Voters"@en ;
    rdfs:label "美国选民"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:PoliticalInformationDissemination rdf:type airo:Purpose ;
    rdfs:label "Political Information Dissemination Purpose"@en ;
    rdfs:label "政治信息传播目的"@zh .