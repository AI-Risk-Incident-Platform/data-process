@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SocialDistancingMonitoringSystem rdf:type airo:AISystem ;
    rdfs:label "Social Distancing Monitoring System"@en ;
    rdfs:label "社交距离监控系统"@zh ;
    airo:hasRisk ex:PrivacyInvasionRisk ;
    airo:hasCapability ex:DistanceMonitoringCapability ;
    airo:isAppliedWithinDomain ex:WorkplaceSafety ;
    airo:hasPurpose ex:EnsuringSocialDistancingCompliance ;
    airo:usesTechnique ex:MachineLearningForBehaviorAnalysis ;
    airo:hasStakeholder ex:Employees ;
    airo:hasAISubject ex:WorkplaceIndividuals .

ex:PrivacyInvasionRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Invasion Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalPrivacyConcerns .

ex:DistanceMonitoringCapability rdf:type airo:AICapability ;
    rdfs:label "Distance Monitoring Capability"@en ;
    rdfs:label "距离监控能力"@zh .

ex:WorkplaceSafety rdf:type airo:Domain ;
    rdfs:label "Workplace Safety Domain"@en ;
    rdfs:label "工作场所安全领域"@zh .

ex:EnsuringSocialDistancingCompliance rdf:type airo:Purpose ;
    rdfs:label "Ensuring Social Distancing Compliance Purpose"@en ;
    rdfs:label "确保社交距离合规的目的"@zh .

ex:MachineLearningForBehaviorAnalysis rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning for Behavior Analysis Technique"@en ;
    rdfs:label "行为分析的机器学习技术"@zh .

ex:Employees rdf:type airo:Stakeholder ;
    rdfs:label "Employees"@en ;
    rdfs:label "员工"@zh .

ex:WorkplaceIndividuals rdf:type airo:AISubject ;
    rdfs:label "Workplace Individuals"@en ;
    rdfs:label "工作场所个人"@zh .

ex:PersonalPrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Concerns Impact"@en ;
    rdfs:label "个人隐私担忧的影响"@zh .