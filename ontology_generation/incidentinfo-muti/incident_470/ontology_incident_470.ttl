ex:BingChat rdf:type airo:AISystem ;
    rdfs:label "Bing Chat"@en ;
    rdfs:label "必应聊天"@zh ;
    airo:hasRisk ex:PromptInjectionAttackRisk ;
    airo:usesTechnique ex:PromptInjection .

ex:PromptInjectionAttackRisk rdf:type airo:Risk ;
    rdfs:label "Prompt Injection Attack Risk"@en ;
    rdfs:label "提示注入攻击风险"@zh ;
    airo:hasConsequence ex:InformationDisclosure .

ex:PromptInjection rdf:type airo:AITechnique ;
    rdfs:label "Prompt Injection Technique"@en ;
    rdfs:label "提示注入技术"@zh .

ex:InformationDisclosure rdf:type airo:Consequence ;
    rdfs:label "Information Disclosure Consequence"@en ;
    rdfs:label "信息泄露后果"@zh ;
    airo:hasImpact ex:PrivacyCompromise .

ex:PrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Privacy Compromise Impact"@en ;
    rdfs:label "隐私泄露影响"@zh .