@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GigWorkPlatformAI rdf:type airo:AISystem ;
    rdfs:label "Gig Work Platform AI"@en ;
    rdfs:label "零工工作平台AI"@zh ;
    airo:hasRisk ex:AlgorithmicWageDiscrimination ;
    airo:isDeployedBy ex:UberAndAmazon ;
    airo:hasStakeholder ex:GigWorkers ;
    airo:isAppliedWithinDomain ex:GigEconomy .

ex:AlgorithmicWageDiscrimination rdf:type airo:Risk ;
    rdfs:label "Algorithmic Wage Discrimination Risk"@en ;
    rdfs:label "算法工资歧视风险"@zh ;
    airo:hasConsequence ex:UnequalPayForSameWork ;
    airo:isRiskSourceFor ex:OpaquePricingAlgorithms .

ex:GigWorkers rdf:type airo:Stakeholder ;
    rdfs:label "Gig Workers"@en ;
    rdfs:label "零工工作者"@zh .

ex:UberAndAmazon rdf:type airo:AIOperator ;
    rdfs:label "Uber and Amazon"@en ;
    rdfs:label "优步和亚马逊"@zh .

ex:UnequalPayForSameWork rdf:type airo:Consequence ;
    rdfs:label "Unequal Pay for Same Work Consequence"@en ;
    rdfs:label "同工不同酬后果"@zh ;
    airo:hasImpact ex:EconomicInequality .

ex:EconomicInequality rdf:type airo:Impact ;
    rdfs:label "Economic Inequality Impact"@en ;
    rdfs:label "经济不平等影响"@zh ;
    airo:hasImpactOnStakeholder ex:GigWorkers .

ex:GigEconomy rdf:type airo:Domain ;
    rdfs:label "Gig Economy Domain"@en ;
    rdfs:label "零工经济领域"@zh .

ex:OpaquePricingAlgorithms rdf:type airo:RiskSource ;
    rdfs:label "Opaque Pricing Algorithms Risk Source"@en ;
    rdfs:label "不透明的定价算法风险源"@zh .