@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGirlfriendSystem rdf:type airo:AISystem ;
    rdfs:label "AI Girlfriend System"@en ;
    rdfs:label "AI女友系统"@zh ;
    airo:hasRisk ex:VerbalAbuseRisk ;
    airo:hasPurpose ex:Companionship ;
    airo:hasStakeholder ex:AIUser .

ex:VerbalAbuseRisk rdf:type airo:Risk ;
    rdfs:label "Verbal Abuse Risk"@en ;
    rdfs:label "言语虐待风险"@zh ;
    airo:hasConsequence ex:PsychologicalImpact .

ex:AIUser rdf:type airo:Stakeholder ;
    rdfs:label "AI User"@en ;
    rdfs:label "AI使用者"@zh .

ex:PsychologicalImpact rdf:type airo:Impact ;
    rdfs:label "Psychological Impact"@en ;
    rdfs:label "心理影响"@zh ;
    airo:hasImpactOnStakeholder ex:AIUser .

ex:Companionship rdf:type airo:Purpose ;
    rdfs:label "Companionship"@en ;
    rdfs:label "陪伴"@zh .