@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SnapAIChatbot rdf:type airo:AISystem ;
    rdfs:label "Snap AI Chatbot"@en ;
    rdfs:label "Snap AI聊天机器人"@zh ;
    airo:hasRisk ex:ChildrenPrivacyRisk ;
    airo:hasAISubject ex:ChildrenUsers ;
    airo:hasStakeholder ex:UKDataWatchdog .

ex:ChildrenPrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Children's Privacy Risk"@en ;
    rdfs:label "儿童隐私风险"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:UKDataWatchdog rdf:type airo:Stakeholder ;
    rdfs:label "UK Data Watchdog"@en ;
    rdfs:label "英国数据监管机构"@zh .

ex:ChildrenUsers rdf:type airo:AISubject ;
    rdfs:label "Children Users"@en ;
    rdfs:label "儿童用户"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:ChildrenUsers .