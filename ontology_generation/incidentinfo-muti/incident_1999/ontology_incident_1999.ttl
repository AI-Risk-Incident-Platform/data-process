@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AdobeAISystem rdf:type airo:AISystem ;
    rdfs:label "Adobe人工智能系统"@zh ;
    rdfs:label "Adobe AI System"@en ;
    airo:hasRisk ex:PrivacyRisk ;
    airo:isDevelopedBy ex:Adobe ;
    airo:hasAISubject ex:UserPhotos ;
    airo:hasPurpose ex:AITraining ;
    airo:usesTechnique ex:MachineLearningTraining ;
    airo:hasStakeholder ex:AdobeUsers .

ex:PrivacyRisk rdf:type airo:Risk ;
    rdfs:label "隐私风险"@zh ;
    rdfs:label "Privacy Risk"@en ;
    airo:isRiskSourceFor ex:OptOutPolicy ;
    airo:hasConsequence ex:UnauthorizedDataUsage .

ex:Adobe rdf:type airo:AIOperator ;
    rdfs:label "Adobe公司"@zh ;
    rdfs:label "Adobe"@en .

ex:UserPhotos rdf:type airo:AISubject ;
    rdfs:label "用户照片"@zh ;
    rdfs:label "User Photos"@en .

ex:AITraining rdf:type airo:Purpose ;
    rdfs:label "AI训练"@zh ;
    rdfs:label "AI Training"@en .

ex:OptOutPolicy rdf:type airo:RiskSource ;
    rdfs:label "选择退出政策"@zh ;
    rdfs:label "Opt-Out Policy"@en .

ex:UnauthorizedDataUsage rdf:type airo:Consequence ;
    rdfs:label "未经授权的数据使用"@zh ;
    rdfs:label "Unauthorized Data Usage"@en ;
    airo:hasImpact ex:PrivacyConcerns .

ex:PrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "隐私担忧"@zh ;
    rdfs:label "Privacy Concerns"@en ;
    airo:hasImpactOnStakeholder ex:AdobeUsers .

ex:MachineLearningTraining rdf:type airo:AITechnique ;
    rdfs:label "机器学习训练"@zh ;
    rdfs:label "Machine Learning Training"@en .

ex:AdobeUsers rdf:type airo:Stakeholder ;
    rdfs:label "Adobe用户"@zh ;
    rdfs:label "Adobe Users"@en .