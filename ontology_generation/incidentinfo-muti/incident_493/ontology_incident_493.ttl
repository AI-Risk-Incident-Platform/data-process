@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:RacialDiscriminationRisk ;
    airo:hasAISubject ex:BlackStudents ;
    airo:hasPurpose ex:CreatingRacistContent ;
    airo:isAppliedWithinDomain ex:EducationSystem ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:hasStakeholder ex:HighSchoolStudents ;
    airo:hasVulnerability ex:EaseOfCreatingHarmfulDeepfakes .

ex:RacialDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Racial Discrimination Risk"@en ;
    rdfs:label "种族歧视风险"@zh ;
    airo:hasConsequence ex:FearAndDistressAmongParents ;
    airo:isRiskSourceFor ex:MisuseOfDeepfakeTechnology ;
    airo:isRiskSourceFor ex:LackOfLegalRegulation .

ex:FearAndDistressAmongParents rdf:type airo:Consequence ;
    rdfs:label "Fear and Distress Among Parents Consequence"@en ;
    rdfs:label "家长恐惧与不安后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToBlackStudents .

ex:HighSchoolStudents rdf:type airo:Stakeholder ;
    rdfs:label "High School Students"@en ;
    rdfs:label "高中生"@zh .

ex:BlackStudents rdf:type airo:AISubject ;
    rdfs:label "Black Students"@en ;
    rdfs:label "黑人学生"@zh .

ex:PsychologicalHarmToBlackStudents rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Black Students Impact"@en ;
    rdfs:label "对黑人学生心理伤害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackStudents .

ex:CreatingRacistContent rdf:type airo:Purpose ;
    rdfs:label "Creating Racist Content Purpose"@en ;
    rdfs:label "制作种族主义内容的目的"@zh .

ex:EducationSystem rdf:type airo:Domain ;
    rdfs:label "Education System Domain"@en ;
    rdfs:label "教育系统领域"@zh .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:EaseOfCreatingHarmfulDeepfakes rdf:type airo:Vulnerability ;
    rdfs:label "Ease of Creating Harmful Deepfakes Vulnerability"@en ;
    rdfs:label "易制作有害深度伪造的脆弱性"@zh .

ex:MisuseOfDeepfakeTechnology rdf:type airo:Threat ;
    rdfs:label "Misuse of Deepfake Technology Threat"@en ;
    rdfs:label "滥用深度伪造技术的威胁"@zh ;
    airo:exploitsVulnerability ex:EaseOfCreatingHarmfulDeepfakes .

ex:LackOfLegalRegulation rdf:type airo:RiskSource ;
    rdfs:label "Lack of Legal Regulation Risk Source"@en ;
    rdfs:label "缺乏法律监管的风险源"@zh ;
    airo:exploitsVulnerability ex:EaseOfCreatingHarmfulDeepfakes .