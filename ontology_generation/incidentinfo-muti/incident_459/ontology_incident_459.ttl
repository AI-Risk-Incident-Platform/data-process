@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedSeinfeldParody rdf:type airo:AISystem ;
    rdfs:label "AI-Generated Seinfeld Parody"@en ;
    rdfs:label "AI生成的老友记模仿秀"@zh ;
    airo:hasRisk ex:TransphobicContentRisk ;
    airo:hasCapability ex:ContinuousContentGeneration ;
    airo:hasPurpose ex:EntertainmentContentCreation ;
    airo:hasStakeholder ex:TransgenderCommunity ;
    airo:hasStakeholder ex:TwitchPlatform ;
    airo:isAppliedWithinDomain ex:OnlineStreamingPlatforms .

ex:TransphobicContentRisk rdf:type airo:Risk ;
    rdfs:label "Transphobic Content Risk"@en ;
    rdfs:label "跨性别歧视内容风险"@zh ;
    airo:hasConsequence ex:PlatformBanConsequence ;
    airo:isRiskSourceFor ex:UnfilteredAIOutput .

ex:PlatformBanConsequence rdf:type airo:Consequence ;
    rdfs:label "Platform Ban Consequence"@en ;
    rdfs:label "平台封禁后果"@zh ;
    airo:hasImpact ex:HarmToMarginalizedGroups .

ex:ContinuousContentGeneration rdf:type airo:AICapability ;
    rdfs:label "Continuous Content Generation Capability"@en ;
    rdfs:label "持续内容生成能力"@zh .

ex:TransgenderCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Transgender Community"@en ;
    rdfs:label "跨性别群体"@zh .

ex:TwitchPlatform rdf:type airo:AIOperator ;
    rdfs:label "Twitch Platform"@en ;
    rdfs:label "Twitch平台"@zh .

ex:HarmToMarginalizedGroups rdf:type airo:Impact ;
    rdfs:label "Harm to Marginalized Groups Impact"@en ;
    rdfs:label "对边缘群体伤害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:TransgenderCommunity .

ex:EntertainmentContentCreation rdf:type airo:Purpose ;
    rdfs:label "Entertainment Content Creation Purpose"@en ;
    rdfs:label "娱乐内容创作的目的"@zh .

ex:OnlineStreamingPlatforms rdf:type airo:Domain ;
    rdfs:label "Online Streaming Platforms Domain"@en ;
    rdfs:label "在线流媒体平台领域"@zh .

ex:UnfilteredAIOutput rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered AI Output Risk Source"@en ;
    rdfs:label "未过滤AI输出的风险源"@zh .