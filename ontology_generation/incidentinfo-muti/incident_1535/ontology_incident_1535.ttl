ex:DeepfakeImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Image Generation System"@en ;
    rdfs:label "深度伪造图像生成系统"@zh ;
    airo:hasRisk ex:PrivacyViolationAndReputationDamageRisk ;
    airo:hasStakeholder ex:AffectedFemaleStudents ;
    airo:isDeployedBy ex:IssaquahTeen ;
    airo:hasPurpose ex:CreatingFakeExplicitContent ;
    airo:isAppliedWithinDomain ex:EducationAndSocialMedia ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:PrivacyViolationAndReputationDamageRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Reputation Damage Risk"@en ;
    rdfs:label "隐私侵犯和名誉损害风险"@zh ;
    airo:hasConsequence ex:PsychologicalHarmToVictims ;
    airo:isRiskSourceFor ex:EasyAccessToDeepfakeTools ;
    airo:hasStakeholder ex:AffectedFemaleStudents .

ex:PsychologicalHarmToVictims rdf:type airo:Consequence ;
    rdfs:label "Psychological Harm to Victims Consequence"@en ;
    rdfs:label "受害者心理伤害后果"@zh ;
    airo:hasImpact ex:SocialStigmaAndEmotionalDistress .

ex:AffectedFemaleStudents rdf:type airo:Stakeholder ;
    rdfs:label "Affected Female Students"@en ;
    rdfs:label "受影响的女生"@zh .

ex:IssaquahTeen rdf:type airo:AIOperator ;
    rdfs:label "Issaquah Teen"@en ;
    rdfs:label "伊萨夸青少年"@zh .

ex:SocialStigmaAndEmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Social Stigma and Emotional Distress Impact"@en ;
    rdfs:label "社会污名和情绪困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:AffectedFemaleStudents .

ex:CreatingFakeExplicitContent rdf:type airo:Purpose ;
    rdfs:label "Creating Fake Explicit Content Purpose"@en ;
    rdfs:label "创建虚假色情内容的目的"@zh .

ex:EducationAndSocialMedia rdf:type airo:Domain ;
    rdfs:label "Education and Social Media Domain"@en ;
    rdfs:label "教育和社交媒体领域"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:EasyAccessToDeepfakeTools rdf:type airo:RiskSource ;
    rdfs:label "Easy Access to Deepfake Tools Risk Source"@en ;
    rdfs:label "深度伪造工具易获取风险源"@zh ;
    airo:isRiskSourceFor ex:PrivacyViolationAndReputationDamageRisk .

ex:NonConsensualImageCreation rdf:type airo:Misuse ;
    rdfs:label "Non-Consensual Image Creation Misuse"@en ;
    rdfs:label "未经同意的图像创建滥用"@zh ;
    airo:hasRisk ex:PrivacyViolationAndReputationDamageRisk .