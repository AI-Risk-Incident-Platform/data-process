@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PerplexityAI rdf:type airo:AISystem ;
    rdfs:label "Perplexity AI"@en ;
    rdfs:label "Perplexity AI"@zh ;
    airo:hasRisk ex:LegalRiskFromContentScraping ;
    airo:hasStakeholder ex:NewsCorp .

ex:LegalRiskFromContentScraping rdf:type airo:Risk ;
    rdfs:label "Legal Risk from Content Scraping"@en ;
    rdfs:label "内容抓取法律风险"@zh ;
    airo:isRiskSourceFor ex:UnauthorizedContentScraping ;
    airo:hasConsequence ex:PotentialLegalPenalties .

ex:NewsCorp rdf:type airo:Stakeholder ;
    rdfs:label "News Corp"@en ;
    rdfs:label "新闻集团"@zh .

ex:UnauthorizedContentScraping rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Content Scraping"@en ;
    rdfs:label "未经授权内容抓取"@zh .

ex:PotentialLegalPenalties rdf:type airo:Consequence ;
    rdfs:label "Potential Legal Penalties"@en ;
    rdfs:label "潜在法律处罚"@zh .