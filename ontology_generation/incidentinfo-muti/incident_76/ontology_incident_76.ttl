@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PokemonGoApp rdf:type airo:AISystem ;
    rdfs:label "Pokémon Go App"@en ;
    rdfs:label "Pokémon Go应用"@zh ;
    airo:hasRisk ex:RacialBiasInLocationBasedServices ;
    airo:hasAISubject ex:MinorityGamers ;
    airo:hasStakeholder ex:CommunitiesOfColor ;
    airo:isAppliedWithinDomain ex:LocationBasedGaming .

ex:RacialBiasInLocationBasedServices rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Location-Based Services Risk"@en ;
    rdfs:label "基于位置服务中的种族偏见风险"@zh ;
    airo:hasConsequence ex:UnequalAccessToGameFeatures ;
    airo:isRiskSourceFor ex:BiasedLocationData .

ex:UnequalAccessToGameFeatures rdf:type airo:Consequence ;
    rdfs:label "Unequal Access to Game Features Consequence"@en ;
    rdfs:label "游戏功能访问不平等后果"@zh ;
    airo:hasImpact ex:DigitalRedliningEffect .

ex:DigitalRedliningEffect rdf:type airo:Impact ;
    rdfs:label "Digital Redlining Effect Impact"@en ;
    rdfs:label "数字红线效应影响"@zh ;
    airo:hasImpactOnStakeholder ex:CommunitiesOfColor .

ex:CommunitiesOfColor rdf:type airo:Stakeholder ;
    rdfs:label "Communities of Color"@en ;
    rdfs:label "有色人种社区"@zh .

ex:MinorityGamers rdf:type airo:AISubject ;
    rdfs:label "Minority Gamers"@en ;
    rdfs:label "少数族裔游戏玩家"@zh .

ex:BiasedLocationData rdf:type airo:RiskSource ;
    rdfs:label "Biased Location Data Risk Source"@en ;
    rdfs:label "有偏见的位置数据风险源"@zh .

ex:LocationBasedGaming rdf:type airo:Domain ;
    rdfs:label "Location-Based Gaming Domain"@en ;
    rdfs:label "基于位置的游戏领域"@zh .