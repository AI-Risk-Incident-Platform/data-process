ex:AIGeneratedPornographySystem rdf:type airo:AISystem ;
    rdfs:label "AI生成色情内容系统"@zh ;
    rdfs:label "AI Generated Pornography System"@en ;
    airo:hasRisk ex:MisuseOfAITechnologyRisk ;
    airo:isAppliedWithinDomain ex:LawEnforcement ;
    airo:hasStakeholder ex:PennsylvaniaStatePolice ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasAIOperator ex:PoliceCorporal .

ex:MisuseOfAITechnologyRisk rdf:type airo:Risk ;
    rdfs:label "AI技术滥用风险"@zh ;
    rdfs:label "Misuse of AI Technology Risk"@en ;
    airo:hasConsequence ex:LegalChargesConsequence ;
    airo:hasRiskSource ex:WorkComputerMisuse .

ex:PennsylvaniaStatePolice rdf:type airo:Stakeholder ;
    rdfs:label "宾夕法尼亚州警察"@zh ;
    rdfs:label "Pennsylvania State Police"@en .

ex:PoliceCorporal rdf:type airo:AIOperator ;
    rdfs:label "警察下士"@zh ;
    rdfs:label "Police Corporal"@en .

ex:WorkComputerMisuse rdf:type airo:Misuse ;
    rdfs:label "工作电脑滥用"@zh ;
    rdfs:label "Work Computer Misuse"@en ;
    airo:isRiskSourceFor ex:MisuseOfAITechnologyRisk .

ex:LegalChargesConsequence rdf:type airo:Consequence ;
    rdfs:label "法律指控后果"@zh ;
    rdfs:label "Legal Charges Consequence"@en ;
    airo:hasImpactOnStakeholder ex:PennsylvaniaStatePolice .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "执法领域"@zh ;
    rdfs:label "Law Enforcement Domain"@en .