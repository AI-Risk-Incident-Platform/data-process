@base <https://w3id.org/airo#> .

ex:AIGeneratedImagesOfTrumpArrest rdf:type airo:AISystem ;
    rdfs:label "AI-Generated Images of Trump Arrest"@en ;
    rdfs:label "特朗普被捕的AI生成图像"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:DeepfakeImageGeneration ;
    airo:hasStakeholder ex:SocialMediaUsers ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:PoliticalConfusion .

ex:PoliticalConfusion rdf:type airo:Impact ;
    rdfs:label "Political Confusion Impact"@en ;
    rdfs:label "政治混乱影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .

ex:DeepfakeImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Image Generation Technique"@en ;
    rdfs:label "深度伪造图像生成技术"@zh .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .