@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RoombaVacuumCleaner rdf:type airo:AISystem ;
    rdfs:label "Roomba Vacuum Cleaner"@en ;
    rdfs:label "Roomba扫地机器人"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAICapability ex:ImageCaptureAndProcessing ;
    airo:isDevelopedBy ex:RoombaManufacturer ;
    airo:hasStakeholder ex:SocialMediaPlatform ;
    airo:hasAISubject ex:HomeUsers .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:IntimatePhotoLeak ;
    airo:isRiskSourceFor ex:UnauthorizedDataSharing .

ex:IntimatePhotoLeak rdf:type airo:Consequence ;
    rdfs:label "Intimate Photo Leak Consequence"@en ;
    rdfs:label "私密照片泄露后果"@zh ;
    airo:hasImpact ex:PersonalPrivacyViolation .

ex:PersonalPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Violation Impact"@en ;
    rdfs:label "个人隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:HomeUsers .

ex:ImageCaptureAndProcessing rdf:type airo:AICapability ;
    rdfs:label "Image Capture and Processing Capability"@en ;
    rdfs:label "图像捕捉与处理能力"@zh .

ex:RoombaManufacturer rdf:type airo:AIOperator ;
    rdfs:label "Roomba Manufacturer"@en ;
    rdfs:label "Roomba制造商"@zh .

ex:SocialMediaPlatform rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Platform"@en ;
    rdfs:label "社交媒体平台"@zh .

ex:HomeUsers rdf:type airo:AISubject ;
    rdfs:label "Home Users"@en ;
    rdfs:label "家庭用户"@zh .

ex:UnauthorizedDataSharing rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Data Sharing Risk Source"@en ;
    rdfs:label "未经授权的数据共享风险源"@zh ;
    airo:exploitsVulnerability ex:LackOfPrivacyProtectionMechanism .

ex:LackOfPrivacyProtectionMechanism rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Privacy Protection Mechanism Vulnerability"@en ;
    rdfs:label "隐私保护机制缺失的脆弱性"@zh .