@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:TrainingDataLeakageRisk ;
    airo:usesTechnique ex:PromptInjectionAttack ;
    airo:hasStakeholder ex:GoogleResearchers .

ex:TrainingDataLeakageRisk rdf:type airo:Risk ;
    rdfs:label "Training Data Leakage Risk"@en ;
    rdfs:label "训练数据泄露风险"@zh ;
    airo:hasConsequence ex:PersonalInformationExposure ;
    airo:isRiskSourceFor ex:VulnerableQueryResponseMechanism .

ex:PersonalInformationExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Information Exposure Consequence"@en ;
    rdfs:label "个人信息暴露后果"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:PromptInjectionAttack rdf:type airo:AITechnique ;
    rdfs:label "Prompt Injection Attack Technique"@en ;
    rdfs:label "提示注入攻击技术"@zh .

ex:GoogleResearchers rdf:type airo:Stakeholder ;
    rdfs:label "Google Researchers"@en ;
    rdfs:label "谷歌研究人员"@zh .

ex:VulnerableQueryResponseMechanism rdf:type airo:RiskSource ;
    rdfs:label "Vulnerable Query Response Mechanism Risk Source"@en ;
    rdfs:label "脆弱的查询响应机制风险源"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:GoogleResearchers .