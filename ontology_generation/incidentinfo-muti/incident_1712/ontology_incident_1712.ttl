@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WaymoSelfDrivingTaxi rdf:type airo:AISystem ;
    rdfs:label "Waymo Self-Driving Taxi"@en ;
    rdfs:label "Waymo自动驾驶出租车"@zh ;
    airo:hasRisk ex:VehicleCollisionRisk ;
    airo:isDeployedBy ex:Waymo .

ex:VehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "Vehicle Collision Risk"@en ;
    rdfs:label "车辆碰撞风险"@zh ;
    airo:hasConsequence ex:SoftwareRecall ;
    airo:isRiskSourceFor ex:SoftwareDefect .

ex:SoftwareRecall rdf:type airo:Consequence ;
    rdfs:label "Software Recall Consequence"@en ;
    rdfs:label "软件召回后果"@zh ;
    airo:hasImpact ex:PublicSafetyConcern .

ex:Waymo rdf:type airo:AIOperator ;
    rdfs:label "Waymo"@en ;
    rdfs:label "Waymo公司"@zh .

ex:SoftwareDefect rdf:type airo:RiskSource ;
    rdfs:label "Software Defect Risk Source"@en ;
    rdfs:label "软件缺陷风险源"@zh .

ex:PublicSafetyConcern rdf:type airo:Impact ;
    rdfs:label "Public Safety Concern Impact"@en ;
    rdfs:label "公共安全担忧影响"@zh ;
    airo:hasImpactOnStakeholder ex:Waymo .