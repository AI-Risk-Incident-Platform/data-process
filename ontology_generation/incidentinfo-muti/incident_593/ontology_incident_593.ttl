@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CruiseSelfDrivingCars rdf:type airo:AISystem ;
    rdfs:label "Cruise Self-Driving Cars"@en ;
    rdfs:label "Cruise自动驾驶汽车"@zh ;
    airo:hasRisk ex:PedestrianSafetyRisk ;
    airo:hasStakeholder ex:AutoSafetyRegulators ;
    airo:isDeployedBy ex:Cruise ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleIndustry .

ex:PedestrianSafetyRisk rdf:type airo:Risk ;
    rdfs:label "Pedestrian Safety Risk"@en ;
    rdfs:label "行人安全风险"@zh .

ex:AutoSafetyRegulators rdf:type airo:Stakeholder ;
    rdfs:label "Auto-Safety Regulators"@en ;
    rdfs:label "汽车安全监管机构"@zh .

ex:AutonomousVehicleIndustry rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Industry"@en ;
    rdfs:label "自动驾驶汽车行业"@zh .

ex:Cruise rdf:type airo:AIOperator ;
    rdfs:label "Cruise"@en ;
    rdfs:label "Cruise公司"@zh .