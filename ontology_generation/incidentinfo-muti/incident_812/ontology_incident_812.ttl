@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "面部识别系统"@zh ;
    rdfs:label "Facial Recognition System"@en ;
    airo:hasRisk ex:FalseArrestRisk ;
    airo:hasStakeholder ex:PoliceDepartment ;
    airo:hasAISubject ex:ArrestedIndividuals .

ex:FalseArrestRisk rdf:type airo:Risk ;
    rdfs:label "错误逮捕风险"@zh ;
    rdfs:label "False Arrest Risk"@en ;
    airo:hasConsequence ex:WrongfulDetention ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:WrongfulDetention rdf:type airo:Consequence ;
    rdfs:label "错误拘留后果"@zh ;
    rdfs:label "Wrongful Detention Consequence"@en ;
    airo:hasImpact ex:CivilRightsViolation .

ex:PoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "警察部门"@zh ;
    rdfs:label "Police Department"@en .

ex:ArrestedIndividuals rdf:type airo:AISubject ;
    rdfs:label "被逮捕个人"@zh ;
    rdfs:label "Arrested Individuals"@en .

ex:CivilRightsViolation rdf:type airo:Impact ;
    rdfs:label "公民权利侵犯影响"@zh ;
    rdfs:label "Civil Rights Violation Impact"@en ;
    airo:hasImpactOnStakeholder ex:ArrestedIndividuals .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "算法偏见风险源"@zh ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    airo:exploitsVulnerability ex:LackOfTransparency .

ex:LackOfTransparency rdf:type airo:Vulnerability ;
    rdfs:label "缺乏透明度脆弱性"@zh ;
    rdfs:label "Lack of Transparency Vulnerability"@en .