@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIChatbotContentFarmSystem rdf:type airo:AISystem ;
    rdfs:label "AI Chatbot Content Farm System"@en ;
    rdfs:label "AI聊天机器人内容农场系统"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:hasCapability ex:AutomatedContentGeneration ;
    airo:hasPurpose ex:MassContentProduction ;
    airo:hasStakeholder ex:OnlineInformationConsumers ;
    airo:isAppliedWithinDomain ex:OnlineMedia ;
    airo:usesTechnique ex:NaturalLanguageGeneration .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:SpamSiteProliferation ;
    airo:isRiskSourceFor ex:UnregulatedAIContentCreation .

ex:SpamSiteProliferation rdf:type airo:Consequence ;
    rdfs:label "Spam Site Proliferation Consequence"@en ;
    rdfs:label "垃圾网站泛滥后果"@zh ;
    airo:hasImpact ex:InformationEcosystemPollution .

ex:AutomatedContentGeneration rdf:type airo:AICapability ;
    rdfs:label "Automated Content Generation Capability"@en ;
    rdfs:label "自动化内容生成能力"@zh .

ex:OnlineInformationConsumers rdf:type airo:Stakeholder ;
    rdfs:label "Online Information Consumers"@en ;
    rdfs:label "在线信息消费者"@zh .

ex:InformationEcosystemPollution rdf:type airo:Impact ;
    rdfs:label "Information Ecosystem Pollution Impact"@en ;
    rdfs:label "信息生态系统污染影响"@zh ;
    airo:hasImpactOnStakeholder ex:OnlineInformationConsumers .

ex:MassContentProduction rdf:type airo:Purpose ;
    rdfs:label "Mass Content Production Purpose"@en ;
    rdfs:label "大规模内容生产目的"@zh .

ex:OnlineMedia rdf:type airo:Domain ;
    rdfs:label "Online Media Domain"@en ;
    rdfs:label "在线媒体领域"@zh .

ex:NaturalLanguageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Natural Language Generation Technique"@en ;
    rdfs:label "自然语言生成技术"@zh .

ex:UnregulatedAIContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Unregulated AI Content Creation Risk Source"@en ;
    rdfs:label "无监管AI内容创建风险源"@zh .