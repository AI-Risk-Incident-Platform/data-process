@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DCSecurityRobot rdf:type airo:AISystem ;
    rdfs:label "DC Security Robot"@en ;
    rdfs:label "华盛顿特区安保机器人"@zh ;
    airo:hasRisk ex:RobotSelfDestructionRisk ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasStakeholder ex:DCPublicSafetyDepartment .

ex:RobotSelfDestructionRisk rdf:type airo:Risk ;
    rdfs:label "Robot Self-Destruction Risk"@en ;
    rdfs:label "机器人自毁风险"@zh ;
    airo:hasConsequence ex:OperationalFailure ;
    airo:isRiskSourceFor ex:SystemMalfunction .

ex:OperationalFailure rdf:type airo:Consequence ;
    rdfs:label "Operational Failure Consequence"@en ;
    rdfs:label "操作失败后果"@zh ;
    airo:hasImpact ex:PublicSafetyConcern .

ex:DCPublicSafetyDepartment rdf:type airo:AIOperator ;
    rdfs:label "DC Public Safety Department"@en ;
    rdfs:label "华盛顿特区公共安全部门"@zh .

ex:PublicSafetyConcern rdf:type airo:Impact ;
    rdfs:label "Public Safety Concern Impact"@en ;
    rdfs:label "公共安全问题影响"@zh ;
    airo:hasImpactOnStakeholder ex:DCPublicSafetyDepartment .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:SystemMalfunction rdf:type airo:RiskSource ;
    rdfs:label "System Malfunction Risk Source"@en ;
    rdfs:label "系统故障风险源"@zh .