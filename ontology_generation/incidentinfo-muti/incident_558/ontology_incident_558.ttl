@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DriverlessTaxi rdf:type airo:AISystem ;
    rdfs:label "Driverless Taxi"@en ;
    rdfs:label "无人驾驶出租车"@zh ;
    airo:hasRisk ex:EmergencyVehicleBlockageRisk ;
    airo:hasStakeholder ex:SanFranciscoFireDepartment ;
    airo:isAppliedWithinDomain ex:UrbanTransportation .

ex:EmergencyVehicleBlockageRisk rdf:type airo:Risk ;
    rdfs:label "Emergency Vehicle Blockage Risk"@en ;
    rdfs:label "紧急车辆受阻风险"@zh ;
    airo:hasConsequence ex:DelayedEmergencyResponse .

ex:DelayedEmergencyResponse rdf:type airo:Consequence ;
    rdfs:label "Delayed Emergency Response Consequence"@en ;
    rdfs:label "紧急响应延迟后果"@zh ;
    airo:hasImpact ex:PotentialLossOfLife .

ex:PotentialLossOfLife rdf:type airo:Impact ;
    rdfs:label "Potential Loss of Life Impact"@en ;
    rdfs:label "潜在生命损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:SanFranciscoFireDepartment .

ex:SanFranciscoFireDepartment rdf:type airo:Stakeholder ;
    rdfs:label "San Francisco Fire Department"@en ;
    rdfs:label "旧金山消防部门"@zh .

ex:UrbanTransportation rdf:type airo:Domain ;
    rdfs:label "Urban Transportation Domain"@en ;
    rdfs:label "城市交通领域"@zh .