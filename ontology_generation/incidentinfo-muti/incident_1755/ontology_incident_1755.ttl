@base <https://w3id.org/airo#> .

ex:OkCupidDatingPlatform rdf:type airo:AISystem ;
    rdfs:label "OkCupid Dating Platform"@en ;
    rdfs:label "OkCupid约会平台"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAICapability ex:UserMatchingAlgorithm ;
    airo:isAppliedWithinDomain ex:OnlineDatingServices ;
    airo:hasStakeholder ex:DanishDataProtectionAuthority .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:DanishDataProtectionAuthority rdf:type airo:Stakeholder ;
    rdfs:label "Danish Data Protection Authority"@en ;
    rdfs:label "丹麦数据保护局"@zh .

ex:UserMatchingAlgorithm rdf:type airo:AICapability ;
    rdfs:label "User Matching Algorithm Capability"@en ;
    rdfs:label "用户匹配算法能力"@zh .

ex:OnlineDatingServices rdf:type airo:Domain ;
    rdfs:label "Online Dating Services Domain"@en ;
    rdfs:label "在线约会服务领域"@zh .