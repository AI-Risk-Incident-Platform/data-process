@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIPoweredInvestmentScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI-Powered Investment Scam System"@en ;
    rdfs:label "AI驱动的投资诈骗系统"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:AIForFraudulentActivities ;
    airo:hasStakeholder ex:InvestmentScamVictims .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpactOnStakeholder ex:InvestmentScamVictims .

ex:InvestmentScamVictims rdf:type airo:Stakeholder ;
    rdfs:label "Investment Scam Victims"@en ;
    rdfs:label "投资诈骗受害者"@zh .

ex:AIForFraudulentActivities rdf:type airo:AITechnique ;
    rdfs:label "AI for Fraudulent Activities Technique"@en ;
    rdfs:label "用于欺诈活动的AI技术"@zh .