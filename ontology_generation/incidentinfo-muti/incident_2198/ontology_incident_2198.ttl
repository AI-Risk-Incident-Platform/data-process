@base <https://w3id.org/airo#> .

ex:7ElevenFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "7-Eleven Facial Recognition System"@en ;
    rdfs:label "7-11面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:7Eleven ;
    airo:hasStakeholder ex:7ElevenCustomers .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:CustomerPrivacyConcerns .

ex:7ElevenCustomers rdf:type airo:Stakeholder ;
    rdfs:label "7-Eleven Customers"@en ;
    rdfs:label "7-11顾客"@zh .

ex:7Eleven rdf:type airo:AIOperator ;
    rdfs:label "7-Eleven"@en ;
    rdfs:label "7-11"@zh .

ex:CustomerPrivacyConcerns rdf:type airo:Consequence ;
    rdfs:label "Customer Privacy Concerns"@en ;
    rdfs:label "顾客隐私担忧"@zh ;
    airo:hasImpactOnStakeholder ex:7ElevenCustomers .