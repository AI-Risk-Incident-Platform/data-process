@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleSelfDrivingCar rdf:type airo:AISystem ;
    rdfs:label "Google Self-Driving Car"@en ;
    rdfs:label "谷歌自动驾驶汽车"@zh ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:hasCapability ex:AutonomousDriving ;
    airo:isDevelopedBy ex:Google ;
    airo:hasPurpose ex:AutonomousTransportation ;
    airo:hasStakeholder ex:PublicRoadUsers ;
    airo:hasAISubject ex:OtherVehiclesOnRoad ;
    airo:isAppliedWithinDomain ex:Transportation ;
    airo:usesTechnique ex:ComputerVision ;
    airo:hasVulnerability ex:SensorLimitations .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    rdfs:label "自动驾驶汽车碰撞风险"@zh ;
    airo:hasConsequence ex:PhysicalDamage ;
    airo:isRiskSourceFor ex:AlgorithmicDecisionMaking , ex:UnexpectedRoadConditions , ex:SystemFailure .

ex:PhysicalDamage rdf:type airo:Consequence ;
    rdfs:label "Physical Damage Consequence"@en ;
    rdfs:label "物理损坏后果"@zh ;
    airo:hasImpact ex:SafetyConcerns .

ex:AutonomousDriving rdf:type airo:AICapability ;
    rdfs:label "Autonomous Driving Capability"@en ;
    rdfs:label "自动驾驶能力"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:PublicRoadUsers rdf:type airo:Stakeholder ;
    rdfs:label "Public Road Users"@en ;
    rdfs:label "公共道路使用者"@zh .

ex:OtherVehiclesOnRoad rdf:type airo:AISubject ;
    rdfs:label "Other Vehicles on Road"@en ;
    rdfs:label "道路上其他车辆"@zh .

ex:SafetyConcerns rdf:type airo:Impact ;
    rdfs:label "Safety Concerns Impact"@en ;
    rdfs:label "安全隐患影响"@zh ;
    airo:hasImpactOnStakeholder ex:PublicRoadUsers .

ex:AutonomousTransportation rdf:type airo:Purpose ;
    rdfs:label "Autonomous Transportation Purpose"@en ;
    rdfs:label "自动驾驶交通目的"@zh .

ex:Transportation rdf:type airo:Domain ;
    rdfs:label "Transportation Domain"@en ;
    rdfs:label "交通领域"@zh .

ex:ComputerVision rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision Technique"@en ;
    rdfs:label "计算机视觉技术"@zh .

ex:SensorLimitations rdf:type airo:Vulnerability ;
    rdfs:label "Sensor Limitations Vulnerability"@en ;
    rdfs:label "传感器限制脆弱性"@zh .

ex:UnexpectedRoadConditions rdf:type airo:Threat ;
    rdfs:label "Unexpected Road Conditions Threat"@en ;
    rdfs:label "意外道路条件威胁"@zh .

ex:SystemFailure rdf:type airo:Hazard ;
    rdfs:label "System Failure Hazard"@en ;
    rdfs:label "系统故障危害"@zh .

ex:AlgorithmicDecisionMaking rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Decision Making Risk Source"@en ;
    rdfs:label "算法决策风险源"@zh .

ex:AutonomousVehicleSafety rdf:type airo:RiskConcept ;
    rdfs:label "Autonomous Vehicle Safety Risk Concept"@en ;
    rdfs:label "自动驾驶汽车安全"@zh .