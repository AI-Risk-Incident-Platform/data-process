@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasAISubject ex:VehicleOccupantsAndPedestrians ;
    airo:isAppliedWithinDomain ex:AutomotiveTransportation .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    airo:hasConsequence ex:FatalTrafficAccident .

ex:FatalTrafficAccident rdf:type airo:Consequence ;
    rdfs:label "致命交通事故后果"@zh ;
    rdfs:label "Fatal Traffic Accident Consequence"@en ;
    airo:hasImpact ex:LossOfHumanLife .

ex:LossOfHumanLife rdf:type airo:Impact ;
    rdfs:label "人员死亡影响"@zh ;
    rdfs:label "Loss of Human Life Impact"@en ;
    airo:hasImpactOnStakeholder ex:VehicleOccupantsAndPedestrians .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:VehicleOccupantsAndPedestrians rdf:type airo:AISubject ;
    rdfs:label "车辆乘员与行人"@zh ;
    rdfs:label "Vehicle Occupants and Pedestrians"@en .

ex:AutomotiveTransportation rdf:type airo:Domain ;
    rdfs:label "汽车交通领域"@zh ;
    rdfs:label "Automotive Transportation Domain"@en .