@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:InvestmentAdvisoryAI rdf:type airo:AISystem ;
    rdfs:label "Investment Advisory AI System"@en ;
    rdfs:label "投资顾问AI系统"@zh ;
    airo:hasRisk ex:MisrepresentationOfAICapabilities ;
    airo:isDeployedBy ex:InvestmentAdvisers ;
    airo:hasStakeholder ex:Investors ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:MisrepresentationOfAICapabilities rdf:type airo:Risk ;
    rdfs:label "Misrepresentation of AI Capabilities Risk"@en ;
    rdfs:label "AI能力虚假陈述风险"@zh ;
    airo:hasConsequence ex:RegulatorySanctions ;
    airo:isRiskSourceFor ex:FalseAIMarketingClaims .

ex:Investors rdf:type airo:Stakeholder ;
    rdfs:label "Investors"@en ;
    rdfs:label "投资者"@zh .

ex:InvestmentAdvisers rdf:type airo:AIOperator ;
    rdfs:label "Investment Advisers"@en ;
    rdfs:label "投资顾问公司"@zh .

ex:RegulatorySanctions rdf:type airo:Consequence ;
    rdfs:label "Regulatory Sanctions Consequence"@en ;
    rdfs:label "监管处罚后果"@zh ;
    airo:hasImpact ex:InvestorTrustErosion .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:FalseAIMarketingClaims rdf:type airo:RiskSource ;
    rdfs:label "False AI Marketing Claims Risk Source"@en ;
    rdfs:label "虚假AI营销声明风险源"@zh .

ex:InvestorTrustErosion rdf:type airo:Impact ;
    rdfs:label "Investor Trust Erosion Impact"@en ;
    rdfs:label "投资者信任受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:Investors .