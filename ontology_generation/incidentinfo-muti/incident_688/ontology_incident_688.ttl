@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAIVirtualAssistant rdf:type airo:AISystem ;
    rdfs:label "OpenAI Virtual Assistant"@en ;
    rdfs:label "OpenAI虚拟助手"@zh ;
    airo:hasRisk ex:VoiceImpersonationRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:ScarlettJohansson .

ex:VoiceImpersonationRisk rdf:type airo:Risk ;
    rdfs:label "Voice Impersonation Risk"@en ;
    rdfs:label "声音模仿风险"@zh ;
    airo:hasConsequence ex:UnauthorizedVoiceUse ;
    airo:isRiskSourceFor ex:LackOfConsent .

ex:ScarlettJohansson rdf:type airo:Stakeholder ;
    rdfs:label "Scarlett Johansson"@en ;
    rdfs:label "斯嘉丽·约翰逊"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:UnauthorizedVoiceUse rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Voice Use Consequence"@en ;
    rdfs:label "未经授权使用声音的后果"@zh ;
    airo:hasImpact ex:PersonalRightsViolation .

ex:PersonalRightsViolation rdf:type airo:Impact ;
    rdfs:label "Personal Rights Violation Impact"@en ;
    rdfs:label "个人权利侵犯的影响"@zh ;
    airo:hasImpactOnStakeholder ex:ScarlettJohansson .

ex:LackOfConsent rdf:type airo:RiskSource ;
    rdfs:label "Lack of Consent Risk Source"@en ;
    rdfs:label "缺乏同意的风险源"@zh .