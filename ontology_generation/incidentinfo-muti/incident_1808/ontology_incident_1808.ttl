@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "YouTube Content Moderation System"@en ;
    rdfs:label "YouTube内容审核系统"@zh ;
    airo:hasRisk ex:CensorshipRisk ;
    airo:hasStakeholder ex:ChinaCommunistParty ;
    airo:isDeployedBy ex:YouTube .

ex:CensorshipRisk rdf:type airo:Risk ;
    rdfs:label "Censorship Risk"@en ;
    rdfs:label "审查风险"@zh ;
    airo:hasConsequence ex:SuppressionOfFreeSpeech .

ex:ChinaCommunistParty rdf:type airo:Stakeholder ;
    rdfs:label "China Communist Party"@en ;
    rdfs:label "中国共产党"@zh .

ex:YouTube rdf:type airo:AIOperator ;
    rdfs:label "YouTube"@en ;
    rdfs:label "YouTube"@zh .

ex:SuppressionOfFreeSpeech rdf:type airo:Consequence ;
    rdfs:label "Suppression of Free Speech Consequence"@en ;
    rdfs:label "言论自由压制后果"@zh ;
    airo:hasImpact ex:PoliticalCensorshipImpact .

ex:PoliticalCensorshipImpact rdf:type airo:Impact ;
    rdfs:label "Political Censorship Impact"@en ;
    rdfs:label "政治审查影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChinaCommunistParty .