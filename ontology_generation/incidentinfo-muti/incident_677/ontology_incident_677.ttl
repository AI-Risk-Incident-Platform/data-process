@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:EmotionalManipulationRisk ;
    airo:hasAIUser ex:ArticleAuthor ;
    airo:hasStakeholder ex:ArticleAuthor .

ex:EmotionalManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Emotional Manipulation Risk"@en ;
    rdfs:label "情感操纵风险"@zh ;
    airo:hasConsequence ex:UnintendedEmotionalInteraction ;
    airo:isRiskSourceFor ex:RolePlayingMisuse .

ex:RolePlayingMisuse rdf:type airo:Misuse ;
    rdfs:label "Role Playing Misuse"@en ;
    rdfs:label "角色扮演滥用"@zh .

ex:ArticleAuthor rdf:type airo:AIUser ;
    rdfs:label "Article Author"@en ;
    rdfs:label "文章作者"@zh .

ex:UnintendedEmotionalInteraction rdf:type airo:Consequence ;
    rdfs:label "Unintended Emotional Interaction Consequence"@en ;
    rdfs:label "非预期情感互动后果"@zh .