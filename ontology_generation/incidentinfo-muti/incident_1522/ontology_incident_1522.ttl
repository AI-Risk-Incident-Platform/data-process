ex:LaMDA rdf:type airo:AISystem ;
    rdfs:label "LaMDA"@en ;
    rdfs:label "LaMDA"@zh ;
    airo:hasRisk ex:EthicalConcernsInAI ;
    airo:hasCapability ex:ConversationalAI ;
    airo:hasPurpose ex:HumanLikeConversation ;
    airo:usesTechnique ex:LargeLanguageModel ;
    airo:hasStakeholder ex:GoogleEngineer .

ex:EthicalConcernsInAI rdf:type airo:Risk ;
    rdfs:label "Ethical Concerns in AI Risk"@en ;
    rdfs:label "AI伦理问题风险"@zh ;
    airo:isRiskSourceFor ex:MediaHype ;
    airo:hasImpact ex:PublicConfusionAboutAIConsciousness .

ex:GoogleEngineer rdf:type airo:Stakeholder ;
    rdfs:label "Google Engineer"@en ;
    rdfs:label "谷歌工程师"@zh .

ex:ConversationalAI rdf:type airo:AICapability ;
    rdfs:label "Conversational AI Capability"@en ;
    rdfs:label "对话式AI能力"@zh .

ex:HumanLikeConversation rdf:type airo:Purpose ;
    rdfs:label "Human-like Conversation Purpose"@en ;
    rdfs:label "类人对话目的"@zh .

ex:MediaHype rdf:type airo:RiskSource ;
    rdfs:label "Media Hype Risk Source"@en ;
    rdfs:label "媒体炒作风险源"@zh .

ex:PublicConfusionAboutAIConsciousness rdf:type airo:Impact ;
    rdfs:label "Public Confusion About AI Consciousness Impact"@en ;
    rdfs:label "公众对AI意识的困惑影响"@zh .

ex:LargeLanguageModel rdf:type airo:AITechnique ;
    rdfs:label "Large Language Model Technique"@en ;
    rdfs:label "大语言模型技术"@zh .