@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DALLE2 rdf:type airo:AISystem ;
    rdfs:label "DALL·E 2"@en ;
    rdfs:label "DALL·E 2"@zh ;
    airo:hasRisk ex:BiasInImageGeneration ;
    airo:hasCapability ex:TextToImageGeneration ;
    airo:usesTechnique ex:DeepLearningBasedImageSynthesis .

ex:BiasInImageGeneration rdf:type airo:Risk ;
    rdfs:label "Bias in Image Generation Risk"@en ;
    rdfs:label "图像生成中的偏见风险"@zh ;
    airo:isRiskSourceFor ex:TrainingDataBias ;
    airo:hasImpact ex:StereotypeReinforcement .

ex:TextToImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Text to Image Generation Capability"@en ;
    rdfs:label "文本到图像生成能力"@zh .

ex:DeepLearningBasedImageSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Image Synthesis Technique"@en ;
    rdfs:label "基于深度学习的图像合成技术"@zh .

ex:StereotypeReinforcement rdf:type airo:Impact ;
    rdfs:label "Stereotype Reinforcement Impact"@en ;
    rdfs:label "刻板印象强化的影响"@zh .

ex:TrainingDataBias rdf:type airo:RiskSource ;
    rdfs:label "Training Data Bias Risk Source"@en ;
    rdfs:label "训练数据偏差的风险源"@zh .