@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VoiceCloningScamAI rdf:type airo:AISystem ;
    rdfs:label "Voice Cloning Scam AI"@en ;
    rdfs:label "语音克隆诈骗AI"@zh ;
    airo:hasRisk ex:ImpersonationScamRisk ;
    airo:usesTechnique ex:DeepfakeVoiceCloning ;
    airo:hasStakeholder ex:SaltLakeCityPoliceDepartment ;
    airo:hasAISubject ex:PotentialScamVictims .

ex:ImpersonationScamRisk rdf:type airo:Risk ;
    rdfs:label "Impersonation Scam Risk"@en ;
    rdfs:label "冒充诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialFraud ;
    airo:isRiskSourceFor ex:AIEnabledImpersonation .

ex:DeepfakeVoiceCloning rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Voice Cloning Technique"@en ;
    rdfs:label "深度伪造语音克隆技术"@zh .

ex:SaltLakeCityPoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "Salt Lake City Police Department"@en ;
    rdfs:label "盐湖城警察局"@zh .

ex:PotentialScamVictims rdf:type airo:AISubject ;
    rdfs:label "Potential Scam Victims"@en ;
    rdfs:label "潜在诈骗受害者"@zh .

ex:FinancialFraud rdf:type airo:Consequence ;
    rdfs:label "Financial Fraud Consequence"@en ;
    rdfs:label "金融诈骗后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:SaltLakeCityPoliceDepartment .

ex:AIEnabledImpersonation rdf:type airo:RiskSource ;
    rdfs:label "AI-Enabled Impersonation Risk Source"@en ;
    rdfs:label "AI驱动的冒充风险源"@zh .