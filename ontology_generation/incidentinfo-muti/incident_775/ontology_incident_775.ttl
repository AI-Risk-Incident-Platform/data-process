@base <https://w3id.org/airo#> .

ex:DeepfakeImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Image Generation System"@en ;
    rdfs:label "深度伪造图像生成系统"@zh ;
    airo:hasRisk ex:PrivacyViolationAndPsychologicalHarmRisk ;
    airo:hasAISubject ex:KoreanFemaleStudents ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasStakeholder ex:KoreanLawEnforcement .

ex:PrivacyViolationAndPsychologicalHarmRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Psychological Harm Risk"@en ;
    rdfs:label "隐私侵犯和心理伤害风险"@zh ;
    airo:hasConsequence ex:PsychologicalDistressForVictims ;
    airo:isRiskSourceFor ex:MaliciousUseOfAITechnology .

ex:PsychologicalDistressForVictims rdf:type airo:Consequence ;
    rdfs:label "Psychological Distress for Victims Consequence"@en ;
    rdfs:label "受害者心理困扰后果"@zh ;
    airo:hasImpact ex:SocialReputationDamage .

ex:KoreanFemaleStudents rdf:type airo:AISubject ;
    rdfs:label "Korean Female Students"@en ;
    rdfs:label "韩国女学生"@zh .

ex:SocialReputationDamage rdf:type airo:Impact ;
    rdfs:label "Social Reputation Damage Impact"@en ;
    rdfs:label "社会声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:KoreanFemaleStudents .

ex:KoreanLawEnforcement rdf:type airo:Stakeholder ;
    rdfs:label "Korean Law Enforcement"@en ;
    rdfs:label "韩国执法部门"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:MaliciousUseOfAITechnology rdf:type airo:RiskSource ;
    rdfs:label "Malicious Use of AI Technology Risk Source"@en ;
    rdfs:label "AI技术恶意使用风险源"@zh .