ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video System"@en ;
    rdfs:label "深度伪造视频系统"@zh ;
    airo:hasRisk ex:IdentityImpersonationRisk ;
    airo:hasCapability ex:FacialSynthesisAndVoiceCloning ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:IdentityImpersonationRisk rdf:type airo:Risk ;
    rdfs:label "Identity Impersonation Risk"@en ;
    rdfs:label "身份冒充风险"@zh ;
    airo:hasImpact ex:PublicConfusionAndMisinformation .

ex:FacialSynthesisAndVoiceCloning rdf:type airo:AICapability ;
    rdfs:label "Facial Synthesis and Voice Cloning Capability"@en ;
    rdfs:label "面部合成与语音克隆能力"@zh .

ex:PublicConfusionAndMisinformation rdf:type airo:Impact ;
    rdfs:label "Public Confusion and Misinformation Impact"@en ;
    rdfs:label "公众混淆与虚假信息影响"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .