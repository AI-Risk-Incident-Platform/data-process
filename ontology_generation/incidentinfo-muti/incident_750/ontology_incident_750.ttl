@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIWrittenObituariesSystem rdf:type airo:AISystem ;
    rdfs:label "AI-Written Obituaries System"@en ;
    rdfs:label "AI撰写讣告系统"@zh ;
    airo:hasRisk ex:CompoundingGriefRisk ;
    airo:hasAISubject ex:BereavedIndividuals ;
    airo:isAppliedWithinDomain ex:FuneralServices .

ex:CompoundingGriefRisk rdf:type airo:Risk ;
    rdfs:label "Compounding Grief Risk"@en ;
    rdfs:label "加剧悲痛风险"@zh ;
    airo:hasConsequence ex:EmotionalDistress .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Emotional Distress Impact"@en ;
    rdfs:label "情绪困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:BereavedIndividuals .

ex:BereavedIndividuals rdf:type airo:AISubject ;
    rdfs:label "Bereaved Individuals"@en ;
    rdfs:label "丧亲者"@zh .

ex:FuneralServices rdf:type airo:Domain ;
    rdfs:label "Funeral Services Domain"@en ;
    rdfs:label "殡葬服务领域"@zh .