@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAdDeliverySystem rdf:type airo:AISystem ;
    rdfs:label "Google Ad Delivery System"@en ;
    rdfs:label "谷歌广告投放系统"@zh ;
    airo:hasRisk ex:RacialBiasInAdDelivery ;
    airo:hasAICapability ex:AutomatedAdTargeting ;
    airo:hasPurpose ex:TargetedAdvertising ;
    airo:isAppliedWithinDomain ex:OnlineAdvertising ;
    airo:hasStakeholder ex:BlackCommunity ;
    airo:isDeployedBy ex:GoogleLLC .

ex:RacialBiasInAdDelivery rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Ad Delivery Risk"@en ;
    rdfs:label "广告投放中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryAdTargeting ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:DiscriminatoryAdTargeting rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Ad Targeting Consequence"@en ;
    rdfs:label "歧视性广告定向后果"@zh ;
    airo:hasImpact ex:RacialStereotyping .

ex:AutomatedAdTargeting rdf:type airo:AICapability ;
    rdfs:label "Automated Ad Targeting Capability"@en ;
    rdfs:label "自动化广告定向能力"@zh .

ex:BlackCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Black Community"@en ;
    rdfs:label "黑人社区"@zh .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌公司"@zh .

ex:RacialStereotyping rdf:type airo:Impact ;
    rdfs:label "Racial Stereotyping Impact"@en ;
    rdfs:label "种族刻板印象影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackCommunity .

ex:TargetedAdvertising rdf:type airo:Purpose ;
    rdfs:label "Targeted Advertising Purpose"@en ;
    rdfs:label "定向广告目的"@zh .

ex:OnlineAdvertising rdf:type airo:Domain ;
    rdfs:label "Online Advertising Domain"@en ;
    rdfs:label "在线广告领域"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .