@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FordBlueCruiseSystem rdf:type airo:AISystem ;
    rdfs:label "福特BlueCruise系统"@zh ;
    rdfs:label "Ford Blue Cruise System"@en ;
    airo:hasRisk ex:AutonomousDrivingFatalityRisk ;
    airo:hasStakeholder ex:FordMotorCompany ;
    airo:hasAIOperator ex:FordMotorCompanyOperator ;
    airo:hasCapability ex:HandsFreeDrivingCapability ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry .

ex:AutonomousDrivingFatalityRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶致命风险"@zh ;
    rdfs:label "Autonomous Driving Fatality Risk"@en ;
    airo:hasConsequence ex:FatalCrashConsequence .

ex:FordMotorCompany rdf:type airo:Stakeholder ;
    rdfs:label "福特汽车公司"@zh ;
    rdfs:label "Ford Motor Company"@en .

ex:FordMotorCompanyOperator rdf:type airo:AIOperator ;
    rdfs:label "福特汽车公司经营者"@zh ;
    rdfs:label "Ford Motor Company Operator"@en .

ex:FatalCrashConsequence rdf:type airo:Consequence ;
    rdfs:label "致命碰撞后果"@zh ;
    rdfs:label "Fatal Crash Consequence"@en ;
    airo:hasImpact ex:LossOfLifeImpact .

ex:LossOfLifeImpact rdf:type airo:Impact ;
    rdfs:label "生命损失影响"@zh ;
    rdfs:label "Loss of Life Impact"@en ;
    airo:hasImpactOnStakeholder ex:FordMotorCompany .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:HandsFreeDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "免提驾驶能力"@zh ;
    rdfs:label "Hands-Free Driving Capability"@en .