@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TwitterPhotoCropAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Twitter Photo Crop Algorithm"@en ;
    rdfs:label "Twitter照片裁剪算法"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInPhotoCropping ;
    airo:hasCapability ex:ImageSaliencyDetection ;
    airo:hasPurpose ex:AutomatedImageOptimization ;
    airo:hasStakeholder ex:TwitterBugBountyParticipants ;
    airo:isDeployedBy ex:TwitterInc ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:usesTechnique ex:NeuralNetworkBasedSaliencyPrediction ;
    airo:hasVulnerability ex:TrainingDataRepresentationBias .

ex:AlgorithmicBiasInPhotoCropping rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Photo Cropping Risk"@en ;
    rdfs:label "照片裁剪中的算法偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryImagePresentation ;
    airo:isRiskSourceFor ex:UnintendedAlgorithmicBias .

ex:DiscriminatoryImagePresentation rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Image Presentation Consequence"@en ;
    rdfs:label "歧视性图像呈现后果"@zh ;
    airo:hasImpact ex:RacialAndGenderBiasAmplification .

ex:ImageSaliencyDetection rdf:type airo:AICapability ;
    rdfs:label "Image Saliency Detection Capability"@en ;
    rdfs:label "图像显著性检测能力"@zh .

ex:TwitterBugBountyParticipants rdf:type airo:Stakeholder ;
    rdfs:label "Twitter Bug Bounty Participants"@en ;
    rdfs:label "Twitter漏洞赏金参与者"@zh .

ex:TwitterInc rdf:type airo:AIOperator ;
    rdfs:label "Twitter Inc."@en ;
    rdfs:label "Twitter公司"@zh .

ex:RacialAndGenderBiasAmplification rdf:type airo:Impact ;
    rdfs:label "Racial and Gender Bias Amplification Impact"@en ;
    rdfs:label "种族和性别偏见放大的影响"@zh ;
    airo:hasImpactOnStakeholder ex:TwitterBugBountyParticipants .

ex:AutomatedImageOptimization rdf:type airo:Purpose ;
    rdfs:label "Automated Image Optimization Purpose"@en ;
    rdfs:label "自动图像优化的目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:NeuralNetworkBasedSaliencyPrediction rdf:type airo:AITechnique ;
    rdfs:label "Neural Network Based Saliency Prediction Technique"@en ;
    rdfs:label "基于神经网络的显著性预测技术"@zh .

ex:TrainingDataRepresentationBias rdf:type airo:Vulnerability ;
    rdfs:label "Training Data Representation Bias Vulnerability"@en ;
    rdfs:label "训练数据代表性偏差的脆弱性"@zh .

ex:UnintendedAlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Unintended Algorithmic Bias Risk Source"@en ;
    rdfs:label "非预期的算法偏见风险源"@zh .