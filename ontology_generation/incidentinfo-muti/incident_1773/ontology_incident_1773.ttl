@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilot rdf:type airo:AISystem ;
    rdfs:label "Tesla Autopilot"@en ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    airo:hasRisk ex:CollisionRiskWithEmergencyVehicles ;
    airo:hasCapability ex:PartialAutomatedDriving ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasAISubject ex:PoliceOfficers ;
    airo:hasPurpose ex:AssistedDriving ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionBasedObjectDetection ;
    airo:hasStakeholder ex:FirstResponders .

ex:CollisionRiskWithEmergencyVehicles rdf:type airo:Risk ;
    rdfs:label "Collision Risk with Emergency Vehicles"@en ;
    rdfs:label "与应急车辆碰撞风险"@zh ;
    airo:hasConsequence ex:PhysicalDamageAndSafetyThreat ;
    airo:isRiskSourceFor ex:SensorLimitations .

ex:PhysicalDamageAndSafetyThreat rdf:type airo:Consequence ;
    rdfs:label "Physical Damage and Safety Threat Consequence"@en ;
    rdfs:label "物理损坏与安全威胁后果"@zh ;
    airo:hasImpact ex:PublicSafetyCompromise .

ex:PartialAutomatedDriving rdf:type airo:AICapability ;
    rdfs:label "Partial Automated Driving Capability"@en ;
    rdfs:label "部分自动驾驶能力"@zh .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:FirstResponders rdf:type airo:Stakeholder ;
    rdfs:label "First Responders"@en ;
    rdfs:label "应急响应人员"@zh .

ex:PoliceOfficers rdf:type airo:AISubject ;
    rdfs:label "Police Officers"@en ;
    rdfs:label "警务人员"@zh .

ex:PublicSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Public Safety Compromise Impact"@en ;
    rdfs:label "公共安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:FirstResponders .

ex:AssistedDriving rdf:type airo:Purpose ;
    rdfs:label "Assisted Driving Purpose"@en ;
    rdfs:label "辅助驾驶目的"@zh .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "Automotive Industry Domain"@en ;
    rdfs:label "汽车工业领域"@zh .

ex:ComputerVisionBasedObjectDetection rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision Based Object Detection Technique"@en ;
    rdfs:label "基于计算机视觉的物体检测技术"@zh .

ex:EmergencyVehicleRecognitionFailure rdf:type airo:Vulnerability ;
    rdfs:label "Emergency Vehicle Recognition Failure Vulnerability"@en ;
    rdfs:label "应急车辆识别失败漏洞"@zh ;
    airo:hasVulnerability ex:SensorLimitations .

ex:StationaryVehicleCollision rdf:type airo:Threat ;
    rdfs:label "Stationary Vehicle Collision Threat"@en ;
    rdfs:label "静止车辆碰撞威胁"@zh ;
    airo:isRiskSourceFor ex:CollisionRiskWithEmergencyVehicles .

ex:FlashingLightsMisinterpretation rdf:type airo:Hazard ;
    rdfs:label "Flashing Lights Misinterpretation Hazard"@en ;
    rdfs:label "闪光灯误判危害"@zh ;
    airo:isRiskSourceFor ex:CollisionRiskWithEmergencyVehicles .

ex:SensorLimitations rdf:type airo:RiskSource ;
    rdfs:label "Sensor Limitations Risk Source"@en ;
    rdfs:label "传感器限制风险源"@zh ;
    airo:exploitsVulnerability ex:EmergencyVehicleRecognitionFailure .

ex:AutonomousVehicleSafetyChallenges rdf:type airo:RiskConcept ;
    rdfs:label "Autonomous Vehicle Safety Challenges Risk Concept"@en ;
    rdfs:label "自动驾驶车辆安全挑战"@zh .