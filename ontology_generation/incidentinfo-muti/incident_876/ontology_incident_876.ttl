@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WaymoRobotaxi rdf:type airo:AISystem ;
    rdfs:label "Waymo Robotaxi"@en ;
    rdfs:label "Waymo自动驾驶出租车"@zh ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasAICapability ex:AutonomousDriving ;
    airo:isDeployedBy ex:Waymo ;
    airo:hasPurpose ex:AutonomousTransportation ;
    airo:hasStakeholder ex:ServeDeliveryRobotOperator ;
    airo:hasAISubject ex:RoadUsers ;
    airo:isAppliedWithinDomain ex:UrbanMobility ;
    airo:usesTechnique ex:ComputerVision ;
    airo:hasVulnerability ex:SensorLimitations .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    airo:hasConsequence ex:PhysicalDamage ;
    airo:isRiskSourceFor ex:MultiAgentInteraction ;
    airo:isRiskSourceFor ex:UrbanEnvironmentComplexity ;
    airo:isRiskSourceFor ex:UnpredictablePedestrianBehavior .

ex:PhysicalDamage rdf:type airo:Consequence ;
    rdfs:label "Physical Damage Consequence"@en ;
    rdfs:label "物理损坏后果"@zh ;
    airo:hasImpact ex:TrafficDisruption .

ex:AutonomousDriving rdf:type airo:AICapability ;
    rdfs:label "Autonomous Driving Capability"@en ;
    rdfs:label "自动驾驶能力"@zh .

ex:Waymo rdf:type airo:AIOperator ;
    rdfs:label "Waymo"@en ;
    rdfs:label "Waymo公司"@zh .

ex:ServeDeliveryRobotOperator rdf:type airo:Stakeholder ;
    rdfs:label "Serve Delivery Robot Operator"@en ;
    rdfs:label "Serve送货机器人运营商"@zh .

ex:RoadUsers rdf:type airo:AISubject ;
    rdfs:label "Road Users"@en ;
    rdfs:label "道路使用者"@zh .

ex:TrafficDisruption rdf:type airo:Impact ;
    rdfs:label "Traffic Disruption Impact"@en ;
    rdfs:label "交通中断影响"@zh ;
    airo:hasImpactOnStakeholder ex:ServeDeliveryRobotOperator .

ex:AutonomousTransportation rdf:type airo:Purpose ;
    rdfs:label "Autonomous Transportation Purpose"@en ;
    rdfs:label "自动驾驶运输目的"@zh .

ex:UrbanMobility rdf:type airo:Domain ;
    rdfs:label "Urban Mobility Domain"@en ;
    rdfs:label "城市交通领域"@zh .

ex:ComputerVision rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision Technique"@en ;
    rdfs:label "计算机视觉技术"@zh .

ex:SensorLimitations rdf:type airo:Vulnerability ;
    rdfs:label "Sensor Limitations Vulnerability"@en ;
    rdfs:label "传感器限制脆弱性"@zh .

ex:MultiAgentInteraction rdf:type airo:RiskSource ;
    rdfs:label "Multi-Agent Interaction Risk Source"@en ;
    rdfs:label "多智能体交互风险源"@zh .

ex:UrbanEnvironmentComplexity rdf:type airo:Threat ;
    rdfs:label "Urban Environment Complexity Threat"@en ;
    rdfs:label "城市环境复杂性威胁"@zh .

ex:UnpredictablePedestrianBehavior rdf:type airo:Hazard ;
    rdfs:label "Unpredictable Pedestrian Behavior Hazard"@en ;
    rdfs:label "不可预测的行人行为危害"@zh .

ex:AutonomousSystemSafety rdf:type airo:RiskConcept ;
    rdfs:label "Autonomous System Safety Risk Concept"@en ;
    rdfs:label "自动驾驶系统安全风险概念"@zh .