@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonAlexa rdf:type airo:AISystem ;
    rdfs:label "Amazon Alexa"@en ;
    rdfs:label "亚马逊Alexa"@zh ;
    airo:hasRisk ex:UnintendedPurchaseRisk ;
    airo:hasCapability ex:VoiceRecognitionAndProcessing ;
    airo:isAppliedWithinDomain ex:SmartHomeDevices .

ex:UnintendedPurchaseRisk rdf:type airo:Risk ;
    rdfs:label "Unintended Purchase Risk"@en ;
    rdfs:label "意外购买风险"@zh ;
    airo:hasConsequence ex:UnauthorizedPurchases ;
    airo:isRiskSourceFor ex:VoiceCommandMisinterpretation .

ex:UnauthorizedPurchases rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Purchases Consequence"@en ;
    rdfs:label "未经授权的购买后果"@zh .

ex:VoiceCommandMisinterpretation rdf:type airo:RiskSource ;
    rdfs:label "Voice Command Misinterpretation Risk Source"@en ;
    rdfs:label "语音指令误解风险源"@zh .

ex:VoiceRecognitionAndProcessing rdf:type airo:AICapability ;
    rdfs:label "Voice Recognition and Processing Capability"@en ;
    rdfs:label "语音识别与处理能力"@zh .

ex:SmartHomeDevices rdf:type airo:Domain ;
    rdfs:label "Smart Home Devices Domain"@en ;
    rdfs:label "智能家居设备领域"@zh .