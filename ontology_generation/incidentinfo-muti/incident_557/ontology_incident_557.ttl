@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIJunkContentGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Junk Content Generation System"@en ;
    rdfs:label "AI垃圾内容生成系统"@zh ;
    airo:hasRisk ex:InternetPollutionRisk ;
    airo:isAppliedWithinDomain ex:InternetContent ;
    airo:usesTechnique ex:AutomatedContentGeneration .

ex:InternetPollutionRisk rdf:type airo:Risk ;
    rdfs:label "Internet Pollution Risk"@en ;
    rdfs:label "互联网污染风险"@zh ;
    airo:hasConsequence ex:InformationQualityDegradation .

ex:InformationQualityDegradation rdf:type airo:Consequence ;
    rdfs:label "Information Quality Degradation Consequence"@en ;
    rdfs:label "信息质量下降后果"@zh ;
    airo:hasImpact ex:UserExperienceDeterioration .

ex:UserExperienceDeterioration rdf:type airo:Impact ;
    rdfs:label "User Experience Deterioration Impact"@en ;
    rdfs:label "用户体验恶化影响"@zh .

ex:InternetContent rdf:type airo:Domain ;
    rdfs:label "Internet Content Domain"@en ;
    rdfs:label "互联网内容领域"@zh .

ex:AutomatedContentGeneration rdf:type airo:AITechnique ;
    rdfs:label "Automated Content Generation Technique"@en ;
    rdfs:label "自动化内容生成技术"@zh .