@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:CulturalAndCreativeImpactRisk ;
    airo:isAppliedWithinDomain ex:CultureAndCreativity .

ex:CulturalAndCreativeImpactRisk rdf:type airo:Risk ;
    rdfs:label "Cultural and Creative Impact Risk"@en ;
    rdfs:label "文化和创意影响风险"@zh ;
    airo:hasConsequence ex:NegativeImpactOnCultureAndCreativity .

ex:NegativeImpactOnCultureAndCreativity rdf:type airo:Impact ;
    rdfs:label "Negative Impact on Culture and Creativity"@en ;
    rdfs:label "对文化和创意的负面影响"@zh .

ex:CultureAndCreativity rdf:type airo:Domain ;
    rdfs:label "Culture and Creativity Domain"@en ;
    rdfs:label "文化和创意领域"@zh .