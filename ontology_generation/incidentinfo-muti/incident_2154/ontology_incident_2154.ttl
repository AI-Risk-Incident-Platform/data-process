@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SocialCardSystem rdf:type airo:AISystem ;
    rdfs:label "Social Card System"@en ;
    rdfs:label "社会卡系统"@zh ;
    airo:hasRisk ex:AlgorithmicDiscriminationRisk ;
    airo:hasStakeholder ex:VulnerableCitizens ;
    airo:isAppliedWithinDomain ex:SocialWelfare .

ex:AlgorithmicDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Discrimination Risk"@en ;
    rdfs:label "算法歧视风险"@zh ;
    airo:hasConsequence ex:ExposureOfVulnerableGroups .

ex:ExposureOfVulnerableGroups rdf:type airo:Consequence ;
    rdfs:label "Exposure of Vulnerable Groups Consequence"@en ;
    rdfs:label "弱势群体暴露后果"@zh ;
    airo:hasImpactOnStakeholder ex:VulnerableCitizens .

ex:VulnerableCitizens rdf:type airo:Stakeholder ;
    rdfs:label "Vulnerable Citizens"@en ;
    rdfs:label "弱势公民"@zh .

ex:SocialWelfare rdf:type airo:Domain ;
    rdfs:label "Social Welfare Domain"@en ;
    rdfs:label "社会福利领域"@zh .