@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MSGFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "MSG面部识别系统"@zh ;
    rdfs:label "MSG Facial Recognition System"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:Lawyers ;
    airo:isDeployedBy ex:MadisonSquareGarden .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:hasConsequence ex:UnjustExclusion .

ex:Lawyers rdf:type airo:Stakeholder ;
    rdfs:label "律师"@zh ;
    rdfs:label "Lawyers"@en .

ex:MadisonSquareGarden rdf:type airo:AIOperator ;
    rdfs:label "麦迪逊广场花园"@zh ;
    rdfs:label "Madison Square Garden"@en .

ex:UnjustExclusion rdf:type airo:Consequence ;
    rdfs:label "不公正排除后果"@zh ;
    rdfs:label "Unjust Exclusion Consequence"@en ;
    airo:hasImpact ex:ProfessionalDiscrimination .

ex:ProfessionalDiscrimination rdf:type airo:Impact ;
    rdfs:label "职业歧视影响"@zh ;
    rdfs:label "Professional Discrimination Impact"@en ;
    airo:hasImpactOnStakeholder ex:Lawyers .