@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AICrimeDetectionSystem rdf:type airo:AISystem ;
    rdfs:label "AI Crime Detection System"@en ;
    rdfs:label "AI犯罪检测系统"@zh ;
    airo:hasRisk ex:AICrimeRisk ;
    airo:hasStakeholder ex:LegalInstitutions ;
    airo:isAppliedWithinDomain ex:LawEnforcement ;
    airo:hasPurpose ex:CrimeDetectionAndPrevention .

ex:AICrimeRisk rdf:type airo:Risk ;
    rdfs:label "AI Crime Risk"@en ;
    rdfs:label "AI犯罪风险"@zh .

ex:LegalInstitutions rdf:type airo:Stakeholder ;
    rdfs:label "Legal Institutions"@en ;
    rdfs:label "法律机构"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .

ex:CrimeDetectionAndPrevention rdf:type airo:Purpose ;
    rdfs:label "Crime Detection and Prevention Purpose"@en ;
    rdfs:label "犯罪检测与预防"@zh .