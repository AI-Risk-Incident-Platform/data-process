@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AITrainingDataset rdf:type airo:AISystem ;
    rdfs:label "AI Training Dataset"@en ;
    rdfs:label "AI训练数据集"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:MedicalPatients .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:InclusionOfPrivateRecords ;
    airo:hasConsequence ex:UnauthorizedUseOfSensitiveData .

ex:UnauthorizedUseOfSensitiveData rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Use of Sensitive Data"@en ;
    rdfs:label "敏感数据未经授权使用"@zh ;
    airo:hasImpact ex:PersonalPrivacyCompromise .

ex:MedicalPatients rdf:type airo:AISubject ;
    rdfs:label "Medical Patients"@en ;
    rdfs:label "医疗患者"@zh ;
    airo:hasImpactOnStakeholder ex:PersonalPrivacyCompromise .

ex:InclusionOfPrivateRecords rdf:type airo:RiskSource ;
    rdfs:label "Inclusion of Private Records"@en ;
    rdfs:label "包含私人记录"@zh .

ex:PersonalPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Compromise"@en ;
    rdfs:label "个人隐私泄露"@zh .