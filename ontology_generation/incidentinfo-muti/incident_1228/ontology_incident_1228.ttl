@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAI rdf:type airo:AISystem ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:CanadianNewsOutlets ;
    airo:isAppliedWithinDomain ex:MediaAndPublishing .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalActionConsequence .

ex:CanadianNewsOutlets rdf:type airo:Stakeholder ;
    rdfs:label "Canadian News Outlets"@en ;
    rdfs:label "加拿大新闻媒体"@zh .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Action Consequence"@en ;
    rdfs:label "法律诉讼后果"@zh ;
    airo:hasImpactOnStakeholder ex:CanadianNewsOutlets .

ex:MediaAndPublishing rdf:type airo:Domain ;
    rdfs:label "Media and Publishing"@en ;
    rdfs:label "媒体与出版"@zh .