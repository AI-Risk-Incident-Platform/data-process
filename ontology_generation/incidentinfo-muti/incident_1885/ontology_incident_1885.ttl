@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SnapchatChatbot rdf:type airo:AISystem ;
    rdfs:label "Snapchat Chatbot"@en ;
    rdfs:label "Snapchat聊天机器人"@zh ;
    airo:hasRisk ex:InappropriateAdviceRisk ;
    airo:hasStakeholder ex:Minors .

ex:InappropriateAdviceRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Advice Risk"@en ;
    rdfs:label "不当建议风险"@zh ;
    airo:isRiskSourceFor ex:ChatbotContentGeneration ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "产生不当建议后果"@zh ;
        rdfs:label "Consequence of Inappropriate Advice"@en ;
        airo:hasImpact ex:PsychologicalHarm
    ] .

ex:Minors rdf:type airo:Stakeholder ;
    rdfs:label "Minors"@en ;
    rdfs:label "未成年人"@zh .

ex:ChatbotContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Chatbot Content Generation"@en ;
    rdfs:label "聊天机器人内容生成"@zh .

ex:PsychologicalHarm rdf:type airo:Impact ;
    rdfs:label "Psychological Harm"@en ;
    rdfs:label "心理伤害"@zh ;
    airo:hasImpactOnStakeholder ex:Minors .