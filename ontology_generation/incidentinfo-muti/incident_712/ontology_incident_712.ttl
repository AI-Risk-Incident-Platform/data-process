@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NYCAIChatbot rdf:type airo:AISystem ;
    rdfs:label "NYC AI Chatbot"@en ;
    rdfs:label "纽约市AI聊天机器人"@zh ;
    airo:hasRisk ex:LegalViolationRisk ;
    airo:isDeployedBy ex:NewYorkCityGovernment ;
    airo:hasStakeholder ex:LocalBusinesses ;
    airo:hasPurpose ex:BusinessRegulationGuidance ;
    airo:isAppliedWithinDomain ex:GovernmentServices .

ex:LegalViolationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Violation Risk"@en ;
    rdfs:label "法律违规风险"@zh ;
    airo:hasConsequence ex:PotentialLegalPenalties ;
    airo:isRiskSourceFor ex:IncorrectLegalAdvice .

ex:PotentialLegalPenalties rdf:type airo:Consequence ;
    rdfs:label "Potential Legal Penalties Consequence"@en ;
    rdfs:label "潜在法律处罚后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:NewYorkCityGovernment rdf:type airo:AIOperator ;
    rdfs:label "New York City Government"@en ;
    rdfs:label "纽约市政府"@zh .

ex:LocalBusinesses rdf:type airo:Stakeholder ;
    rdfs:label "Local Businesses"@en ;
    rdfs:label "当地企业"@zh .

ex:BusinessRegulationGuidance rdf:type airo:Purpose ;
    rdfs:label "Business Regulation Guidance Purpose"@en ;
    rdfs:label "商业法规指导目的"@zh .

ex:GovernmentServices rdf:type airo:Domain ;
    rdfs:label "Government Services Domain"@en ;
    rdfs:label "政府服务领域"@zh .

ex:IncorrectLegalAdvice rdf:type airo:RiskSource ;
    rdfs:label "Incorrect Legal Advice Risk Source"@en ;
    rdfs:label "错误法律建议风险源"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:LocalBusinesses .