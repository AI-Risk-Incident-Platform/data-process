@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UKPassportPhotoChecker rdf:type airo:AISystem ;
    rdfs:label "UK Passport Photo Checker"@en ;
    rdfs:label "英国护照照片检查器"@zh ;
    airo:hasRisk ex:RacialBiasInPhotoRecognition ;
    airo:hasAISubject ex:DarkSkinnedWomen ;
    airo:hasStakeholder ex:UKGovernment ;
    airo:isAppliedWithinDomain ex:GovernmentServices .

ex:RacialBiasInPhotoRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Photo Recognition Risk"@en ;
    rdfs:label "照片识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminationAgainstDarkSkinnedWomen ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:DiscriminationAgainstDarkSkinnedWomen rdf:type airo:Consequence ;
    rdfs:label "Discrimination Against Dark-Skinned Women Consequence"@en ;
    rdfs:label "对深肤色女性的歧视后果"@zh ;
    airo:hasImpact ex:SocialInequality .

ex:DarkSkinnedWomen rdf:type airo:AISubject ;
    rdfs:label "Dark-Skinned Women"@en ;
    rdfs:label "深肤色女性"@zh .

ex:UKGovernment rdf:type airo:Stakeholder ;
    rdfs:label "UK Government"@en ;
    rdfs:label "英国政府"@zh .

ex:SocialInequality rdf:type airo:Impact ;
    rdfs:label "Social Inequality Impact"@en ;
    rdfs:label "社会不平等影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKGovernment .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:GovernmentServices rdf:type airo:Domain ;
    rdfs:label "Government Services Domain"@en ;
    rdfs:label "政府服务领域"@zh .