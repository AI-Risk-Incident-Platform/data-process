@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForBiologicalWeapons rdf:type airo:AISystem ;
    rdfs:label "AI for Biological Weapons"@en ;
    rdfs:label "用于生物武器的AI"@zh ;
    airo:hasRisk ex:BiologicalWeaponsRisk ;
    airo:hasStakeholder ex:EricSchmidt .

ex:BiologicalWeaponsRisk rdf:type airo:Risk ;
    rdfs:label "Biological Weapons Risk"@en ;
    rdfs:label "生物武器风险"@zh ;
    airo:isRiskSourceFor ex:MisuseOfAI ;
    airo:hasImpact ex:GlobalSecurityThreat .

ex:EricSchmidt rdf:type airo:Stakeholder ;
    rdfs:label "Eric Schmidt"@en ;
    rdfs:label "埃里克·施密特"@zh .

ex:MisuseOfAI rdf:type airo:RiskSource ;
    rdfs:label "Misuse of AI"@en ;
    rdfs:label "AI滥用"@zh .

ex:GlobalSecurityThreat rdf:type airo:Impact ;
    rdfs:label "Global Security Threat"@en ;
    rdfs:label "全球安全威胁"@zh ;
    airo:hasImpactOnStakeholder ex:EricSchmidt .