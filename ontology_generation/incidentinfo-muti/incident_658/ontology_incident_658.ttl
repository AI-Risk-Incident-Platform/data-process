@base <https://w3id.org/airo#> .

ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video System"@en ;
    rdfs:label "深度伪造视频系统"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasConsequence ex:PublicConfusion .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion Consequence"@en ;
    rdfs:label "公众混淆后果"@zh ;
    airo:hasImpact ex:ElectionIntegrityThreat .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:ElectionIntegrityThreat rdf:type airo:Impact ;
    rdfs:label "Election Integrity Threat Impact"@en ;
    rdfs:label "选举诚信威胁影响"@zh .