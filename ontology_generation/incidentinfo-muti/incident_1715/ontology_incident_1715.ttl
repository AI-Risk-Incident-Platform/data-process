@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutomatorsAISystem rdf:type airo:AISystem ;
    rdfs:label "Automators AI系统"@zh ;
    rdfs:label "Automators AI System"@en ;
    airo:hasRisk ex:DeceptiveMarketingRisk ;
    airo:hasStakeholder ex:FTC ;
    airo:isDeployedBy ex:AutomatorsLLC .

ex:DeceptiveMarketingRisk rdf:type airo:Risk ;
    rdfs:label "欺骗性营销风险"@zh ;
    rdfs:label "Deceptive Marketing Risk"@en ;
    airo:hasConsequence ex:ConsumerFinancialLoss .

ex:FTC rdf:type airo:Stakeholder ;
    rdfs:label "联邦贸易委员会"@zh ;
    rdfs:label "Federal Trade Commission"@en .

ex:AutomatorsLLC rdf:type airo:AIOperator ;
    rdfs:label "Automators有限责任公司"@zh ;
    rdfs:label "Automators LLC"@en .

ex:ConsumerFinancialLoss rdf:type airo:Consequence ;
    rdfs:label "消费者财务损失后果"@zh ;
    rdfs:label "Consumer Financial Loss Consequence"@en ;
    airo:hasImpact ex:RegulatoryScrutinyImpact .

ex:RegulatoryScrutinyImpact rdf:type airo:Impact ;
    rdfs:label "监管审查影响"@zh ;
    rdfs:label "Regulatory Scrutiny Impact"@en ;
    airo:hasImpactOnStakeholder ex:FTC .