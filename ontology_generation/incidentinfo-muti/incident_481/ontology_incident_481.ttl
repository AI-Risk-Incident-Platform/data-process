@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CBPFacialRecognitionApp rdf:type airo:AISystem ;
    rdfs:label "CBP Facial Recognition App"@en ;
    rdfs:label "CBP面部识别应用"@zh ;
    airo:hasRisk ex:RacialBiasInFacialRecognition ;
    airo:hasCapability ex:FacialRecognitionForImmigration ;
    airo:hasStakeholder ex:BlackAsylumApplicants .

ex:RacialBiasInFacialRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Facial Recognition Risk"@en ;
    rdfs:label "面部识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:BarredFromSeekingAsylum ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:BarredFromSeekingAsylum rdf:type airo:Consequence ;
    rdfs:label "Barred from Seeking Asylum Consequence"@en ;
    rdfs:label "被禁止寻求庇护的后果"@zh ;
    airo:hasImpact ex:DiscriminationInImmigrationProcess .

ex:FacialRecognitionForImmigration rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition for Immigration Capability"@en ;
    rdfs:label "用于移民的面部识别能力"@zh .

ex:BlackAsylumApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Black Asylum Applicants"@en ;
    rdfs:label "黑人庇护申请者"@zh ;
    airo:hasImpactOnStakeholder ex:DiscriminationInImmigrationProcess .

ex:DiscriminationInImmigrationProcess rdf:type airo:Impact ;
    rdfs:label "Discrimination in Immigration Process Impact"@en ;
    rdfs:label "移民过程中的歧视影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .