@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookContentModerationAI rdf:type airo:AISystem ;
    rdfs:label "Facebook Content Moderation AI"@en ;
    rdfs:label "Facebook内容审核AI"@zh ;
    airo:hasRisk ex:DiscriminatoryContentModeration ;
    airo:hasAISubject ex:AmericanBlackActivists ;
    airo:hasStakeholder ex:PalestinianActivists .

ex:DiscriminatoryContentModeration rdf:type airo:Risk ;
    rdfs:label "Discriminatory Content Moderation Risk"@en ;
    rdfs:label "歧视性内容审核风险"@zh ;
    airo:hasConsequence ex:SuppressionOfActivistVoices ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:PalestinianActivists rdf:type airo:Stakeholder ;
    rdfs:label "Palestinian Activists"@en ;
    rdfs:label "巴勒斯坦活动家"@zh .

ex:AmericanBlackActivists rdf:type airo:AISubject ;
    rdfs:label "American Black Activists"@en ;
    rdfs:label "美国黑人活动家"@zh .

ex:SuppressionOfActivistVoices rdf:type airo:Impact ;
    rdfs:label "Suppression of Activist Voices Impact"@en ;
    rdfs:label "压制活动家声音的影响"@zh ;
    airo:hasImpactOnStakeholder ex:PalestinianActivists .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .