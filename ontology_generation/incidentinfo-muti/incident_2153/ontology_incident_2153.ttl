@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AISystemForReducingSchoolDropouts rdf:type airo:AISystem ;
    rdfs:label "AI System for Reducing School Dropouts"@en ;
    rdfs:label "减少辍学的AI系统"@zh ;
    airo:hasRisk ex:PotentialRiskOfAIInEducation ;
    airo:hasPurpose ex:ReduceSchoolDropoutRates ;
    airo:isAppliedWithinDomain ex:EducationSector .

ex:PotentialRiskOfAIInEducation rdf:type airo:Risk ;
    rdfs:label "Potential Risk of AI in Education"@en ;
    rdfs:label "教育中AI的潜在风险"@zh .

ex:ReduceSchoolDropoutRates rdf:type airo:Purpose ;
    rdfs:label "Reduce School Dropout Rates"@en ;
    rdfs:label "降低辍学率"@zh .

ex:EducationSector rdf:type airo:Domain ;
    rdfs:label "Education Sector"@en ;
    rdfs:label "教育领域"@zh .