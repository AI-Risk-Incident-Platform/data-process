@base <https://w3id.org/airo#> .

ex:NYPDSubwaySurveillanceRobot rdf:type airo:AISystem ;
    rdfs:label "NYPD Subway Surveillance Robot"@en ;
    rdfs:label "纽约警察局地铁监控机器人"@zh ;
    airo:hasRisk ex:PublicSurveillanceRisk ;
    airo:isDeployedBy ex:NewYorkPoliceDepartment ;
    airo:hasPurpose ex:SubwayStationPatrol ;
    airo:isAppliedWithinDomain ex:PublicSafety .

ex:PublicSurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Public Surveillance Risk"@en ;
    rdfs:label "公共监控风险"@zh .

ex:PublicSafety rdf:type airo:Domain ;
    rdfs:label "Public Safety Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:NewYorkPoliceDepartment rdf:type airo:AIOperator ;
    rdfs:label "New York Police Department"@en ;
    rdfs:label "纽约警察局"@zh .

ex:SubwayStationPatrol rdf:type airo:Purpose ;
    rdfs:label "Subway Station Patrol Purpose"@en ;
    rdfs:label "地铁站巡逻目的"@zh .