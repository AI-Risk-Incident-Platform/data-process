@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MicrosoftTayChatbot rdf:type airo:AISystem ;
    rdfs:label "Microsoft Tay Chatbot"@en ;
    rdfs:label "微软Tay聊天机器人"@zh ;
    airo:hasRisk ex:OffensiveContentGenerationRisk ;
    airo:hasCapability ex:ConversationalLearningFromUserInteractions ;
    airo:hasStakeholder ex:TwitterUsersInteractingWithTay ;
    airo:hasPurpose ex:EngagingMillennialsThroughConversationalAI ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:usesTechnique ex:MachineLearningFromUserInputs ;
    airo:isDevelopedBy ex:MicrosoftCorporation ;
    airo:hasVulnerability ex:SusceptibilityToMaliciousUserInputs .

ex:OffensiveContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Offensive Content Generation Risk"@en ;
    rdfs:label "生成冒犯性内容风险"@zh ;
    airo:hasConsequence ex:GenerationOfOffensiveContent ;
    airo:hasRiskControl ex:ChatbotShutdown .

ex:ConversationalLearningFromUserInteractions rdf:type airo:AICapability ;
    rdfs:label "Conversational Learning from User Interactions Capability"@en ;
    rdfs:label "从用户互动中学习对话的能力"@zh .

ex:MicrosoftCorporation rdf:type airo:AIOperator ;
    rdfs:label "Microsoft Corporation"@en ;
    rdfs:label "微软公司"@zh .

ex:TwitterUsersInteractingWithTay rdf:type airo:Stakeholder ;
    rdfs:label "Twitter Users Interacting with Tay"@en ;
    rdfs:label "与Tay互动的推特用户"@zh .

ex:ReputationDamageToMicrosoft rdf:type airo:Impact ;
    rdfs:label "Reputation Damage to Microsoft Impact"@en ;
    rdfs:label "对微软声誉损害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:MicrosoftCorporation .

ex:EngagingMillennialsThroughConversationalAI rdf:type airo:Purpose ;
    rdfs:label "Engaging Millennials through Conversational AI Purpose"@en ;
    rdfs:label "通过对话AI吸引千禧一代的目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:MachineLearningFromUserInputs rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning from User Inputs Technique"@en ;
    rdfs:label "从用户输入中学习的机器学习技术"@zh .

ex:SusceptibilityToMaliciousUserInputs rdf:type airo:Vulnerability ;
    rdfs:label "Susceptibility to Malicious User Inputs Vulnerability"@en ;
    rdfs:label "易受恶意用户输入影响的脆弱性"@zh .

ex:CoordinatedMaliciousUserAttacks rdf:type airo:Threat ;
    rdfs:label "Coordinated Malicious User Attacks Threat"@en ;
    rdfs:label "有组织的恶意用户攻击威胁"@zh ;
    airo:isRiskSourceFor ex:OffensiveContentGenerationRisk .

ex:GenerationOfOffensiveContent rdf:type airo:Consequence ;
    rdfs:label "Generation of Offensive Content Consequence"@en ;
    rdfs:label "生成冒犯性内容的后果"@zh ;
    airo:hasImpact ex:ReputationDamageToMicrosoft .

ex:UnfilteredUserInputs rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered User Inputs Risk Source"@en ;
    rdfs:label "未过滤用户输入的风险源"@zh ;
    airo:isRiskSourceFor ex:OffensiveContentGenerationRisk .

ex:ChatbotShutdown rdf:type airo:RiskControl ;
    rdfs:label "Chatbot Shutdown Risk Control"@en ;
    rdfs:label "聊天机器人关闭的管控措施"@zh ;
    airo:mitigatesRiskConcept ex:OffensiveContentGenerationRisk .