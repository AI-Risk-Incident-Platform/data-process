ex:AIVoiceCloningScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI Voice Cloning Scam System"@en ;
    rdfs:label "AI语音克隆诈骗系统"@zh ;
    airo:hasRisk ex:FakeKidnappingScamRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasStakeholder ex:TelecomCompanies .

ex:FakeKidnappingScamRisk rdf:type airo:Risk ;
    rdfs:label "Fake Kidnapping Scam Risk"@en ;
    rdfs:label "虚假绑架诈骗风险"@zh ;
    airo:hasConsequence ex:PsychologicalDistressToVictims .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:TelecomCompanies rdf:type airo:Stakeholder ;
    rdfs:label "Telecom Companies"@en ;
    rdfs:label "电信公司"@zh .

ex:PsychologicalDistressToVictims rdf:type airo:Consequence ;
    rdfs:label "Psychological Distress to Victims Consequence"@en ;
    rdfs:label "受害者心理困扰后果"@zh ;
    airo:hasImpact ex:PublicTrustErosionInCommunicationSystems .

ex:PublicTrustErosionInCommunicationSystems rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion in Communication Systems Impact"@en ;
    rdfs:label "公众对通信系统信任度下降的影响"@zh ;
    airo:hasImpactOnStakeholder ex:TelecomCompanies .