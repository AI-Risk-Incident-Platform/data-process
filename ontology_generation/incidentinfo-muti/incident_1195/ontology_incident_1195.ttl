@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LucieAI rdf:type airo:AISystem ;
    rdfs:label "Lucie AI"@en ;
    rdfs:label "Lucie人工智能"@zh ;
    airo:hasRisk ex:IncorrectAnswerRisk ;
    airo:hasCapability ex:QuestionAnswering .

ex:IncorrectAnswerRisk rdf:type airo:Risk ;
    rdfs:label "Incorrect Answer Risk"@en ;
    rdfs:label "错误回答风险"@zh ;
    airo:hasConsequence ex:SystemSuspension .

ex:SystemSuspension rdf:type airo:Consequence ;
    rdfs:label "System Suspension Consequence"@en ;
    rdfs:label "系统暂停后果"@zh ;
    airo:hasImpact ex:PublicEmbarrassment .

ex:QuestionAnswering rdf:type airo:AICapability ;
    rdfs:label "Question Answering Capability"@en ;
    rdfs:label "问答能力"@zh .

ex:PublicEmbarrassment rdf:type airo:Impact ;
    rdfs:label "Public Embarrassment Impact"@en ;
    rdfs:label "公众尴尬影响"@zh .