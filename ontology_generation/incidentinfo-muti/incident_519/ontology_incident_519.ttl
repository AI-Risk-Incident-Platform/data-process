@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AdDeliveryAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Ad Delivery Algorithm"@en ;
    rdfs:label "广告投放算法"@zh ;
    airo:hasRisk ex:PoliticalBiasRisk ;
    airo:isAppliedWithinDomain ex:PoliticalAdvertising ;
    airo:hasPurpose ex:TargetedPoliticalMessaging .

ex:PoliticalBiasRisk rdf:type airo:Risk ;
    rdfs:label "Political Bias Risk"@en ;
    rdfs:label "政治偏见风险"@zh ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Consequence of Political Bias"@en ;
        rdfs:label "政治偏见后果"@zh ;
        airo:hasImpact ex:PoliticalOpinionManipulation
    ] ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInAdTargeting .

ex:PoliticalAdvertising rdf:type airo:Domain ;
    rdfs:label "Political Advertising Domain"@en ;
    rdfs:label "政治广告领域"@zh .

ex:TargetedPoliticalMessaging rdf:type airo:Purpose ;
    rdfs:label "Targeted Political Messaging Purpose"@en ;
    rdfs:label "定向政治信息投放目的"@zh .

ex:PoliticalOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "Political Opinion Manipulation Impact"@en ;
    rdfs:label "政治观点操纵影响"@zh .

ex:AlgorithmicBiasInAdTargeting rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Ad Targeting Risk Source"@en ;
    rdfs:label "广告定向中的算法偏见风险源"@zh .