@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:DataPrivacyViolationRisk ;
    airo:hasStakeholder ex:OpenAI ;
    airo:isProvidedBy ex:OpenAI .

ex:DataPrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Data Privacy Violation Risk"@en ;
    rdfs:label "数据隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:UnauthorizedDataCollection ;
    airo:hasConsequence ex:LegalActionAgainstAIProvider .

ex:OpenAI rdf:type airo:AIProvider ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:UnauthorizedDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Data Collection"@en ;
    rdfs:label "未经授权的数据收集"@zh .

ex:LegalActionAgainstAIProvider rdf:type airo:Consequence ;
    rdfs:label "Legal Action Against AI Provider"@en ;
    rdfs:label "对AI提供者的法律诉讼"@zh .