@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WeChatTranslationSystem rdf:type airo:AISystem ;
    rdfs:label "WeChat Translation System"@en ;
    rdfs:label "微信翻译系统"@zh ;
    airo:hasRisk ex:RacialDiscriminationRisk ;
    airo:isDeployedBy ex:Tencent ;
    airo:hasStakeholder ex:BlackCommunity .

ex:RacialDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Racial Discrimination Risk"@en ;
    rdfs:label "种族歧视风险"@zh ;
    airo:hasConsequence ex:OffensiveTranslationOutput ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:OffensiveTranslationOutput rdf:type airo:Consequence ;
    rdfs:label "Offensive Translation Output"@en ;
    rdfs:label "冒犯性翻译输出"@zh ;
    airo:hasImpact ex:HarmToBlackCommunity .

ex:HarmToBlackCommunity rdf:type airo:Impact ;
    rdfs:label "Harm to Black Community"@en ;
    rdfs:label "对黑人社区的伤害"@zh ;
    airo:hasImpactOnStakeholder ex:BlackCommunity .

ex:Tencent rdf:type airo:AIOperator ;
    rdfs:label "Tencent"@en ;
    rdfs:label "腾讯"@zh .

ex:BlackCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Black Community"@en ;
    rdfs:label "黑人社区"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .