@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AnonymousChessPlayerIdentificationSystem rdf:type airo:AISystem ;
    rdfs:label "Anonymous Chess Player Identification System"@en ;
    rdfs:label "匿名棋手识别系统"@zh ;
    airo:hasRisk ex:PrivacyRisk ;
    airo:hasAICapability ex:PlayerIdentification ;
    airo:hasAISubject ex:AnonymousChessPlayers .

ex:PrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Risk"@en ;
    rdfs:label "隐私风险"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:PlayerIdentification rdf:type airo:AICapability ;
    rdfs:label "Player Identification Capability"@en ;
    rdfs:label "玩家识别能力"@zh .

ex:AnonymousChessPlayers rdf:type airo:AISubject ;
    rdfs:label "Anonymous Chess Players"@en ;
    rdfs:label "匿名棋手"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:AnonymousChessPlayers .