@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TikTokRecommendationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "TikTok Recommendation Algorithm"@en ;
    rdfs:label "TikTok推荐算法"@zh ;
    airo:hasRisk ex:MisogynisticContentPropagationRisk ;
    airo:hasStakeholder ex:YoungMaleUsers .

ex:MisogynisticContentPropagationRisk rdf:type airo:Risk ;
    rdfs:label "Misogynistic Content Propagation Risk"@en ;
    rdfs:label "厌女内容传播风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmicAmplificationOfExtremeContent ;
    airo:hasImpact ex:GenderStereotypeReinforcement .

ex:YoungMaleUsers rdf:type airo:Stakeholder ;
    rdfs:label "Young Male Users"@en ;
    rdfs:label "年轻男性用户"@zh ;
    airo:hasImpactOnStakeholder ex:GenderStereotypeReinforcement .

ex:GenderStereotypeReinforcement rdf:type airo:Impact ;
    rdfs:label "Gender Stereotype Reinforcement Impact"@en ;
    rdfs:label "性别刻板印象强化影响"@zh .

ex:AlgorithmicAmplificationOfExtremeContent rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Amplification of Extreme Content Risk Source"@en ;
    rdfs:label "极端内容的算法放大风险源"@zh .