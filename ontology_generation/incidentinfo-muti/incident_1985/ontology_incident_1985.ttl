@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DatasetStewardshipSystem rdf:type airo:AISystem ;
    rdfs:label "Dataset Stewardship System"@en ;
    rdfs:label "数据集管理系统"@zh ;
    airo:hasRisk ex:DatasetHarmRisk .

ex:DatasetHarmRisk rdf:type airo:Risk ;
    rdfs:label "Dataset Harm Risk"@en ;
    rdfs:label "数据集危害风险"@zh ;
    airo:hasConsequence ex:AlgorithmicBias ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .

ex:AlgorithmicBias rdf:type airo:Consequence ;
    rdfs:label "Algorithmic Bias"@en ;
    rdfs:label "算法偏见"@zh ;
    airo:hasImpact ex:DiscriminatoryOutcomes .

ex:DiscriminatoryOutcomes rdf:type airo:Impact ;
    rdfs:label "Discriminatory Outcomes"@en ;
    rdfs:label "歧视性结果"@zh .