@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RisCanvi rdf:type airo:AISystem ;
    rdfs:label "RisCanvi风险评估工具"@zh ;
    rdfs:label "RisCanvi Risk Assessment Tool"@en ;
    airo:hasRisk ex:UnreliableRiskAssessment ;
    airo:hasCapability ex:PrisonerRiskEvaluation ;
    airo:hasStakeholder ex:CatalanPrisonAdministration ;
    airo:hasAISubject ex:CatalanPrisoners ;
    airo:isAppliedWithinDomain ex:CriminalJusticeSystem .

ex:UnreliableRiskAssessment rdf:type airo:Risk ;
    rdfs:label "不可靠风险评估风险"@zh ;
    rdfs:label "Unreliable Risk Assessment Risk"@en ;
    airo:hasConsequence ex:InaccurateRiskClassification ;
    airo:isRiskSourceFor ex:AlgorithmicDeficiencies .

ex:CriminalJusticeSystem rdf:type airo:Domain ;
    rdfs:label "刑事司法系统领域"@zh ;
    rdfs:label "Criminal Justice System Domain"@en .

ex:PrisonerRiskEvaluation rdf:type airo:AICapability ;
    rdfs:label "囚犯风险评估能力"@zh ;
    rdfs:label "Prisoner Risk Evaluation Capability"@en .

ex:InaccurateRiskClassification rdf:type airo:Consequence ;
    rdfs:label "不准确风险分类后果"@zh ;
    rdfs:label "Inaccurate Risk Classification Consequence"@en ;
    airo:hasImpact ex:PotentialPublicSafetyThreat .

ex:CatalanPrisonAdministration rdf:type airo:Stakeholder ;
    rdfs:label "加泰罗尼亚监狱管理部门"@zh ;
    rdfs:label "Catalan Prison Administration Stakeholder"@en .

ex:CatalanPrisoners rdf:type airo:AISubject ;
    rdfs:label "加泰罗尼亚囚犯"@zh ;
    rdfs:label "Catalan Prisoners Subject"@en .

ex:AlgorithmicDeficiencies rdf:type airo:RiskSource ;
    rdfs:label "算法缺陷风险源"@zh ;
    rdfs:label "Algorithmic Deficiencies Risk Source"@en .

ex:PotentialPublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "潜在公共安全威胁影响"@zh ;
    rdfs:label "Potential Public Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:CatalanPrisonAdministration .