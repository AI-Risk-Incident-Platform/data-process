@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeCandidateCampaignSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Candidate Campaign System"@en ;
    rdfs:label "深度伪造候选人竞选系统"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:isAppliedWithinDomain ex:PoliticalCampaigns ;
    airo:hasStakeholder ex:Voters .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicDeception .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:Voters rdf:type airo:Stakeholder ;
    rdfs:label "Voters"@en ;
    rdfs:label "选民"@zh .

ex:PoliticalCampaigns rdf:type airo:Domain ;
    rdfs:label "Political Campaigns Domain"@en ;
    rdfs:label "政治竞选领域"@zh .

ex:PublicDeception rdf:type airo:Consequence ;
    rdfs:label "Public Deception Consequence"@en ;
    rdfs:label "公众欺骗后果"@zh ;
    airo:hasImpact ex:ElectionIntegrityThreat .

ex:ElectionIntegrityThreat rdf:type airo:Impact ;
    rdfs:label "Election Integrity Threat Impact"@en ;
    rdfs:label "选举诚信威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:Voters .