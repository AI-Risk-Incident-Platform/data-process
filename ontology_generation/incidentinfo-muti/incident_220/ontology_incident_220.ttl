@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ContentModerationAI rdf:type airo:AISystem ;
    rdfs:label "Content Moderation AI"@en ;
    rdfs:label "内容审核AI"@zh ;
    airo:hasRisk ex:PsychologicalHarmRisk ;
    airo:hasPurpose ex:OnlineContentModeration ;
    airo:hasStakeholder ex:ContentModerators .

ex:PsychologicalHarmRisk rdf:type airo:Risk ;
    rdfs:label "Psychological Harm Risk"@en ;
    rdfs:label "心理伤害风险"@zh ;
    airo:hasConsequence ex:MentalHealthImpact .

ex:ContentModerators rdf:type airo:Stakeholder ;
    rdfs:label "Content Moderators"@en ;
    rdfs:label "内容审核员"@zh .

ex:MentalHealthImpact rdf:type airo:Impact ;
    rdfs:label "Mental Health Impact"@en ;
    rdfs:label "心理健康影响"@zh ;
    airo:hasImpactOnStakeholder ex:ContentModerators .

ex:OnlineContentModeration rdf:type airo:Purpose ;
    rdfs:label "Online Content Moderation"@en ;
    rdfs:label "在线内容审核"@zh .