@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAI rdf:type airo:AISystem ;
    rdfs:label "生成式AI"@zh ;
    rdfs:label "Generative AI"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:Authors ;
    airo:isDeployedBy ex:Meta .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:isRiskSourceFor ex:PiratedBooks ;
    airo:hasConsequence ex:LegalAction ;
    airo:hasRiskSource ex:PiratedBooks ;
    airo:hasMisuse ex:UnauthorizedUseOfCopyrightedMaterial .

ex:Authors rdf:type airo:Stakeholder ;
    rdfs:label "作者"@zh ;
    rdfs:label "Authors"@en .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@zh ;
    rdfs:label "Meta"@en .

ex:PiratedBooks rdf:type airo:RiskSource ;
    rdfs:label "盗版书籍"@zh ;
    rdfs:label "Pirated Books"@en .

ex:LegalAction rdf:type airo:Consequence ;
    rdfs:label "法律诉讼"@zh ;
    rdfs:label "Legal Action"@en .

ex:UnauthorizedUseOfCopyrightedMaterial rdf:type airo:Misuse ;
    rdfs:label "未经授权使用受版权保护材料"@zh ;
    rdfs:label "Unauthorized Use of Copyrighted Material"@en .