@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonRoboticFulfillmentCenter rdf:type airo:AISystem ;
    rdfs:label "Amazon Robotic Fulfillment Center"@en ;
    rdfs:label "亚马逊机器人配送中心"@zh ;
    airo:hasRisk ex:WorkplaceInjuryRisk ;
    airo:isDeployedBy ex:Amazon ;
    airo:hasStakeholder ex:WarehouseWorkers ;
    airo:isAppliedWithinDomain ex:WarehouseAutomation .

ex:WorkplaceInjuryRisk rdf:type airo:Risk ;
    rdfs:label "Workplace Injury Risk"@en ;
    rdfs:label "工作场所伤害风险"@zh ;
    airo:hasConsequence ex:HigherSeriousInjuryRates .

ex:HigherSeriousInjuryRates rdf:type airo:Consequence ;
    rdfs:label "Higher Serious Injury Rates"@en ;
    rdfs:label "更高的严重伤害率"@zh ;
    airo:hasImpact ex:WorkerSafetyConcerns .

ex:WarehouseWorkers rdf:type airo:Stakeholder ;
    rdfs:label "Warehouse Workers"@en ;
    rdfs:label "仓库工人"@zh .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:WarehouseAutomation rdf:type airo:Domain ;
    rdfs:label "Warehouse Automation"@en ;
    rdfs:label "仓库自动化"@zh .

ex:WorkerSafetyConcerns rdf:type airo:Impact ;
    rdfs:label "Worker Safety Concerns"@en ;
    rdfs:label "工人安全问题"@zh ;
    airo:hasImpactOnStakeholder ex:WarehouseWorkers .