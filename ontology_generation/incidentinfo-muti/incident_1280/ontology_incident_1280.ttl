@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LinkedInAISystem rdf:type airo:AISystem ;
    rdfs:label "LinkedIn AI System"@en ;
    rdfs:label "LinkedIn人工智能系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:Microsoft ;
    airo:hasStakeholder ex:LinkedInUsers ;
    airo:hasPurpose ex:AIModelTraining ;
    airo:isAppliedWithinDomain ex:ProfessionalSocialNetworking .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:DataScrapingWithoutConsent ;
    airo:hasConsequence ex:UnauthorizedDataUsage .

ex:LinkedInUsers rdf:type airo:Stakeholder ;
    rdfs:label "LinkedIn Users"@en ;
    rdfs:label "LinkedIn用户"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:DataScrapingWithoutConsent rdf:type airo:RiskSource ;
    rdfs:label "Data Scraping Without Consent"@en ;
    rdfs:label "未经同意的数据抓取"@zh .

ex:UnauthorizedDataUsage rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Data Usage"@en ;
    rdfs:label "未经授权的数据使用"@zh ;
    airo:hasImpact ex:UserTrustErosion .

ex:UserTrustErosion rdf:type airo:Impact ;
    rdfs:label "User Trust Erosion"@en ;
    rdfs:label "用户信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:LinkedInUsers .

ex:AIModelTraining rdf:type airo:Purpose ;
    rdfs:label "AI Model Training"@en ;
    rdfs:label "AI模型训练"@zh .

ex:ProfessionalSocialNetworking rdf:type airo:Domain ;
    rdfs:label "Professional Social Networking"@en ;
    rdfs:label "职业社交网络"@zh .