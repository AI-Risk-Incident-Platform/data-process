@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:JournalRankingSystem rdf:type airo:AISystem ;
    rdfs:label "Journal Ranking System"@en ;
    rdfs:label "期刊排名系统"@zh ;
    airo:hasRisk ex:FakeJournalInclusionRisk ;
    airo:isAppliedWithinDomain ex:AcademicPublishing .

ex:FakeJournalInclusionRisk rdf:type airo:Risk ;
    rdfs:label "Fake Journal Inclusion Risk"@en ;
    rdfs:label "虚假期刊收录风险"@zh ;
    airo:hasConsequence ex:AcademicReputationDamage ;
    airo:isRiskSourceFor ex:InadequateVerificationMechanism .

ex:AcademicReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Academic Reputation Damage Consequence"@en ;
    rdfs:label "学术声誉损害后果"@zh .

ex:AcademicPublishing rdf:type airo:Domain ;
    rdfs:label "Academic Publishing Domain"@en ;
    rdfs:label "学术出版领域"@zh .

ex:InadequateVerificationMechanism rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Verification Mechanism Risk Source"@en ;
    rdfs:label "验证机制不足风险源"@zh .