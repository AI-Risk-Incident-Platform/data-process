@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PublicFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Public Facial Recognition System"@en ;
    rdfs:label "公共面部识别系统"@zh ;
    airo:hasRisk ex:WrongfulArrestRisk ;
    airo:hasAISubject ex:InnocentCivilians ;
    airo:hasStakeholder ex:ArgentinePolice ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:WrongfulArrestRisk rdf:type airo:Risk ;
    rdfs:label "Wrongful Arrest Risk"@en ;
    rdfs:label "错误逮捕风险"@zh ;
    airo:hasConsequence ex:InnocentPeopleArrested ;
    airo:isRiskSourceFor ex:FacialRecognitionError .

ex:InnocentPeopleArrested rdf:type airo:Consequence ;
    rdfs:label "Innocent People Arrested Consequence"@en ;
    rdfs:label "无辜者被捕后果"@zh ;
    airo:hasImpact ex:JusticeSystemErosion .

ex:InnocentCivilians rdf:type airo:AISubject ;
    rdfs:label "Innocent Civilians"@en ;
    rdfs:label "无辜平民"@zh .

ex:ArgentinePolice rdf:type airo:Stakeholder ;
    rdfs:label "Argentine Police"@en ;
    rdfs:label "阿根廷警方"@zh .

ex:JusticeSystemErosion rdf:type airo:Impact ;
    rdfs:label "Justice System Erosion Impact"@en ;
    rdfs:label "司法系统侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:ArgentinePolice .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:FacialRecognitionError rdf:type airo:RiskSource ;
    rdfs:label "Facial Recognition Error Risk Source"@en ;
    rdfs:label "面部识别错误风险源"@zh .