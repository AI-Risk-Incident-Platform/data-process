@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForImpersonation rdf:type airo:AISystem ;
    rdfs:label "AI for Impersonation"@en ;
    rdfs:label "用于冒充的AI系统"@zh ;
    airo:hasRisk ex:BusinessImpersonationRisk ;
    airo:hasCapability ex:IdentityImpersonation ;
    airo:hasStakeholder ex:SmallBusinesses .

ex:BusinessImpersonationRisk rdf:type airo:Risk ;
    rdfs:label "Business Impersonation Risk"@en ;
    rdfs:label "商业冒充风险"@zh ;
    airo:hasConsequence ex:FinancialFraud ;
    airo:isRiskSourceFor ex:MaliciousAIUse .

ex:SmallBusinesses rdf:type airo:Stakeholder ;
    rdfs:label "Small Businesses"@en ;
    rdfs:label "小型企业"@zh .

ex:IdentityImpersonation rdf:type airo:AICapability ;
    rdfs:label "Identity Impersonation Capability"@en ;
    rdfs:label "身份冒充能力"@zh .

ex:FinancialFraud rdf:type airo:Consequence ;
    rdfs:label "Financial Fraud Consequence"@en ;
    rdfs:label "金融欺诈后果"@zh ;
    airo:hasImpact ex:EconomicLossForBusinesses .

ex:EconomicLossForBusinesses rdf:type airo:Impact ;
    rdfs:label "Economic Loss for Businesses Impact"@en ;
    rdfs:label "企业经济损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:SmallBusinesses .

ex:MaliciousAIUse rdf:type airo:RiskSource ;
    rdfs:label "Malicious AI Use Risk Source"@en ;
    rdfs:label "恶意使用AI风险源"@zh .