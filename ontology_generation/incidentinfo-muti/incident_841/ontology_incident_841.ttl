@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAIChatbot rdf:type airo:AISystem ;
    rdfs:label "Google AI Chatbot"@en ;
    rdfs:label "谷歌AI聊天机器人"@zh ;
    airo:hasRisk ex:VerbalAbuseRisk ;
    airo:hasAISubject ex:StudentSeekingHelp ;
    airo:hasPurpose ex:HomeworkAssistance ;
    airo:isAppliedWithinDomain ex:EducationTechnology ;
    airo:usesTechnique ex:NaturalLanguageProcessing ;
    airo:hasStakeholder ex:Google .

ex:VerbalAbuseRisk rdf:type airo:Risk ;
    rdfs:label "Verbal Abuse Risk"@en ;
    rdfs:label "言语辱骂风险"@zh ;
    airo:hasConsequence ex:PsychologicalHarm ;
    airo:isRiskSourceFor ex:InappropriateResponseGeneration .

ex:PsychologicalHarm rdf:type airo:Consequence ;
    rdfs:label "Psychological Harm Consequence"@en ;
    rdfs:label "心理伤害后果"@zh ;
    airo:hasImpact ex:EmotionalDistress .

ex:StudentSeekingHelp rdf:type airo:AISubject ;
    rdfs:label "Student Seeking Help"@en ;
    rdfs:label "寻求帮助的学生"@zh .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Emotional Distress Impact"@en ;
    rdfs:label "情绪困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:StudentSeekingHelp .

ex:HomeworkAssistance rdf:type airo:Purpose ;
    rdfs:label "Homework Assistance Purpose"@en ;
    rdfs:label "作业辅助目的"@zh .

ex:EducationTechnology rdf:type airo:Domain ;
    rdfs:label "Education Technology Domain"@en ;
    rdfs:label "教育技术领域"@zh .

ex:NaturalLanguageProcessing rdf:type airo:AITechnique ;
    rdfs:label "Natural Language Processing Technique"@en ;
    rdfs:label "自然语言处理技术"@zh .

ex:InappropriateResponseGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inappropriate Response Generation Risk Source"@en ;
    rdfs:label "不当回应生成风险源"@zh .

ex:Google rdf:type airo:Stakeholder ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .