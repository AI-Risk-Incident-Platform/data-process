@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeRecommendationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "YouTube推荐算法"@zh ;
    rdfs:label "YouTube Recommendation Algorithm"@en ;
    airo:hasRisk ex:SelfHarmContentPromotionRisk ;
    airo:hasAISubject ex:YoungTeenagers ;
    airo:hasStakeholder ex:YouTubePlatform ;
    airo:hasCapability ex:ContentRecommendationSystem .

ex:SelfHarmContentPromotionRisk rdf:type airo:Risk ;
    rdfs:label "自残内容推广风险"@zh ;
    rdfs:label "Self-Harm Content Promotion Risk"@en ;
    airo:hasConsequence ex:ExposureToHarmfulContent ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInContentSelection .

ex:ExposureToHarmfulContent rdf:type airo:Consequence ;
    rdfs:label "接触有害内容后果"@zh ;
    rdfs:label "Exposure to Harmful Content Consequence"@en ;
    airo:hasImpact ex:MentalHealthDeterioration .

ex:YoungTeenagers rdf:type airo:AISubject ;
    rdfs:label "青少年"@zh ;
    rdfs:label "Young Teenagers"@en .

ex:YouTubePlatform rdf:type airo:Stakeholder ;
    rdfs:label "YouTube平台"@zh ;
    rdfs:label "YouTube Platform"@en .

ex:MentalHealthDeterioration rdf:type airo:Impact ;
    rdfs:label "心理健康恶化影响"@zh ;
    rdfs:label "Mental Health Deterioration Impact"@en ;
    airo:hasImpactOnStakeholder ex:YoungTeenagers .

ex:ContentRecommendationSystem rdf:type airo:AICapability ;
    rdfs:label "内容推荐系统能力"@zh ;
    rdfs:label "Content Recommendation System Capability"@en .

ex:AlgorithmicBiasInContentSelection rdf:type airo:RiskSource ;
    rdfs:label "内容选择中的算法偏见风险源"@zh ;
    rdfs:label "Algorithmic Bias in Content Selection Risk Source"@en .