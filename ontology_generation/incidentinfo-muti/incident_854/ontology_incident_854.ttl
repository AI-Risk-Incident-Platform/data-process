@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIElectionQuestionAnsweringSystem rdf:type airo:AISystem ;
    rdfs:label "AI Election Question Answering System"@en ;
    rdfs:label "AI选举问题回答系统"@zh ;
    airo:hasRisk ex:LanguageBiasInAIResponses ;
    airo:hasAICapability ex:MultilingualQuestionAnswering ;
    airo:isAppliedWithinDomain ex:PoliticalInformation .

ex:LanguageBiasInAIResponses rdf:type airo:Risk ;
    rdfs:label "Language Bias in AI Responses Risk"@en ;
    rdfs:label "AI回答中的语言偏见风险"@zh ;
    airo:hasImpact ex:MisinformationInNonEnglishLanguages .

ex:MultilingualQuestionAnswering rdf:type airo:AICapability ;
    rdfs:label "Multilingual Question Answering Capability"@en ;
    rdfs:label "多语言问题回答能力"@zh .

ex:PoliticalInformation rdf:type airo:Domain ;
    rdfs:label "Political Information Domain"@en ;
    rdfs:label "政治信息领域"@zh .

ex:MisinformationInNonEnglishLanguages rdf:type airo:Impact ;
    rdfs:label "Misinformation in Non-English Languages Impact"@en ;
    rdfs:label "非英语语言中的错误信息影响"@zh .