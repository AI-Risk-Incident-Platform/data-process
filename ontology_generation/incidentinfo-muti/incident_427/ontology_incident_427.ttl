@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CoupangSearchAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Coupang Search Algorithm"@en ;
    rdfs:label "Coupang搜索算法"@zh ;
    airo:hasRisk ex:AlgorithmManipulationRisk ;
    airo:hasStakeholder ex:FederalTradeCommission ;
    airo:isAppliedWithinDomain ex:ECommercePlatform .

ex:AlgorithmManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Algorithm Manipulation Risk"@en ;
    rdfs:label "算法操纵风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmTampering .

ex:FederalTradeCommission rdf:type airo:Stakeholder ;
    rdfs:label "Federal Trade Commission"@en ;
    rdfs:label "联邦贸易委员会"@zh .

ex:AlgorithmTampering rdf:type airo:RiskSource ;
    rdfs:label "Algorithm Tampering"@en ;
    rdfs:label "算法篡改"@zh .

ex:ECommercePlatform rdf:type airo:Domain ;
    rdfs:label "E-Commerce Platform"@en ;
    rdfs:label "电子商务平台"@zh .