@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NvidiaAI rdf:type airo:AISystem ;
    rdfs:label "Nvidia人工智能系统"@zh ;
    rdfs:label "Nvidia AI System"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDevelopedBy ex:NvidiaCorporation ;
    airo:hasStakeholder ex:Authors .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:hasConsequence ex:LegalAction ;
    airo:isRiskSourceFor ex:UseOfCopyrightedWorks .

ex:Authors rdf:type airo:Stakeholder ;
    rdfs:label "作者"@zh ;
    rdfs:label "Authors"@en .

ex:NvidiaCorporation rdf:type airo:AIOperator ;
    rdfs:label "Nvidia公司"@zh ;
    rdfs:label "Nvidia Corporation"@en .

ex:LegalAction rdf:type airo:Consequence ;
    rdfs:label "法律诉讼"@zh ;
    rdfs:label "Legal Action"@en ;
    airo:hasImpactOnStakeholder ex:Authors .

ex:UseOfCopyrightedWorks rdf:type airo:RiskSource ;
    rdfs:label "使用受版权保护作品"@zh ;
    rdfs:label "Use of Copyrighted Works"@en .