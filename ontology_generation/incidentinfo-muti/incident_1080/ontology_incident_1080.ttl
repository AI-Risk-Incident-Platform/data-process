@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EngineerAI rdf:type airo:AISystem ;
    rdfs:label "Engineer.ai"@en ;
    rdfs:label "Engineer.ai"@zh ;
    airo:hasRisk ex:MisrepresentationOfAICapabilities ;
    airo:isDevelopedBy ex:EngineerAICompany ;
    airo:hasPurpose ex:AutomatedAppDevelopment ;
    airo:isAppliedWithinDomain ex:SoftwareDevelopment ;
    airo:hasStakeholder ex:Investors .

ex:MisrepresentationOfAICapabilities rdf:type airo:Risk ;
    rdfs:label "Misrepresentation of AI Capabilities Risk"@en ;
    rdfs:label "AI能力虚假陈述风险"@zh ;
    airo:isRiskSourceFor ex:LackOfTransparencyInAIUsage ;
    airo:hasConsequence ex:InvestorDeception .

ex:LackOfTransparencyInAIUsage rdf:type airo:RiskSource ;
    rdfs:label "Lack of Transparency in AI Usage Risk Source"@en ;
    rdfs:label "AI使用缺乏透明度风险源"@zh .

ex:InvestorDeception rdf:type airo:Consequence ;
    rdfs:label "Investor Deception Consequence"@en ;
    rdfs:label "投资者欺骗后果"@zh ;
    airo:hasImpact ex:LossOfTrustInAIStartups .

ex:LossOfTrustInAIStartups rdf:type airo:Impact ;
    rdfs:label "Loss of Trust in AI Startups Impact"@en ;
    rdfs:label "AI初创企业信任丧失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Investors .

ex:Investors rdf:type airo:Stakeholder ;
    rdfs:label "Investors"@en ;
    rdfs:label "投资者"@zh .

ex:EngineerAICompany rdf:type airo:AIDeveloper ;
    rdfs:label "Engineer.ai Company"@en ;
    rdfs:label "Engineer.ai公司"@zh .

ex:AutomatedAppDevelopment rdf:type airo:Purpose ;
    rdfs:label "Automated App Development Purpose"@en ;
    rdfs:label "自动化应用开发目的"@zh .

ex:SoftwareDevelopment rdf:type airo:Domain ;
    rdfs:label "Software Development Domain"@en ;
    rdfs:label "软件开发领域"@zh .

ex:HumanLaborMisrepresentedAsAI rdf:type airo:Misuse ;
    rdfs:label "Human Labor Misrepresented as AI Misuse"@en ;
    rdfs:label "人工劳动冒充AI滥用"@zh .