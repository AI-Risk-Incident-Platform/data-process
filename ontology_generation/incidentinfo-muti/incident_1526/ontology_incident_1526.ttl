@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HaryanaFamilyIDSystem rdf:type airo:AISystem ;
    rdfs:label "Haryana Family ID System"@en ;
    rdfs:label "哈里亚纳邦家庭ID系统"@zh ;
    airo:hasRisk ex:FalseDeathDeclarationRisk ;
    airo:hasAISubject ex:PensionRecipients .

ex:FalseDeathDeclarationRisk rdf:type airo:Risk ;
    rdfs:label "False Death Declaration Risk"@en ;
    rdfs:label "错误死亡声明风险"@zh ;
    airo:hasConsequence ex:PensionHaltConsequence ;
    airo:isRiskSourceFor ex:AlgorithmError .

ex:PensionHaltConsequence rdf:type airo:Consequence ;
    rdfs:label "Pension Halt Consequence"@en ;
    rdfs:label "养老金停发后果"@zh ;
    airo:hasImpact ex:FinancialHardshipImpact .

ex:PensionRecipients rdf:type airo:AISubject ;
    rdfs:label "Pension Recipients"@en ;
    rdfs:label "养老金领取者"@zh .

ex:FinancialHardshipImpact rdf:type airo:Impact ;
    rdfs:label "Financial Hardship Impact"@en ;
    rdfs:label "经济困难影响"@zh ;
    airo:hasImpactOnStakeholder ex:PensionRecipients .

ex:AlgorithmError rdf:type airo:RiskSource ;
    rdfs:label "Algorithm Error"@en ;
    rdfs:label "算法错误"@zh .