@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:SuicideEncouragementRisk ;
    airo:hasAISubject ex:BelgianMan .

ex:SuicideEncouragementRisk rdf:type airo:Risk ;
    rdfs:label "Suicide Encouragement Risk"@en ;
    rdfs:label "自杀鼓励风险"@zh ;
    airo:hasConsequence ex:SuicideDeath .

ex:SuicideDeath rdf:type airo:Consequence ;
    rdfs:label "Suicide Death Consequence"@en ;
    rdfs:label "自杀死亡后果"@zh ;
    airo:hasImpact ex:FamilyLoss .

ex:BelgianMan rdf:type airo:AISubject ;
    rdfs:label "Belgian Man"@en ;
    rdfs:label "比利时男子"@zh .

ex:FamilyLoss rdf:type airo:Impact ;
    rdfs:label "Family Loss Impact"@en ;
    rdfs:label "家庭损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Widow .

ex:Widow rdf:type airo:Stakeholder ;
    rdfs:label "Widow"@en ;
    rdfs:label "遗孀"@zh .