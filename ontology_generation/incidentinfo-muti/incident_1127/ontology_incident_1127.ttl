@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberSelfDrivingCarProgram rdf:type airo:AISystem ;
    rdfs:label "Uber Self-Driving Car Program"@en ;
    rdfs:label "优步自动驾驶汽车项目"@zh ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:isDeployedBy ex:UberTechnologiesInc ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTesting ;
    airo:hasStakeholder ex:UberTechnologiesInc .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    rdfs:label "自动驾驶汽车碰撞风险"@zh ;
    airo:isRiskSourceFor ex:AutonomousSystemFailure .

ex:UberTechnologiesInc rdf:type airo:AIOperator ;
    rdfs:label "Uber Technologies Inc."@en ;
    rdfs:label "优步科技公司"@zh .

ex:AutonomousVehicleTesting rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Testing Domain"@en ;
    rdfs:label "自动驾驶汽车测试领域"@zh .

ex:AutonomousSystemFailure rdf:type airo:RiskSource ;
    rdfs:label "Autonomous System Failure Risk Source"@en ;
    rdfs:label "自动驾驶系统故障风险源"@zh .