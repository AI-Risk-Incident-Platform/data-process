@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造视频系统"@zh ;
    rdfs:label "Deepfake Video System"@en ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:hasPurpose ex:FraudulentPromotion ;
    airo:hasStakeholder ex:InvestmentScamVictims ;
    airo:isAppliedWithinDomain ex:FinancialInvestment .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "金融欺诈风险"@zh ;
    rdfs:label "Financial Fraud Risk"@en ;
    airo:hasConsequence ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "经济损失后果"@zh ;
    rdfs:label "Financial Loss Consequence"@en ;
    airo:hasImpact ex:InvestorTrustErosion .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "深度伪造生成技术"@zh ;
    rdfs:label "Deepfake Generation Technique"@en .

ex:InvestmentScamVictims rdf:type airo:Stakeholder ;
    rdfs:label "投资诈骗受害者"@zh ;
    rdfs:label "Investment Scam Victims"@en .

ex:FraudulentPromotion rdf:type airo:Purpose ;
    rdfs:label "欺诈性宣传目的"@zh ;
    rdfs:label "Fraudulent Promotion Purpose"@en .

ex:FinancialInvestment rdf:type airo:Domain ;
    rdfs:label "金融投资领域"@zh ;
    rdfs:label "Financial Investment Domain"@en .

ex:InvestorTrustErosion rdf:type airo:Impact ;
    rdfs:label "投资者信任侵蚀影响"@zh ;
    rdfs:label "Investor Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:InvestmentScamVictims .