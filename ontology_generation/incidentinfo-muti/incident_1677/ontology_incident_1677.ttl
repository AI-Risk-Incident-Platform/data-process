@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ProfessionalHeadshotAITool rdf:type airo:AISystem ;
    rdfs:label "Professional Headshot AI Tool"@en ;
    rdfs:label "专业头像AI工具"@zh ;
    airo:hasRisk ex:RacialBiasInAI ;
    airo:hasAISubject ex:AsianMITStudent ;
    airo:hasStakeholder ex:MinorityProfessionals .

ex:RacialBiasInAI rdf:type airo:Risk ;
    rdfs:label "Racial Bias in AI Risk"@en ;
    rdfs:label "AI中的种族偏见风险"@zh ;
    airo:hasConsequence ex:RacialMisrepresentation ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:RacialMisrepresentation rdf:type airo:Consequence ;
    rdfs:label "Racial Misrepresentation Consequence"@en ;
    rdfs:label "种族错误表征后果"@zh ;
    airo:hasImpact ex:CulturalIdentityErasure .

ex:AsianMITStudent rdf:type airo:AISubject ;
    rdfs:label "Asian MIT Student"@en ;
    rdfs:label "MIT亚裔学生"@zh .

ex:CulturalIdentityErasure rdf:type airo:Impact ;
    rdfs:label "Cultural Identity Erasure Impact"@en ;
    rdfs:label "文化身份抹除影响"@zh ;
    airo:hasImpactOnStakeholder ex:MinorityProfessionals .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "偏见训练数据风险源"@zh .

ex:MinorityProfessionals rdf:type airo:Stakeholder ;
    rdfs:label "Minority Professionals"@en ;
    rdfs:label "少数族裔专业人士"@zh .