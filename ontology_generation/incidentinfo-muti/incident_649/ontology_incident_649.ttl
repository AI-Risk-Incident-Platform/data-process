@base <https://w3id.org/airo#> .

ex:DeepfakeAudioDetectionSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造音频检测系统"@zh ;
    rdfs:label "Deepfake Audio Detection System"@en ;
    airo:hasRisk ex:DeepfakeMisinformationRisk ;
    airo:usesTechnique ex:DeepfakeGenerationTechnique .

ex:DeepfakeMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "深度伪造信息风险"@zh ;
    rdfs:label "Deepfake Misinformation Risk"@en ;
    airo:hasConsequence ex:PublicMisinformation .

ex:DeepfakeGenerationTechnique rdf:type airo:AITechnique ;
    rdfs:label "深度伪造生成技术"@zh ;
    rdfs:label "Deepfake Generation Technique"@en .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "公众误导后果"@zh ;
    rdfs:label "Public Misinformation Consequence"@en ;
    airo:hasImpact ex:TrustErosionInPublicFigures .

ex:TrustErosionInPublicFigures rdf:type airo:Impact ;
    rdfs:label "公众人物信任度下降影响"@zh ;
    rdfs:label "Trust Erosion in Public Figures Impact"@en .