@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepFakeCampaignSystem rdf:type airo:AISystem ;
    rdfs:label "Deep Fake Campaign System"@en ;
    rdfs:label "深度伪造宣传活动系统"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:usesTechnique ex:DeepFakeTechnology ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:isDeployedBy ex:RussianGovernment ;
    airo:hasStakeholder ex:TurkishPresidentialCandidate .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasConsequence ex:UnderminedElectionIntegrity ;
    airo:isRiskSourceFor ex:ForeignGovernmentIntervention .

ex:TurkishPresidentialCandidate rdf:type airo:Stakeholder ;
    rdfs:label "Turkish Presidential Candidate"@en ;
    rdfs:label "土耳其总统候选人"@zh .

ex:RussianGovernment rdf:type airo:AIOperator ;
    rdfs:label "Russian Government"@en ;
    rdfs:label "俄罗斯政府"@zh .

ex:DeepFakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deep Fake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:ForeignGovernmentIntervention rdf:type airo:RiskSource ;
    rdfs:label "Foreign Government Intervention Risk Source"@en ;
    rdfs:label "外国政府干预风险源"@zh .

ex:UnderminedElectionIntegrity rdf:type airo:Consequence ;
    rdfs:label "Undermined Election Integrity Consequence"@en ;
    rdfs:label "选举公正性受损后果"@zh .