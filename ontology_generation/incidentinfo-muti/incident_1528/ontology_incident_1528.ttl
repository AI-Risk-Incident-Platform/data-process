@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FindFaceApp rdf:type airo:AISystem ;
    rdfs:label "FindFace App"@en ;
    rdfs:label "FindFace应用"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasCapability ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:GeneralPublic .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LossOfPublicAnonymity .

ex:FacialRecognitionTechnology rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition Technology Capability"@en ;
    rdfs:label "面部识别技术能力"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "普通公众"@zh .

ex:LossOfPublicAnonymity rdf:type airo:Consequence ;
    rdfs:label "Loss of Public Anonymity Consequence"@en ;
    rdfs:label "公共匿名性丧失后果"@zh ;
    airo:hasImpact ex:PersonalSecurityThreat .

ex:PersonalSecurityThreat rdf:type airo:Impact ;
    rdfs:label "Personal Security Threat Impact"@en ;
    rdfs:label "个人安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .