@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SunoGenerativeAIMusicPlatform rdf:type airo:AISystem ;
    rdfs:label "Suno生成式AI音乐平台"@zh ;
    rdfs:label "Suno Generative AI Music Platform"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDeployedBy ex:SunoInc ;
    airo:hasStakeholder ex:GEMA ;
    airo:isAppliedWithinDomain ex:MusicIndustry .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:hasConsequence ex:LegalLiability ;
    airo:isRiskSourceFor ex:UnlicensedUseOfCopyrightedMaterial .

ex:GEMA rdf:type airo:Stakeholder ;
    rdfs:label "德国音乐版权协会"@zh ;
    rdfs:label "German Music Copyright Association"@en .

ex:SunoInc rdf:type airo:AIOperator ;
    rdfs:label "Suno公司"@zh ;
    rdfs:label "Suno Inc."@en .

ex:MusicIndustry rdf:type airo:Domain ;
    rdfs:label "音乐产业"@zh ;
    rdfs:label "Music Industry"@en .

ex:UnlicensedUseOfCopyrightedMaterial rdf:type airo:RiskSource ;
    rdfs:label "未经授权使用受版权保护材料"@zh ;
    rdfs:label "Unlicensed Use of Copyrighted Material"@en .

ex:LegalLiability rdf:type airo:Consequence ;
    rdfs:label "法律责任"@zh ;
    rdfs:label "Legal Liability"@en ;
    airo:hasImpact ex:FinancialPenalties .

ex:FinancialPenalties rdf:type airo:Impact ;
    rdfs:label "经济处罚"@zh ;
    rdfs:label "Financial Penalties"@en ;
    airo:hasImpactOnStakeholder ex:GEMA .