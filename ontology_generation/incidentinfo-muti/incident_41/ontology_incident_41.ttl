@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ElectricElves rdf:type airo:AISystem ;
    rdfs:label "Electric Elves"@en ;
    rdfs:label "电子精灵"@zh ;
    airo:hasRisk ex:SystemFailureRisk .

ex:SystemFailureRisk rdf:type airo:Risk ;
    rdfs:label "System Failure Risk"@en ;
    rdfs:label "系统故障风险"@zh ;
    airo:isRiskSourceFor ex:SoftwareBug ;
    airo:hasConsequence ex:ServiceDisruption .

ex:SoftwareBug rdf:type airo:RiskSource ;
    rdfs:label "Software Bug"@en ;
    rdfs:label "软件缺陷"@zh .

ex:ServiceDisruption rdf:type airo:Consequence ;
    rdfs:label "Service Disruption"@en ;
    rdfs:label "服务中断"@zh ;
    airo:hasImpact ex:OperationalDisruption .

ex:OperationalDisruption rdf:type airo:Impact ;
    rdfs:label "Operational Disruption"@en ;
    rdfs:label "运营中断"@zh .