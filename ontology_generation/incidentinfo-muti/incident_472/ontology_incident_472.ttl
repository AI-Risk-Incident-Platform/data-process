@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:McDonaldsDriveThruAI rdf:type airo:AISystem ;
    rdfs:label "McDonald's Drive-Thru AI Ordering System"@en ;
    rdfs:label "麦当劳得来速AI点餐系统"@zh ;
    airo:hasRisk ex:OrderProcessingErrorRisk ;
    airo:hasAICapability ex:VoiceRecognitionForOrderTaking ;
    airo:hasPurpose ex:AutomatingDriveThruOrdering ;
    airo:hasStakeholder ex:McDonaldsCustomers ;
    airo:isAppliedWithinDomain ex:FastFoodIndustry .

ex:OrderProcessingErrorRisk rdf:type airo:Risk ;
    rdfs:label "Order Processing Error Risk"@en ;
    rdfs:label "订单处理错误风险"@zh ;
    airo:hasConsequence ex:CustomerDissatisfaction ;
    airo:isRiskSourceFor ex:SpeechRecognitionErrors .

ex:CustomerDissatisfaction rdf:type airo:Consequence ;
    rdfs:label "Customer Dissatisfaction Consequence"@en ;
    rdfs:label "顾客不满后果"@zh ;
    airo:hasImpact ex:BrandReputationDamage .

ex:VoiceRecognitionForOrderTaking rdf:type airo:AICapability ;
    rdfs:label "Voice Recognition for Order Taking Capability"@en ;
    rdfs:label "语音识别接单能力"@zh .

ex:McDonaldsCustomers rdf:type airo:Stakeholder ;
    rdfs:label "McDonald's Customers"@en ;
    rdfs:label "麦当劳顾客"@zh .

ex:BrandReputationDamage rdf:type airo:Impact ;
    rdfs:label "Brand Reputation Damage Impact"@en ;
    rdfs:label "品牌声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:McDonaldsCustomers .

ex:AutomatingDriveThruOrdering rdf:type airo:Purpose ;
    rdfs:label "Automating Drive-Thru Ordering Purpose"@en ;
    rdfs:label "自动化得来速点餐目的"@zh .

ex:FastFoodIndustry rdf:type airo:Domain ;
    rdfs:label "Fast Food Industry Domain"@en ;
    rdfs:label "快餐行业领域"@zh .

ex:SpeechRecognitionErrors rdf:type airo:RiskSource ;
    rdfs:label "Speech Recognition Errors Risk Source"@en ;
    rdfs:label "语音识别错误风险源"@zh .