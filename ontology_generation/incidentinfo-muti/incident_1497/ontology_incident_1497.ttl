ex:MicrosoftCopilotAI rdf:type airo:AISystem ;
    rdfs:label "Microsoft Copilot AI"@en ;
    rdfs:label "微软Copilot AI"@zh ;
    airo:hasRisk ex:FakeInformationGenerationRisk ;
    airo:isDevelopedBy ex:Microsoft ;
    airo:hasStakeholder ex:PublicFigures ;
    airo:hasAISubject ex:PoliticalLeaders ;
    airo:hasPurpose ex:InformationRetrieval ;
    airo:isAppliedWithinDomain ex:SearchEngine ;
    airo:usesTechnique ex:NaturalLanguageGeneration ;
    airo:hasVulnerability ex:FactCheckingDeficiency .

ex:FakeInformationGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Fake Information Generation Risk"@en ;
    rdfs:label "虚假信息生成风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:AlgorithmicHallucination ;
    airo:isRiskSourceFor ex:DisinformationCampaign ;
    airo:isRiskSourceFor ex:UnverifiedDataSources .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:PublicFigures rdf:type airo:Stakeholder ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:PoliticalLeaders rdf:type airo:AISubject ;
    rdfs:label "Political Leaders"@en ;
    rdfs:label "政治领导人"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh .

ex:InformationRetrieval rdf:type airo:Purpose ;
    rdfs:label "Information Retrieval Purpose"@en ;
    rdfs:label "信息检索目的"@zh .

ex:SearchEngine rdf:type airo:Domain ;
