@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilot rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot"@en ;
    airo:hasRisk ex:AutonomousVehicleSafetyRisk ;
    airo:hasStakeholder ex:TeslaInc ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:hasCapability ex:AutonomousDriving .

ex:AutonomousVehicleSafetyRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆安全风险"@zh ;
    rdfs:label "Autonomous Vehicle Safety Risk"@en ;
    airo:isRiskSourceFor ex:SystemFailure ;
    airo:hasConsequence ex:TrafficAccidents .

ex:TeslaInc rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业"@zh ;
    rdfs:label "Automotive Industry"@en .

ex:AutonomousDriving rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:SystemFailure rdf:type airo:RiskSource ;
    rdfs:label "系统故障风险源"@zh ;
    rdfs:label "System Failure Risk Source"@en .

ex:TrafficAccidents rdf:type airo:Consequence ;
    rdfs:label "交通事故后果"@zh ;
    rdfs:label "Traffic Accidents Consequence"@en ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "公共安全威胁影响"@zh ;
    rdfs:label "Public Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaInc .