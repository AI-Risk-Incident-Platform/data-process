@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Autopilot System"@en ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:hasAICapability ex:AutonomousDrivingFunction ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasStakeholder ex:TrafficSafetyRegulators ;
    airo:hasAISubject ex:VehiclePassengersAndPedestrians ;
    airo:hasPurpose ex:AutomatedVehicleControl ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionAndMachineLearning .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    rdfs:label "自动驾驶车辆致命风险"@zh ;
    airo:hasConsequence ex:FatalTrafficAccident ;
    airo:isRiskSourceFor ex:InsufficientSystemSafetyMeasures ;
    airo:isRiskSourceFor ex:SystemOverrelianceByHumanDrivers ;
    airo:isRiskSourceFor ex:UnpredictableRoadConditions .

ex:FatalTrafficAccident rdf:type airo:Consequence ;
    rdfs:label "Fatal Traffic Accident Consequence"@en ;
    rdfs:label "致命交通事故后果"@zh ;
    airo:hasImpact ex:LossOfHumanLife .

ex:AutonomousDrivingFunction rdf:type airo:AICapability ;
    rdfs:label "Autonomous Driving Function Capability"@en ;
    rdfs:label "自动驾驶功能能力"@zh .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:TrafficSafetyRegulators rdf:type airo:Stakeholder ;
    rdfs:label "Traffic Safety Regulators"@en ;
    rdfs:label "交通安全监管机构"@zh .

ex:VehiclePassengersAndPedestrians rdf:type airo:AISubject ;
    rdfs:label "Vehicle Passengers and Pedestrians"@en ;
    rdfs:label "车辆乘客和行人"@zh .

ex:LossOfHumanLife rdf:type airo:Impact ;
    rdfs:label "Loss of Human Life Impact"@en ;
    rdfs:label "人员死亡影响"@zh ;
    airo:hasImpactOnStakeholder ex:VehiclePassengersAndPedestrians .

ex:AutomatedVehicleControl rdf:type airo:Purpose ;
    rdfs:label "Automated Vehicle Control Purpose"@en ;
    rdfs:label "自动化车辆控制目的"@zh .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "Automotive Industry Domain"@en ;
    rdfs:label "汽车工业领域"@zh .

ex:ComputerVisionAndMachineLearning rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision and Machine Learning Technique"@en ;
    rdfs:label "计算机视觉和机器学习技术"@zh .

ex:SensorLimitationsInComplexEnvironments rdf:type airo:Vulnerability ;
    rdfs:label "Sensor Limitations in Complex Environments Vulnerability"@en ;
    rdfs:label "复杂环境中传感器限制的脆弱性"@zh ;
    airo:isExploitedBy ex:UnpredictableRoadConditions .

ex:SystemOverrelianceByHumanDrivers rdf:type airo:Threat ;
    rdfs:label "System Overreliance by Human Drivers Threat"@en ;
    rdfs:label "人类驾驶员过度依赖系统的威胁"@zh .

ex:UnpredictableRoadConditions rdf:type airo:Hazard ;
    rdfs:label "Unpredictable Road Conditions Hazard"@en ;
    rdfs:label "不可预测的道路条件危害"@zh .

ex:InsufficientSystemSafetyMeasures rdf:type airo:RiskSource ;
    rdfs:label "Insufficient System Safety Measures Risk Source"@en ;
    rdfs:label "系统安全措施不足的风险源"@zh .

ex:AutonomousVehicleSafetyConcerns rdf:type airo:RiskConcept ;
    rdfs:label "Autonomous Vehicle Safety Concerns Risk Concept"@en ;
    rdfs:label "自动驾驶车辆安全问题"@zh .