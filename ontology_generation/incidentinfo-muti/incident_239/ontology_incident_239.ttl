@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HealthInsuranceAI rdf:type airo:AISystem ;
    rdfs:label "Health Insurance AI System"@en ;
    rdfs:label "健康保险AI系统"@zh ;
    airo:hasRisk ex:InsuranceCoverageRisk ;
    airo:hasStakeholder ex:InsurancePolicyholders .

ex:InsuranceCoverageRisk rdf:type airo:Risk ;
    rdfs:label "Insurance Coverage Risk"@en ;
    rdfs:label "保险覆盖风险"@zh ;
    airo:hasConsequence ex:LossOfHealthCoverage .

ex:LossOfHealthCoverage rdf:type airo:Impact ;
    rdfs:label "Loss of Health Coverage"@en ;
    rdfs:label "健康保险丧失"@zh ;
    airo:hasImpactOnStakeholder ex:InsurancePolicyholders .

ex:InsurancePolicyholders rdf:type airo:Stakeholder ;
    rdfs:label "Insurance Policyholders"@en ;
    rdfs:label "保险投保人"@zh .