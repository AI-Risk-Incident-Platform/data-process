@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PoliceRobots rdf:type airo:AISystem ;
    rdfs:label "Police Robots"@en ;
    rdfs:label "警察机器人"@zh ;
    airo:hasRisk ex:DeadlyForceRisk ;
    airo:hasPurpose ex:LawEnforcementOperations ;
    airo:isAppliedWithinDomain ex:PublicSafety ;
    airo:hasCapability ex:LethalForceCapability ;
    airo:hasStakeholder ex:SanFranciscoPoliceDepartment .

ex:DeadlyForceRisk rdf:type airo:Risk ;
    rdfs:label "Deadly Force Risk"@en ;
    rdfs:label "致命武力风险"@zh ;
    airo:hasConsequence ex:PotentialCivilianCasualties ;
    airo:isRiskSourceFor ex:AutonomousWeaponization ;
    airo:hasRiskControl ex:PolicyReevaluation .

ex:SanFranciscoPoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "San Francisco Police Department"@en ;
    rdfs:label "旧金山警察局"@zh .

ex:PotentialCivilianCasualties rdf:type airo:Consequence ;
    rdfs:label "Potential Civilian Casualties Consequence"@en ;
    rdfs:label "潜在平民伤亡后果"@zh ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Public Safety Threat Impact"@en ;
    rdfs:label "公共安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:SanFranciscoPoliceDepartment .

ex:LawEnforcementOperations rdf:type airo:Purpose ;
    rdfs:label "Law Enforcement Operations Purpose"@en ;
    rdfs:label "执法行动目的"@zh .

ex:PublicSafety rdf:type airo:Domain ;
    rdfs:label "Public Safety Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:LethalForceCapability rdf:type airo:AICapability ;
    rdfs:label "Lethal Force Capability"@en ;
    rdfs:label "致命武力能力"@zh .

ex:AutonomousWeaponization rdf:type airo:RiskSource ;
    rdfs:label "Autonomous Weaponization Risk Source"@en ;
    rdfs:label "自主武器化风险源"@zh .

ex:PolicyReevaluation rdf:type airo:RiskControl ;
    rdfs:label "Policy Reevaluation RiskControl"@en ;
    rdfs:label "政策重新评估管控"@zh ;
    airo:mitigatesRiskConcept ex:DeadlyForceRisk .