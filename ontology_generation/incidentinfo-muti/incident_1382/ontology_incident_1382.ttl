@base <https://w3id.org/airo#> .

ex:GeoGPT rdf:type airo:AISystem ;
    rdfs:label "GeoGPT"@en ;
    rdfs:label "GeoGPT"@zh ;
    airo:hasRisk ex:CensorshipAndBiasRisk ;
    airo:hasStakeholder ex:Geologists .

ex:CensorshipAndBiasRisk rdf:type airo:Risk ;
    rdfs:label "Censorship and Bias Risk"@en ;
    rdfs:label "审查与偏见风险"@zh ;
    airo:isRiskSourceFor ex:PotentialCensorshipMechanisms ;
    airo:hasImpact ex:ScientificInformationDistortion .

ex:Geologists rdf:type airo:Stakeholder ;
    rdfs:label "Geologists"@en ;
    rdfs:label "地质学家"@zh .

ex:PotentialCensorshipMechanisms rdf:type airo:RiskSource ;
    rdfs:label "Potential Censorship Mechanisms"@en ;
    rdfs:label "潜在审查机制"@zh .

ex:ScientificInformationDistortion rdf:type airo:Impact ;
    rdfs:label "Scientific Information Distortion"@en ;
    rdfs:label "科学信息失真"@zh .