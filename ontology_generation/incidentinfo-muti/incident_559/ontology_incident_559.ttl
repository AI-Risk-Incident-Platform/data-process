@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VoiceDeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Voice Deepfake Technology"@en ;
    rdfs:label "语音深度伪造技术"@zh ;
    airo:hasRisk ex:FinancialFraudRisk .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:isRiskSourceFor ex:VoiceImpersonation ;
    airo:hasConsequence ex:UnauthorizedBankTransactions .

ex:VoiceImpersonation rdf:type airo:RiskSource ;
    rdfs:label "Voice Impersonation"@en ;
    rdfs:label "语音冒充"@zh .

ex:UnauthorizedBankTransactions rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Bank Transactions"@en ;
    rdfs:label "未经授权的银行交易"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh .