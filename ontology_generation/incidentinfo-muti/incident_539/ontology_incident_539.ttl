ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:DemocraticProcessUndermining .

ex:TurkishPoliticalParties rdf:type airo:Stakeholder ;
    rdfs:label "Turkish Political Parties"@en ;
    rdfs:label "土耳其政党"@zh .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:DemocraticProcessUndermining rdf:type airo:Impact ;
    rdfs:label "Democratic Process Undermining Impact"@en ;
    rdfs:label "民主进程破坏影响"@zh ;
