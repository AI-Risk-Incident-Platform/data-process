@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:WrongfulArrestRisk ;
    airo:hasAISubject ex:ChristopherGatlin .

ex:WrongfulArrestRisk rdf:type airo:Risk ;
    rdfs:label "Wrongful Arrest Risk"@en ;
    rdfs:label "错误逮捕风险"@zh ;
    airo:hasConsequence ex:CivilRightsViolation .

ex:CivilRightsViolation rdf:type airo:Consequence ;
    rdfs:label "Civil Rights Violation Consequence"@en ;
    rdfs:label "公民权利侵犯后果"@zh ;
    airo:hasImpact ex:PersonalFreedomDeprivation .

ex:ChristopherGatlin rdf:type airo:AISubject ;
    rdfs:label "Christopher Gatlin"@en ;
    rdfs:label "克里斯托弗·加特林"@zh .

ex:PersonalFreedomDeprivation rdf:type airo:Impact ;
    rdfs:label "Personal Freedom Deprivation Impact"@en ;
    rdfs:label "人身自由剥夺影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChristopherGatlin .