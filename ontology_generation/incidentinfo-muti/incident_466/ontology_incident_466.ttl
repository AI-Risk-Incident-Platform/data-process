@base <https://w3id.org/airo#> .

ex:GoogleAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Google Algorithm"@en ;
    rdfs:label "谷歌算法"@zh ;
    airo:hasRisk ex:ObjectificationOfWomensBodies ;
    airo:hasAICapability ex:ImageClassification ;
    airo:hasPurpose ex:ContentModeration ;
    airo:isAppliedWithinDomain ex:OnlineAdvertising ;
    airo:usesTechnique ex:ComputerVision ;
    airo:hasStakeholder ex:WomenInPhotos ;
    airo:isDeployedBy ex:Google .

ex:ObjectificationOfWomensBodies rdf:type airo:Risk ;
    rdfs:label "Objectification of Women's Bodies Risk"@en ;
    rdfs:label "女性身体物化风险"@zh ;
    airo:hasConsequence ex:SexualizationOfWomensImages ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:SexualizationOfWomensImages rdf:type airo:Consequence ;
    rdfs:label "Sexualization of Women's Images Consequence"@en ;
    rdfs:label "女性图像性化后果"@zh ;
    airo:hasImpact ex:GenderStereotyping .

ex:ImageClassification rdf:type airo:AICapability ;
    rdfs:label "Image Classification Capability"@en ;
    rdfs:label "图像分类能力"@zh .

ex:WomenInPhotos rdf:type airo:Stakeholder ;
    rdfs:label "Women in Photos"@en ;
    rdfs:label "照片中的女性"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:GenderStereotyping rdf:type airo:Impact ;
    rdfs:label "Gender Stereotyping Impact"@en ;
    rdfs:label "性别刻板印象影响"@zh ;
    airo:hasImpactOnStakeholder ex:WomenInPhotos .

ex:ContentModeration rdf:type airo:Purpose ;
    rdfs:label "Content Moderation Purpose"@en ;
    rdfs:label "内容审核目的"@zh .

ex:OnlineAdvertising rdf:type airo:Domain ;
    rdfs:label "Online Advertising Domain"@en ;
    rdfs:label "在线广告领域"@zh .

ex:ComputerVision rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision Technique"@en ;
    rdfs:label "计算机视觉技术"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .