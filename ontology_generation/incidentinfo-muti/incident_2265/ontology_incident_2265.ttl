@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UighurIdentificationAISystem rdf:type airo:AISystem ;
    rdfs:label "Uighur Identification AI System"@en ;
    rdfs:label "维吾尔族识别AI系统"@zh ;
    airo:hasRisk ex:RacialDiscriminationRisk ;
    airo:hasAISubject ex:UighurMuslims ;
    airo:hasPurpose ex:EthnicGroupIdentification ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:RacialDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Racial Discrimination Risk"@en ;
    rdfs:label "种族歧视风险"@zh ;
    airo:hasImpact ex:EthnicMinoritySurveillance .

ex:UighurMuslims rdf:type airo:AISubject ;
    rdfs:label "Uighur Muslims"@en ;
    rdfs:label "维吾尔族穆斯林"@zh .

ex:EthnicMinoritySurveillance rdf:type airo:Impact ;
    rdfs:label "Ethnic Minority Surveillance Impact"@en ;
    rdfs:label "少数民族监控影响"@zh ;
    airo:hasImpactOnStakeholder ex:UighurMuslims .

ex:EthnicGroupIdentification rdf:type airo:Purpose ;
    rdfs:label "Ethnic Group Identification Purpose"@en ;
    rdfs:label "民族识别目的"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .