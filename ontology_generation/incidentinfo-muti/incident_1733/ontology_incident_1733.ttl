@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FakeHalloweenParadeAd rdf:type airo:AISystem ;
    rdfs:label "Fake Halloween Parade Ad"@en ;
    rdfs:label "虚假万圣节游行广告"@zh ;
    airo:hasRisk ex:PublicDeceptionRisk ;
    airo:hasStakeholder ex:IrishPublic .

ex:PublicDeceptionRisk rdf:type airo:Risk ;
    rdfs:label "Public Deception Risk"@en ;
    rdfs:label "公众欺骗风险"@zh ;
    airo:hasConsequence ex:MassGatheringForNonexistentEvent ;
    airo:isRiskSourceFor ex:AIgeneratedHoax .

ex:MassGatheringForNonexistentEvent rdf:type airo:Consequence ;
    rdfs:label "Mass Gathering for Nonexistent Event"@en ;
    rdfs:label "为不存在的事件大规模聚集"@zh ;
    airo:hasImpact ex:PublicConfusionAndInconvenience .

ex:IrishPublic rdf:type airo:Stakeholder ;
    rdfs:label "Irish Public"@en ;
    rdfs:label "爱尔兰公众"@zh .

ex:AIgeneratedHoax rdf:type airo:RiskSource ;
    rdfs:label "AI-generated Hoax"@en ;
    rdfs:label "AI生成的骗局"@zh .

ex:PublicConfusionAndInconvenience rdf:type airo:Impact ;
    rdfs:label "Public Confusion and Inconvenience"@en ;
    rdfs:label "公众困惑与不便"@zh ;
    airo:hasImpactOnStakeholder ex:IrishPublic .