@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PrisonerRiskAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Prisoner Risk Algorithm"@en ;
    rdfs:label "囚犯风险评估算法"@zh ;
    airo:hasRisk ex:AlgorithmicRacismRisk ;
    airo:isAppliedWithinDomain ex:CriminalJusticeSystem ;
    airo:hasStakeholder ex:MinistryOfJustice .

ex:AlgorithmicRacismRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Racism Risk"@en ;
    rdfs:label "算法种族主义风险"@zh ;
    airo:isRiskSourceFor ex:BiasedAlgorithmDesign ;
    airo:hasImpact ex:RacialDiscriminationInJusticeSystem .

ex:CriminalJusticeSystem rdf:type airo:Domain ;
    rdfs:label "Criminal Justice System"@en ;
    rdfs:label "刑事司法系统"@zh .

ex:MinistryOfJustice rdf:type airo:Stakeholder ;
    rdfs:label "Ministry of Justice"@en ;
    rdfs:label "司法部"@zh .

ex:BiasedAlgorithmDesign rdf:type airo:RiskSource ;
    rdfs:label "Biased Algorithm Design"@en ;
    rdfs:label "有偏见的算法设计"@zh .

ex:RacialDiscriminationInJusticeSystem rdf:type airo:Impact ;
    rdfs:label "Racial Discrimination in Justice System"@en ;
    rdfs:label "司法系统中的种族歧视"@zh ;
    airo:hasImpactOnStakeholder ex:MinistryOfJustice .