@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:StateFarmClaimsProcessingAI rdf:type airo:AISystem ;
    rdfs:label "State Farm理赔处理AI系统"@zh ;
    rdfs:label "State Farm Claims Processing AI System"@en ;
    airo:hasRisk ex:RacialBiasInInsuranceClaims ;
    airo:hasStakeholder ex:BlackInsuranceCustomers ;
    airo:isDeployedBy ex:StateFarm ;
    airo:isAppliedWithinDomain ex:InsuranceIndustry .

ex:RacialBiasInInsuranceClaims rdf:type airo:Risk ;
    rdfs:label "保险理赔中的种族偏见风险"@zh ;
    rdfs:label "Racial Bias in Insurance Claims Risk"@en ;
    airo:hasConsequence ex:UnequalClaimPayouts ;
    airo:isRiskSourceFor ex:BiasedAlgorithmicDecisionMaking .

ex:BlackInsuranceCustomers rdf:type airo:Stakeholder ;
    rdfs:label "黑人保险客户"@zh ;
    rdfs:label "Black Insurance Customers"@en .

ex:StateFarm rdf:type airo:AIOperator ;
    rdfs:label "State Farm保险公司"@zh ;
    rdfs:label "State Farm"@en .

ex:UnequalClaimPayouts rdf:type airo:Impact ;
    rdfs:label "不平等理赔支付的影响"@zh ;
    rdfs:label "Unequal Claim Payouts Impact"@en ;
    airo:hasImpactOnStakeholder ex:BlackInsuranceCustomers .

ex:InsuranceIndustry rdf:type airo:Domain ;
    rdfs:label "保险行业领域"@zh ;
    rdfs:label "Insurance Industry Domain"@en .

ex:BiasedAlgorithmicDecisionMaking rdf:type airo:RiskSource ;
    rdfs:label "有偏见的算法决策风险源"@zh ;
    rdfs:label "Biased Algorithmic Decision Making Risk Source"@en .