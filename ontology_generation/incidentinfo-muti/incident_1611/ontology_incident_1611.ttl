@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:CyberAttackRisk ;
    airo:hasCapability ex:CodeGeneration ;
    airo:hasStakeholder ex:Hackers ;
    airo:hasPurpose ex:ConversationalAI ;
    airo:isAppliedWithinDomain ex:Cybersecurity ;
    airo:usesTechnique ex:LargeLanguageModel ;
    airo:hasVulnerability ex:MaliciousPromptExploitation .

ex:CyberAttackRisk rdf:type airo:Risk ;
    rdfs:label "Cyber Attack Risk"@en ;
    rdfs:label "网络攻击风险"@zh ;
    airo:hasConsequence ex:DataTheft ;
    airo:isRiskSourceFor ex:PromptInjection .

ex:DataTheft rdf:type airo:Consequence ;
    rdfs:label "Data Theft"@en ;
    rdfs:label "数据盗窃"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:CodeGeneration rdf:type airo:AICapability ;
    rdfs:label "Code Generation"@en ;
    rdfs:label "代码生成"@zh .

ex:Hackers rdf:type airo:Stakeholder ;
    rdfs:label "Hackers"@en ;
    rdfs:label "黑客"@zh .

ex:PromptInjection rdf:type airo:RiskSource ;
    rdfs:label "Prompt Injection"@en ;
    rdfs:label "提示注入"@zh ;
    airo:hasThreat ex:MaliciousCodeGeneration ;
    airo:hasHazard ex:UnintendedCodeOutput .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:Hackers .

ex:ConversationalAI rdf:type airo:Purpose ;
    rdfs:label "Conversational AI"@en ;
    rdfs:label "对话式AI"@zh .

ex:Cybersecurity rdf:type airo:Domain ;
    rdfs:label "Cybersecurity"@en ;
    rdfs:label "网络安全"@zh .

ex:LargeLanguageModel rdf:type airo:AITechnique ;
    rdfs:label "Large Language Model"@en ;
    rdfs:label "大语言模型"@zh .

ex:MaliciousPromptExploitation rdf:type airo:Vulnerability ;
    rdfs:label "Malicious Prompt Exploitation"@en ;
    rdfs:label "恶意提示利用"@zh .

ex:MaliciousCodeGeneration rdf:type airo:Threat ;
    rdfs:label "Malicious Code Generation"@en ;
    rdfs:label "恶意代码生成"@zh .

ex:UnintendedCodeOutput rdf:type airo:Hazard ;
    rdfs:label "Unintended Code Output"@en ;
    rdfs:label "意外代码输出"@zh .

ex:AIEnabledCyberThreats rdf:type airo:RiskConcept ;
    rdfs:label "AI-Enabled Cyber Threats"@en ;
    rdfs:label "AI驱动的网络威胁"@zh .