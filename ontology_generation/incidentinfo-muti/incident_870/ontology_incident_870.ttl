ex:DeepfakeVideoAdverts rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Adverts"@en ;
    rdfs:label "深度伪造视频广告"@zh ;
    airo:hasRisk ex:AIRiskToElection ;
    airo:hasAISubject ex:RishiSunak ;
    airo:hasStakeholder ex:MetaPlatform ;
    airo:isAppliedWithinDomain ex:PoliticalAdvertising ;
    airo:usesTechnique ex:DeepfakeTechnology .

ex:AIRiskToElection rdf:type airo:Risk ;
    rdfs:label "AI Risk to Election"@en ;
    rdfs:label "AI对选举的风险"@zh ;
    airo:hasConsequence ex:ElectionInterference .

ex:MetaPlatform rdf:type airo:Stakeholder ;
    rdfs:label "Meta Platform"@en ;
    rdfs:label "Meta平台"@zh .

ex:RishiSunak rdf:type airo:AISubject ;
    rdfs:label "Rishi Sunak"@en ;
    rdfs:label "Rishi Sunak"@zh .

ex:ElectionInterference rdf:type airo:Impact ;
    rdfs:label "Election Interference"@en ;
    rdfs:label "选举干预"@zh ;
    airo:hasImpactOnStakeholder ex:MetaPlatform .

ex:PoliticalAdvertising rdf:type airo:Domain ;
    rdfs:label "Political Advertising"@en ;
    rdfs:label "政治广告"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .