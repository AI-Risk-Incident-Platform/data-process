    rdfs:label "Beijing Internet Court"@en ;
    rdfs:label "北京互联网法院"@zh .

ex:LegalSystem rdf:type airo:Domain ;
    rdfs:label "Legal System"@en ;
    rdfs:label "法律系统"@zh .

ex:AudiobookProduction rdf:type airo:Purpose ;
    rdfs:label "Audiobook Production"@en ;
    rdfs:label "有声书制作"@zh .