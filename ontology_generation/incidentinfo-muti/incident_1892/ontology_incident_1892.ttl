ex:GenerativeAIVoiceSystem rdf:type airo:AISystem ;
    rdfs:label "Generative AI Voice System"@en ;
    rdfs:label "生成式AI语音系统"@zh ;
    airo:hasRisk ex:PersonalityRightsInfringementRisk ;
    airo:hasPurpose ex:AudiobookProduction ;
