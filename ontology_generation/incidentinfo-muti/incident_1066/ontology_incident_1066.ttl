@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedLegalBriefs rdf:type airo:AISystem ;
    rdfs:label "AI生成法律简报"@zh ;
    rdfs:label "AI Generated Legal Briefs"@en ;
    airo:hasRisk ex:LegalDocumentInaccuracyRisk ;
    airo:usesTechnique ex:LegalDocumentGenerationAI ;
    airo:hasStakeholder ex:LawFirmsUsingAI .

ex:LegalDocumentInaccuracyRisk rdf:type airo:Risk ;
    rdfs:label "法律文件不准确风险"@zh ;
    rdfs:label "Legal Document Inaccuracy Risk"@en ;
    airo:hasConsequence ex:FinancialPenaltyConsequence .

ex:FinancialPenaltyConsequence rdf:type airo:Consequence ;
    rdfs:label "经济处罚后果"@zh ;
    rdfs:label "Financial Penalty Consequence"@en ;
    airo:hasImpact ex:ProfessionalReputationDamage .

ex:LawFirmsUsingAI rdf:type airo:Stakeholder ;
    rdfs:label "使用AI的律师事务所"@zh ;
    rdfs:label "Law Firms Using AI"@en .

ex:LegalDocumentGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "法律文件生成AI技术"@zh ;
    rdfs:label "Legal Document Generation AI Technique"@en .

ex:ProfessionalReputationDamage rdf:type airo:Impact ;
    rdfs:label "专业声誉损害影响"@zh ;
    rdfs:label "Professional Reputation Damage Impact"@en ;
    airo:hasImpactOnStakeholder ex:LawFirmsUsingAI .