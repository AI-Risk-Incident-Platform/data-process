@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CollaborativeFilteringRecommenderSystem rdf:type airo:AISystem ;
    rdfs:label "Collaborative Filtering Recommender System"@en ;
    rdfs:label "协同过滤推荐系统"@zh ;
    airo:hasRisk ex:PopularityBiasRisk ;
    airo:hasCapability ex:UserPreferencePrediction ;
    airo:usesTechnique ex:CollaborativeFilteringAlgorithm ;
    airo:isAppliedWithinDomain ex:ECommerce .

ex:PopularityBiasRisk rdf:type airo:Risk ;
    rdfs:label "Popularity Bias Risk"@en ;
    rdfs:label "流行度偏见风险"@zh ;
    airo:hasConsequence ex:PoorRecommendationQuality ;
    airo:isRiskSourceFor ex:OverrepresentationOfPopularItems .

ex:UserPreferencePrediction rdf:type airo:AICapability ;
    rdfs:label "User Preference Prediction Capability"@en ;
    rdfs:label "用户偏好预测能力"@zh .

ex:PoorRecommendationQuality rdf:type airo:Consequence ;
    rdfs:label "Poor Recommendation Quality Consequence"@en ;
    rdfs:label "推荐质量低下后果"@zh ;
    airo:hasImpact ex:UserDissatisfaction .

ex:OverrepresentationOfPopularItems rdf:type airo:RiskSource ;
    rdfs:label "Overrepresentation of Popular Items Risk Source"@en ;
    rdfs:label "热门商品过度代表风险源"@zh .

ex:UserDissatisfaction rdf:type airo:Impact ;
    rdfs:label "User Dissatisfaction Impact"@en ;
    rdfs:label "用户不满影响"@zh .

ex:ECommerce rdf:type airo:Domain ;
    rdfs:label "E-Commerce Domain"@en ;
    rdfs:label "电子商务领域"@zh .

ex:CollaborativeFilteringAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Collaborative Filtering Algorithm Technique"@en ;
    rdfs:label "协同过滤算法技术"@zh .