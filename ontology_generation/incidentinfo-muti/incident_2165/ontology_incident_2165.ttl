@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeliverooAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Deliveroo Algorithm"@en ;
    rdfs:label "Deliveroo算法"@zh ;
    airo:hasRisk ex:WorkLifeBalanceDisruptionRisk ;
    airo:hasStakeholder ex:DeliveryRiders ;
    airo:isAppliedWithinDomain ex:FoodDeliveryIndustry .

ex:WorkLifeBalanceDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Work-Life Balance Disruption Risk"@en ;
    rdfs:label "工作生活平衡破坏风险"@zh ;
    airo:hasConsequence ex:FamilyLifeDisruption .

ex:DeliveryRiders rdf:type airo:Stakeholder ;
    rdfs:label "Delivery Riders"@en ;
    rdfs:label "外卖骑手"@zh .

ex:FamilyLifeDisruption rdf:type airo:Impact ;
    rdfs:label "Family Life Disruption Impact"@en ;
    rdfs:label "家庭生活破坏影响"@zh ;
    airo:hasImpactOnStakeholder ex:DeliveryRiders .

ex:FoodDeliveryIndustry rdf:type airo:Domain ;
    rdfs:label "Food Delivery Industry Domain"@en ;
    rdfs:label "食品配送行业"@zh .