@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:MisidentificationRisk ;
    airo:hasAISubject ex:FootballFans ;
    airo:hasStakeholder ex:MilitaryPolice ;
    airo:hasPurpose ex:PublicSecurityMonitoring ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:MisidentificationRisk rdf:type airo:Risk ;
    rdfs:label "Misidentification Risk"@en ;
    rdfs:label "错误识别风险"@zh ;
    airo:hasConsequence ex:WrongfulDetention ;
    airo:isRiskSourceFor ex:AlgorithmicError .

ex:WrongfulDetention rdf:type airo:Consequence ;
    rdfs:label "Wrongful Detention Consequence"@en ;
    rdfs:label "错误拘留后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:FootballFans rdf:type airo:AISubject ;
    rdfs:label "Football Fans"@en ;
    rdfs:label "足球迷"@zh .

ex:MilitaryPolice rdf:type airo:Stakeholder ;
    rdfs:label "Military Police"@en ;
    rdfs:label "军警"@zh ;
    airo:hasImpactOnStakeholder ex:PsychologicalDistress .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh .

ex:PublicSecurityMonitoring rdf:type airo:Purpose ;
    rdfs:label "Public Security Monitoring Purpose"@en ;
    rdfs:label "公共安全监控目的"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:AlgorithmicError rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Error Risk Source"@en ;
    rdfs:label "算法错误风险源"@zh .