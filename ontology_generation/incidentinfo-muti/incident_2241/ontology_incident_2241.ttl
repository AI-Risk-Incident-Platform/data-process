@base <https://w3id.org/airo#> .

ex:GovernmentAuditOfAI rdf:type airo:AISystem ;
    rdfs:label "Government Audit of AI"@en ;
    rdfs:label "政府AI审计"@zh ;
    airo:hasRisk ex:PotentialWhiteSupremacyTiesRisk ;
    airo:hasStakeholder ex:GovernmentAuditors .

ex:PotentialWhiteSupremacyTiesRisk rdf:type airo:Risk ;
    rdfs:label "Potential White Supremacy Ties Risk"@en ;
    rdfs:label "潜在白人至上主义关联风险"@zh ;
    airo:isRiskSourceFor ex:UnverifiedWhiteSupremacyTies ;
    airo:hasConsequence ex:PublicConcernOverAIEthics .

ex:GovernmentAuditors rdf:type airo:Stakeholder ;
    rdfs:label "Government Auditors"@en ;
    rdfs:label "政府审计人员"@zh .

ex:UnverifiedWhiteSupremacyTies rdf:type airo:RiskSource ;
    rdfs:label "Unverified White Supremacy Ties"@en ;
    rdfs:label "未经验证的白人至上主义关联"@zh .

ex:PublicConcernOverAIEthics rdf:type airo:Consequence ;
    rdfs:label "Public Concern Over AI Ethics"@en ;
    rdfs:label "公众对AI伦理的担忧"@zh .