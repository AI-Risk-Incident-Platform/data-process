@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LiveFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Live Facial Recognition System"@en ;
    rdfs:label "实时面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:Children ;
    airo:hasAICapability ex:RealTimeFacialRecognition ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:Misidentification ;
    airo:hasImpact ex:PsychologicalImpactOnChildren .

ex:Children rdf:type airo:AISubject ;
    rdfs:label "Children"@en ;
    rdfs:label "儿童"@zh .

ex:PsychologicalImpactOnChildren rdf:type airo:Impact ;
    rdfs:label "Psychological Impact on Children"@en ;
    rdfs:label "对儿童的心理影响"@zh ;
    airo:hasImpactOnStakeholder ex:Children .

ex:Misidentification rdf:type airo:RiskSource ;
    rdfs:label "Misidentification"@en ;
    rdfs:label "错误识别"@zh .

ex:RealTimeFacialRecognition rdf:type airo:AICapability ;
    rdfs:label "Real-time Facial Recognition Capability"@en ;
    rdfs:label "实时面部识别能力"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .