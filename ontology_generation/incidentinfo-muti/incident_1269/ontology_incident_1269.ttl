@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PerplexityAI rdf:type airo:AISystem ;
    rdfs:label "Perplexity AI"@en ;
    rdfs:label "Perplexity AI"@zh ;
    airo:hasRisk ex:ContentMisuseRisk ;
    airo:hasStakeholder ex:NewYorkTimes ;
    airo:isDeployedBy ex:PerplexityAIOperator .

ex:ContentMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Content Misuse Risk"@en ;
    rdfs:label "内容滥用风险"@zh ;
    airo:isRiskSourceFor ex:UnauthorizedContentUse .

ex:NewYorkTimes rdf:type airo:Stakeholder ;
    rdfs:label "New York Times"@en ;
    rdfs:label "纽约时报"@zh .

ex:PerplexityAIOperator rdf:type airo:AIOperator ;
    rdfs:label "Perplexity AI Operator"@en ;
    rdfs:label "Perplexity AI运营商"@zh .

ex:UnauthorizedContentUse rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Content Use"@en ;
    rdfs:label "未经授权的内容使用"@zh .