@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ParagonSpyware rdf:type airo:AISystem ;
    rdfs:label "Paragon Spyware"@en ;
    rdfs:label "Paragon间谍软件"@zh ;
    airo:hasRisk ex:CivilSocietySurveillanceRisk ;
    airo:hasStakeholder ex:CivilSocietyOrganizations ;
    airo:isDeployedBy ex:NationStateActor ;
    airo:isAppliedWithinDomain ex:Cybersecurity .

ex:CivilSocietySurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Civil Society Surveillance Risk"@en ;
    rdfs:label "公民社会监控风险"@zh ;
    airo:hasConsequence ex:UnauthorizedDataCollection .

ex:CivilSocietyOrganizations rdf:type airo:Stakeholder ;
    rdfs:label "Civil Society Organizations"@en ;
    rdfs:label "公民社会组织"@zh .

ex:NationStateActor rdf:type airo:AIOperator ;
    rdfs:label "Nation-State Actor"@en ;
    rdfs:label "国家行为体"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:CivilSocietyOrganizations .

ex:UnauthorizedDataCollection rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Data Collection"@en ;
    rdfs:label "未经授权的数据收集"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:Cybersecurity rdf:type airo:Domain ;
    rdfs:label "Cybersecurity"@en ;
    rdfs:label "网络安全"@zh .