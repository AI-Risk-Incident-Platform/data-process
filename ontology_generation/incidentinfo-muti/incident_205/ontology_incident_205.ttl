@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VoiceMimickingAI rdf:type airo:AISystem ;
    rdfs:label "Voice Mimicking AI"@en ;
    rdfs:label "语音模仿AI"@zh ;
    airo:hasRisk ex:VoicePhishingRisk ;
    airo:usesTechnique ex:VoiceSynthesis ;
    airo:hasStakeholder ex:Fraudsters .

ex:VoicePhishingRisk rdf:type airo:Risk ;
    rdfs:label "Voice Phishing Risk"@en ;
    rdfs:label "语音钓鱼风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:Fraudsters rdf:type airo:Stakeholder ;
    rdfs:label "Fraudsters"@en ;
    rdfs:label "诈骗者"@zh .

ex:VoiceSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Voice Synthesis Technique"@en ;
    rdfs:label "语音合成技术"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "财务损失后果"@zh ;
    airo:hasImpact ex:CorporateSecurityThreat .

ex:CorporateSecurityThreat rdf:type airo:Impact ;
    rdfs:label "Corporate Security Threat Impact"@en ;
    rdfs:label "企业安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:Fraudsters .