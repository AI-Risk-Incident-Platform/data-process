ex:GrokAI rdf:type airo:AISystem ;
    rdfs:label "Grok AI"@en ;
    rdfs:label "Grok AI"@zh ;
    airo:hasRisk ex:FalseNewsDisseminationRisk ;
    airo:hasStakeholder ex:IndianGovernment ;
    airo:isDevelopedBy ex:ElonMusk .

ex:FalseNewsDisseminationRisk rdf:type airo:Risk ;
    rdfs:label "False News Dissemination Risk"@en ;
    rdfs:label "虚假新闻传播风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpactOnStakeholder ex:IndianGovernment .

ex:IndianGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Indian Government"@en ;
    rdfs:label "印度政府"@zh .

ex:ElonMusk rdf:type airo:AIOperator ;
    rdfs:label "Elon Musk"@en ;
    rdfs:label "埃隆·马斯克"@zh .