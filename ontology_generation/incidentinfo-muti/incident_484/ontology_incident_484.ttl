@base <https://w3id.org/airo#> .

ex:DeepfakeNewsSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake News System"@en ;
    rdfs:label "深度伪造新闻系统"@zh ;
    airo:hasRisk ex:DisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:PropagandaDissemination .

ex:DisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Disinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasImpact ex:PublicOpinionManipulation .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PropagandaDissemination rdf:type airo:Purpose ;
    rdfs:label "Propaganda Dissemination"@en ;
    rdfs:label "宣传传播"@zh .

ex:PublicOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "Public Opinion Manipulation"@en ;
    rdfs:label "舆论操纵"@zh .