@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAIForIllegalImages rdf:type airo:AISystem ;
    rdfs:label "Generative AI for Illegal Images"@en ;
    rdfs:label "生成非法图像的生成式AI"@zh ;
    airo:hasRisk ex:ChildExploitationRisk ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:ChildExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Risk"@en ;
    rdfs:label "儿童剥削风险"@zh ;
    airo:hasConsequence ex:IllegalContentCreation ;
    airo:isRiskSourceFor ex:ChildSexAbusePicturesInDataset .

ex:IllegalContentCreation rdf:type airo:Consequence ;
    rdfs:label "Illegal Content Creation Consequence"@en ;
    rdfs:label "非法内容创建后果"@zh ;
    airo:hasImpact ex:HarmToChildren .

ex:ChildSexAbusePicturesInDataset rdf:type airo:RiskSource ;
    rdfs:label "Child Sex Abuse Pictures in Dataset Risk Source"@en ;
    rdfs:label "数据集中存在的儿童性虐待图片风险源"@zh .

ex:HarmToChildren rdf:type airo:Impact ;
    rdfs:label "Harm to Children Impact"@en ;
    rdfs:label "对儿童的伤害影响"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .