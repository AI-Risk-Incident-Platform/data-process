@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAndAmazonAI rdf:type airo:AISystem ;
    rdfs:label "Google and Amazon AI"@en ;
    rdfs:label "谷歌和亚马逊AI"@zh ;
    airo:hasRisk ex:MisclassificationOfHarmfulContent ;
    airo:hasCapability ex:ContentClassification ;
    airo:hasStakeholder ex:GoogleAndAmazon .

ex:MisclassificationOfHarmfulContent rdf:type airo:Risk ;
    rdfs:label "Misclassification of Harmful Content Risk"@en ;
    rdfs:label "有害内容误分类风险"@zh ;
    airo:hasConsequence ex:PromotionOfHateSpeech ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:PromotionOfHateSpeech rdf:type airo:Consequence ;
    rdfs:label "Promotion of Hate Speech Consequence"@en ;
    rdfs:label "仇恨言论传播后果"@zh ;
    airo:hasImpact ex:SocialHarm .

ex:SocialHarm rdf:type airo:Impact ;
    rdfs:label "Social Harm Impact"@en ;
    rdfs:label "社会危害影响"@zh ;
    airo:hasImpactOnStakeholder ex:GoogleAndAmazon .

ex:ContentClassification rdf:type airo:AICapability ;
    rdfs:label "Content Classification Capability"@en ;
    rdfs:label "内容分类能力"@zh .

ex:GoogleAndAmazon rdf:type airo:Stakeholder ;
    rdfs:label "Google and Amazon"@en ;
    rdfs:label "谷歌和亚马逊"@zh .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .