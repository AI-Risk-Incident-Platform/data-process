ex:AIPodcastReplicationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Podcast Replication System"@en ;
    rdfs:label "AI播客复制系统"@zh ;
    airo:hasRisk ex:EthicalConcernsInAIReplication ;
    airo:hasPurpose ex:ReplicatingCelebrityForEntertainment ;
    airo:hasStakeholder ex:ParkinsonFamily ;
    airo:isDeployedBy ex:AIPodcastProducer .

ex:EthicalConcernsInAIReplication rdf:type airo:Risk ;
    rdfs:label "Ethical Concerns in AI Replication Risk"@en ;
    rdfs:label "AI复制中的伦理问题风险"@zh .

ex:ParkinsonFamily rdf:type airo:Stakeholder ;
    rdfs:label "Parkinson Family"@en ;
    rdfs:label "帕金森家族"@zh .

ex:AIPodcastProducer rdf:type airo:AIOperator ;
    rdfs:label "AI Podcast Producer"@en ;
    rdfs:label "AI播客制作人"@zh .

ex:ReplicatingCelebrityForEntertainment rdf:type airo:Purpose ;
    rdfs:label "Replicating Celebrity for Entertainment Purpose"@en ;
    rdfs:label "复制名人用于娱乐的目的"@zh .