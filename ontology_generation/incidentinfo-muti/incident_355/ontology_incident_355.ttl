@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:McDonaldsVoiceRecognitionDriveThru rdf:type airo:AISystem ;
    rdfs:label "McDonald's Voice Recognition Drive-Thru System"@en ;
    rdfs:label "麦当劳语音识别点餐系统"@zh ;
    airo:hasRisk ex:BiometricsPrivacyLawViolationRisk ;
    airo:usesTechnique ex:VoiceRecognitionTechnology ;
    airo:isAppliedWithinDomain ex:FastFoodDriveThruServices ;
    airo:hasPurpose ex:AutomatedDriveThruOrdering ;
    airo:hasStakeholder ex:McDonaldsCorporation .

ex:BiometricsPrivacyLawViolationRisk rdf:type airo:Risk ;
    rdfs:label "Biometrics Privacy Law Violation Risk"@en ;
    rdfs:label "生物识别隐私法违规风险"@zh ;
    airo:hasConsequence ex:PotentialLegalPenalties ;
    airo:isRiskSourceFor ex:BiometricDataCollectionWithoutConsent .

ex:McDonaldsCorporation rdf:type airo:Stakeholder ;
    rdfs:label "McDonald's Corporation"@en ;
    rdfs:label "麦当劳公司"@zh .

ex:VoiceRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Recognition Technology"@en ;
    rdfs:label "语音识别技术"@zh .

ex:FastFoodDriveThruServices rdf:type airo:Domain ;
    rdfs:label "Fast Food Drive-Thru Services Domain"@en ;
    rdfs:label "快餐店免下车服务领域"@zh .

ex:AutomatedDriveThruOrdering rdf:type airo:Purpose ;
    rdfs:label "Automated Drive-Thru Ordering Purpose"@en ;
    rdfs:label "自动化免下车点餐目的"@zh .

ex:PotentialLegalPenalties rdf:type airo:Consequence ;
    rdfs:label "Potential Legal Penalties Consequence"@en ;
    rdfs:label "潜在法律处罚后果"@zh .

ex:BiometricDataCollectionWithoutConsent rdf:type airo:RiskSource ;
    rdfs:label "Biometric Data Collection Without Consent Risk Source"@en ;
    rdfs:label "未经同意的生物特征数据收集风险源"@zh .