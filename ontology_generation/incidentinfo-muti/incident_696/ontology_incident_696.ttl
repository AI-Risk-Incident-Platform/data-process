ex:ImageGenerationAI rdf:type airo:AISystem ;
    rdfs:label "Image Generation AI"@en ;
    rdfs:label "图像生成AI"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "声誉损害后果"@zh ;
    airo:hasImpact ex:PublicMisperception .

ex:PublicMisperception rdf:type airo:Impact ;
    rdfs:label "Public Misperception Impact"@en ;
    rdfs:label "公众误解影响"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .