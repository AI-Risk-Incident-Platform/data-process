@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FactoryRobot rdf:type airo:AISystem ;
    rdfs:label "Factory Robot"@en ;
    rdfs:label "工厂机器人"@zh ;
    airo:hasRisk ex:HumanFatalityRisk ;
    airo:hasStakeholder ex:RobotSupplier ;
    airo:isDevelopedBy ex:JapaneseMaker .

ex:HumanFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Human Fatality Risk"@en ;
    rdfs:label "人类死亡风险"@zh ;
    airo:hasConsequence ex:HumanDeath ;
    airo:isRiskSourceFor ex:NegligenceInSafetyMeasures .

ex:HumanDeath rdf:type airo:Consequence ;
    rdfs:label "Human Death Consequence"@en ;
    rdfs:label "人类死亡后果"@zh ;
    airo:hasImpact ex:FamilyLoss .

ex:RobotSupplier rdf:type airo:Stakeholder ;
    rdfs:label "Robot Supplier"@en ;
    rdfs:label "机器人供应商"@zh .

ex:JapaneseMaker rdf:type airo:AIOperator ;
    rdfs:label "Japanese Maker"@en ;
    rdfs:label "日本制造商"@zh .

ex:FamilyLoss rdf:type airo:Impact ;
    rdfs:label "Family Loss Impact"@en ;
    rdfs:label "家庭损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:RobotSupplier .

ex:NegligenceInSafetyMeasures rdf:type airo:RiskSource ;
    rdfs:label "Negligence in Safety Measures Risk Source"@en ;
    rdfs:label "安全措施疏忽风险源"@zh ;
    airo:exploitsVulnerability ex:LackOfHumanSafetyProtocols .

ex:LackOfHumanSafetyProtocols rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Human Safety Protocols Vulnerability"@en ;
    rdfs:label "缺乏人类安全协议的脆弱性"@zh .