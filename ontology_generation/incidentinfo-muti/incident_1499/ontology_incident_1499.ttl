@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TaxPrepChatbots rdf:type airo:AISystem ;
    rdfs:label "Tax Prep Chatbots"@en ;
    rdfs:label "税务准备聊天机器人"@zh ;
    airo:hasRisk ex:IncorrectTaxAdviceRisk ;
    airo:isDeployedBy ex:TurboTax ;
    airo:isAppliedWithinDomain ex:TaxPreparation ;
    airo:hasPurpose ex:AutomatedTaxAdvice .

ex:IncorrectTaxAdviceRisk rdf:type airo:Risk ;
    rdfs:label "Incorrect Tax Advice Risk"@en ;
    rdfs:label "错误税务建议风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:AlgorithmicErrors .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "财务损失"@zh ;
    airo:hasImpact ex:TaxpayerHarm .

ex:TurboTax rdf:type airo:AIOperator ;
    rdfs:label "TurboTax"@en ;
    rdfs:label "TurboTax"@zh .

ex:TaxPreparation rdf:type airo:Domain ;
    rdfs:label "Tax Preparation"@en ;
    rdfs:label "税务准备"@zh .

ex:AutomatedTaxAdvice rdf:type airo:Purpose ;
    rdfs:label "Automated Tax Advice"@en ;
    rdfs:label "自动化税务建议"@zh .

ex:AlgorithmicErrors rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Errors"@en ;
    rdfs:label "算法错误"@zh .

ex:TaxpayerHarm rdf:type airo:Impact ;
    rdfs:label "Taxpayer Harm"@en ;
    rdfs:label "纳税人损害"@zh ;
    airo:hasImpactOnStakeholder ex:TurboTax .