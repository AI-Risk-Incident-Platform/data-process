@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeToolsForElection rdf:type airo:AISystem ;
    rdfs:label "Deepfake Tools for Election"@en ;
    rdfs:label "选举用深度伪造工具"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:IndianPoliticalParties ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:IndianPoliticalParties rdf:type airo:Stakeholder ;
    rdfs:label "Indian Political Parties"@en ;
    rdfs:label "印度政党"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:IndianPoliticalParties .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections"@en ;
    rdfs:label "政治选举"@zh .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread"@en ;
    rdfs:label "虚假信息传播"@zh ;
    airo:hasImpact ex:PublicTrustErosion .