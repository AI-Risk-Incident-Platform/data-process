@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedTravelGuides rdf:type airo:AISystem ;
    rdfs:label "AI Generated Travel Guides"@en ;
    rdfs:label "AI生成旅行指南"@zh ;
    airo:hasRisk ex:ScamRisk ;
    airo:isAppliedWithinDomain ex:ECommercePlatform .

ex:ScamRisk rdf:type airo:Risk ;
    rdfs:label "Scam Risk"@en ;
    rdfs:label "诈骗风险"@zh ;
    airo:hasConsequence ex:ConsumerDeception ;
    airo:isRiskSourceFor ex:LowQualityContentGeneration .

ex:ECommercePlatform rdf:type airo:Domain ;
    rdfs:label "E-Commerce Platform"@en ;
    rdfs:label "电子商务平台"@zh .

ex:LowQualityContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Low Quality Content Generation"@en ;
    rdfs:label "低质量内容生成"@zh .

ex:ConsumerDeception rdf:type airo:Consequence ;
    rdfs:label "Consumer Deception"@en ;
    rdfs:label "消费者欺骗"@zh ;
    airo:hasImpact ex:MarketTrustErosion .

ex:MarketTrustErosion rdf:type airo:Impact ;
    rdfs:label "Market Trust Erosion"@en ;
    rdfs:label "市场信任侵蚀"@zh .