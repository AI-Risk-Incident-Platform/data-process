@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GovernmentRobot rdf:type airo:AISystem ;
    rdfs:label "Government Robot"@en ;
    rdfs:label "政府机器人"@zh ;
    airo:hasRisk ex:RobotSelfDestructionRisk ;
    airo:hasStakeholder ex:Government .

ex:RobotSelfDestructionRisk rdf:type airo:Risk ;
    rdfs:label "Robot Self-Destruction Risk"@en ;
    rdfs:label "机器人自毁风险"@zh ;
    airo:hasConsequence ex:RobotMalfunction .

ex:RobotMalfunction rdf:type airo:Consequence ;
    rdfs:label "Robot Malfunction"@en ;
    rdfs:label "机器人故障"@zh ;
    airo:hasImpact ex:ServiceDisruption .

ex:ServiceDisruption rdf:type airo:Impact ;
    rdfs:label "Service Disruption"@en ;
    rdfs:label "服务中断"@zh ;
    airo:hasImpactOnStakeholder ex:Government .

ex:Government rdf:type airo:Stakeholder ;
    rdfs:label "Government"@en ;
    rdfs:label "政府"@zh .