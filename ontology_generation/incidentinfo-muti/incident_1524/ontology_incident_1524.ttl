@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TVNetworkAIEditingTool rdf:type airo:AISystem ;
    rdfs:label "电视台AI图像编辑工具"@zh ;
    rdfs:label "TV Network AI Editing Tool"@en ;
    airo:hasRisk ex:SexualizationOfPublicFiguresRisk ;
    airo:isAppliedWithinDomain ex:NewsMedia ;
    airo:hasStakeholder ex:VictorianMP .

ex:SexualizationOfPublicFiguresRisk rdf:type airo:Risk ;
    rdfs:label "公众人物形象性化风险"@zh ;
    rdfs:label "Sexualization of Public Figures Risk"@en ;
    airo:hasConsequence ex:InappropriateImageModification .

ex:VictorianMP rdf:type airo:Stakeholder ;
    rdfs:label "维多利亚州议员"@zh ;
    rdfs:label "Victorian MP"@en .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "声誉损害影响"@zh ;
    rdfs:label "Reputation Damage Impact"@en ;
    airo:hasImpactOnStakeholder ex:VictorianMP .

ex:InappropriateImageModification rdf:type airo:Misuse ;
    rdfs:label "不当图像修改滥用"@zh ;
    rdfs:label "Inappropriate Image Modification Misuse"@en ;
    airo:hasImpact ex:ReputationDamage .

ex:NewsMedia rdf:type airo:Domain ;
    rdfs:label "新闻媒体领域"@zh ;
    rdfs:label "News Media Domain"@en .