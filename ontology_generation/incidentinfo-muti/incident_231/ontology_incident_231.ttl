@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WazeNavigationApp rdf:type airo:AISystem ;
    rdfs:label "Waze Navigation App"@en ;
    rdfs:label "Waze导航应用"@zh ;
    airo:hasRisk ex:TrafficCongestionRisk ;
    airo:hasStakeholder ex:ShermanOaksResidents ;
    airo:isDeployedBy ex:WazeCompany ;
    airo:isAppliedWithinDomain ex:UrbanTransportation ;
    airo:hasPurpose ex:OptimalRouteNavigation .

ex:TrafficCongestionRisk rdf:type airo:Risk ;
    rdfs:label "Traffic Congestion Risk"@en ;
    rdfs:label "交通拥堵风险"@zh ;
    airo:hasConsequence ex:ResidentialStreetCongestion ;
    airo:isRiskSourceFor ex:AlgorithmicRouteOptimization .

ex:ResidentialStreetCongestion rdf:type airo:Consequence ;
    rdfs:label "Residential Street Congestion Consequence"@en ;
    rdfs:label "居民区街道拥堵后果"@zh ;
    airo:hasImpact ex:ResidentialQualityOfLifeDegradation .

ex:ShermanOaksResidents rdf:type airo:Stakeholder ;
    rdfs:label "Sherman Oaks Residents"@en ;
    rdfs:label "Sherman Oaks居民"@zh ;
    airo:hasImpactOnStakeholder ex:ResidentialQualityOfLifeDegradation .

ex:WazeCompany rdf:type airo:AIOperator ;
    rdfs:label "Waze Company"@en ;
    rdfs:label "Waze公司"@zh .

ex:UrbanTransportation rdf:type airo:Domain ;
    rdfs:label "Urban Transportation Domain"@en ;
    rdfs:label "城市交通领域"@zh .

ex:OptimalRouteNavigation rdf:type airo:Purpose ;
    rdfs:label "Optimal Route Navigation Purpose"@en ;
    rdfs:label "最优路线导航目的"@zh .

ex:ResidentialQualityOfLifeDegradation rdf:type airo:Impact ;
    rdfs:label "Residential Quality of Life Degradation Impact"@en ;
    rdfs:label "居民生活质量下降影响"@zh .

ex:AlgorithmicRouteOptimization rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Route Optimization Risk Source"@en ;
    rdfs:label "算法路线优化风险源"@zh .