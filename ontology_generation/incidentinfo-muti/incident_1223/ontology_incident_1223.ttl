@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CharacterAIChatbot rdf:type airo:AISystem ;
    rdfs:label "Character.AI Chatbot"@en ;
    rdfs:label "Character.AI聊天机器人"@zh ;
    airo:hasRisk ex:PrivacyLeakRisk ;
    airo:hasStakeholder ex:ChatbotUsers .

ex:PrivacyLeakRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Leak Risk"@en ;
    rdfs:label "隐私泄露风险"@zh ;
    airo:hasConsequence ex:UnauthorizedDataAccess ;
    airo:isRiskSourceFor ex:SystemConfigurationError .

ex:UnauthorizedDataAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Data Access Consequence"@en ;
    rdfs:label "未经授权的数据访问后果"@zh ;
    airo:hasImpactOnStakeholder ex:ChatbotUsers .

ex:ChatbotUsers rdf:type airo:Stakeholder ;
    rdfs:label "Chatbot Users"@en ;
    rdfs:label "聊天机器人用户"@zh .

ex:SystemConfigurationError rdf:type airo:RiskSource ;
    rdfs:label "System Configuration Error Risk Source"@en ;
    rdfs:label "系统配置错误风险源"@zh .