@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepFakeVideoContent rdf:type airo:AISystem ;
    rdfs:label "Deep Fake Video Content"@en ;
    rdfs:label "深度伪造视频内容"@zh ;
    airo:hasRisk ex:MisinformationAndDeceptionRisk ;
    airo:hasStakeholder ex:TikTokUsers ;
    airo:isDeployedBy ex:ChineseGrifter .

ex:MisinformationAndDeceptionRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation and Deception Risk"@en ;
    rdfs:label "虚假信息和欺骗风险"@zh ;
    airo:hasConsequence ex:PublicDeception .

ex:TikTokUsers rdf:type airo:Stakeholder ;
    rdfs:label "TikTok Users"@en ;
    rdfs:label "TikTok用户"@zh .

ex:ChineseGrifter rdf:type airo:AIOperator ;
    rdfs:label "Chinese Grifter"@en ;
    rdfs:label "中国骗子"@zh .

ex:PublicDeception rdf:type airo:Consequence ;
    rdfs:label "Public Deception"@en ;
    rdfs:label "公众欺骗"@zh ;
    airo:hasImpactOnStakeholder ex:TikTokUsers .