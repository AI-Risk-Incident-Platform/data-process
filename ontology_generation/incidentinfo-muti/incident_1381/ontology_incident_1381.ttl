@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PerplexityAI rdf:type airo:AISystem ;
    rdfs:label "Perplexity AI"@en ;
    rdfs:label "Perplexity AI"@zh ;
    airo:hasRisk ex:WebContentMisuseRisk ;
    airo:hasAICapability ex:WebContentGeneration ;
    airo:hasStakeholder ex:WebContentCreators .

ex:WebContentMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Web Content Misuse Risk"@en ;
    rdfs:label "网络内容滥用风险"@zh ;
    airo:hasImpact ex:WebContentQualityDegradation .

ex:WebContentCreators rdf:type airo:Stakeholder ;
    rdfs:label "Web Content Creators"@en ;
    rdfs:label "网络内容创作者"@zh .

ex:WebContentGeneration rdf:type airo:AICapability ;
    rdfs:label "Web Content Generation Capability"@en ;
    rdfs:label "网络内容生成能力"@zh .

ex:WebContentQualityDegradation rdf:type airo:Impact ;
    rdfs:label "Web Content Quality Degradation Impact"@en ;
    rdfs:label "网络内容质量下降的影响"@zh ;
    airo:hasImpactOnStakeholder ex:WebContentCreators .