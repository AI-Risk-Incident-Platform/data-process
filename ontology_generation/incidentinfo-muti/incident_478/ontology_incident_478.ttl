ex:JoeRoganDeepfakeVideo rdf:type airo:AISystem ;
    rdfs:label "Joe Rogan Deepfake Video"@en ;
    rdfs:label "Joe Rogan深度伪造视频"@zh ;
    airo:hasRisk ex:MisleadingAdvertisingRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:ProductPromotion ;
    airo:isAppliedWithinDomain ex:SocialMediaAdvertising ;
    airo:hasStakeholder ex:TikTokPlatform .

ex:MisleadingAdvertisingRisk rdf:type airo:Risk ;
    rdfs:label "Misleading Advertising Risk"@en ;
    rdfs:label "误导性广告风险"@zh ;
