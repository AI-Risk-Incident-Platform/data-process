@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVoiceCloningSystem rdf:type airo:AISystem ;
    rdfs:label "AI Voice Cloning System"@en ;
    rdfs:label "AI语音克隆系统"@zh ;
    airo:hasRisk ex:VoiceActorDoxingAndHarassmentRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasStakeholder ex:VideoGameVoiceActors ;
    airo:hasRisk ex:MaliciousUseOfVoiceCloning .

ex:VoiceActorDoxingAndHarassmentRisk rdf:type airo:Risk ;
    rdfs:label "Voice Actor Doxing and Harassment Risk"@en ;
    rdfs:label "配音演员人肉搜索与骚扰风险"@zh ;
    airo:hasConsequence ex:PersonalPrivacyViolation .

ex:PersonalPrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Personal Privacy Violation Consequence"@en ;
    rdfs:label "个人隐私侵犯后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToVoiceActors .

ex:PsychologicalHarmToVoiceActors rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Voice Actors Impact"@en ;
    rdfs:label "对配音演员的心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:VideoGameVoiceActors .

ex:VideoGameVoiceActors rdf:type airo:Stakeholder ;
    rdfs:label "Video Game Voice Actors"@en ;
    rdfs:label "电子游戏配音演员"@zh .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:MaliciousUseOfVoiceCloning rdf:type airo:Misuse ;
    rdfs:label "Malicious Use of Voice Cloning"@en ;
    rdfs:label "语音克隆技术的恶意使用"@zh ;
    airo:isRiskSourceFor ex:VoiceActorDoxingAndHarassmentRisk .