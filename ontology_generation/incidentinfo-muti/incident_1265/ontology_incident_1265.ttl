@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NudifyDeepfakeBots rdf:type airo:AISystem ;
    rdfs:label "Nudify Deepfake Bots"@en ;
    rdfs:label "Nudify深度伪造机器人"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAICapability ex:DeepfakeImageGeneration ;
    airo:hasStakeholder ex:TelegramUsers ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:NonConsensualImageCreation ;
    airo:isRiskSourceFor ex:EasyAccessToDeepfakeTechnology .

ex:NonConsensualImageCreation rdf:type airo:Consequence ;
    rdfs:label "Non-Consensual Image Creation Consequence"@en ;
    rdfs:label "非自愿图像创建后果"@zh ;
    airo:hasImpact ex:PsychologicalHarm .

ex:DeepfakeImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Deepfake Image Generation Capability"@en ;
    rdfs:label "深度伪造图像生成能力"@zh .

ex:TelegramUsers rdf:type airo:Stakeholder ;
    rdfs:label "Telegram Users"@en ;
    rdfs:label "Telegram用户"@zh .

ex:PsychologicalHarm rdf:type airo:Impact ;
    rdfs:label "Psychological Harm Impact"@en ;
    rdfs:label "心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:TelegramUsers .

ex:EasyAccessToDeepfakeTechnology rdf:type airo:RiskSource ;
    rdfs:label "Easy Access to Deepfake Technology Risk Source"@en ;
    rdfs:label "深度伪造技术易获取风险源"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .