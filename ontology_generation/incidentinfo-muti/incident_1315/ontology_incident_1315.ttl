@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en ;
    airo:hasRisk ex:IdentityMisuseRisk ;
    airo:hasAISubject ex:PublicFigure ;
    airo:usesTechnique ex:FacialManipulationAlgorithm ;
    airo:hasPurpose ex:FraudulentActivities .

ex:IdentityMisuseRisk rdf:type airo:Risk ;
    rdfs:label "身份滥用风险"@zh ;
    rdfs:label "Identity Misuse Risk"@en ;
    airo:hasConsequence ex:ReputationDamage .

ex:PublicFigure rdf:type airo:AISubject ;
    rdfs:label "公众人物"@zh ;
    rdfs:label "Public Figure"@en .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "名誉损害后果"@zh ;
    rdfs:label "Reputation Damage Consequence"@en ;
    airo:hasImpact ex:EmotionalDistress .

ex:FacialManipulationAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "面部操纵算法"@zh ;
    rdfs:label "Facial Manipulation Algorithm"@en .

ex:FraudulentActivities rdf:type airo:Purpose ;
    rdfs:label "欺诈活动目的"@zh ;
    rdfs:label "Fraudulent Activities Purpose"@en .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "情绪困扰影响"@zh ;
    rdfs:label "Emotional Distress Impact"@en ;
    airo:hasImpactOnStakeholder ex:MediaPersonality .

ex:MediaPersonality rdf:type airo:Stakeholder ;
    rdfs:label "媒体人士"@zh ;
    rdfs:label "Media Personality"@en .