@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SlackAISystem rdf:type airo:AISystem ;
    rdfs:label "Slack AI System"@en ;
    rdfs:label "Slack人工智能系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:SlackCompany ;
    airo:hasStakeholder ex:SlackUsers ;
    airo:hasPurpose ex:AIModelTraining ;
    airo:isAppliedWithinDomain ex:EnterpriseCommunication .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:DataScrapingWithoutConsent ;
    airo:hasConsequence ex:LossOfUserTrust .

ex:SlackCompany rdf:type airo:AIOperator ;
    rdfs:label "Slack Company"@en ;
    rdfs:label "Slack公司"@zh .

ex:SlackUsers rdf:type airo:Stakeholder ;
    rdfs:label "Slack Users"@en ;
    rdfs:label "Slack用户"@zh .

ex:DataScrapingWithoutConsent rdf:type airo:RiskSource ;
    rdfs:label "Data Scraping Without Consent"@en ;
    rdfs:label "未经同意的数据抓取"@zh .

ex:LossOfUserTrust rdf:type airo:Consequence ;
    rdfs:label "Loss of User Trust"@en ;
    rdfs:label "用户信任丧失"@zh ;
    airo:hasImpact ex:ReputationalDamage .

ex:ReputationalDamage rdf:type airo:Impact ;
    rdfs:label "Reputational Damage"@en ;
    rdfs:label "声誉损害"@zh ;
    airo:hasImpactOnStakeholder ex:SlackUsers .

ex:AIModelTraining rdf:type airo:Purpose ;
    rdfs:label "AI Model Training"@en ;
    rdfs:label "AI模型训练"@zh .

ex:EnterpriseCommunication rdf:type airo:Domain ;
    rdfs:label "Enterprise Communication"@en ;
    rdfs:label "企业通信"@zh .