@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClothOffApp rdf:type airo:AISystem ;
    rdfs:label "ClothOff App"@en ;
    rdfs:label "ClothOff应用"@zh ;
    airo:hasRisk ex:ChildExploitationRisk ;
    airo:usesTechnique ex:DeepfakeImageGeneration ;
    airo:hasPurpose ex:ImageManipulation ;
    airo:hasStakeholder ex:SpanishPolice ;
    airo:hasAISubject ex:SchoolGirls ;
    airo:isAppliedWithinDomain ex:EducationEnvironment .

ex:ChildExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Risk"@en ;
    rdfs:label "儿童剥削风险"@zh ;
    airo:hasConsequence ex:PsychologicalHarmToMinors ;
    airo:isRiskSourceFor ex:UnauthorizedImageGeneration .

ex:PsychologicalHarmToMinors rdf:type airo:Consequence ;
    rdfs:label "Psychological Harm to Minors"@en ;
    rdfs:label "对未成年人心理伤害"@zh ;
    airo:hasImpact ex:SocialStigma .

ex:DeepfakeImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Image Generation Technique"@en ;
    rdfs:label "深度伪造图像生成技术"@zh .

ex:SpanishPolice rdf:type airo:Stakeholder ;
    rdfs:label "Spanish Police"@en ;
    rdfs:label "西班牙警方"@zh .

ex:SchoolGirls rdf:type airo:AISubject ;
    rdfs:label "School Girls"@en ;
    rdfs:label "女学生"@zh .

ex:SocialStigma rdf:type airo:Impact ;
    rdfs:label "Social Stigma Impact"@en ;
    rdfs:label "社会污名化影响"@zh ;
    airo:hasImpactOnStakeholder ex:SchoolGirls .

ex:ImageManipulation rdf:type airo:Purpose ;
    rdfs:label "Image Manipulation Purpose"@en ;
    rdfs:label "图像操纵目的"@zh .

ex:EducationEnvironment rdf:type airo:Domain ;
    rdfs:label "Education Environment Domain"@en ;
    rdfs:label "教育环境领域"@zh .

ex:UnauthorizedImageGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Image Generation Risk Source"@en ;
    rdfs:label "未经授权的图像生成风险源"@zh .