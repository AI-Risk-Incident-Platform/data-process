@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:iPhoneXFaceIDSystem rdf:type airo:AISystem ;
    rdfs:label "iPhone X Face ID System"@en ;
    rdfs:label "iPhone X面部识别系统"@zh ;
    airo:hasRisk ex:UnauthorizedAccessRisk ;
    airo:hasAISubject ex:ChildUser ;
    airo:hasVulnerability ex:FacialRecognitionVulnerability .

ex:UnauthorizedAccessRisk rdf:type airo:Risk ;
    rdfs:label "Unauthorized Access Risk"@en ;
    rdfs:label "未经授权访问风险"@zh ;
    airo:hasConsequence ex:PrivacySecurityImpact .

ex:FacialRecognitionVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "Facial Recognition Vulnerability"@en ;
    rdfs:label "面部识别漏洞"@zh .

ex:ChildUser rdf:type airo:AISubject ;
    rdfs:label "Child User"@en ;
    rdfs:label "儿童用户"@zh .

ex:PrivacySecurityImpact rdf:type airo:Impact ;
    rdfs:label "Privacy Security Impact"@en ;
    rdfs:label "隐私安全影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChildUser .