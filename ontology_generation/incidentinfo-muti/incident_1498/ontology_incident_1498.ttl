@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:LegalMisinformationRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:BcLawyer ;
    airo:hasPurpose ex:LegalResearchAssistance ;
    airo:isAppliedWithinDomain ex:LegalServices .

ex:LegalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Misinformation Risk"@en ;
    rdfs:label "法律错误信息风险"@zh ;
    airo:isRiskSourceFor ex:HallucinatedCaseLaw ;
    airo:hasConsequence ex:LegalProceedingsDisruption .

ex:LegalProceedingsDisruption rdf:type airo:Consequence ;
    rdfs:label "Legal Proceedings Disruption Consequence"@en ;
    rdfs:label "法律程序中断后果"@zh ;
    airo:hasImpact ex:LegalSystemIntegrityUndermined .

ex:BcLawyer rdf:type airo:Stakeholder ;
    rdfs:label "B.C. Lawyer"@en ;
    rdfs:label "BC省律师"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:LegalSystemIntegrityUndermined rdf:type airo:Impact ;
    rdfs:label "Legal System Integrity Undermined Impact"@en ;
    rdfs:label "法律系统完整性受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:BcLawyer .

ex:LegalResearchAssistance rdf:type airo:Purpose ;
    rdfs:label "Legal Research Assistance Purpose"@en ;
    rdfs:label "法律研究辅助目的"@zh .

ex:LegalServices rdf:type airo:Domain ;
    rdfs:label "Legal Services Domain"@en ;
    rdfs:label "法律服务领域"@zh .

ex:HallucinatedCaseLaw rdf:type airo:RiskSource ;
    rdfs:label "Hallucinated Case Law Risk Source"@en ;
    rdfs:label "虚构案例法风险源"@zh ;
    airo:exploitsVulnerability ex:LackOfLegalDomainVerification .

ex:LackOfLegalDomainVerification rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Legal Domain Verification Vulnerability"@en ;
    rdfs:label "缺乏法律领域验证的脆弱性"@zh .