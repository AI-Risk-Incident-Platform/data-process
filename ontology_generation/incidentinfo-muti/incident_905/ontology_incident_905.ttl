@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FakeAIVersionsOfAcademics rdf:type airo:AISystem ;
    rdfs:label "Fake AI Versions of Academics"@en ;
    rdfs:label "伪造世界知名学者AI版本"@zh ;
    airo:hasRisk ex:DisinformationSpreadRisk ;
    airo:hasStakeholder ex:WorldRenownedAcademics ;
    airo:isAppliedWithinDomain ex:GeopoliticalConflict .

ex:DisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Disinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasImpact ex:PoliticalManipulationImpact .

ex:WorldRenownedAcademics rdf:type airo:Stakeholder ;
    rdfs:label "World-Renowned Academics"@en ;
    rdfs:label "世界知名学者"@zh .

ex:PoliticalManipulationImpact rdf:type airo:Impact ;
    rdfs:label "Political Manipulation Impact"@en ;
    rdfs:label "政治操纵影响"@zh ;
    airo:hasImpactOnStakeholder ex:WorldRenownedAcademics .

ex:GeopoliticalConflict rdf:type airo:Domain ;
    rdfs:label "Geopolitical Conflict Domain"@en ;
    rdfs:label "地缘政治冲突领域"@zh .