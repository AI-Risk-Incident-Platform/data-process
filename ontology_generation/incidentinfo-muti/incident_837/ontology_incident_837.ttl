@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造生成系统"@zh ;
    rdfs:label "Deepfake Generation System"@en ;
    airo:hasRisk ex:ReputationDamageRisk ;
    airo:usesTechnique ex:ImageSynthesisTechnology ;
    airo:hasStakeholder ex:MinisterAlaiba .

ex:ReputationDamageRisk rdf:type airo:Risk ;
    rdfs:label "声誉损害风险"@zh ;
    rdfs:label "Reputation Damage Risk"@en ;
    airo:hasConsequence ex:PublicMisinformation .

ex:MinisterAlaiba rdf:type airo:Stakeholder ;
    rdfs:label "Alaiba部长"@zh ;
    rdfs:label "Minister Alaiba"@en .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "公众误导后果"@zh ;
    rdfs:label "Public Misinformation Consequence"@en ;
    airo:hasImpact ex:PoliticalTrustErosion .

ex:ImageSynthesisTechnology rdf:type airo:AITechnique ;
    rdfs:label "图像合成技术"@zh ;
    rdfs:label "Image Synthesis Technology"@en .

ex:PoliticalTrustErosion rdf:type airo:Impact ;
    rdfs:label "政治信任侵蚀影响"@zh ;
    rdfs:label "Political Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:MinisterAlaiba .