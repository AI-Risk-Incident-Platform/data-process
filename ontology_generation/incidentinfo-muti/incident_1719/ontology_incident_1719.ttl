ex:MagicTheGatheringPromoAIArt rdf:type airo:AISystem ;
    rdfs:label "Magic The Gathering Promo AI Art"@en ;
    rdfs:label "万智牌宣传AI艺术"@zh ;
    airo:hasRisk ex:AIIntellectualPropertyRisk ;
    airo:hasStakeholder ex:WizardsOfTheCoast .

ex:AIIntellectualPropertyRisk rdf:type airo:Risk ;
    rdfs:label "AI Intellectual Property Risk"@en ;
    rdfs:label "AI知识产权风险"@zh ;
    airo:hasConsequence ex:CommunityBacklash ;
    airo:isRiskSourceFor ex:PolicyViolation .

ex:WizardsOfTheCoast rdf:type airo:Stakeholder ;
    rdfs:label "Wizards of the Coast"@en ;
    rdfs:label "海岸巫师公司"@zh .

ex:CommunityBacklash rdf:type airo:Consequence ;
    rdfs:label "Community Backlash"@en ;
    rdfs:label "社区强烈反对"@zh .

ex:PolicyViolation rdf:type airo:Misuse ;
    rdfs:label "Policy Violation"@en ;
    rdfs:label "政策违规"@zh .