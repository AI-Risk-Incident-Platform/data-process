@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAIGeneratedMushroomImages rdf:type airo:AISystem ;
    rdfs:label "Google AI-Generated Mushroom Images"@en ;
    rdfs:label "谷歌AI生成蘑菇图片"@zh ;
    airo:hasRisk ex:HealthCrisisRisk ;
    airo:hasStakeholder ex:Google ;
    airo:isAppliedWithinDomain ex:PublicHealth .

ex:HealthCrisisRisk rdf:type airo:Risk ;
    rdfs:label "Health Crisis Risk"@en ;
    rdfs:label "健康危机风险"@zh ;
    airo:hasConsequence ex:DevastatingConsequences ;
    airo:isRiskSourceFor ex:InaccurateAIGeneratedContent .

ex:DevastatingConsequences rdf:type airo:Consequence ;
    rdfs:label "Devastating Consequences"@en ;
    rdfs:label "毁灭性后果"@zh ;
    airo:hasImpact ex:PublicHealthThreat .

ex:PublicHealthThreat rdf:type airo:Impact ;
    rdfs:label "Public Health Threat"@en ;
    rdfs:label "公共卫生威胁"@zh ;
    airo:hasImpactOnStakeholder ex:Google .

ex:Google rdf:type airo:Stakeholder ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:PublicHealth rdf:type airo:Domain ;
    rdfs:label "Public Health"@en ;
    rdfs:label "公共卫生"@zh .

ex:InaccurateAIGeneratedContent rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate AI-Generated Content"@en ;
    rdfs:label "不准确的AI生成内容"@zh .