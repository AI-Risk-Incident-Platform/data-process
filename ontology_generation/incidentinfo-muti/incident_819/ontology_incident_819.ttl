@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WelfareAlgorithmSystem rdf:type airo:AISystem ;
    rdfs:label "Welfare Algorithm System"@en ;
    rdfs:label "福利算法系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasRisk ;
    airo:hasStakeholder ex:SocialSecurityAgency ;
    airo:isAppliedWithinDomain ex:SocialWelfareSystemDomain .

ex:AlgorithmicBiasRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias Risk"@en ;
    rdfs:label "算法偏见风险"@zh ;
    airo:hasImpact ex:DiscriminatoryImpact .

ex:SocialSecurityAgency rdf:type airo:Stakeholder ;
    rdfs:label "Social Security Agency"@en ;
    rdfs:label "社会保障机构"@zh .

ex:DiscriminatoryImpact rdf:type airo:Impact ;
    rdfs:label "Discriminatory Impact"@en ;
    rdfs:label "歧视性影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialSecurityAgency .

ex:SocialWelfareSystemDomain rdf:type airo:Domain ;
    rdfs:label "Social Welfare System Domain"@en ;
    rdfs:label "社会福利系统领域"@zh .