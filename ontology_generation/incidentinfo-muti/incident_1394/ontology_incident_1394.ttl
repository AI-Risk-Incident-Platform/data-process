    rdfs:label "Facial Recognition Technique"@en ;
    rdfs:label "面部识别技术"@zh .

ex:IndividualsBeingIdentified rdf:type airo:Stakeholder ;
    rdfs:label "Individuals Being Identified"@en ;
    rdfs:label "被识别个体"@zh .

ex:IdentityVerification rdf:type airo:Purpose ;
    rdfs:label "Identity Verification Purpose"@en ;
    rdfs:label "身份验证"@zh .