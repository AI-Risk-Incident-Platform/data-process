@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeRecommendationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "YouTube Recommendation Algorithm"@en ;
    rdfs:label "YouTube推荐算法"@zh ;
    airo:hasRisk ex:RadicalizationRisk ;
    airo:hasCapability ex:ContentRecommendation ;
    airo:hasStakeholder ex:YouTubeUsers ;
    airo:hasAISubject ex:OnlineViewers ;
    airo:hasPurpose ex:UserEngagementMaximization ;
    airo:isAppliedWithinDomain ex:OnlineVideoPlatform ;
    airo:usesTechnique ex:MachineLearningBasedRecommendation .

ex:RadicalizationRisk rdf:type airo:Risk ;
    rdfs:label "Radicalization Risk"@en ;
    rdfs:label "极端化风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmicFeedbackLoop ;
    airo:hasImpact ex:IdeologicalRadicalization .

ex:ContentRecommendation rdf:type airo:AICapability ;
    rdfs:label "Content Recommendation Capability"@en ;
    rdfs:label "内容推荐能力"@zh .

ex:YouTubeUsers rdf:type airo:Stakeholder ;
    rdfs:label "YouTube Users"@en ;
    rdfs:label "YouTube用户"@zh .

ex:OnlineViewers rdf:type airo:AISubject ;
    rdfs:label "Online Viewers"@en ;
    rdfs:label "在线观众"@zh .

ex:IdeologicalRadicalization rdf:type airo:Impact ;
    rdfs:label "Ideological Radicalization Impact"@en ;
    rdfs:label "意识形态极端化影响"@zh .

ex:UserEngagementMaximization rdf:type airo:Purpose ;
    rdfs:label "User Engagement Maximization Purpose"@en ;
    rdfs:label "用户参与度最大化目的"@zh .

ex:OnlineVideoPlatform rdf:type airo:Domain ;
    rdfs:label "Online Video Platform Domain"@en ;
    rdfs:label "在线视频平台领域"@zh .

ex:MachineLearningBasedRecommendation rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning Based Recommendation Technique"@en ;
    rdfs:label "基于机器学习的推荐技术"@zh .

ex:AlgorithmicFeedbackLoop rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Feedback Loop Risk Source"@en ;
    rdfs:label "算法反馈循环风险源"@zh .