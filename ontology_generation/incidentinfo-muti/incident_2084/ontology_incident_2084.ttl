ex:DeepfakeAudioSynthesis rdf:type airo:AITechnique ;
    rdfs:label "深度伪造音频合成技术"@zh ;
    rdfs:label "Deepfake Audio Synthesis Technique"@en .

ex:DocumentaryProductionTeam rdf:type airo:Stakeholder ;
    rdfs:label "纪录片制作团队"@zh ;
    rdfs:label "Documentary Production Team"@en .

ex:PotentialMisrepresentationOfDeceasedIndividuals rdf:type airo:Impact ;
    rdfs:label "可能对逝者形象造成歪曲的影响"@zh ;
    rdfs:label "Potential Misrepresentation of Deceased Individuals Impact"@en ;
    airo:hasImpactOnStakeholder ex:DocumentaryProductionTeam .

ex:DocumentaryNarrationEnhancement rdf:type airo:Purpose ;
    rdfs:label "增强纪录片叙述的目的"@zh ;
    rdfs:label "Documentary Narration Enhancement Purpose"@en .