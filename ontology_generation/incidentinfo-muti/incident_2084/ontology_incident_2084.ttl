ex:AnthonyBourdainVoiceDeepfakeSystem rdf:type airo:AISystem ;
    rdfs:label "安东尼·波登语音深度伪造系统"@zh ;
    rdfs:label "Anthony Bourdain Voice Deepfake System"@en ;
    airo:hasRisk ex:EthicalConcernsInPosthumousVoiceReplication ;
    airo:usesTechnique ex:DeepfakeAudioSynthesis ;
    airo:hasPurpose ex:DocumentaryNarrationEnhancement ;
    airo:hasStakeholder ex:DocumentaryProductionTeam .

ex:EthicalConcernsInPosthumousVoiceReplication rdf:type airo:Risk ;
    rdfs:label "死后语音复制的伦理问题风险"@zh ;
    rdfs:label "Ethical Concerns in Posthumous Voice Replication Risk"@en ;
