@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVoiceCloningSystem rdf:type airo:AISystem ;
    rdfs:label "AI Voice Cloning System"@en ;
    rdfs:label "AI语音克隆系统"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasPurpose ex:VoiceImpersonation ;
    airo:hasAISubject ex:FormerBoeingEmployee ;
    airo:hasStakeholder ex:ElderlyVictim .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:ScammerPosingAsTVHost ;
    airo:hasVulnerability ex:ElderlyTrustInMediaFigures .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:PsychologicalTrauma .

ex:ElderlyVictim rdf:type airo:Stakeholder ;
    rdfs:label "Elderly Victim"@en ;
    rdfs:label "老年受害者"@zh .

ex:FormerBoeingEmployee rdf:type airo:AISubject ;
    rdfs:label "Former Boeing Employee"@en ;
    rdfs:label "前波音公司员工"@zh .

ex:PsychologicalTrauma rdf:type airo:Impact ;
    rdfs:label "Psychological Trauma Impact"@en ;
    rdfs:label "心理创伤影响"@zh ;
    airo:hasImpactOnStakeholder ex:ElderlyVictim .

ex:VoiceImpersonation rdf:type airo:Purpose ;
    rdfs:label "Voice Impersonation Purpose"@en ;
    rdfs:label "语音模仿目的"@zh .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:ScammerPosingAsTVHost rdf:type airo:RiskSource ;
    rdfs:label "Scammer Posing As TV Host"@en ;
    rdfs:label "冒充电视主持人的诈骗者"@zh .

ex:ElderlyTrustInMediaFigures rdf:type airo:Vulnerability ;
    rdfs:label "Elderly Trust In Media Figures"@en ;
    rdfs:label "老年人对媒体人物的信任"@zh .