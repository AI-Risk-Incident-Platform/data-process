@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIPhotoGenerators rdf:type airo:AISystem ;
    rdfs:label "AI Photo Generators"@en ;
    rdfs:label "AI照片生成器"@zh ;
    airo:hasRisk ex:MisleadingElectionImagesRisk ;
    airo:hasCapability ex:ImageGenerationCapability ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:MisleadingElectionImagesRisk rdf:type airo:Risk ;
    rdfs:label "Misleading Election Images Risk"@en ;
    rdfs:label "误导性选举图像风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:UnverifiedImageGeneration .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:ElectionIntegrityThreat .

ex:ImageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Image Generation Capability"@en ;
    rdfs:label "图像生成能力"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:ElectionIntegrityThreat rdf:type airo:Impact ;
    rdfs:label "Election Integrity Threat Impact"@en ;
    rdfs:label "选举诚信威胁影响"@zh .

ex:UnverifiedImageGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unverified Image Generation Risk Source"@en ;
    rdfs:label "未经验证的图像生成风险源"@zh .