@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LokiPromoPosterAI rdf:type airo:AISystem ;
    rdfs:label "Loki Promo Poster AI System"@en ;
    rdfs:label "洛基宣传海报AI系统"@zh ;
    airo:hasRisk ex:PublicBacklashRisk ;
    airo:isDeployedBy ex:Disney ;
    airo:hasStakeholder ex:DisneyMarvel .

ex:PublicBacklashRisk rdf:type airo:Risk ;
    rdfs:label "Public Backlash Risk"@en ;
    rdfs:label "公众反弹风险"@zh ;
    airo:hasConsequence ex:BrandReputationDamage ;
    airo:isRiskSourceFor ex:AIArtGenerationAllegations .

ex:DisneyMarvel rdf:type airo:Stakeholder ;
    rdfs:label "Disney Marvel"@en ;
    rdfs:label "迪士尼漫威"@zh .

ex:Disney rdf:type airo:AIOperator ;
    rdfs:label "Disney"@en ;
    rdfs:label "迪士尼"@zh .

ex:BrandReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Brand Reputation Damage Consequence"@en ;
    rdfs:label "品牌声誉损害后果"@zh ;
    airo:hasImpact ex:FanDisappointment .

ex:AIArtGenerationAllegations rdf:type airo:RiskSource ;
    rdfs:label "AI Art Generation Allegations Risk Source"@en ;
    rdfs:label "AI艺术生成指控风险源"@zh .

ex:FanDisappointment rdf:type airo:Impact ;
    rdfs:label "Fan Disappointment Impact"@en ;
    rdfs:label "粉丝失望影响"@zh ;
    airo:hasImpactOnStakeholder ex:DisneyMarvel .