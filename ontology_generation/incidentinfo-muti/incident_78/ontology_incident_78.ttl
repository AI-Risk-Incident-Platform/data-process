@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:FalseIdentificationRisk ;
    airo:hasAISubject ex:ArrestedIndividual ;
    airo:hasStakeholder ex:DetroitPoliceDepartment ;
    airo:isAppliedWithinDomain ex:LawEnforcement .

ex:FalseIdentificationRisk rdf:type airo:Risk ;
    rdfs:label "False Identification Risk"@en ;
    rdfs:label "错误识别风险"@zh ;
    airo:hasConsequence ex:WrongfulArrestAndImprisonment ;
    airo:isRiskSourceFor ex:AlgorithmInaccuracy .

ex:WrongfulArrestAndImprisonment rdf:type airo:Consequence ;
    rdfs:label "Wrongful Arrest and Imprisonment Consequence"@en ;
    rdfs:label "错误逮捕和监禁后果"@zh ;
    airo:hasImpact ex:PersonalRightsViolation .

ex:PersonalRightsViolation rdf:type airo:Impact ;
    rdfs:label "Personal Rights Violation Impact"@en ;
    rdfs:label "个人权利侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:ArrestedIndividual .

ex:DetroitPoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "Detroit Police Department"@en ;
    rdfs:label "底特律警察局"@zh .

ex:ArrestedIndividual rdf:type airo:AISubject ;
    rdfs:label "Arrested Individual"@en ;
    rdfs:label "被逮捕个人"@zh .

ex:AlgorithmInaccuracy rdf:type airo:RiskSource ;
    rdfs:label "Algorithm Inaccuracy Risk Source"@en ;
    rdfs:label "算法不准确风险源"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .