@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonPanorama rdf:type airo:AISystem ;
    rdfs:label "亚马逊全景设备"@zh ;
    rdfs:label "Amazon Panorama"@en ;
    airo:hasRisk ex:SurveillancePrivacyRisk ;
    airo:hasCapability ex:MachineLearningForSurveillance ;
    airo:hasPurpose ex:EmployeeTracking ;
    airo:isAppliedWithinDomain ex:WorkplaceSurveillance ;
    airo:hasStakeholder ex:AmazonCustomers ;
    airo:isDeployedBy ex:AmazonWebServices .

ex:SurveillancePrivacyRisk rdf:type airo:Risk ;
    rdfs:label "监控隐私风险"@zh ;
    rdfs:label "Surveillance Privacy Risk"@en ;
    airo:hasConsequence ex:PrivacyInvasion .

ex:MachineLearningForSurveillance rdf:type airo:AICapability ;
    rdfs:label "用于监控的机器学习能力"@zh ;
    rdfs:label "Machine Learning for Surveillance Capability"@en .

ex:EmployeeTracking rdf:type airo:Purpose ;
    rdfs:label "员工追踪目的"@zh ;
    rdfs:label "Employee Tracking Purpose"@en .

ex:WorkplaceSurveillance rdf:type airo:Domain ;
    rdfs:label "工作场所监控领域"@zh ;
    rdfs:label "Workplace Surveillance Domain"@en .

ex:AmazonCustomers rdf:type airo:Stakeholder ;
    rdfs:label "亚马逊客户"@zh ;
    rdfs:label "Amazon Customers"@en .

ex:AmazonWebServices rdf:type airo:AIOperator ;
    rdfs:label "亚马逊网络服务"@zh ;
    rdfs:label "Amazon Web Services"@en .

ex:PrivacyInvasion rdf:type airo:Consequence ;
    rdfs:label "隐私侵犯后果"@zh ;
    rdfs:label "Privacy Invasion Consequence"@en .