ex:AdobeFirefly rdf:type airo:AISystem ;
    rdfs:label "Adobe Firefly"@en ;
    rdfs:label "Adobe Firefly"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDevelopedBy ex:Adobe ;
    airo:hasPurpose ex:ImageAndVideoGeneration ;
    airo:usesTechnique ex:GenerativeAI ;
    airo:isAppliedWithinDomain ex:CreativeContentProduction ;
    airo:hasStakeholder ex:Adobe .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
