    rdfs:label "Creative Content Production"@en ;
    rdfs:label "创意内容生产"@zh .

ex:GenerativeAI rdf:type airo:AITechnique ;
    rdfs:label "Generative AI"@en ;
    rdfs:label "生成式AI"@zh .