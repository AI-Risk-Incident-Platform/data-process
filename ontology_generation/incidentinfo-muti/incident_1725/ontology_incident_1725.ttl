ex:Adobe rdf:type airo:AIOperator ;
    rdfs:label "Adobe"@en ;
    rdfs:label "Adobe公司"@zh .

ex:ImageAndVideoGeneration rdf:type airo:Purpose ;
    rdfs:label "Image and Video Generation"@en ;
    rdfs:label "图像和视频生成"@zh .

ex:LegalLiabilityProtection rdf:type airo:RiskControl ;
    rdfs:label "Legal Liability Protection"@en ;
    rdfs:label "法律责任保护"@zh .

