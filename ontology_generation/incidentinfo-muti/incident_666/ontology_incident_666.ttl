@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video System"@en ;
    rdfs:label "深度伪造视频系统"@zh ;
    airo:hasRisk ex:PoliticalManipulationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasAISubject ex:PresidentSandu ;
    airo:hasStakeholder ex:MoldovanGovernment .

ex:PoliticalManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Political Manipulation Risk"@en ;
    rdfs:label "政治操纵风险"@zh ;
    airo:isRiskSourceFor ex:MaliciousContentCreation ;
    airo:hasImpact ex:PublicTrustErosion .

ex:MoldovanGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Moldovan Government"@en ;
    rdfs:label "摩尔多瓦政府"@zh .

ex:PresidentSandu rdf:type airo:AISubject ;
    rdfs:label "President Sandu"@en ;
    rdfs:label "桑杜总统"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:MoldovanGovernment .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:MaliciousContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creation"@en ;
    rdfs:label "恶意内容创建"@zh .