@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepMindNHSApp rdf:type airo:AISystem ;
    rdfs:label "DeepMind NHS App"@en ;
    rdfs:label "DeepMind与NHS合作的应用"@zh ;
    airo:hasRisk ex:DataPrivacyRisk ;
    airo:isDevelopedBy ex:GoogleDeepMind ;
    airo:hasStakeholder ex:RoyalFreeNHS ;
    airo:isAppliedWithinDomain ex:Healthcare .

ex:DataPrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Data Privacy Risk"@en ;
    rdfs:label "数据隐私风险"@zh ;
    airo:isRiskSourceFor ex:DataSharingControversy .

ex:RoyalFreeNHS rdf:type airo:Stakeholder ;
    rdfs:label "Royal Free NHS Trust"@en ;
    rdfs:label "皇家自由NHS信托"@zh .

ex:GoogleDeepMind rdf:type airo:AIOperator ;
    rdfs:label "Google DeepMind"@en ;
    rdfs:label "谷歌DeepMind"@zh .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "Healthcare Domain"@en ;
    rdfs:label "医疗保健领域"@zh .

ex:DataSharingControversy rdf:type airo:RiskSource ;
    rdfs:label "Data Sharing Controversy Risk Source"@en ;
    rdfs:label "数据共享争议风险源"@zh .