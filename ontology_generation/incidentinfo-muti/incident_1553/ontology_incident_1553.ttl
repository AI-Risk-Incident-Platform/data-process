ex:OpenAIAndMetaAISystems rdf:type airo:AISystem ;
    rdfs:label "OpenAI和Meta的AI系统"@zh ;
    rdfs:label "OpenAI and Meta AI Systems"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:SarahSilverman ;
    airo:hasStakeholder ex:Meta .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:hasConsequence ex:LegalActionConsequence ;
    airo:isRiskSourceFor ex:AITrainingDataCopyrightIssues .

ex:SarahSilverman rdf:type airo:Stakeholder ;
    rdfs:label "莎拉·西尔弗曼"@zh ;
    rdfs:label "Sarah Silverman"@en .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@zh ;
    rdfs:label "Meta"@en .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "法律诉讼后果"@zh ;
    rdfs:label "Legal Action Consequence"@en ;
    airo:hasImpactOnStakeholder ex:SarahSilverman .

ex:AITrainingDataCopyrightIssues rdf:type airo:RiskSource ;
    rdfs:label "AI训练数据版权问题"@zh ;
    rdfs:label "AI Training Data Copyright Issues"@en .