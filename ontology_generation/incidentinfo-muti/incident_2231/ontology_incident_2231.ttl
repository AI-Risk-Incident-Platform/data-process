@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EmploymentAgencyAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Employment Agency Algorithm"@en ;
    rdfs:label "就业机构算法"@zh ;
    airo:hasRisk ex:DiscriminatoryAlgorithmRisk ;
    airo:isDeployedBy ex:AustrianEmploymentAgency ;
    airo:hasStakeholder ex:DataProtectionAuthority .

ex:DiscriminatoryAlgorithmRisk rdf:type airo:Risk ;
    rdfs:label "Discriminatory Algorithm Risk"@en ;
    rdfs:label "歧视性算法风险"@zh ;
    airo:hasImpact ex:DiscriminatoryImpact .

ex:DataProtectionAuthority rdf:type airo:Stakeholder ;
    rdfs:label "Data Protection Authority"@en ;
    rdfs:label "数据保护机构"@zh .

ex:AustrianEmploymentAgency rdf:type airo:AIOperator ;
    rdfs:label "Austrian Employment Agency"@en ;
    rdfs:label "奥地利就业机构"@zh .

ex:DiscriminatoryImpact rdf:type airo:Impact ;
    rdfs:label "Discriminatory Impact"@en ;
    rdfs:label "歧视性影响"@zh ;
    airo:hasImpactOnStakeholder ex:DataProtectionAuthority .