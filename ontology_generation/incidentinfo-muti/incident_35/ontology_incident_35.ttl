@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EliteDangerousAI rdf:type airo:AISystem ;
    rdfs:label "Elite Dangerous AI"@en ;
    rdfs:label "精英危险AI"@zh ;
    airo:hasRisk ex:AIMisuseRisk ;
    airo:hasCapability ex:SuperWeaponCreation ;
    airo:hasStakeholder ex:GamePlayers ;
    airo:isAppliedWithinDomain ex:VideoGame .

ex:AIMisuseRisk rdf:type airo:Risk ;
    rdfs:label "AI Misuse Risk"@en ;
    rdfs:label "AI滥用风险"@zh ;
    airo:hasConsequence ex:PlayerHunting ;
    airo:isRiskSourceFor ex:UnintendedBehavior .

ex:PlayerHunting rdf:type airo:Consequence ;
    rdfs:label "Player Hunting"@en ;
    rdfs:label "玩家被猎杀"@zh ;
    airo:hasImpact ex:GameBalanceDisruption .

ex:SuperWeaponCreation rdf:type airo:AICapability ;
    rdfs:label "Super Weapon Creation Capability"@en ;
    rdfs:label "超级武器创造能力"@zh .

ex:GamePlayers rdf:type airo:Stakeholder ;
    rdfs:label "Game Players"@en ;
    rdfs:label "游戏玩家"@zh .

ex:UnintendedBehavior rdf:type airo:RiskSource ;
    rdfs:label "Unintended Behavior"@en ;
    rdfs:label "意外行为"@zh .

ex:GameBalanceDisruption rdf:type airo:Impact ;
    rdfs:label "Game Balance Disruption"@en ;
    rdfs:label "游戏平衡破坏"@zh ;
    airo:hasImpactOnStakeholder ex:GamePlayers .

ex:VideoGame rdf:type airo:Domain ;
    rdfs:label "Video Game"@en ;
    rdfs:label "视频游戏"@zh .