@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DisneyAIArtGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Disney AI Art Generation System"@en ;
    rdfs:label "迪士尼AI艺术生成系统"@zh ;
    airo:hasRisk ex:PublicBacklashRisk ;
    airo:hasPurpose ex:HolidayArtCreation ;
    airo:hasStakeholder ex:DisneyCorporation .

ex:PublicBacklashRisk rdf:type airo:Risk ;
    rdfs:label "Public Backlash Risk"@en ;
    rdfs:label "公众强烈反对风险"@zh ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        airo:hasImpact ex:BrandReputationDamage
    ] .

ex:DisneyCorporation rdf:type airo:Stakeholder ;
    rdfs:label "Disney Corporation"@en ;
    rdfs:label "迪士尼公司"@zh .

ex:BrandReputationDamage rdf:type airo:Impact ;
    rdfs:label "Brand Reputation Damage"@en ;
    rdfs:label "品牌声誉损害"@zh ;
    airo:hasImpactOnStakeholder ex:DisneyCorporation .

ex:HolidayArtCreation rdf:type airo:Purpose ;
    rdfs:label "Holiday Art Creation"@en ;
    rdfs:label "节日艺术创作"@zh .