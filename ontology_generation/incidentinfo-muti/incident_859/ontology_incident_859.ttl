@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIImpersonationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Impersonation System"@en ;
    rdfs:label "AI冒充系统"@zh ;
    airo:hasRisk ex:HousingScamRisk ;
    airo:hasStakeholder ex:HousingAgent ;
    airo:isAppliedWithinDomain ex:RealEstate .

ex:HousingScamRisk rdf:type airo:Risk ;
    rdfs:label "Housing Scam Risk"@en ;
    rdfs:label "住房诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialFraud ;
    airo:isRiskSourceFor ex:AIImpersonation .

ex:HousingAgent rdf:type airo:Stakeholder ;
    rdfs:label "Housing Agent"@en ;
    rdfs:label "房产经纪人"@zh .

ex:AIImpersonation rdf:type airo:RiskSource ;
    rdfs:label "AI Impersonation"@en ;
    rdfs:label "AI冒充"@zh .

ex:FinancialFraud rdf:type airo:Consequence ;
    rdfs:label "Financial Fraud"@en ;
    rdfs:label "金融欺诈"@zh ;
    airo:hasImpact ex:EconomicLoss .

ex:RealEstate rdf:type airo:Domain ;
    rdfs:label "Real Estate"@en ;
    rdfs:label "房地产"@zh .

ex:EconomicLoss rdf:type airo:Impact ;
    rdfs:label "Economic Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpactOnStakeholder ex:HousingAgent .