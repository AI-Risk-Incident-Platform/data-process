@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChineseAIChatAndSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Chinese AI Chat and Surveillance System"@en ;
    rdfs:label "中国AI聊天与监控系统"@zh ;
    airo:hasRisk ex:HumanRightsViolationRisk ;
    airo:isDevelopedBy ex:ChineseAIFirm ;
    airo:hasStakeholder ex:MIT ;
    airo:hasImpactOnStakeholder ex:MIT .

ex:HumanRightsViolationRisk rdf:type airo:Risk ;
    rdfs:label "Human Rights Violation Risk"@en ;
    rdfs:label "人权侵犯风险"@zh ;
    airo:hasConsequence ex:HumanRightsConcerns .

ex:MIT rdf:type airo:Stakeholder ;
    rdfs:label "MIT"@en ;
    rdfs:label "麻省理工学院"@zh .

ex:ChineseAIFirm rdf:type airo:AIOperator ;
    rdfs:label "Chinese AI Firm"@en ;
    rdfs:label "中国AI公司"@zh .

ex:HumanRightsConcerns rdf:type airo:Impact ;
    rdfs:label "Human Rights Concerns Impact"@en ;
    rdfs:label "人权问题影响"@zh ;
    airo:hasImpactOnStakeholder ex:MIT .