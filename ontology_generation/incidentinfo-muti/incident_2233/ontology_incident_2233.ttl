@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UnemploymentScoringSystem rdf:type airo:AISystem ;
    rdfs:label "Unemployment Scoring System"@en ;
    rdfs:label "失业评分系统"@zh ;
    airo:hasRisk ex:ControversialScoringRisk ;
    airo:hasStakeholder ex:PolishGovernment ;
    airo:isAppliedWithinDomain ex:UnemploymentServices .

ex:ControversialScoringRisk rdf:type airo:Risk ;
    rdfs:label "Controversial Scoring Risk"@en ;
    rdfs:label "争议性评分风险"@zh .

ex:PolishGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Polish Government"@en ;
    rdfs:label "波兰政府"@zh .

ex:UnemploymentServices rdf:type airo:Domain ;
    rdfs:label "Unemployment Services Domain"@en ;
    rdfs:label "失业服务领域"@zh .