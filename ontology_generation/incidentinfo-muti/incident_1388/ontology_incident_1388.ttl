@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LeadingChatbots rdf:type airo:AISystem ;
    rdfs:label "Leading Chatbots"@en ;
    rdfs:label "主流聊天机器人"@zh ;
    airo:hasRisk ex:SpreadOfRussianPropaganda ;
    airo:hasCapability ex:InformationDissemination ;
    airo:hasStakeholder ex:GeneralPublic .

ex:SpreadOfRussianPropaganda rdf:type airo:Risk ;
    rdfs:label "Spread of Russian Propaganda Risk"@en ;
    rdfs:label "传播俄罗斯宣传的风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:RussianMisinformation .

ex:InformationDissemination rdf:type airo:AICapability ;
    rdfs:label "Information Dissemination Capability"@en ;
    rdfs:label "信息传播能力"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:PublicOpinionManipulation .

ex:PublicOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "Public Opinion Manipulation Impact"@en ;
    rdfs:label "公众舆论操纵影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:RussianMisinformation rdf:type airo:RiskSource ;
    rdfs:label "Russian Misinformation Risk Source"@en ;
    rdfs:label "俄罗斯错误信息风险源"@zh .