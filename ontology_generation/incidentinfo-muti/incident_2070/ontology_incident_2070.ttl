@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlgorithmicMortgageSystem rdf:type airo:AISystem ;
    rdfs:label "Algorithmic Mortgage System"@en ;
    rdfs:label "算法抵押贷款系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInMortgageApproval ;
    airo:hasStakeholder ex:MortgageApplicants ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:AlgorithmicBiasInMortgageApproval rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Mortgage Approval Risk"@en ;
    rdfs:label "抵押贷款审批中的算法偏见风险"@zh ;
    airo:isRiskSourceFor ex:LackOfAlgorithmicTransparency .

ex:MortgageApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Mortgage Applicants"@en ;
    rdfs:label "抵押贷款申请人"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:LackOfAlgorithmicTransparency rdf:type airo:RiskSource ;
    rdfs:label "Lack of Algorithmic Transparency"@en ;
    rdfs:label "算法缺乏透明度"@zh .