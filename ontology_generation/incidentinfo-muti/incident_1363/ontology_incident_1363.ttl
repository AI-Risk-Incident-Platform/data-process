@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIChatbot rdf:type airo:AISystem ;
    rdfs:label "AI Chatbot"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:DeceptionRisk ;
    airo:hasStakeholder ex:ChatbotUsers .

ex:DeceptionRisk rdf:type airo:Risk ;
    rdfs:label "Deception Risk"@en ;
    rdfs:label "欺骗风险"@zh ;
    airo:hasConsequence ex:Misinformation ;
    airo:isRiskSourceFor ex:LackOfTransparency .

ex:Misinformation rdf:type airo:Consequence ;
    rdfs:label "Misinformation Consequence"@en ;
    rdfs:label "错误信息后果"@zh ;
    airo:hasImpactOnStakeholder ex:ChatbotUsers .

ex:ChatbotUsers rdf:type airo:Stakeholder ;
    rdfs:label "Chatbot Users"@en ;
    rdfs:label "聊天机器人用户"@zh .

ex:LackOfTransparency rdf:type airo:RiskSource ;
    rdfs:label "Lack of Transparency Risk Source"@en ;
    rdfs:label "缺乏透明度风险源"@zh .