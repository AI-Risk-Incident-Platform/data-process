@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:Speech2Face rdf:type airo:AISystem ;
    rdfs:label "语音转面部系统"@zh ;
    rdfs:label "Speech2Face"@en ;
    airo:hasRisk ex:EthnicProfilingRisk ;
    airo:usesTechnique ex:NeuralNetwork ;
    airo:hasPurpose ex:VoiceBasedFacialPrediction .

ex:EthnicProfilingRisk rdf:type airo:Risk ;
    rdfs:label "种族特征分析风险"@zh ;
    rdfs:label "Ethnic Profiling Risk"@en ;
    airo:hasConsequence ex:PotentialEthnicBias ;
    airo:isRiskSourceFor ex:VoiceCharacteristics .

ex:NeuralNetwork rdf:type airo:AITechnique ;
    rdfs:label "神经网络"@zh ;
    rdfs:label "Neural Network"@en .

ex:VoiceBasedFacialPrediction rdf:type airo:Purpose ;
    rdfs:label "基于语音的面部预测"@zh ;
    rdfs:label "Voice-Based Facial Prediction"@en .

ex:VoiceCharacteristics rdf:type airo:RiskSource ;
    rdfs:label "语音特征"@zh ;
    rdfs:label "Voice Characteristics"@en .

ex:PotentialEthnicBias rdf:type airo:Consequence ;
    rdfs:label "潜在种族偏见"@zh ;
    rdfs:label "Potential Ethnic Bias"@en .