@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIWorkAssistant rdf:type airo:AISystem ;
    rdfs:label "AI Work Assistant"@en ;
    rdfs:label "AI工作助手"@zh ;
    airo:hasRisk ex:OverRelianceOnHumanAssistanceRisk ;
    airo:hasStakeholder ex:AIUsersInWorkplace ;
    airo:hasPurpose ex:WorkTaskAutomation .

ex:OverRelianceOnHumanAssistanceRisk rdf:type airo:Risk ;
    rdfs:label "Over Reliance on Human Assistance Risk"@en ;
    rdfs:label "过度依赖人工协助风险"@zh ;
    airo:hasConsequence ex:ReducedWorkEfficiency .

ex:AIUsersInWorkplace rdf:type airo:Stakeholder ;
    rdfs:label "AI Users in Workplace"@en ;
    rdfs:label "工作场所AI使用者"@zh .

ex:ReducedWorkEfficiency rdf:type airo:Consequence ;
    rdfs:label "Reduced Work Efficiency Consequence"@en ;
    rdfs:label "工作效率降低后果"@zh ;
    airo:hasImpactOnStakeholder ex:AIUsersInWorkplace .

ex:WorkTaskAutomation rdf:type airo:Purpose ;
    rdfs:label "Work Task Automation Purpose"@en ;
    rdfs:label "工作任务自动化目的"@zh .