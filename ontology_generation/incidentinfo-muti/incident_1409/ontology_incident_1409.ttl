@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh ;
    airo:hasRisk ex:MisidentificationRisk ;
    airo:hasAISubject ex:AntiKnifeActivist .

ex:MisidentificationRisk rdf:type airo:Risk ;
    rdfs:label "Misidentification Risk"@en ;
    rdfs:label "误识别风险"@zh ;
    airo:hasConsequence ex:WrongfulDetention ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:WrongfulDetention rdf:type airo:Consequence ;
    rdfs:label "Wrongful Detention Consequence"@en ;
    rdfs:label "错误拘留后果"@zh ;
    airo:hasImpact ex:LegalChallenges .

ex:AntiKnifeActivist rdf:type airo:AISubject ;
    rdfs:label "Anti-Knife Activist"@en ;
    rdfs:label "反刀具活动家"@zh .

ex:LegalChallenges rdf:type airo:Impact ;
    rdfs:label "Legal Challenges Impact"@en ;
    rdfs:label "法律挑战影响"@zh ;
    airo:hasImpactOnStakeholder ex:AntiKnifeActivist .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .