@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OfqualAlevelAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Ofqual A-level Algorithm"@en ;
    rdfs:label "Ofqual A-level算法"@zh ;
    airo:hasRisk ex:AlgorithmicGradingFailureRisk ;
    airo:hasPurpose ex:AutomatedExamGrading ;
    airo:isAppliedWithinDomain ex:EducationAssessment ;
    airo:hasStakeholder ex:UKStudents ;
    airo:isDeployedBy ex:Ofqual .

ex:AlgorithmicGradingFailureRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Grading Failure Risk"@en ;
    rdfs:label "算法评分失败风险"@zh ;
    airo:hasConsequence ex:UnfairGradeAllocation .

ex:UnfairGradeAllocation rdf:type airo:Consequence ;
    rdfs:label "Unfair Grade Allocation Consequence"@en ;
    rdfs:label "不公平成绩分配后果"@zh ;
    airo:hasImpact ex:StudentFutureOpportunitiesAffected .

ex:UKStudents rdf:type airo:Stakeholder ;
    rdfs:label "UK Students"@en ;
    rdfs:label "英国学生"@zh ;
    airo:hasImpactOnStakeholder ex:StudentFutureOpportunitiesAffected .

ex:Ofqual rdf:type airo:AIOperator ;
    rdfs:label "Ofqual"@en ;
    rdfs:label "Ofqual"@zh .

ex:AutomatedExamGrading rdf:type airo:Purpose ;
    rdfs:label "Automated Exam Grading Purpose"@en ;
    rdfs:label "自动化考试评分目的"@zh .

ex:EducationAssessment rdf:type airo:Domain ;
    rdfs:label "Education Assessment Domain"@en ;
    rdfs:label "教育评估领域"@zh .

ex:StudentFutureOpportunitiesAffected rdf:type airo:Impact ;
    rdfs:label "Student Future Opportunities Affected Impact"@en ;
    rdfs:label "学生未来机会受影响的影响"@zh .