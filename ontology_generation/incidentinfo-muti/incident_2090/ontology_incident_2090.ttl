@base <https://w3id.org/airo#> .

ex:FaceTrackingAdTabletsInUbers rdf:type airo:AISystem ;
    rdfs:label "Face-Tracking Ad Tablets in Ubers"@en ;
    rdfs:label "优步车内的面部追踪广告平板"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:Uber ;
    airo:hasStakeholder ex:USSenators ;
    airo:usesTechnique ex:FaceTrackingTechnology .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PassengerPrivacyConcerns .

ex:USSenators rdf:type airo:Stakeholder ;
    rdfs:label "US Senators"@en ;
    rdfs:label "美国参议员"@zh .

ex:Uber rdf:type airo:AIOperator ;
    rdfs:label "Uber"@en ;
    rdfs:label "优步"@zh .

ex:FaceTrackingTechnology rdf:type airo:AITechnique ;
    rdfs:label "Face-Tracking Technology"@en ;
    rdfs:label "面部追踪技术"@zh .

ex:PassengerPrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "Passenger Privacy Concerns"@en ;
    rdfs:label "乘客隐私担忧"@zh ;
    airo:hasImpactOnStakeholder ex:USSenators .