@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:RacialBiasRisk ;
    airo:hasStakeholder ex:SchoolDistrict .

ex:RacialBiasRisk rdf:type airo:Risk ;
    rdfs:label "Racial Bias Risk"@en ;
    rdfs:label "种族偏见风险"@zh ;
    airo:isRiskSourceFor ex:MisrepresentationOfTechnology ;
    airo:hasConsequence ex:DiscriminatoryImpact .

ex:SchoolDistrict rdf:type airo:Stakeholder ;
    rdfs:label "School District"@en ;
    rdfs:label "学区"@zh ;
    airo:hasImpactOnStakeholder ex:DiscriminatoryImpact .

ex:MisrepresentationOfTechnology rdf:type airo:RiskSource ;
    rdfs:label "Misrepresentation of Technology"@en ;
    rdfs:label "技术虚假陈述"@zh .

ex:DiscriminatoryImpact rdf:type airo:Impact ;
    rdfs:label "Discriminatory Impact"@en ;
    rdfs:label "歧视性影响"@zh .