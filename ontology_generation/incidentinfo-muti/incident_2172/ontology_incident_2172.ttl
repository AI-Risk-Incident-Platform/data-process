@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilot rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot"@en ;
    airo:hasRisk ex:AutonomousVehicleHackingRisk ;
    airo:hasCapability ex:AutonomousDrivingCapability ;
    airo:usesTechnique ex:ComputerVisionBasedAutonomousDriving ;
    airo:hasStakeholder ex:TeslaResearchers .

ex:AutonomousVehicleHackingRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆被黑客攻击风险"@zh ;
    rdfs:label "Autonomous Vehicle Hacking Risk"@en ;
    airo:hasConsequence ex:VehicleSteeringIntoOncomingTraffic ;
    airo:isRiskSourceFor ex:AutopilotSystemVulnerability ;
    airo:isRiskSourceFor ex:AutonomousVehicleHackingThreat .

ex:VehicleSteeringIntoOncomingTraffic rdf:type airo:Consequence ;
    rdfs:label "车辆驶入对向车道的后果"@zh ;
    rdfs:label "Vehicle Steering Into Oncoming Traffic Consequence"@en ;
    airo:hasImpact ex:TrafficSafetyThreat .

ex:AutonomousDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:TeslaResearchers rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉研究人员"@zh ;
    rdfs:label "Tesla Researchers"@en .

ex:AutopilotSystemVulnerability rdf:type airo:RiskSource ;
    rdfs:label "自动驾驶系统漏洞风险源"@zh ;
    rdfs:label "Autopilot System Vulnerability Risk Source"@en .

ex:TrafficSafetyThreat rdf:type airo:Impact ;
    rdfs:label "交通安全威胁的影响"@zh ;
    rdfs:label "Traffic Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaResearchers .

ex:ComputerVisionBasedAutonomousDriving rdf:type airo:AITechnique ;
    rdfs:label "基于计算机视觉的自动驾驶技术"@zh ;
    rdfs:label "Computer Vision Based Autonomous Driving Technique"@en .

ex:AutonomousVehicleHackingThreat rdf:type airo:Threat ;
    rdfs:label "自动驾驶车辆被黑客攻击的威胁"@zh ;
    rdfs:label "Autonomous Vehicle Hacking Threat"@en .