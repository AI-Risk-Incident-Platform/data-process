@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:FalseArrestRisk ;
    airo:hasAISubject ex:InnocentIndividuals ;
    airo:hasStakeholder ex:LawEnforcementAgencies .

ex:FalseArrestRisk rdf:type airo:Risk ;
    rdfs:label "False Arrest Risk"@en ;
    rdfs:label "错误逮捕风险"@zh ;
    airo:hasConsequence ex:WrongfulAccusation ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:WrongfulAccusation rdf:type airo:Consequence ;
    rdfs:label "Wrongful Accusation Consequence"@en ;
    rdfs:label "错误指控后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:InnocentIndividuals rdf:type airo:AISubject ;
    rdfs:label "Innocent Individuals"@en ;
    rdfs:label "无辜个体"@zh .

ex:LawEnforcementAgencies rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement Agencies"@en ;
    rdfs:label "执法机构"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "名誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:InnocentIndividuals .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh ;
    airo:exploitsVulnerability ex:RacialBiasInTrainingData .

ex:RacialBiasInTrainingData rdf:type airo:Vulnerability ;
    rdfs:label "Racial Bias in Training Data Vulnerability"@en ;
    rdfs:label "训练数据中的种族偏见脆弱性"@zh .