@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystemInRio rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System in Rio"@en ;
    rdfs:label "里约热内卢面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:RealTimeFacialRecognition ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasStakeholder ex:OiTelecommunications .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalDataExposure .

ex:OiTelecommunications rdf:type airo:Stakeholder ;
    rdfs:label "Oi Telecommunications"@en ;
    rdfs:label "Oi电信公司"@zh .

ex:PersonalDataExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Data Exposure Consequence"@en ;
    rdfs:label "个人数据暴露后果"@zh ;
    airo:hasImpact ex:CivilLibertiesInfringement .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:RealTimeFacialRecognition rdf:type airo:AITechnique ;
    rdfs:label "Real-Time Facial Recognition Technique"@en ;
    rdfs:label "实时面部识别技术"@zh .

ex:CivilLibertiesInfringement rdf:type airo:Impact ;
    rdfs:label "Civil Liberties Infringement Impact"@en ;
    rdfs:label "公民自由权侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:OiTelecommunications .