@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NaverSearchAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Naver Search Algorithm"@en ;
    rdfs:label "Naver搜索算法"@zh ;
    airo:hasRisk ex:AlgorithmManipulationRisk ;
    airo:isDeployedBy ex:NaverCorporation ;
    airo:isAppliedWithinDomain ex:SearchEngineMarket .

ex:AlgorithmManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Algorithm Manipulation Risk"@en ;
    rdfs:label "算法操纵风险"@zh ;
    airo:hasConsequence ex:UnfairCompetition ;
    airo:isRiskSourceFor ex:IntentionalAlgorithmManipulation .

ex:NaverCorporation rdf:type airo:AIOperator ;
    rdfs:label "Naver Corporation"@en ;
    rdfs:label "Naver公司"@zh .

ex:UnfairCompetition rdf:type airo:Consequence ;
    rdfs:label "Unfair Competition Consequence"@en ;
    rdfs:label "不公平竞争后果"@zh ;
    airo:hasImpact ex:MarketDistortion .

ex:MarketDistortion rdf:type airo:Impact ;
    rdfs:label "Market Distortion Impact"@en ;
    rdfs:label "市场扭曲影响"@zh .

ex:IntentionalAlgorithmManipulation rdf:type airo:RiskSource ;
    rdfs:label "Intentional Algorithm Manipulation Risk Source"@en ;
    rdfs:label "故意算法操纵风险源"@zh .

ex:SearchEngineMarket rdf:type airo:Domain ;
    rdfs:label "Search Engine Market Domain"@en ;
    rdfs:label "搜索引擎市场领域"@zh .