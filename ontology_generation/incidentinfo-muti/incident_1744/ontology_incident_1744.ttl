@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ProjectNessieAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Project Nessie Algorithm"@en ;
    rdfs:label "Project Nessie算法"@zh ;
    airo:hasRisk ex:PriceGougingRisk ;
    airo:hasStakeholder ex:Amazon ;
    airo:isAppliedWithinDomain ex:ECommerce .

ex:PriceGougingRisk rdf:type airo:Risk ;
    rdfs:label "Price Gouging Risk"@en ;
    rdfs:label "价格欺诈风险"@zh ;
    airo:hasConsequence ex:ConsumerFinancialLoss ;
    airo:isRiskSourceFor ex:AlgorithmicPricingManipulation .

ex:Amazon rdf:type airo:Stakeholder ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:ECommerce rdf:type airo:Domain ;
    rdfs:label "E-Commerce"@en ;
    rdfs:label "电子商务"@zh .

ex:ConsumerFinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Consumer Financial Loss"@en ;
    rdfs:label "消费者财务损失"@zh ;
    airo:hasImpact ex:MarketTrustErosion .

ex:MarketTrustErosion rdf:type airo:Impact ;
    rdfs:label "Market Trust Erosion"@en ;
    rdfs:label "市场信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:Amazon .

ex:AlgorithmicPricingManipulation rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Pricing Manipulation"@en ;
    rdfs:label "算法定价操纵"@zh .