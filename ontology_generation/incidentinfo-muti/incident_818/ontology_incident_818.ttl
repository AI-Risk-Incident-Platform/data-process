@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RadishRunAutonomousTaxi rdf:type airo:AISystem ;
    rdfs:label "Radish Run Autonomous Taxi"@en ;
    rdfs:label "萝卜快跑无人驾驶出租车"@zh ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasCapability ex:AutonomousDriving ;
    airo:isDeployedBy ex:RadishRunOfficial ;
    airo:isAppliedWithinDomain ex:Transportation .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    airo:hasConsequence ex:VehicleDamage .

ex:VehicleDamage rdf:type airo:Consequence ;
    rdfs:label "Vehicle Damage Consequence"@en ;
    rdfs:label "车辆损坏后果"@zh ;
    airo:hasImpact ex:PublicSafetyConcern .

ex:AutonomousDriving rdf:type airo:AICapability ;
    rdfs:label "Autonomous Driving Capability"@en ;
    rdfs:label "自动驾驶能力"@zh .

ex:RadishRunOfficial rdf:type airo:AIOperator ;
    rdfs:label "Radish Run Official"@en ;
    rdfs:label "萝卜快跑官方"@zh .

ex:Transportation rdf:type airo:Domain ;
    rdfs:label "Transportation Domain"@en ;
    rdfs:label "交通运输领域"@zh .

ex:PublicSafetyConcern rdf:type airo:Impact ;
    rdfs:label "Public Safety Concern Impact"@en ;
    rdfs:label "公共安全担忧影响"@zh .