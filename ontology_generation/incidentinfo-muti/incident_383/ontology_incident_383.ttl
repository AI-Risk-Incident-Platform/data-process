@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GMAutonomousCar rdf:type airo:AISystem ;
    rdfs:label "GM Autonomous Car"@en ;
    rdfs:label "通用汽车自动驾驶汽车"@zh ;
    airo:hasRisk ex:EmergencyResponseObstructionRisk ;
    airo:isDeployedBy ex:CruiseLLC ;
    airo:isAppliedWithinDomain ex:UrbanTransportation .

ex:EmergencyResponseObstructionRisk rdf:type airo:Risk ;
    rdfs:label "Emergency Response Obstruction Risk"@en ;
    rdfs:label "紧急响应受阻风险"@zh ;
    airo:hasConsequence ex:DelayedEmergencyResponse .

ex:DelayedEmergencyResponse rdf:type airo:Consequence ;
    rdfs:label "Delayed Emergency Response Consequence"@en ;
    rdfs:label "紧急响应延迟后果"@zh ;
    airo:hasImpact ex:PublicSafetyCompromise .

ex:CruiseLLC rdf:type airo:AIOperator ;
    rdfs:label "Cruise LLC"@en ;
    rdfs:label "Cruise有限责任公司"@zh .

ex:UrbanTransportation rdf:type airo:Domain ;
    rdfs:label "Urban Transportation Domain"@en ;
    rdfs:label "城市交通领域"@zh .

ex:PublicSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Public Safety Compromise Impact"@en ;
    rdfs:label "公共安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:CruiseLLC .