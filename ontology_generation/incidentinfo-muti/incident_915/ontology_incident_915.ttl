@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIAidedScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI Aided Scam System"@en ;
    rdfs:label "AI辅助诈骗系统"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasStakeholder ex:BeautyQueenVictim ;
    airo:isDeployedBy ex:ScamPerpetrators .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLossConsequence .

ex:BeautyQueenVictim rdf:type airo:Stakeholder ;
    rdfs:label "Beauty Queen Victim"@en ;
    rdfs:label "选美皇后受害者"@zh .

ex:FinancialLossConsequence rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpactOnStakeholder ex:BeautyQueenVictim .

ex:ScamPerpetrators rdf:type airo:AIOperator ;
    rdfs:label "Scam Perpetrators"@en ;
    rdfs:label "诈骗实施者"@zh .