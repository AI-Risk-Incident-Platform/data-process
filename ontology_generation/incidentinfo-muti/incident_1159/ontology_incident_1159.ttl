@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberSelfDrivingCar rdf:type airo:AISystem ;
    rdfs:label "Uber Self-Driving Car"@en ;
    rdfs:label "优步自动驾驶汽车"@zh ;
    airo:hasRisk ex:TrafficViolationRisk ;
    airo:isDeployedBy ex:UberTechnologies ;
    airo:isAppliedWithinDomain ex:AutonomousVehicles ;
    airo:hasStakeholder ex:UberTechnologies .

ex:TrafficViolationRisk rdf:type airo:Risk ;
    rdfs:label "Traffic Violation Risk"@en ;
    rdfs:label "交通违规风险"@zh ;
    airo:hasConsequence ex:PotentialAccident ;
    airo:isRiskSourceFor ex:SystemMalfunction .

ex:PotentialAccident rdf:type airo:Consequence ;
    rdfs:label "Potential Accident Consequence"@en ;
    rdfs:label "潜在事故后果"@zh ;
    airo:hasImpact ex:AccidentImpact .

ex:UberTechnologies rdf:type airo:AIOperator ;
    rdfs:label "Uber Technologies"@en ;
    rdfs:label "优步科技"@zh .

ex:AutonomousVehicles rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicles Domain"@en ;
    rdfs:label "自动驾驶汽车领域"@zh .

ex:SystemMalfunction rdf:type airo:RiskSource ;
    rdfs:label "System Malfunction Risk Source"@en ;
    rdfs:label "系统故障风险源"@zh .

ex:AccidentImpact rdf:type airo:Impact ;
    rdfs:label "Accident Impact"@en ;
    rdfs:label "事故影响"@zh ;
    airo:hasImpactOnStakeholder ex:UberTechnologies .