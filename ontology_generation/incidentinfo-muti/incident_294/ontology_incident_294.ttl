@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Autopilot System"@en ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    airo:hasRisk ex:AutopilotMalfunctionRisk ;
    airo:isDevelopedBy ex:TeslaInc ;
    airo:hasAISubject ex:TeslaModel3Driver .

ex:AutopilotMalfunctionRisk rdf:type airo:Risk ;
    rdfs:label "Autopilot Malfunction Risk"@en ;
    rdfs:label "自动驾驶故障风险"@zh ;
    airo:hasConsequence ex:VehicleCrash .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:TeslaModel3Driver rdf:type airo:AISubject ;
    rdfs:label "Tesla Model 3 Driver"@en ;
    rdfs:label "特斯拉Model 3驾驶员"@zh .

ex:VehicleCrash rdf:type airo:Consequence ;
    rdfs:label "Vehicle Crash Consequence"@en ;
    rdfs:label "车辆碰撞后果"@zh .