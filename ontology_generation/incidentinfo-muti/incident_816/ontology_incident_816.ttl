@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ProKYCDeepfakeTool rdf:type airo:AISystem ;
    rdfs:label "ProKYC Deepfake Tool"@en ;
    rdfs:label "ProKYC深度伪造工具"@zh ;
    airo:hasRisk ex:AccountFraudRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:hasPurpose ex:IdentityVerificationBypass ;
    airo:hasStakeholder ex:FinancialInstitutions ;
    airo:isAppliedWithinDomain ex:FinancialSecurity .

ex:AccountFraudRisk rdf:type airo:Risk ;
    rdfs:label "Account Fraud Risk"@en ;
    rdfs:label "账户欺诈风险"@zh ;
    airo:isRiskSourceFor ex:DeepfakeTechnology ;
    airo:hasConsequence ex:FinancialLoss .

ex:DeepfakeTechnology rdf:type airo:RiskSource ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpact ex:TrustErosion .

ex:FinancialInstitutions rdf:type airo:Stakeholder ;
    rdfs:label "Financial Institutions"@en ;
    rdfs:label "金融机构"@zh .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:IdentityVerificationBypass rdf:type airo:Purpose ;
    rdfs:label "Identity Verification Bypass"@en ;
    rdfs:label "身份验证绕过"@zh .

ex:FinancialSecurity rdf:type airo:Domain ;
    rdfs:label "Financial Security Domain"@en ;
    rdfs:label "金融安全领域"@zh .

ex:TrustErosion rdf:type airo:Impact ;
    rdfs:label "Trust Erosion"@en ;
    rdfs:label "信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:FinancialInstitutions .