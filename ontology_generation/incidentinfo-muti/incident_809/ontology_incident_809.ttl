ex:AIGeneratedFakeNudes rdf:type airo:AISystem ;
    rdfs:label "AI Generated Fake Nudes"@en ;
    rdfs:label "AI生成虚假裸照"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:SchoolGirls .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PsychologicalHarmToVictims ;
    airo:isRiskSourceFor ex:DeepfakeTechnology ;
    airo:hasVulnerability ex:LackOfLegalRecourse .

ex:PsychologicalHarmToVictims rdf:type airo:Consequence ;
    rdfs:label "Psychological Harm to Victims"@en ;
    rdfs:label "受害者心理伤害"@zh ;
    airo:hasImpact ex:SocialStigma .

ex:SchoolGirls rdf:type airo:AISubject ;
    rdfs:label "School Girls"@en ;
    rdfs:label "女学生"@zh .

ex:SocialStigma rdf:type airo:Impact ;
    rdfs:label "Social Stigma"@en ;
    rdfs:label "社会污名"@zh ;
    airo:hasImpactOnStakeholder ex:SchoolGirls .

ex:DeepfakeTechnology rdf:type airo:RiskSource ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:LackOfLegalRecourse rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Legal Recourse"@en ;
    rdfs:label "缺乏法律救济"@zh .