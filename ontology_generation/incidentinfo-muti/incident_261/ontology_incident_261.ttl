@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FederalDenaturalizationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "联邦撤销国籍算法"@zh ;
    rdfs:label "Federal Denaturalization Algorithm"@en ;
    airo:hasRisk ex:UnjustCitizenshipRevocationRisk ;
    airo:hasStakeholder ex:USGovernment ;
    airo:hasAISubject ex:AmericanCitizens ;
    airo:isAppliedWithinDomain ex:ImmigrationEnforcement .

ex:UnjustCitizenshipRevocationRisk rdf:type airo:Risk ;
    rdfs:label "不公正撤销国籍风险"@zh ;
    rdfs:label "Unjust Citizenship Revocation Risk"@en ;
    airo:hasImpact ex:LossOfCitizenshipRights .

ex:USGovernment rdf:type airo:Stakeholder ;
    rdfs:label "美国政府"@zh ;
    rdfs:label "US Government"@en .

ex:AmericanCitizens rdf:type airo:AISubject ;
    rdfs:label "美国公民"@zh ;
    rdfs:label "American Citizens"@en .

ex:LossOfCitizenshipRights rdf:type airo:Impact ;
    rdfs:label "公民权利丧失影响"@zh ;
    rdfs:label "Loss of Citizenship Rights Impact"@en ;
    airo:hasImpactOnStakeholder ex:AmericanCitizens .

ex:ImmigrationEnforcement rdf:type airo:Domain ;
    rdfs:label "移民执法领域"@zh ;
    rdfs:label "Immigration Enforcement Domain"@en .