@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PersonalVoiceAssistants rdf:type airo:AISystem ;
    rdfs:label "Personal Voice Assistants"@en ;
    rdfs:label "个人语音助手"@zh ;
    airo:hasRisk ex:RacialBiasInSpeechRecognition ;
    airo:hasAISubject ex:BlackVoiceUsers ;
    airo:hasCapability ex:AutomatedSpeechRecognition .

ex:RacialBiasInSpeechRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Speech Recognition Risk"@en ;
    rdfs:label "语音识别中的种族偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:DiscriminationAgainstBlackUsers .

ex:AutomatedSpeechRecognition rdf:type airo:AICapability ;
    rdfs:label "Automated Speech Recognition Capability"@en ;
    rdfs:label "自动语音识别能力"@zh .

ex:DiscriminationAgainstBlackUsers rdf:type airo:Impact ;
    rdfs:label "Discrimination Against Black Users Impact"@en ;
    rdfs:label "对黑人用户的歧视影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:BlackVoiceUsers rdf:type airo:AISubject ;
    rdfs:label "Black Voice Users"@en ;
    rdfs:label "黑人语音用户"@zh .