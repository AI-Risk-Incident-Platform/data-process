@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HealthcareDecisionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "医疗决策算法"@zh ;
    rdfs:label "Healthcare Decision Algorithm"@en ;
    airo:hasRisk ex:MedicalRacismRisk ;
    airo:hasStakeholder ex:Patients .

ex:MedicalRacismRisk rdf:type airo:Risk ;
    rdfs:label "医疗种族主义风险"@zh ;
    rdfs:label "Medical Racism Risk"@en ;
    airo:hasConsequence ex:HealthCareDisparity ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:HealthCareDisparity rdf:type airo:Consequence ;
    rdfs:label "医疗保健差异"@zh ;
    rdfs:label "Health Care Disparity"@en ;
    airo:hasImpact ex:ReducedAccessToCare .

ex:ReducedAccessToCare rdf:type airo:Impact ;
    rdfs:label "减少获得医疗服务的机会"@zh ;
    rdfs:label "Reduced Access to Care"@en ;
    airo:hasImpactOnStakeholder ex:Patients .

ex:Patients rdf:type airo:Stakeholder ;
    rdfs:label "患者"@zh ;
    rdfs:label "Patients"@en .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "有偏见的训练数据"@zh ;
    rdfs:label "Biased Training Data"@en .