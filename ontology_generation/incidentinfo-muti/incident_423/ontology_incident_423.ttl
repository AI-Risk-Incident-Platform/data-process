@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:LawyersBannedFromEvents ;
    airo:hasPurpose ex:SecurityAndAccessControl ;
    airo:isAppliedWithinDomain ex:EntertainmentVenues ;
    airo:isDeployedBy ex:MadisonSquareGarden .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:UnjustExclusionFromEvents .

ex:MadisonSquareGarden rdf:type airo:AIOperator ;
    rdfs:label "Madison Square Garden"@en ;
    rdfs:label "麦迪逊广场花园"@zh .

ex:LawyersBannedFromEvents rdf:type airo:Stakeholder ;
    rdfs:label "Lawyers Banned From Events"@en ;
    rdfs:label "被禁止参加活动的律师"@zh .

ex:UnjustExclusionFromEvents rdf:type airo:Consequence ;
    rdfs:label "Unjust Exclusion From Events"@en ;
    rdfs:label "不公正地被排除在活动之外"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:SecurityAndAccessControl rdf:type airo:Purpose ;
    rdfs:label "Security and Access Control"@en ;
    rdfs:label "安全和访问控制"@zh .

ex:EntertainmentVenues rdf:type airo:Domain ;
    rdfs:label "Entertainment Venues"@en ;
    rdfs:label "娱乐场所"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage"@en ;
    rdfs:label "声誉损害"@zh ;
    airo:hasImpactOnStakeholder ex:LawyersBannedFromEvents .