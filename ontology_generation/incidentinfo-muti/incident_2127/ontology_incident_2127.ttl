@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceReadingAI rdf:type airo:AISystem ;
    rdfs:label "Face Reading AI"@en ;
    rdfs:label "面部识别AI"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasCapability ex:PoliticalOrientationPrediction ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:StanfordUniversity .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalPrivacyExposure ;
    airo:isRiskSourceFor ex:BiometricDataCollection .

ex:PoliticalOrientationPrediction rdf:type airo:AICapability ;
    rdfs:label "Political Orientation Prediction Capability"@en ;
    rdfs:label "政治倾向预测能力"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:StanfordUniversity rdf:type airo:Stakeholder ;
    rdfs:label "Stanford University"@en ;
    rdfs:label "斯坦福大学"@zh .

ex:PersonalPrivacyExposure rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Exposure Impact"@en ;
    rdfs:label "个人隐私暴露影响"@zh .

ex:BiometricDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Biometric Data Collection Risk Source"@en ;
    rdfs:label "生物特征数据收集风险源"@zh .