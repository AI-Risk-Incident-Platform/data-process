@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NissanAutomaticEmergencyBrakingSystem rdf:type airo:AISystem ;
    rdfs:label "Nissan Automatic Emergency Braking System"@en ;
    rdfs:label "日产自动紧急制动系统"@zh ;
    airo:hasRisk ex:SafetyRiskInAutonomousBraking ;
    airo:hasStakeholder ex:NissanVehicleOwners ;
    airo:isDeployedBy ex:NissanMotorCompany ;
    airo:isAppliedWithinDomain ex:AutomotiveSafetySystems .

ex:SafetyRiskInAutonomousBraking rdf:type airo:Risk ;
    rdfs:label "Safety Risk in Autonomous Braking"@en ;
    rdfs:label "自动驾驶制动安全风险"@zh ;
    airo:hasConsequence ex:PotentialAccidentsDueToSystemFailure .

ex:PotentialAccidentsDueToSystemFailure rdf:type airo:Consequence ;
    rdfs:label "Potential Accidents Due to System Failure Consequence"@en ;
    rdfs:label "系统故障导致的潜在事故后果"@zh ;
    airo:hasImpactOnStakeholder ex:NissanVehicleOwners .

ex:NissanVehicleOwners rdf:type airo:Stakeholder ;
    rdfs:label "Nissan Vehicle Owners"@en ;
    rdfs:label "日产车主"@zh .

ex:NissanMotorCompany rdf:type airo:AIOperator ;
    rdfs:label "Nissan Motor Company"@en ;
    rdfs:label "日产汽车公司"@zh .

ex:AutomotiveSafetySystems rdf:type airo:Domain ;
    rdfs:label "Automotive Safety Systems Domain"@en ;
    rdfs:label "汽车安全系统领域"@zh .