@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:Qihoo360SmartwatchAI rdf:type airo:AISystem ;
    rdfs:label "Qihoo 360 Smartwatch AI"@en ;
    rdfs:label "奇虎360智能手表AI"@zh ;
    airo:hasRisk ex:RacialDiscriminationRisk ;
    airo:isDeployedBy ex:Qihoo360 .

ex:RacialDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Racial Discrimination Risk"@en ;
    rdfs:label "种族歧视风险"@zh ;
    airo:hasConsequence ex:PublicOutrageAndReputationDamage ;
    airo:isRiskSourceFor ex:AIHallucination .

ex:PublicOutrageAndReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Public Outrage and Reputation Damage"@en ;
    rdfs:label "公众愤怒与声誉损害"@zh ;
    airo:hasImpact ex:SocialHarmonyDisruption .

ex:Qihoo360 rdf:type airo:AIOperator ;
    rdfs:label "Qihoo 360"@en ;
    rdfs:label "奇虎360"@zh .

ex:AIHallucination rdf:type airo:RiskSource ;
    rdfs:label "AI Hallucination"@en ;
    rdfs:label "AI幻觉"@zh .

ex:SocialHarmonyDisruption rdf:type airo:Impact ;
    rdfs:label "Social Harmony Disruption"@en ;
    rdfs:label "社会和谐破坏"@zh ;
    airo:hasImpactOnStakeholder ex:Qihoo360 .