@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MachineLearningInMedicine rdf:type airo:AISystem ;
    rdfs:label "Machine Learning in Medicine"@en ;
    rdfs:label "医学中的机器学习"@zh ;
    airo:hasRisk ex:CredibilityCrisisRisk ;
    airo:isAppliedWithinDomain ex:MedicalDomain .

ex:CredibilityCrisisRisk rdf:type airo:Risk ;
    rdfs:label "Credibility Crisis Risk"@en ;
    rdfs:label "可信度危机风险"@zh ;
    airo:isRiskSourceFor ex:CommonPitfallsInMedicalML ;
    airo:hasConsequence ex:InaccurateDiagnosis .

ex:MedicalDomain rdf:type airo:Domain ;
    rdfs:label "Medical Domain"@en ;
    rdfs:label "医疗领域"@zh .

ex:CommonPitfallsInMedicalML rdf:type airo:RiskSource ;
    rdfs:label "Common Pitfalls in Medical ML"@en ;
    rdfs:label "医学机器学习中的常见陷阱"@zh .

ex:InaccurateDiagnosis rdf:type airo:Consequence ;
    rdfs:label "Inaccurate Diagnosis"@en ;
    rdfs:label "不准确诊断"@zh .