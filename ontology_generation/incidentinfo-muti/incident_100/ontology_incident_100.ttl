@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilot rdf:type airo:AISystem ;
    rdfs:label "Tesla Autopilot"@en ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    airo:hasRisk ex:TrafficSignalMisidentificationRisk ;
    airo:hasCapability ex:TrafficSignalRecognition .

ex:TrafficSignalMisidentificationRisk rdf:type airo:Risk ;
    rdfs:label "Traffic Signal Misidentification Risk"@en ;
    rdfs:label "交通信号误识别风险"@zh ;
    airo:hasConsequence ex:PotentialTrafficViolation ;
    airo:isRiskSourceFor ex:VisualPatternConfusion .

ex:VisualPatternConfusion rdf:type airo:RiskSource ;
    rdfs:label "Visual Pattern Confusion"@en ;
    rdfs:label "视觉模式混淆"@zh .

ex:PotentialTrafficViolation rdf:type airo:Consequence ;
    rdfs:label "Potential Traffic Violation"@en ;
    rdfs:label "潜在交通违规"@zh .

ex:TrafficSignalRecognition rdf:type airo:AICapability ;
    rdfs:label "Traffic Signal Recognition Capability"@en ;
    rdfs:label "交通信号识别能力"@zh .