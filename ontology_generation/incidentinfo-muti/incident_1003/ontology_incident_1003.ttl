@base <https://w3id.org/airo#> .

ex:DeepfakeAudioGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Audio Generation System"@en ;
    rdfs:label "深度伪造音频生成系统"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:DeepLearningAudioSynthesis .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PublicConfusionAndMisinformation .

ex:DeepLearningAudioSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Audio Synthesis Technique"@en ;
    rdfs:label "深度学习音频合成技术"@zh .

ex:PublicConfusionAndMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Confusion and Misinformation Consequence"@en ;
    rdfs:label "公众困惑和虚假信息后果"@zh ;
    airo:hasImpact ex:ErosionOfPublicTrust .

ex:ErosionOfPublicTrust rdf:type airo:Impact ;
    rdfs:label "Erosion of Public Trust Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh .