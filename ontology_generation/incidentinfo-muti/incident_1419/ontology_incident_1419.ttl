@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HoodlineAIJournalists rdf:type airo:AISystem ;
    rdfs:label "Hoodline AI Journalists"@en ;
    rdfs:label "Hoodline AI记者"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:ContentGenerationAI ;
    airo:hasPurpose ex:AutomatedContentProduction ;
    airo:isAppliedWithinDomain ex:Journalism ;
    airo:hasStakeholder ex:NewsReaders ;
    airo:isDeployedBy ex:Hoodline .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:ReaderConfusion ;
    airo:isRiskSourceFor ex:UndisclosedAIUsage .

ex:ReaderConfusion rdf:type airo:Consequence ;
    rdfs:label "Reader Confusion"@en ;
    rdfs:label "读者困惑"@zh ;
    airo:hasImpact ex:TrustErosionInMedia .

ex:ContentGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Content Generation AI"@en ;
    rdfs:label "内容生成AI"@zh .

ex:NewsReaders rdf:type airo:Stakeholder ;
    rdfs:label "News Readers"@en ;
    rdfs:label "新闻读者"@zh .

ex:Hoodline rdf:type airo:AIOperator ;
    rdfs:label "Hoodline"@en ;
    rdfs:label "Hoodline"@zh .

ex:TrustErosionInMedia rdf:type airo:Impact ;
    rdfs:label "Trust Erosion in Media"@en ;
    rdfs:label "媒体信任度下降"@zh ;
    airo:hasImpactOnStakeholder ex:NewsReaders .

ex:AutomatedContentProduction rdf:type airo:Purpose ;
    rdfs:label "Automated Content Production"@en ;
    rdfs:label "自动化内容生产"@zh .

ex:Journalism rdf:type airo:Domain ;
    rdfs:label "Journalism"@en ;
    rdfs:label "新闻业"@zh .

ex:UndisclosedAIUsage rdf:type airo:RiskSource ;
    rdfs:label "Undisclosed AI Usage"@en ;
    rdfs:label "未披露的AI使用"@zh .