@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CourtSentencingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Court Sentencing Algorithm"@en ;
    rdfs:label "法庭量刑算法"@zh ;
    airo:hasRisk ex:RacialBiasInSentencing ;
    airo:usesTechnique ex:PredictiveModeling ;
    airo:hasStakeholder ex:BlackDefendants .

ex:RacialBiasInSentencing rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Sentencing Risk"@en ;
    rdfs:label "量刑中的种族偏见风险"@zh ;
    airo:hasConsequence ex:UnfairSentencingDecisions ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:UnfairSentencingDecisions rdf:type airo:Consequence ;
    rdfs:label "Unfair Sentencing Decisions Consequence"@en ;
    rdfs:label "不公平的量刑决策后果"@zh ;
    airo:hasImpact ex:DisproportionateImpactOnBlackMen .

ex:DisproportionateImpactOnBlackMen rdf:type airo:Impact ;
    rdfs:label "Disproportionate Impact on Black Men"@en ;
    rdfs:label "对黑人男性不成比例的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackDefendants .

ex:BlackDefendants rdf:type airo:Stakeholder ;
    rdfs:label "Black Defendants"@en ;
    rdfs:label "黑人被告"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .

ex:PredictiveModeling rdf:type airo:AITechnique ;
    rdfs:label "Predictive Modeling"@en ;
    rdfs:label "预测建模"@zh .