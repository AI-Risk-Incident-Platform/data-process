@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FakeAIInvestmentScam rdf:type airo:AISystem ;
    rdfs:label "Fake AI Investment Scam"@en ;
    rdfs:label "虚假AI投资骗局"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasStakeholder ex:BankOfCyprus ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:isRiskSourceFor ex:MisrepresentationOfAICapabilities ;
    airo:hasImpact ex:FinancialLossForInvestors .

ex:BankOfCyprus rdf:type airo:Stakeholder ;
    rdfs:label "Bank of Cyprus"@en ;
    rdfs:label "塞浦路斯银行"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services"@en ;
    rdfs:label "金融服务"@zh .

ex:MisrepresentationOfAICapabilities rdf:type airo:RiskSource ;
    rdfs:label "Misrepresentation of AI Capabilities"@en ;
    rdfs:label "AI能力虚假陈述"@zh .

ex:FinancialLossForInvestors rdf:type airo:Impact ;
    rdfs:label "Financial Loss for Investors"@en ;
    rdfs:label "投资者财务损失"@zh .