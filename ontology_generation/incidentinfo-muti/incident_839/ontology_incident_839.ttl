@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForAcademicWork rdf:type airo:AISystem ;
    rdfs:label "AI for Academic Work"@en ;
    rdfs:label "学术工作AI"@zh ;
    airo:hasRisk ex:AcademicIntegrityRisk ;
    airo:hasAISubject ex:Student ;
    airo:hasStakeholder ex:SchoolDistrict .

ex:AcademicIntegrityRisk rdf:type airo:Risk ;
    rdfs:label "Academic Integrity Risk"@en ;
    rdfs:label "学术诚信风险"@zh ;
    airo:hasConsequence ex:GradePenalty .

ex:SchoolDistrict rdf:type airo:Stakeholder ;
    rdfs:label "School District"@en ;
    rdfs:label "学区"@zh .

ex:Student rdf:type airo:AISubject ;
    rdfs:label "Student"@en ;
    rdfs:label "学生"@zh .

ex:GradePenalty rdf:type airo:Consequence ;
    rdfs:label "Grade Penalty"@en ;
    rdfs:label "成绩处罚"@zh ;
    airo:hasImpact ex:AcademicRecordImpact .

ex:AcademicRecordImpact rdf:type airo:Impact ;
    rdfs:label "Academic Record Impact"@en ;
    rdfs:label "学业记录影响"@zh ;
    airo:hasImpactOnStakeholder ex:Student .