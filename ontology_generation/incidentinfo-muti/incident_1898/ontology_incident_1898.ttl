ex:StudentDataTrainingAI rdf:type airo:AISystem ;
    rdfs:label "Student Data Training AI System"@en ;
    rdfs:label "学生数据训练AI系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:UniversityOfMichigan ;
    airo:hasAISubject ex:UniversityStudents .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
