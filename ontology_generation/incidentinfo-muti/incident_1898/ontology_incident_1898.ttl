ex:UniversityOfMichigan rdf:type airo:Stakeholder ;
    rdfs:label "University of Michigan"@en ;
    rdfs:label "密歇根大学"@zh .

ex:UniversityStudents rdf:type airo:AISubject ;
    rdfs:label "University Students"@en ;
    rdfs:label "大学生"@zh .

ex:StudentPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Student Privacy Compromise Impact"@en ;
    rdfs:label "学生隐私泄露影响"@zh ;
    airo:hasImpactOnStakeholder ex:UniversityStudents .