@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:AcademicMisconductRisk ;
    airo:hasAISubject ex:StudentsUsingChatGPT ;
    airo:hasPurpose ex:AcademicAssignmentCompletion ;
    airo:isAppliedWithinDomain ex:HigherEducation ;
    airo:hasStakeholder ex:TAMUCommerceInstructor .

ex:AcademicMisconductRisk rdf:type airo:Risk ;
    rdfs:label "Academic Misconduct Risk"@en ;
    rdfs:label "学术不端风险"@zh ;
    airo:hasConsequence ex:AcademicPenalties .

ex:TAMUCommerceInstructor rdf:type airo:Stakeholder ;
    rdfs:label "TAMU Commerce Instructor"@en ;
    rdfs:label "TAMU商业学院教师"@zh .

ex:StudentsUsingChatGPT rdf:type airo:AISubject ;
    rdfs:label "Students Using ChatGPT"@en ;
    rdfs:label "使用ChatGPT的学生"@zh .

ex:AcademicPenalties rdf:type airo:Consequence ;
    rdfs:label "Academic Penalties Consequence"@en ;
    rdfs:label "学术处罚后果"@zh ;
    airo:hasImpactOnStakeholder ex:StudentsUsingChatGPT .

ex:AcademicAssignmentCompletion rdf:type airo:Purpose ;
    rdfs:label "Academic Assignment Completion Purpose"@en ;
    rdfs:label "学术作业完成目的"@zh .

ex:HigherEducation rdf:type airo:Domain ;
    rdfs:label "Higher Education Domain"@en ;
    rdfs:label "高等教育领域"@zh .