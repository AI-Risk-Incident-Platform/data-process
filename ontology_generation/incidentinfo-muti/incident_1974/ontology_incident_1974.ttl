@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:InaccurateInformationRisk ;
    airo:isProvidedBy ex:OpenAI ;
    airo:hasPurpose ex:InformationProvision ;
    airo:isAppliedWithinDomain ex:InformationServices ;
    airo:hasStakeholder ex:ServiceProviders .

ex:InaccurateInformationRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Information Risk"@en ;
    rdfs:label "信息不准确风险"@zh ;
    airo:hasConsequence ex:MisleadingServiceClaims .

ex:MisleadingServiceClaims rdf:type airo:Consequence ;
    rdfs:label "Misleading Service Claims Consequence"@en ;
    rdfs:label "误导性服务声明后果"@zh ;
    airo:hasImpact ex:UserHarm .

ex:UserHarm rdf:type airo:Impact ;
    rdfs:label "User Harm Impact"@en ;
    rdfs:label "用户伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:ServiceProviders .

ex:ServiceProviders rdf:type airo:Stakeholder ;
    rdfs:label "Service Providers"@en ;
    rdfs:label "服务提供商"@zh .

ex:OpenAI rdf:type airo:AIProvider ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:InformationProvision rdf:type airo:Purpose ;
    rdfs:label "Information Provision Purpose"@en ;
    rdfs:label "信息提供目的"@zh .

ex:InformationServices rdf:type airo:Domain ;
    rdfs:label "Information Services Domain"@en ;
    rdfs:label "信息服务领域"@zh .