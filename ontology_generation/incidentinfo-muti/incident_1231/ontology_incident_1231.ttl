@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GrokAI rdf:type airo:AISystem ;
    rdfs:label "Grok AI"@en ;
    rdfs:label "Grok AI"@zh ;
    airo:hasRisk ex:PrivacyConcernsWithHealthData ;
    airo:hasAISubject ex:MedicalImagingData ;
    airo:hasPurpose ex:MedicalImageInterpretation ;
    airo:isAppliedWithinDomain ex:Healthcare ;
    airo:hasCapability ex:MedicalImageAnalysis ;
    airo:hasStakeholder ex:MedicalDataProviders .

ex:PrivacyConcernsWithHealthData rdf:type airo:Risk ;
    rdfs:label "Privacy Concerns with Health Data Risk"@en ;
    rdfs:label "健康数据隐私风险"@zh ;
    airo:hasConsequence ex:PotentialDataBreach ;
    airo:isRiskSourceFor ex:PublicMedicalDataCollection .

ex:MedicalDataProviders rdf:type airo:Stakeholder ;
    rdfs:label "Medical Data Providers"@en ;
    rdfs:label "医疗数据提供者"@zh .

ex:MedicalImagingData rdf:type airo:AISubject ;
    rdfs:label "Medical Imaging Data"@en ;
    rdfs:label "医学影像数据"@zh .

ex:MedicalImageInterpretation rdf:type airo:Purpose ;
    rdfs:label "Medical Image Interpretation"@en ;
    rdfs:label "医学影像解读"@zh .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "Healthcare"@en ;
    rdfs:label "医疗保健"@zh .

ex:PotentialDataBreach rdf:type airo:Consequence ;
    rdfs:label "Potential Data Breach"@en ;
    rdfs:label "潜在数据泄露"@zh ;
    airo:hasImpact ex:PatientPrivacyViolation .

ex:PatientPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Patient Privacy Violation"@en ;
    rdfs:label "患者隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:MedicalDataProviders .

ex:PublicMedicalDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Public Medical Data Collection"@en ;
    rdfs:label "公共医疗数据收集"@zh .

ex:MedicalImageAnalysis rdf:type airo:AICapability ;
    rdfs:label "Medical Image Analysis"@en ;
    rdfs:label "医学影像分析"@zh .