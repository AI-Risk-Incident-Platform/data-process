@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleBard rdf:type airo:AISystem ;
    rdfs:label "Google Bard"@en ;
    rdfs:label "谷歌Bard"@zh ;
    airo:hasRisk ex:PrivacyLeakRisk ;
    airo:isDevelopedBy ex:Google ;
    airo:hasStakeholder ex:Google .

ex:PrivacyLeakRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Leak Risk"@en ;
    rdfs:label "隐私泄露风险"@zh ;
    airo:hasConsequence ex:UserConversationExposure ;
    airo:isRiskSourceFor ex:SearchEngineIndexing ;
    airo:hasRiskControl ex:BlockConversationIndexing .

ex:UserConversationExposure rdf:type airo:Consequence ;
    rdfs:label "User Conversation Exposure Consequence"@en ;
    rdfs:label "用户对话暴露后果"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:SearchEngineIndexing rdf:type airo:RiskSource ;
    rdfs:label "Search Engine Indexing Risk Source"@en ;
    rdfs:label "搜索引擎索引风险源"@zh .

ex:BlockConversationIndexing rdf:type airo:RiskControl ;
    rdfs:label "Block Conversation Indexing RiskControl"@en ;
    rdfs:label "阻止对话索引管控"@zh .