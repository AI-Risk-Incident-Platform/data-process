@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVideo rdf:type airo:AISystem ;
    rdfs:label "AI Video"@en ;
    rdfs:label "AI视频"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:hasStakeholder ex:SpanishOppositionParty ;
    airo:isAppliedWithinDomain ex:PoliticalCampaigning .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PoliticalTensions .

ex:SpanishOppositionParty rdf:type airo:Stakeholder ;
    rdfs:label "Spanish Opposition Party"@en ;
    rdfs:label "西班牙反对党"@zh .

ex:PoliticalTensions rdf:type airo:Impact ;
    rdfs:label "Political Tensions"@en ;
    rdfs:label "政治紧张"@zh ;
    airo:hasImpactOnStakeholder ex:SpanishOppositionParty .

ex:PoliticalCampaigning rdf:type airo:Domain ;
    rdfs:label "Political Campaigning"@en ;
    rdfs:label "政治竞选"@zh .