@base <https://w3id.org/airo#> .

ex:DeepfakePornographySystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Pornography System"@en ;
    rdfs:label "深度伪造色情系统"@zh ;
    airo:hasRisk ex:CelebrityPrivacyViolationRisk ;
    airo:hasAISubject ex:Celebrities ;
    airo:usesTechnique ex:DeepfakeTechnology .

ex:CelebrityPrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Celebrity Privacy Violation Risk"@en ;
    rdfs:label "名人隐私侵犯风险"@zh ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Consequence of Privacy Violation"@en ;
        rdfs:label "隐私侵犯后果"@zh ;
        airo:hasImpact ex:ReputationDamage
    ] ;
    airo:isRiskSourceFor ex:NonConsensualImageUse .

ex:Celebrities rdf:type airo:AISubject ;
    rdfs:label "Celebrities"@en ;
    rdfs:label "名人"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage"@en ;
    rdfs:label "名誉损害"@zh ;
    airo:hasImpactOnStakeholder ex:Celebrities .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:NonConsensualImageUse rdf:type airo:RiskSource ;
    rdfs:label "Non-Consensual Image Use"@en ;
    rdfs:label "未经同意的图像使用"@zh ;
    rdfs:isDefinedBy "Nearly 4,000 celebrities found to be victims of deepfake pornography" .