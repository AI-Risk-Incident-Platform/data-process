@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaRobotaxiAI rdf:type airo:AISystem ;
    rdfs:label "特斯拉Robotaxi AI"@zh ;
    rdfs:label "Tesla Robotaxi AI"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasStakeholder ex:BladeRunner2049Producers ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTechnology .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:hasConsequence ex:LegalActionConsequence .

ex:BladeRunner2049Producers rdf:type airo:Stakeholder ;
    rdfs:label "银翼杀手2049制片方"@zh ;
    rdfs:label "Blade Runner 2049 Producers"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "法律诉讼后果"@zh ;
    rdfs:label "Legal Action Consequence"@en ;
    airo:hasImpactOnStakeholder ex:BladeRunner2049Producers .

ex:AutonomousVehicleTechnology rdf:type airo:Domain ;
    rdfs:label "自动驾驶汽车技术领域"@zh ;
    rdfs:label "Autonomous Vehicle Technology Domain"@en .