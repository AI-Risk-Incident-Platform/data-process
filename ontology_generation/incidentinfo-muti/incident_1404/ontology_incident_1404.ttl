@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PerplexityAISearchEngine rdf:type airo:AISystem ;
    rdfs:label "Perplexity AI Search Engine"@en ;
    rdfs:label "Perplexity AI搜索引擎"@zh ;
    airo:hasRisk ex:ContentTheftRisk ;
    airo:hasAICapability ex:ContentScrapingAndReproduction ;
    airo:isDeployedBy ex:PerplexityAICompany ;
    airo:hasStakeholder ex:NewsOutlets ;
    airo:isAppliedWithinDomain ex:SearchEngineTechnology .

ex:ContentTheftRisk rdf:type airo:Risk ;
    rdfs:label "Content Theft Risk"@en ;
    rdfs:label "内容盗窃风险"@zh ;
    airo:hasConsequence ex:NewsOutletContentLoss ;
    airo:isRiskSourceFor ex:LackOfContentAttribution .

ex:NewsOutletContentLoss rdf:type airo:Consequence ;
    rdfs:label "News Outlet Content Loss Consequence"@en ;
    rdfs:label "新闻媒体内容损失后果"@zh ;
    airo:hasImpact ex:EconomicLossForContentCreators .

ex:NewsOutlets rdf:type airo:Stakeholder ;
    rdfs:label "News Outlets"@en ;
    rdfs:label "新闻媒体"@zh .

ex:ContentScrapingAndReproduction rdf:type airo:AICapability ;
    rdfs:label "Content Scraping and Reproduction Capability"@en ;
    rdfs:label "内容抓取与复制能力"@zh .

ex:EconomicLossForContentCreators rdf:type airo:Impact ;
    rdfs:label "Economic Loss for Content Creators Impact"@en ;
    rdfs:label "内容创作者经济损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewsOutlets .

ex:LackOfContentAttribution rdf:type airo:RiskSource ;
    rdfs:label "Lack of Content Attribution Risk Source"@en ;
    rdfs:label "缺乏内容归属的风险源"@zh .

ex:PerplexityAICompany rdf:type airo:AIOperator ;
    rdfs:label "Perplexity AI Company"@en ;
    rdfs:label "Perplexity AI公司"@zh .

ex:SearchEngineTechnology rdf:type airo:Domain ;
    rdfs:label "Search Engine Technology Domain"@en ;
    rdfs:label "搜索引擎技术领域"@zh .