@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:SocialConfusionRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:hasStakeholder ex:AmazonWorkers .

ex:SocialConfusionRisk rdf:type airo:Risk ;
    rdfs:label "Social Confusion Risk"@en ;
    rdfs:label "社会混淆风险"@zh ;
    airo:hasImpact ex:PublicConfusion .

ex:AmazonWorkers rdf:type airo:Stakeholder ;
    rdfs:label "Amazon Workers"@en ;
    rdfs:label "亚马逊员工"@zh .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:PublicConfusion rdf:type airo:Impact ;
    rdfs:label "Public Confusion Impact"@en ;
    rdfs:label "公众混淆影响"@zh ;
    airo:hasImpactOnStakeholder ex:AmazonWorkers .