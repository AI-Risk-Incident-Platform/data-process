@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LegoAIImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "乐高AI图像生成系统"@zh ;
    rdfs:label "Lego AI Image Generation System"@en ;
    airo:hasRisk ex:PublicBacklashRisk ;
    airo:isDeployedBy ex:LegoCompany ;
    airo:hasStakeholder ex:LegoCustomers ;
    airo:hasPurpose ex:ProductMarketing ;
    airo:isAppliedWithinDomain ex:ToyIndustry ;
    airo:usesTechnique ex:ImageGenerationAI .

ex:PublicBacklashRisk rdf:type airo:Risk ;
    rdfs:label "公众反弹风险"@zh ;
    rdfs:label "Public Backlash Risk"@en ;
    airo:hasConsequence ex:BrandReputationDamage .

ex:BrandReputationDamage rdf:type airo:Consequence ;
    rdfs:label "品牌声誉损害后果"@zh ;
    rdfs:label "Brand Reputation Damage Consequence"@en ;
    airo:hasImpact ex:ConsumerTrustErosion .

ex:LegoCompany rdf:type airo:AIOperator ;
    rdfs:label "乐高公司"@zh ;
    rdfs:label "Lego Company"@en .

ex:LegoCustomers rdf:type airo:Stakeholder ;
    rdfs:label "乐高客户"@zh ;
    rdfs:label "Lego Customers"@en .

ex:ConsumerTrustErosion rdf:type airo:Impact ;
    rdfs:label "消费者信任侵蚀影响"@zh ;
    rdfs:label "Consumer Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:LegoCustomers .

ex:ProductMarketing rdf:type airo:Purpose ;
    rdfs:label "产品营销目的"@zh ;
    rdfs:label "Product Marketing Purpose"@en .

ex:ToyIndustry rdf:type airo:Domain ;
    rdfs:label "玩具行业领域"@zh ;
    rdfs:label "Toy Industry Domain"@en .

ex:ImageGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "图像生成AI技术"@zh ;
    rdfs:label "Image Generation AI Technique"@en .