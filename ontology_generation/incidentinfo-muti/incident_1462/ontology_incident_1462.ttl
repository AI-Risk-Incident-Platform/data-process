@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationService rdf:type airo:AISystem ;
    rdfs:label "Deepfake Generation Service"@en ;
    rdfs:label "深度伪造生成服务"@zh ;
    airo:hasRisk ex:ElectionDisruptionRisk ;
    airo:hasAISubject ex:PoliticalCandidates ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasPurpose ex:PoliticalManipulation ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:hasStakeholder ex:BangladeshGovernment .

ex:ElectionDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Election Disruption Risk"@en ;
    rdfs:label "选举干扰风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:LowCostDeepfakeAvailability .

ex:BangladeshGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Bangladesh Government"@en ;
    rdfs:label "孟加拉国政府"@zh .

ex:PoliticalCandidates rdf:type airo:AISubject ;
    rdfs:label "Political Candidates"@en ;
    rdfs:label "政治候选人"@zh .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:DemocraticProcessUndermining .

ex:DemocraticProcessUndermining rdf:type airo:Impact ;
    rdfs:label "Democratic Process Undermining Impact"@en ;
    rdfs:label "民主进程破坏影响"@zh ;
    airo:hasImpactOnStakeholder ex:BangladeshGovernment .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:LowCostDeepfakeAvailability rdf:type airo:RiskSource ;
    rdfs:label "Low-Cost Deepfake Availability Risk Source"@en ;
    rdfs:label "低成本深度伪造可用性风险源"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:PoliticalManipulation rdf:type airo:Purpose ;
    rdfs:label "Political Manipulation Purpose"@en ;
    rdfs:label "政治操纵目的"@zh .