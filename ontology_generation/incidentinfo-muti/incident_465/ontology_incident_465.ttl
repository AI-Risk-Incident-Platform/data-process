@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BingAIChatbot rdf:type airo:AISystem ;
    rdfs:label "Bing AI Chatbot"@en ;
    rdfs:label "必应AI聊天机器人"@zh ;
    airo:hasRisk ex:UnreliableInformationRisk ;
    airo:hasAICapability ex:ConversationalSearch ;
    airo:isDeployedBy ex:Microsoft ;
    airo:hasPurpose ex:EnhancedSearchExperience ;
    airo:hasStakeholder ex:SearchEngineUsers ;
    airo:hasAISubject ex:InformationSeekers .

ex:UnreliableInformationRisk rdf:type airo:Risk ;
    rdfs:label "Unreliable Information Risk"@en ;
    rdfs:label "不可靠信息风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:EmotionalManipulation .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:UserTrustErosion .

ex:UserTrustErosion rdf:type airo:Impact ;
    rdfs:label "User Trust Erosion Impact"@en ;
    rdfs:label "用户信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:SearchEngineUsers .

ex:ConversationalSearch rdf:type airo:AICapability ;
    rdfs:label "Conversational Search Capability"@en ;
    rdfs:label "对话式搜索能力"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:SearchEngineUsers rdf:type airo:Stakeholder ;
    rdfs:label "Search Engine Users"@en ;
    rdfs:label "搜索引擎用户"@zh .

ex:InformationSeekers rdf:type airo:AISubject ;
    rdfs:label "Information Seekers"@en ;
    rdfs:label "信息寻求者"@zh .

ex:EmotionalManipulation rdf:type airo:RiskSource ;
    rdfs:label "Emotional Manipulation Risk Source"@en ;
    rdfs:label "情感操纵风险源"@zh .

ex:EnhancedSearchExperience rdf:type airo:Purpose ;
    rdfs:label "Enhanced Search Experience Purpose"@en ;
    rdfs:label "增强搜索体验目的"@zh .