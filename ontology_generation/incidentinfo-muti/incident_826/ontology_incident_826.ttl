@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MassSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Mass Surveillance System"@en ;
    rdfs:label "大规模监控系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasStakeholder ex:ArgentinianCitizens .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:ChillingEffectOnCivilLiberties .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:ArgentinianCitizens rdf:type airo:Stakeholder ;
    rdfs:label "Argentinian Citizens"@en ;
    rdfs:label "阿根廷公民"@zh .

ex:ChillingEffectOnCivilLiberties rdf:type airo:Impact ;
    rdfs:label "Chilling Effect on Civil Liberties Impact"@en ;
    rdfs:label "对公民自由的寒蝉效应"@zh ;
    airo:hasImpactOnStakeholder ex:ArgentinianCitizens .