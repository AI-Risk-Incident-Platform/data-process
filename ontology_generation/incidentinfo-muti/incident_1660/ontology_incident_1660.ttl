@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:PotentialHarmToWorld ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:OpenAI ;
    airo:hasAIOperator ex:CEOSamAltman .

ex:PotentialHarmToWorld rdf:type airo:Risk ;
    rdfs:label "Potential Harm to World Risk"@en ;
    rdfs:label "对世界潜在危害风险"@zh ;
    airo:isRiskSourceFor ex:PolicyViolationInPoliticalMessaging ;
    airo:hasConsequence ex:PoliticalCampaignMisuse .

ex:OpenAI rdf:type airo:Stakeholder ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:CEOSamAltman rdf:type airo:AIOperator ;
    rdfs:label "CEO Sam Altman"@en ;
    rdfs:label "CEO萨姆·奥尔特曼"@zh .

ex:PolicyViolationInPoliticalMessaging rdf:type airo:RiskSource ;
    rdfs:label "Policy Violation in Political Messaging Risk Source"@en ;
    rdfs:label "政治信息传递中的政策违规风险源"@zh .

ex:PoliticalCampaignMisuse rdf:type airo:Misuse ;
    rdfs:label "Political Campaign Misuse"@en ;
    rdfs:label "政治竞选滥用"@zh .