ex:NudifyingTool rdf:type airo:AISystem ;
    rdfs:label "Nudifying Tool"@en ;
    rdfs:label "裸体化工具"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:Women ;
    airo:hasStakeholder ex:MariaMiller .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasImpact ex:PsychologicalHarm .

ex:MariaMiller rdf:type airo:Stakeholder ;
    rdfs:label "Maria Miller"@en ;
    rdfs:label "玛丽亚·米勒"@zh .

ex:PsychologicalHarm rdf:type airo:Impact ;
    rdfs:label "Psychological Harm"@en ;
    rdfs:label "心理伤害"@zh ;
    airo:hasImpactOnStakeholder ex:Women .

ex:Women rdf:type airo:AISubject ;
    rdfs:label "Women"@en ;
    rdfs:label "女性"@zh .