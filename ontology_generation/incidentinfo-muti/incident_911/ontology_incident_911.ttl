@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAI rdf:type airo:AISystem ;
    rdfs:label "Generative AI"@en ;
    rdfs:label "生成式AI"@zh ;
    airo:hasRisk ex:FalseInformationRisk ;
    airo:hasStakeholder ex:AlaskaEducationOfficial ;
    airo:isAppliedWithinDomain ex:EducationSector .

ex:FalseInformationRisk rdf:type airo:Risk ;
    rdfs:label "False Information Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:MisinformationInEducation .

ex:AlaskaEducationOfficial rdf:type airo:Stakeholder ;
    rdfs:label "Alaska Education Official"@en ;
    rdfs:label "阿拉斯加教育官员"@zh .

ex:MisinformationInEducation rdf:type airo:Consequence ;
    rdfs:label "Misinformation in Education Consequence"@en ;
    rdfs:label "教育领域错误信息后果"@zh ;
    airo:hasImpactOnStakeholder ex:AlaskaEducationOfficial .

ex:EducationSector rdf:type airo:Domain ;
    rdfs:label "Education Sector"@en ;
    rdfs:label "教育领域"@zh .