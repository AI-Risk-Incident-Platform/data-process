@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GeminiAIChatbot rdf:type airo:AISystem ;
    rdfs:label "Gemini AI Chatbot"@en ;
    rdfs:label "Gemini AI聊天机器人"@zh ;
    airo:hasRisk ex:DiversityRepresentationErrorRisk ;
    airo:hasCapability ex:HumanImageGeneration ;
    airo:isDevelopedBy ex:Google .

ex:DiversityRepresentationErrorRisk rdf:type airo:Risk ;
    rdfs:label "Diversity Representation Error Risk"@en ;
    rdfs:label "多样性表现错误风险"@zh ;
    airo:hasConsequence ex:PublicControversy ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInDiversityRepresentation .

ex:PublicControversy rdf:type airo:Consequence ;
    rdfs:label "Public Controversy Consequence"@en ;
    rdfs:label "公众争议后果"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:HumanImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Human Image Generation Capability"@en ;
    rdfs:label "人类图像生成能力"@zh .

ex:AlgorithmicBiasInDiversityRepresentation rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Diversity Representation Risk Source"@en ;
    rdfs:label "多样性表现中的算法偏见风险源"@zh .