@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForDeepfakeScam rdf:type airo:AISystem ;
    rdfs:label "用于深度伪造诈骗的AI系统"@zh ;
    rdfs:label "AI for Deepfake Scam"@en ;
    airo:hasRisk ex:IdentityFraudRisk ;
    airo:hasStakeholder ex:WhoopiGoldberg ;
    airo:hasAISubject ex:AudienceMembers .

ex:IdentityFraudRisk rdf:type airo:Risk ;
    rdfs:label "身份欺诈风险"@zh ;
    rdfs:label "Identity Fraud Risk"@en ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:CelebrityImageMisuse ;
    airo:hasRiskControl ex:PublicWarning .

ex:WhoopiGoldberg rdf:type airo:Stakeholder ;
    rdfs:label "乌比·戈德堡"@zh ;
    rdfs:label "Whoopi Goldberg"@en .

ex:AudienceMembers rdf:type airo:AISubject ;
    rdfs:label "观众成员"@zh ;
    rdfs:label "Audience Members"@en .

ex:CelebrityImageMisuse rdf:type airo:Misuse ;
    rdfs:label "名人图像滥用"@zh ;
    rdfs:label "Celebrity Image Misuse"@en .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "经济损失"@zh ;
    rdfs:label "Financial Loss"@en .

ex:PublicWarning rdf:type airo:RiskControl ;
    rdfs:label "公开警告"@zh ;
    rdfs:label "Public Warning"@en .