@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TencentFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "腾讯人脸识别系统"@zh ;
    rdfs:label "Tencent Facial Recognition System"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAICapability ex:MinorDetectionCapability ;
    airo:isDevelopedBy ex:Tencent ;
    airo:hasPurpose ex:PreventMinorGamingAtNight ;
    airo:isAppliedWithinDomain ex:OnlineGaming ;
    airo:hasAISubject ex:Minors ;
    airo:usesTechnique ex:FacialRecognition .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:hasConsequence ex:PrivacyConcerns ;
    airo:isRiskSourceFor ex:BiometricDataCollection .

ex:MinorDetectionCapability rdf:type airo:AICapability ;
    rdfs:label "未成年人检测能力"@zh ;
    rdfs:label "Minor Detection Capability"@en .

ex:Tencent rdf:type airo:AIOperator ;
    rdfs:label "腾讯"@zh ;
    rdfs:label "Tencent"@en .

ex:PreventMinorGamingAtNight rdf:type airo:Purpose ;
    rdfs:label "防止未成年人夜间游戏"@zh ;
    rdfs:label "Prevent Minor Gaming at Night"@en .

ex:OnlineGaming rdf:type airo:Domain ;
    rdfs:label "在线游戏"@zh ;
    rdfs:label "Online Gaming"@en .

ex:Minors rdf:type airo:AISubject ;
    rdfs:label "未成年人"@zh ;
    rdfs:label "Minors"@en .

ex:PrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "隐私担忧"@zh ;
    rdfs:label "Privacy Concerns"@en ;
    airo:hasImpactOnStakeholder ex:Minors .

ex:FacialRecognition rdf:type airo:AITechnique ;
    rdfs:label "人脸识别"@zh ;
    rdfs:label "Facial Recognition"@en .

ex:BiometricDataCollection rdf:type airo:RiskSource ;
    rdfs:label "生物特征数据收集"@zh ;
    rdfs:label "Biometric Data Collection"@en .