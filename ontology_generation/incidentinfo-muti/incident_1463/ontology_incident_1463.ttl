@base <https://w3id.org/airo#> .

ex:DeepfakeRobocallSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Robocall System"@en ;
    rdfs:label "深度伪造自动电话系统"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasPurpose ex:PoliticalManipulation ;
    airo:hasStakeholder ex:NewHampshireVoters ;
    airo:isDeployedBy ex:PoliticalConsultant ;
    airo:isAppliedWithinDomain ex:PoliticalCampaigns .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:VoterDeception ;
    airo:isRiskSourceFor ex:AIEnabledDisinformation .

ex:VoterDeception rdf:type airo:Consequence ;
    rdfs:label "Voter Deception Consequence"@en ;
    rdfs:label "选民欺骗后果"@zh ;
    airo:hasImpact ex:ElectionInterference .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:NewHampshireVoters rdf:type airo:Stakeholder ;
    rdfs:label "New Hampshire Voters"@en ;
    rdfs:label "新罕布什尔州选民"@zh .

ex:PoliticalConsultant rdf:type airo:AIOperator ;
    rdfs:label "Political Consultant"@en ;
    rdfs:label "政治顾问"@zh .

ex:ElectionInterference rdf:type airo:Impact ;
    rdfs:label "Election Interference Impact"@en ;
    rdfs:label "选举干预影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewHampshireVoters .

ex:PoliticalManipulation rdf:type airo:Purpose ;
    rdfs:label "Political Manipulation Purpose"@en ;
    rdfs:label "政治操纵目的"@zh .

ex:PoliticalCampaigns rdf:type airo:Domain ;
    rdfs:label "Political Campaigns Domain"@en ;
    rdfs:label "政治竞选领域"@zh .

ex:AIEnabledDisinformation rdf:type airo:RiskSource ;
    rdfs:label "AI-Enabled Disinformation Risk Source"@en ;
    rdfs:label "AI驱动的虚假信息风险源"@zh .