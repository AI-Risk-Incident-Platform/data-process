@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAIInfrastructure rdf:type airo:AISystem ;
    rdfs:label "谷歌AI基础设施"@zh ;
    rdfs:label "Google AI Infrastructure"@en ;
    airo:hasRisk ex:IncreasedCarbonEmissionsRisk ;
    airo:hasStakeholder ex:GoogleCorporation ;
    airo:isAppliedWithinDomain ex:ArtificialIntelligenceResearch .

ex:IncreasedCarbonEmissionsRisk rdf:type airo:Risk ;
    rdfs:label "碳排放增加风险"@zh ;
    rdfs:label "Increased Carbon Emissions Risk"@en ;
    airo:hasImpact ex:EnvironmentalDegradationImpact .

ex:EnvironmentalDegradationImpact rdf:type airo:Impact ;
    rdfs:label "环境退化影响"@zh ;
    rdfs:label "Environmental Degradation Impact"@en ;
    airo:hasImpactOnStakeholder ex:GoogleCorporation .

ex:GoogleCorporation rdf:type airo:Stakeholder ;
    rdfs:label "谷歌公司"@zh ;
    rdfs:label "Google Corporation"@en .

ex:ArtificialIntelligenceResearch rdf:type airo:Domain ;
    rdfs:label "人工智能研究领域"@zh ;
    rdfs:label "Artificial Intelligence Research Domain"@en .