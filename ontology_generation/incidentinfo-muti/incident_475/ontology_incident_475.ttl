@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    rdfs:label "Tesla Full Self-Driving System"@en ;
    airo:hasRisk ex:PotentialCrashRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasPurpose ex:AutonomousDrivingAssistance ;
    airo:hasAICapability ex:SelfDrivingFunctionality ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustryDomain .

ex:PotentialCrashRisk rdf:type airo:Risk ;
    rdfs:label "潜在碰撞风险"@zh ;
    rdfs:label "Potential Crash Risk"@en ;
    airo:isRiskSourceFor ex:SoftwareDefectInFSDBeta ;
    airo:hasConsequence ex:VehicleRecallsConsequence .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:VehicleRecallsConsequence rdf:type airo:Consequence ;
    rdfs:label "车辆召回后果"@zh ;
    rdfs:label "Vehicle Recalls Consequence"@en .

ex:SoftwareDefectInFSDBeta rdf:type airo:RiskSource ;
    rdfs:label "FSD Beta软件缺陷风险源"@zh ;
    rdfs:label "Software Defect in FSD Beta Risk Source"@en .

ex:AutomotiveIndustryDomain rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:AutonomousDrivingAssistance rdf:type airo:Purpose ;
    rdfs:label "自动驾驶辅助目的"@zh ;
    rdfs:label "Autonomous Driving Assistance Purpose"@en .

ex:SelfDrivingFunctionality rdf:type airo:AICapability ;
    rdfs:label "自动驾驶功能能力"@zh ;
    rdfs:label "Self-Driving Functionality Capability"@en .