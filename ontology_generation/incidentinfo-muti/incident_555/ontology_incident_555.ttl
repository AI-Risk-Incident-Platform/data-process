@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasAICapability ex:AutonomousDriving ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:isAppliedWithinDomain ex:Transportation .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    airo:hasConsequence ex:VehicleDamage ;
    airo:isRiskSourceFor ex:ObjectDetectionFailure .

ex:VehicleDamage rdf:type airo:Consequence ;
    rdfs:label "车辆损坏后果"@zh ;
    rdfs:label "Vehicle Damage Consequence"@en ;
    airo:hasImpact ex:TrafficDisruption .

ex:AutonomousDriving rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:Transportation rdf:type airo:Domain ;
    rdfs:label "交通运输领域"@zh ;
    rdfs:label "Transportation Domain"@en .

ex:ObjectDetectionFailure rdf:type airo:RiskSource ;
    rdfs:label "物体检测失败风险源"@zh ;
    rdfs:label "Object Detection Failure Risk Source"@en .

ex:TrafficDisruption rdf:type airo:Impact ;
    rdfs:label "交通中断影响"@zh ;
    rdfs:label "Traffic Disruption Impact"@en .