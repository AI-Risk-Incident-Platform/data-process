@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPTResumeGenerator rdf:type airo:AISystem ;
    rdfs:label "ChatGPT Resume Generator"@en ;
    rdfs:label "ChatGPT简历生成器"@zh ;
    airo:hasRisk ex:FloodOfAIApplicationsRisk ;
    airo:hasAISubject ex:JobApplicants ;
    airo:usesTechnique ex:NaturalLanguageGeneration ;
    airo:hasPurpose ex:AutomatedResumeCreation ;
    airo:isAppliedWithinDomain ex:HumanResources ;
    airo:hasStakeholder ex:JobRecruiters .

ex:FloodOfAIApplicationsRisk rdf:type airo:Risk ;
    rdfs:label "Flood of AI-Generated Applications Risk"@en ;
    rdfs:label "AI生成申请泛滥风险"@zh ;
    airo:hasConsequence ex:RecruiterOverload .

ex:JobRecruiters rdf:type airo:Stakeholder ;
    rdfs:label "Job Recruiters"@en ;
    rdfs:label "招聘人员"@zh .

ex:JobApplicants rdf:type airo:AISubject ;
    rdfs:label "Job Applicants"@en ;
    rdfs:label "求职者"@zh .

ex:RecruiterOverload rdf:type airo:Consequence ;
    rdfs:label "Recruiter Overload Consequence"@en ;
    rdfs:label "招聘人员超负荷后果"@zh ;
    airo:hasImpact ex:ReducedHiringEfficiency .

ex:NaturalLanguageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Natural Language Generation Technique"@en ;
    rdfs:label "自然语言生成技术"@zh .

ex:AutomatedResumeCreation rdf:type airo:Purpose ;
    rdfs:label "Automated Resume Creation Purpose"@en ;
    rdfs:label "自动化简历创建目的"@zh .

ex:HumanResources rdf:type airo:Domain ;
    rdfs:label "Human Resources Domain"@en ;
    rdfs:label "人力资源领域"@zh .

ex:ReducedHiringEfficiency rdf:type airo:Impact ;
    rdfs:label "Reduced Hiring Efficiency Impact"@en ;
    rdfs:label "招聘效率降低影响"@zh ;
    airo:hasImpactOnStakeholder ex:JobRecruiters .