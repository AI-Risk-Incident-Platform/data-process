@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    rdfs:label "Deepfake Video Generation System"@en ;
    airo:hasRisk ex:PoliticalDisinformationRisk ;
    airo:hasStakeholder ex:RussianPropagandaUnit ;
    airo:usesTechnique ex:DeepLearningBasedFaceSwapping .

ex:PoliticalDisinformationRisk rdf:type airo:Risk ;
    rdfs:label "政治虚假信息风险"@zh ;
    rdfs:label "Political Disinformation Risk"@en ;
    airo:hasConsequence ex:PublicTrustErosion ;
    airo:isRiskSourceFor ex:StateSponsoredDisinformationCampaign .

ex:RussianPropagandaUnit rdf:type airo:Stakeholder ;
    rdfs:label "俄罗斯宣传部门"@zh ;
    rdfs:label "Russian Propaganda Unit"@en .

ex:PublicTrustErosion rdf:type airo:Consequence ;
    rdfs:label "公众信任侵蚀后果"@zh ;
    rdfs:label "Public Trust Erosion Consequence"@en ;
    airo:hasImpact ex:PoliticalInstability .

ex:PoliticalInstability rdf:type airo:Impact ;
    rdfs:label "政治不稳定影响"@zh ;
    rdfs:label "Political Instability Impact"@en ;
    airo:hasImpactOnStakeholder ex:RussianPropagandaUnit .

ex:DeepLearningBasedFaceSwapping rdf:type airo:AITechnique ;
    rdfs:label "基于深度学习的人脸交换技术"@zh ;
    rdfs:label "Deep Learning Based Face Swapping Technique"@en .

ex:StateSponsoredDisinformationCampaign rdf:type airo:RiskSource ;
    rdfs:label "国家支持的虚假信息宣传活动风险源"@zh ;
    rdfs:label "State-Sponsored Disinformation Campaign Risk Source"@en .