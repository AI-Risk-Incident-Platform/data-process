ex:ReplicaSmartCityTechnology rdf:type airo:AISystem ;
    rdfs:label "Replica Smart City Technology"@en ;
    rdfs:label "Replica智能城市技术"@zh ;
    airo:hasRisk ex:SmartCityProjectTerminationRisk ;
    airo:hasStakeholder ex:PortlandCityGovernment ;
    airo:isDeployedBy ex:SidewalkLabs ;
    airo:isAppliedWithinDomain ex:SmartCityDevelopmentDomain .

ex:SmartCityProjectTerminationRisk rdf:type airo:Risk ;
    rdfs:label "Smart City Project Termination Risk"@en ;
    rdfs:label "智能城市项目终止风险"@zh .

ex:PortlandCityGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Portland City Government"@en ;
    rdfs:label "波特兰市政府"@zh .

ex:SidewalkLabs rdf:type airo:AIOperator ;
    rdfs:label "Sidewalk Labs"@en ;
    rdfs:label "Sidewalk Labs"@zh .

ex:SmartCityDevelopmentDomain rdf:type airo:Domain ;
    rdfs:label "Smart City Development Domain"@en ;
    rdfs:label "智能城市发展领域"@zh .