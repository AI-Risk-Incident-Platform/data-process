@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Content Moderation System"@en ;
    rdfs:label "Facebook内容审核系统"@zh ;
    airo:hasRisk ex:InappropriateContentFlaggingRisk ;
    airo:isDeployedBy ex:FacebookInc ;
    airo:usesTechnique ex:AutomatedContentFiltering .

ex:InappropriateContentFlaggingRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Flagging Risk"@en ;
    rdfs:label "不当内容标记风险"@zh ;
    airo:hasConsequence ex:MisidentificationOfLegitimateContent .

ex:MisidentificationOfLegitimateContent rdf:type airo:Consequence ;
    rdfs:label "Misidentification of Legitimate Content Consequence"@en ;
    rdfs:label "合法内容误识别后果"@zh ;
    airo:hasImpact ex:PublicConfusionAndFrustration .

ex:FacebookInc rdf:type airo:AIOperator ;
    rdfs:label "Facebook Inc."@en ;
    rdfs:label "Facebook公司"@zh .

ex:PublicConfusionAndFrustration rdf:type airo:Impact ;
    rdfs:label "Public Confusion and Frustration Impact"@en ;
    rdfs:label "公众困惑与沮丧影响"@zh ;
    airo:hasImpactOnStakeholder ex:FacebookInc .

ex:AutomatedContentFiltering rdf:type airo:AITechnique ;
    rdfs:label "Automated Content Filtering Technique"@en ;
    rdfs:label "自动化内容过滤技术"@zh .