@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IBScoringAlgorithm rdf:type airo:AISystem ;
    rdfs:label "IB评分算法"@zh ;
    rdfs:label "IB Scoring Algorithm"@en ;
    airo:hasRisk ex:UnfairGradingRisk ;
    airo:hasPurpose ex:AutomatedGrading ;
    airo:hasStakeholder ex:Students ;
    airo:isAppliedWithinDomain ex:Education .

ex:UnfairGradingRisk rdf:type airo:Risk ;
    rdfs:label "不公平评分风险"@zh ;
    rdfs:label "Unfair Grading Risk"@en ;
    airo:hasConsequence ex:StudentsLowMarks .

ex:StudentsLowMarks rdf:type airo:Consequence ;
    rdfs:label "学生低分后果"@zh ;
    rdfs:label "Students' Low Marks Consequence"@en ;
    airo:hasImpact ex:CollegeAdmissionImpact .

ex:CollegeAdmissionImpact rdf:type airo:Impact ;
    rdfs:label "大学录取影响"@zh ;
    rdfs:label "College Admission Impact"@en ;
    airo:hasImpactOnStakeholder ex:Students .

ex:Students rdf:type airo:Stakeholder ;
    rdfs:label "学生"@zh ;
    rdfs:label "Students"@en .

ex:AutomatedGrading rdf:type airo:Purpose ;
    rdfs:label "自动评分目的"@zh ;
    rdfs:label "Automated Grading Purpose"@en .

ex:Education rdf:type airo:Domain ;
    rdfs:label "教育领域"@zh ;
    rdfs:label "Education Domain"@en .