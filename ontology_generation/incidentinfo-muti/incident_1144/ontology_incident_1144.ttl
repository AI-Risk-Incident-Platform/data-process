@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceID rdf:type airo:AISystem ;
    rdfs:label "Face ID"@en ;
    rdfs:label "面容ID"@zh ;
    airo:hasRisk ex:FacialRecognitionFailureRisk ;
    airo:hasCapability ex:FacialRecognition ;
    airo:hasVulnerability ex:SimilarFacialFeatures .

ex:FacialRecognitionFailureRisk rdf:type airo:Risk ;
    rdfs:label "Facial Recognition Failure Risk"@en ;
    rdfs:label "面部识别失败风险"@zh ;
    airo:hasConsequence ex:UnauthorizedAccess ;
    airo:isRiskSourceFor ex:GeneticSimilarity .

ex:UnauthorizedAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Access Consequence"@en ;
    rdfs:label "未经授权访问后果"@zh .

ex:FacialRecognition rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition Capability"@en ;
    rdfs:label "面部识别能力"@zh .

ex:SimilarFacialFeatures rdf:type airo:Vulnerability ;
    rdfs:label "Similar Facial Features Vulnerability"@en ;
    rdfs:label "相似面部特征脆弱性"@zh .

ex:GeneticSimilarity rdf:type airo:RiskSource ;
    rdfs:label "Genetic Similarity Risk Source"@en ;
    rdfs:label "基因相似性风险源"@zh .