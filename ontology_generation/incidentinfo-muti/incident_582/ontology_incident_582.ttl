@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RussianDisinformationAISystem rdf:type airo:AISystem ;
    rdfs:label "俄罗斯虚假信息AI系统"@zh ;
    rdfs:label "Russian Disinformation AI System"@en ;
    airo:hasRisk ex:DisinformationSpreadRisk ;
    airo:isDeployedBy ex:RussianGovernment ;
    airo:hasStakeholder ex:USGovernment ;
    airo:isAppliedWithinDomain ex:GeopoliticalInformationWarfareDomain ;
    airo:hasPurpose ex:InfluenceForeignPublicOpinionPurpose .

ex:DisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "虚假信息传播风险"@zh ;
    rdfs:label "Disinformation Spread Risk"@en ;
    airo:hasConsequence ex:PublicOpinionManipulationImpact .

ex:USGovernment rdf:type airo:Stakeholder ;
    rdfs:label "美国政府"@zh ;
    rdfs:label "US Government"@en .

ex:RussianGovernment rdf:type airo:AIOperator ;
    rdfs:label "俄罗斯政府"@zh ;
    rdfs:label "Russian Government"@en .

ex:GeopoliticalInformationWarfareDomain rdf:type airo:Domain ;
    rdfs:label "地缘政治信息战领域"@zh ;
    rdfs:label "Geopolitical Information Warfare Domain"@en .

ex:PublicOpinionManipulationImpact rdf:type airo:Impact ;
    rdfs:label "舆论操纵影响"@zh ;
    rdfs:label "Public Opinion Manipulation Impact"@en ;
    airo:hasImpactOnStakeholder ex:USGovernment .

ex:InfluenceForeignPublicOpinionPurpose rdf:type airo:Purpose ;
    rdfs:label "影响外国舆论的目的"@zh ;
    rdfs:label "Influence Foreign Public Opinion Purpose"@en .