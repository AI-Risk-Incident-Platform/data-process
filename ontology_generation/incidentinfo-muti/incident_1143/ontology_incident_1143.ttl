@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceID rdf:type airo:AISystem ;
    rdfs:label "Face ID"@en ;
    rdfs:label "面容ID"@zh ;
    airo:hasRisk ex:FacialRecognitionFailureRisk ;
    airo:hasAISubject ex:iPhoneXUsers ;
    airo:usesTechnique ex:3DFacialMapping .

ex:FacialRecognitionFailureRisk rdf:type airo:Risk ;
    rdfs:label "Facial Recognition Failure Risk"@en ;
    rdfs:label "面部识别失败风险"@zh ;
    airo:hasConsequence ex:UnauthorizedDeviceAccess ;
    airo:hasVulnerability ex:GeneticSimilarityVulnerability .

ex:UnauthorizedDeviceAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Device Access Consequence"@en ;
    rdfs:label "未经授权的设备访问后果"@zh .

ex:GeneticSimilarityVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "Genetic Similarity Vulnerability"@en ;
    rdfs:label "基因相似性漏洞"@zh .

ex:iPhoneXUsers rdf:type airo:AISubject ;
    rdfs:label "iPhone X Users"@en ;
    rdfs:label "iPhone X用户"@zh .

ex:3DFacialMapping rdf:type airo:AITechnique ;
    rdfs:label "3D Facial Mapping Technique"@en ;
    rdfs:label "3D面部映射技术"@zh .