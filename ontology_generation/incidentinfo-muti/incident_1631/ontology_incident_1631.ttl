@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeKidsApp rdf:type airo:AISystem ;
    rdfs:label "YouTube Kids App"@en ;
    rdfs:label "YouTube儿童应用"@zh ;
    airo:hasRisk ex:InappropriateContentRisk ;
    airo:hasCapability ex:ContentFilteringAlgorithm ;
    airo:hasPurpose ex:ChildEntertainmentPlatform ;
    airo:hasStakeholder ex:Children ;
    airo:isAppliedWithinDomain ex:OnlineVideoPlatform ;
    airo:usesTechnique ex:RecommendationAlgorithm ;
    airo:hasVulnerability ex:LackOfContentVerification .

ex:InappropriateContentRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Risk"@en ;
    rdfs:label "不当内容风险"@zh ;
    airo:hasConsequence ex:ChildMisinformationExposure ;
    airo:isRiskSourceFor ex:MisinformationContent .

ex:ContentFilteringAlgorithm rdf:type airo:AICapability ;
    rdfs:label "Content Filtering Algorithm"@en ;
    rdfs:label "内容过滤算法"@zh .

ex:Children rdf:type airo:Stakeholder ;
    rdfs:label "Children"@en ;
    rdfs:label "儿童"@zh .

ex:MisinformationContent rdf:type airo:RiskSource ;
    rdfs:label "Misinformation Content"@en ;
    rdfs:label "虚假信息内容"@zh ;
    airo:exploitsVulnerability ex:LackOfContentVerification ;
    airo:hasThreat ex:TargetedDisinformationCampaign ;
    airo:hasHazard ex:UnverifiedUserGeneratedContent .

ex:ChildMisinformationExposure rdf:type airo:Consequence ;
    rdfs:label "Child Misinformation Exposure Consequence"@en ;
    rdfs:label "儿童接触虚假信息后果"@zh ;
    airo:hasImpact ex:CognitiveDevelopmentImpact .

ex:CognitiveDevelopmentImpact rdf:type airo:Impact ;
    rdfs:label "Cognitive Development Impact"@en ;
    rdfs:label "认知发展影响"@zh ;
    airo:hasImpactOnStakeholder ex:Children .

ex:ChildEntertainmentPlatform rdf:type airo:Purpose ;
    rdfs:label "Child Entertainment Platform Purpose"@en ;
    rdfs:label "儿童娱乐平台目的"@zh .

ex:OnlineVideoPlatform rdf:type airo:Domain ;
    rdfs:label "Online Video Platform Domain"@en ;
    rdfs:label "在线视频平台领域"@zh .

ex:RecommendationAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Recommendation Algorithm Technique"@en ;
    rdfs:label "推荐算法技术"@zh .

ex:LackOfContentVerification rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Content Verification Vulnerability"@en ;
    rdfs:label "内容验证缺失漏洞"@zh .

ex:TargetedDisinformationCampaign rdf:type airo:Threat ;
    rdfs:label "Targeted Disinformation Campaign Threat"@en ;
    rdfs:label "定向虚假信息传播威胁"@zh .

ex:UnverifiedUserGeneratedContent rdf:type airo:Hazard ;
    rdfs:label "Unverified User-Generated Content Hazard"@en ;
    rdfs:label "未经核实的用户生成内容危害"@zh .

ex:ChildTargetedDisinformation rdf:type airo:RiskConcept ;
    rdfs:label "Child-Targeted Disinformation Risk Concept"@en ;
    rdfs:label "针对儿童的虚假信息风险概念"@zh .