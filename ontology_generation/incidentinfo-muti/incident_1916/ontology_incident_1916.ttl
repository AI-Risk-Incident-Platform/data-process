@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MidjourneyAI rdf:type airo:AISystem ;
    rdfs:label "Midjourney AI"@en ;
    rdfs:label "Midjourney AI"@zh ;
    airo:hasRisk ex:ArtistsDataPrivacyRisk ;
    airo:hasAISubject ex:ArtistsInTrainingData ;
    airo:hasStakeholder ex:ArtCommunity .

ex:ArtistsDataPrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Artists Data Privacy Risk"@en ;
    rdfs:label "艺术家数据隐私风险"@zh ;
    airo:isRiskSourceFor ex:PublicDatabaseLeak ;
    airo:hasImpact ex:ArtistsPrivacyViolation .

ex:ArtistsInTrainingData rdf:type airo:AISubject ;
    rdfs:label "Artists in Training Data"@en ;
    rdfs:label "训练数据中的艺术家"@zh .

ex:PublicDatabaseLeak rdf:type airo:RiskSource ;
    rdfs:label "Public Database Leak"@en ;
    rdfs:label "公共数据库泄露"@zh .

ex:ArtistsPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Artists Privacy Violation"@en ;
    rdfs:label "艺术家隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:ArtCommunity .

ex:ArtCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Art Community"@en ;
    rdfs:label "艺术社区"@zh .