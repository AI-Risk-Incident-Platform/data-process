@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClearviewAIFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Clearview AI Facial Recognition System"@en ;
    rdfs:label "Clearview AI面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:ClearviewAI ;
    airo:hasStakeholder ex:ICEAgency ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:isAppliedWithinDomain ex:LawEnforcementAndSecurity .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:UnauthorizedDataAccess ;
    airo:isRiskSourceFor ex:SourceCodeSecurityLapse .

ex:ICEAgency rdf:type airo:Stakeholder ;
    rdfs:label "ICE Agency"@en ;
    rdfs:label "ICE机构"@zh .

ex:ClearviewAI rdf:type airo:AIOperator ;
    rdfs:label "Clearview AI Company"@en ;
    rdfs:label "Clearview AI公司"@zh .

ex:SourceCodeSecurityLapse rdf:type airo:RiskSource ;
    rdfs:label "Source Code Security Lapse"@en ;
    rdfs:label "源代码安全漏洞"@zh .

ex:UnauthorizedDataAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Data Access Consequence"@en ;
    rdfs:label "未经授权的数据访问后果"@zh ;
    airo:hasImpact ex:PersonalPrivacyCompromise .

ex:PersonalPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Compromise Impact"@en ;
    rdfs:label "个人隐私泄露影响"@zh ;
    airo:hasImpactOnStakeholder ex:ICEAgency .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:LawEnforcementAndSecurity rdf:type airo:Domain ;
    rdfs:label "Law Enforcement and Security Domain"@en ;
    rdfs:label "执法与安全领域"@zh .