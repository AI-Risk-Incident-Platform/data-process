@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DoNotPayRobotLawyer rdf:type airo:AISystem ;
    rdfs:label "DoNotPay Robot Lawyer"@en ;
    rdfs:label "DoNotPay机器人律师"@zh ;
    airo:hasRisk ex:LegalServiceMisrepresentationRisk ;
    airo:hasPurpose ex:AutomatedLegalAssistance ;
    airo:hasStakeholder ex:LegalConsumers ;
    airo:isAppliedWithinDomain ex:LegalServices .

ex:LegalServiceMisrepresentationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Service Misrepresentation Risk"@en ;
    rdfs:label "法律服务虚假陈述风险"@zh ;
    airo:hasConsequence ex:InadequateLegalRepresentation .

ex:LegalConsumers rdf:type airo:Stakeholder ;
    rdfs:label "Legal Consumers"@en ;
    rdfs:label "法律消费者"@zh .

ex:AutomatedLegalAssistance rdf:type airo:Purpose ;
    rdfs:label "Automated Legal Assistance"@en ;
    rdfs:label "自动化法律协助"@zh .

ex:LegalServices rdf:type airo:Domain ;
    rdfs:label "Legal Services"@en ;
    rdfs:label "法律服务"@zh .

ex:InadequateLegalRepresentation rdf:type airo:Consequence ;
    rdfs:label "Inadequate Legal Representation"@en ;
    rdfs:label "法律代表不足"@zh ;
    airo:hasImpact ex:LegalRightsCompromise .

ex:LegalRightsCompromise rdf:type airo:Impact ;
    rdfs:label "Legal Rights Compromise"@en ;
    rdfs:label "法律权利受损"@zh .