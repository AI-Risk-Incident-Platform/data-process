ex:ChatGPTAndCopilot rdf:type airo:AISystem ;
    rdfs:label "ChatGPT and Copilot"@en ;
    rdfs:label "ChatGPT和Copilot"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:isDevelopedBy ex:OpenAIAndMicrosoft ;
    airo:hasPurpose ex:InformationProvision ;
    airo:hasStakeholder ex:Public ;
    airo:isAppliedWithinDomain ex:PoliticalInformation ;
    airo:usesTechnique ex:LanguageModeling .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:TrainingDataBias .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误信虚假信息后果"@zh ;
    airo:hasImpact ex:ElectionInterference .

ex:Public rdf:type airo:Stakeholder ;
    rdfs:label "Public"@en ;
    rdfs:label "公众"@zh ;
    airo:hasImpactOnStakeholder ex:ElectionInterference .

ex:OpenAIAndMicrosoft rdf:type airo:AIOperator ;
    rdfs:label "OpenAI and Microsoft"@en ;
    rdfs:label "OpenAI和微软"@zh .

ex:ElectionInterference rdf:type airo:Impact ;
    rdfs:label "Election Interference Impact"@en ;
    rdfs:label "选举干扰影响"@zh .

ex:InformationProvision rdf:type airo:Purpose ;
    rdfs:label "Information Provision Purpose"@en ;
    rdfs:label "信息提供目的"@zh .

ex:PoliticalInformation rdf:type airo:Domain ;
    rdfs:label "Political Information Domain"@en ;
    rdfs:label "政治信息领域"@zh .

ex:LanguageModeling rdf:type airo:AITechnique ;
    rdfs:label "Language Modeling Technique"@en ;
    rdfs:label "语言建模技术"@zh .

ex:TrainingDataBias rdf:type airo:RiskSource ;
    rdfs:label "Training Data Bias Risk Source"@en ;
    rdfs:label "训练数据偏差风险源"@zh .