@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "人脸识别系统"@zh ;
    airo:hasRisk ex:FalsePositiveIdentificationRisk ;
    airo:hasAISubject ex:BusinesswomanInAdvertisement ;
    airo:usesTechnique ex:ImageRecognitionAlgorithm ;
    airo:hasStakeholder ex:TrafficManagementAuthority ;
    airo:hasVulnerability ex:StaticImageMisclassification .

ex:FalsePositiveIdentificationRisk rdf:type airo:Risk ;
    rdfs:label "False Positive Identification Risk"@en ;
    rdfs:label "误识别风险"@zh ;
    airo:hasConsequence ex:IncorrectJaywalkingCitation .

ex:IncorrectJaywalkingCitation rdf:type airo:Consequence ;
    rdfs:label "Incorrect Jaywalking Citation Consequence"@en ;
    rdfs:label "错误闯红灯处罚后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:BusinesswomanInAdvertisement rdf:type airo:AISubject ;
    rdfs:label "Businesswoman in Advertisement"@en ;
    rdfs:label "广告中的商业女性"@zh .

ex:TrafficManagementAuthority rdf:type airo:Stakeholder ;
    rdfs:label "Traffic Management Authority"@en ;
    rdfs:label "交通管理部门"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:BusinesswomanInAdvertisement .

ex:StaticImageMisclassification rdf:type airo:Vulnerability ;
    rdfs:label "Static Image Misclassification Vulnerability"@en ;
    rdfs:label "静态图像误分类漏洞"@zh .

ex:ImageRecognitionAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Image Recognition Algorithm"@en ;
    rdfs:label "图像识别算法"@zh .