@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Full Self-Driving System"@en ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    airo:hasRisk ex:VehicleSafetyRisk ;
    airo:hasStakeholder ex:TeslaEmployee .

ex:VehicleSafetyRisk rdf:type airo:Risk ;
    rdfs:label "Vehicle Safety Risk"@en ;
    rdfs:label "车辆安全风险"@zh ;
    airo:hasConsequence ex:PublicSafetyConcerns ;
    airo:isRiskSourceFor ex:SafetyComplaints .

ex:TeslaEmployee rdf:type airo:Stakeholder ;
    rdfs:label "Tesla Employee"@en ;
    rdfs:label "特斯拉员工"@zh .

ex:SafetyComplaints rdf:type airo:RiskSource ;
    rdfs:label "Safety Complaints"@en ;
    rdfs:label "安全投诉"@zh .

ex:PublicSafetyConcerns rdf:type airo:Consequence ;
    rdfs:label "Public Safety Concerns"@en ;
    rdfs:label "公共安全担忧"@zh .