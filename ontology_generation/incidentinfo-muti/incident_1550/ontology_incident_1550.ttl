@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TechConferenceSpeakerManagementSystem rdf:type airo:AISystem ;
    rdfs:label "Tech Conference Speaker Management System"@en ;
    rdfs:label "技术会议演讲者管理系统"@zh ;
    airo:hasRisk ex:FakeSpeakerProfileRisk ;
    airo:hasStakeholder ex:ConferenceOrganizers .

ex:FakeSpeakerProfileRisk rdf:type airo:Risk ;
    rdfs:label "Fake Speaker Profile Risk"@en ;
    rdfs:label "虚假演讲者资料风险"@zh ;
    airo:hasConsequence ex:ConferenceCancellation .

ex:ConferenceOrganizers rdf:type airo:Stakeholder ;
    rdfs:label "Conference Organizers"@en ;
    rdfs:label "会议组织者"@zh .

ex:ConferenceCancellation rdf:type airo:Consequence ;
    rdfs:label "Conference Cancellation Consequence"@en ;
    rdfs:label "会议取消后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:ConferenceOrganizers .