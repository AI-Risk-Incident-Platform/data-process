@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AISystem rdf:type airo:AISystem ;
    rdfs:label "AI System"@en ;
    rdfs:label "AI系统"@zh ;
    airo:hasRisk ex:Risk ;
    airo:hasStakeholder ex:Stakeholder .

ex:Risk rdf:type airo:Risk ;
    rdfs:label "Risk"@en ;
    rdfs:label "风险"@zh ;
    airo:hasConsequence ex:Consequence .

ex:Stakeholder rdf:type airo:Stakeholder ;
    rdfs:label "Stakeholder"@en ;
    rdfs:label "利益相关者"@zh .

ex:Consequence rdf:type airo:Consequence ;
    rdfs:label "Consequence"@en ;
    rdfs:label "后果"@zh ;
    airo:hasImpact ex:Impact .

ex:Impact rdf:type airo:Impact ;
    rdfs:label "Impact"@en ;
    rdfs:label "影响"@zh ;
    airo:hasImpactOnStakeholder ex:Stakeholder .