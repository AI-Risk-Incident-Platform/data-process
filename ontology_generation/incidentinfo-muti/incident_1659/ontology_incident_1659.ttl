@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleSearchAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Google Search Algorithm"@en ;
    rdfs:label "谷歌搜索算法"@zh ;
    airo:hasRisk ex:SpreadOfFalseInformationWithRightwingBias ;
    airo:hasCapability ex:SearchAutoCompleteFunctionality ;
    airo:isDevelopedBy ex:GoogleLLC ;
    airo:usesTechnique ex:MachineLearningBasedSearchRanking ;
    airo:hasPurpose ex:InformationRetrievalAndRanking ;
    airo:isAppliedWithinDomain ex:WebSearchEngine .

ex:SpreadOfFalseInformationWithRightwingBias rdf:type airo:Risk ;
    rdfs:label "Spread of False Information with Rightwing Bias Risk"@en ;
    rdfs:label "传播带有右翼偏见的虚假信息风险"@zh ;
    airo:hasConsequence ex:DisseminationOfSlanderAndDisinformation ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInSearchResults .

ex:DisseminationOfSlanderAndDisinformation rdf:type airo:Consequence ;
    rdfs:label "Dissemination of Slander and Disinformation Consequence"@en ;
    rdfs:label "诽谤和虚假信息传播后果"@zh ;
    airo:hasImpact ex:PublicMisinformationAndHarmfulContentExposure .

ex:PublicMisinformationAndHarmfulContentExposure rdf:type airo:Impact ;
    rdfs:label "Public Misinformation and Harmful Content Exposure Impact"@en ;
    rdfs:label "公众错误信息和有害内容暴露影响"@zh .

ex:SearchAutoCompleteFunctionality rdf:type airo:AICapability ;
    rdfs:label "Search Auto-Complete Functionality Capability"@en ;
    rdfs:label "搜索自动完成功能能力"@zh .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌有限责任公司"@zh .

ex:AlgorithmicBiasInSearchResults rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Search Results Risk Source"@en ;
    rdfs:label "搜索结果中的算法偏见风险源"@zh .

ex:MachineLearningBasedSearchRanking rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning Based Search Ranking Technique"@en ;
    rdfs:label "基于机器学习的搜索排名技术"@zh .

ex:InformationRetrievalAndRanking rdf:type airo:Purpose ;
    rdfs:label "Information Retrieval and Ranking Purpose"@en ;
    rdfs:label "信息检索和排名目的"@zh .

ex:WebSearchEngine rdf:type airo:Domain ;
    rdfs:label "Web Search Engine Domain"@en ;
    rdfs:label "网络搜索引擎领域"@zh .