@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AINewscasters rdf:type airo:AISystem ;
    rdfs:label "AI Newscasters"@en ;
    rdfs:label "AI新闻主播"@zh ;
    airo:hasRisk ex:JobDisplacementRisk ;
    airo:isDeployedBy ex:HawaiiNewspaper ;
    airo:hasStakeholder ex:HumanJournalists .

ex:JobDisplacementRisk rdf:type airo:Risk ;
    rdfs:label "Job Displacement Risk"@en ;
    rdfs:label "工作替代风险"@zh ;
    airo:hasConsequence ex:WorkforceDisplacement .

ex:HumanJournalists rdf:type airo:Stakeholder ;
    rdfs:label "Human Journalists"@en ;
    rdfs:label "人类记者"@zh .

ex:WorkforceDisplacement rdf:type airo:Impact ;
    rdfs:label "Workforce Displacement Impact"@en ;
    rdfs:label "劳动力替代影响"@zh ;
    airo:hasImpactOnStakeholder ex:HumanJournalists .

ex:HawaiiNewspaper rdf:type airo:AIOperator ;
    rdfs:label "Hawaii Newspaper"@en ;
    rdfs:label "夏威夷报社"@zh .