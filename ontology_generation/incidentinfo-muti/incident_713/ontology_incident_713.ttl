@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:AustralianPublic .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:EconomicHarm .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:AustralianPublic rdf:type airo:Stakeholder ;
    rdfs:label "Australian Public"@en ;
    rdfs:label "澳大利亚公众"@zh .

ex:EconomicHarm rdf:type airo:Impact ;
    rdfs:label "Economic Harm Impact"@en ;
    rdfs:label "经济损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:AustralianPublic .