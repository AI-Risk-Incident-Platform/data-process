@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VoiceAssistant rdf:type airo:AISystem ;
    rdfs:label "Voice Assistant"@en ;
    rdfs:label "语音助手"@zh ;
    airo:hasRisk ex:HinderingSocialCognitiveDevelopment ;
    airo:hasAISubject ex:Children .

ex:HinderingSocialCognitiveDevelopment rdf:type airo:Risk ;
    rdfs:label "Hindering Social and Cognitive Development Risk"@en ;
    rdfs:label "阻碍社交和认知发展风险"@zh ;
    airo:hasConsequence ex:ReducedHumanInteraction .

ex:Children rdf:type airo:AISubject ;
    rdfs:label "Children"@en ;
    rdfs:label "儿童"@zh .

ex:ReducedHumanInteraction rdf:type airo:Consequence ;
    rdfs:label "Reduced Human Interaction Consequence"@en ;
    rdfs:label "减少人际互动后果"@zh ;
    airo:hasImpact ex:ImpairedSocialSkills .

ex:ImpairedSocialSkills rdf:type airo:Impact ;
    rdfs:label "Impaired Social Skills Impact"@en ;
    rdfs:label "社交能力受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:Children .