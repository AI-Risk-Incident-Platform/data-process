@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CrisisTextLine rdf:type airo:AISystem ;
    rdfs:label "Crisis Text Line"@en ;
    rdfs:label "危机短信热线"@zh ;
    airo:hasRisk ex:DataPrivacyRisk ;
    airo:hasStakeholder ex:ForProfitAICompany .

ex:DataPrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Data Privacy Risk"@en ;
    rdfs:label "数据隐私风险"@zh ;
    airo:hasConsequence [ 
        rdf:type airo:Consequence ;
        airo:hasImpact ex:LossOfTrust ;
        rdfs:label "因数据共享导致的后果"@zh ;
        rdfs:label "Consequence due to data sharing"@en 
    ] ;
    airo:isRiskSourceFor ex:DataSharingWithThirdParty .

ex:ForProfitAICompany rdf:type airo:Stakeholder ;
    rdfs:label "For-Profit AI Company"@en ;
    rdfs:label "营利性AI公司"@zh .

ex:DataSharingWithThirdParty rdf:type airo:RiskSource ;
    rdfs:label "Data Sharing with Third Party"@en ;
    rdfs:label "与第三方共享数据"@zh .

ex:LossOfTrust rdf:type airo:Impact ;
    rdfs:label "Loss of Trust"@en ;
    rdfs:label "信任丧失"@zh ;
    airo:hasImpactOnStakeholder ex:ForProfitAICompany .