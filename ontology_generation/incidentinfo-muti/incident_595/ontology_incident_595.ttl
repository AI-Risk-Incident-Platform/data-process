@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh ;
    airo:hasRisk ex:WrongfulArrestRisk ;
    airo:isDeployedBy ex:JPSO ;
    airo:hasAISubject ex:ArrestedMan .

ex:WrongfulArrestRisk rdf:type airo:Risk ;
    rdfs:label "Wrongful Arrest Risk"@en ;
    rdfs:label "错误逮捕风险"@zh ;
    airo:hasConsequence ex:IncorrectIdentification ;
    airo:isRiskSourceFor ex:TechnologyError .

ex:IncorrectIdentification rdf:type airo:Consequence ;
    rdfs:label "Incorrect Identification Consequence"@en ;
    rdfs:label "错误识别后果"@zh ;
    airo:hasImpact ex:PersonalRightsViolation .

ex:PersonalRightsViolation rdf:type airo:Impact ;
    rdfs:label "Personal Rights Violation Impact"@en ;
    rdfs:label "个人权利侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:ArrestedMan .

ex:JPSO rdf:type airo:AIOperator ;
    rdfs:label "JPSO"@en ;
    rdfs:label "JPSO"@zh .

ex:ArrestedMan rdf:type airo:AISubject ;
    rdfs:label "Arrested Man"@en ;
    rdfs:label "被逮捕者"@zh .

ex:TechnologyError rdf:type airo:RiskSource ;
    rdfs:label "Technology Error Risk Source"@en ;
    rdfs:label "技术错误风险源"@zh .