ex:FacialRecognitionSystemInComo rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System in Como"@en ;
    rdfs:label "科莫市面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasCapability ex:FacialRecognitionCapability ;
    airo:isAppliedWithinDomain ex:PublicSurveillance ;
    airo:hasStakeholder ex:ComoCityGovernment .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:ComoCityGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Como City Government"@en ;
    rdfs:label "科莫市政府"@zh .

ex:FacialRecognitionCapability rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition Capability"@en ;
    rdfs:label "面部识别能力"@zh .

ex:PublicSurveillance rdf:type airo:Domain ;
    rdfs:label "Public Surveillance Domain"@en ;
    rdfs:label "公共监控领域"@zh .