@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IndustrialRobotArm rdf:type airo:AISystem ;
    rdfs:label "Industrial Robot Arm"@en ;
    rdfs:label "工业机器人手臂"@zh ;
    airo:hasRisk ex:WorkplaceFatalityRisk ;
    airo:isAppliedWithinDomain ex:IndustrialManufacturing ;
    airo:hasStakeholder ex:FactoryWorker ;
    airo:isDeployedBy ex:FactoryManagement .

ex:WorkplaceFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Workplace Fatality Risk"@en ;
    rdfs:label "工作场所致命风险"@zh ;
    airo:hasConsequence ex:WorkerDeath ;
    airo:isRiskSourceFor ex:RobotMalfunction .

ex:WorkerDeath rdf:type airo:Consequence ;
    rdfs:label "Worker Death Consequence"@en ;
    rdfs:label "工人死亡后果"@zh ;
    airo:hasImpact ex:HumanLifeLoss .

ex:HumanLifeLoss rdf:type airo:Impact ;
    rdfs:label "Human Life Loss Impact"@en ;
    rdfs:label "人员生命损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:FactoryWorker .

ex:IndustrialManufacturing rdf:type airo:Domain ;
    rdfs:label "Industrial Manufacturing Domain"@en ;
    rdfs:label "工业制造领域"@zh .

ex:FactoryWorker rdf:type airo:Stakeholder ;
    rdfs:label "Factory Worker"@en ;
    rdfs:label "工厂工人"@zh .

ex:RobotMalfunction rdf:type airo:RiskSource ;
    rdfs:label "Robot Malfunction Risk Source"@en ;
    rdfs:label "机器人故障风险源"@zh .

ex:FactoryManagement rdf:type airo:AIOperator ;
    rdfs:label "Factory Management"@en ;
    rdfs:label "工厂管理层"@zh .