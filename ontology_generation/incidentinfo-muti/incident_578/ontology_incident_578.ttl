@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LungProblemDiagnosisSoftware rdf:type airo:AISystem ;
    rdfs:label "Lung Problem Diagnosis Software"@en ;
    rdfs:label "肺部问题诊断软件"@zh ;
    airo:hasRisk ex:RacialBiasInDiagnosis .

ex:RacialBiasInDiagnosis rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Diagnosis Risk"@en ;
    rdfs:label "诊断中的种族偏见风险"@zh ;
    airo:hasConsequence ex:UnderdiagnosisOfLungProblems ;
    airo:isRiskSourceFor ex:BiasedAlgorithm .

ex:UnderdiagnosisOfLungProblems rdf:type airo:Consequence ;
    rdfs:label "Underdiagnosis of Lung Problems Consequence"@en ;
    rdfs:label "肺部问题诊断不足的后果"@zh ;
    airo:hasImpact ex:HealthDisparities .

ex:HealthDisparities rdf:type airo:Impact ;
    rdfs:label "Health Disparities Impact"@en ;
    rdfs:label "健康差异的影响"@zh .

ex:BiasedAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Biased Algorithm Risk Source"@en ;
    rdfs:label "有偏见的算法风险源"@zh .