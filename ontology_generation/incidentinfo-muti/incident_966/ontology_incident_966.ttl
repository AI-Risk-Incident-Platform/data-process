@base <https://w3id.org/airo#> .

ex:WesternAITools rdf:type airo:AISystem ;
    rdfs:label "西方人工智能工具"@zh ;
    rdfs:label "Western AI Tools"@en ;
    airo:hasRisk ex:RussianPropagandaInfectionRisk ;
    airo:usesTechnique ex:ChatbotTechnology ;
    airo:hasStakeholder ex:WesternAIUsers .

ex:RussianPropagandaInfectionRisk rdf:type airo:Risk ;
    rdfs:label "俄罗斯宣传感染风险"@zh ;
    rdfs:label "Russian Propaganda Infection Risk"@en ;
    airo:hasConsequence ex:SpreadOfRussianDisinformation ;
    airo:isRiskSourceFor ex:MoscowBasedGlobalNewsNetwork .

ex:MoscowBasedGlobalNewsNetwork rdf:type airo:RiskSource ;
    rdfs:label "莫斯科全球新闻网络"@zh ;
    rdfs:label "Moscow-Based Global News Network"@en .

ex:SpreadOfRussianDisinformation rdf:type airo:Consequence ;
    rdfs:label "俄罗斯虚假信息传播后果"@zh ;
    rdfs:label "Spread of Russian Disinformation Consequence"@en ;
    airo:hasImpact ex:PublicOpinionManipulation .

ex:PublicOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "公众舆论操纵影响"@zh ;
    rdfs:label "Public Opinion Manipulation Impact"@en ;
    airo:hasImpactOnStakeholder ex:WesternAIUsers .

ex:WesternAIUsers rdf:type airo:Stakeholder ;
    rdfs:label "西方AI使用者"@zh ;
    rdfs:label "Western AI Users"@en .

ex:ChatbotTechnology rdf:type airo:AITechnique ;
    rdfs:label "聊天机器人技术"@zh ;
    rdfs:label "Chatbot Technology"@en .