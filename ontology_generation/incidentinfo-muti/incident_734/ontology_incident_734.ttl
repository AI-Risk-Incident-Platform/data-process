ex:OpenAIModels rdf:type airo:AISystem ;
    rdfs:label "OpenAI模型"@zh ;
    rdfs:label "OpenAI Models"@en ;
    airo:hasRisk ex:MaliciousServicesRisk ;
    airo:hasStakeholder ex:Researchers ;
    airo:hasCapability ex:PoweringMaliciousServices .

ex:MaliciousServicesRisk rdf:type airo:Risk ;
    rdfs:label "恶意服务风险"@zh ;
    rdfs:label "Malicious Services Risk"@en ;
    airo:isRiskSourceFor ex:UndergroundMarket .

ex:Researchers rdf:type airo:Stakeholder ;
    rdfs:label "研究人员"@zh ;
    rdfs:label "Researchers"@en .

ex:UndergroundMarket rdf:type airo:RiskSource ;
    rdfs:label "地下市场"@zh ;
    rdfs:label "Underground Market"@en .

ex:PoweringMaliciousServices rdf:type airo:AICapability ;
    rdfs:label "支持恶意服务的能力"@zh ;
    rdfs:label "Powering Malicious Services Capability"@en .