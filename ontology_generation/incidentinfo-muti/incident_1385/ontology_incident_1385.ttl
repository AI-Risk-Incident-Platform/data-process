@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaAI rdf:type airo:AISystem ;
    rdfs:label "Meta AI"@en ;
    rdfs:label "Meta人工智能"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:Meta ;
    airo:usesTechnique ex:DataScraping ;
    airo:hasStakeholder ex:InstagramArtists .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:UserDataCollectionWithoutConsent ;
    airo:hasConsequence ex:ArtistContentMisappropriation .

ex:InstagramArtists rdf:type airo:Stakeholder ;
    rdfs:label "Instagram Artists"@en ;
    rdfs:label "Instagram艺术家"@zh .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@en ;
    rdfs:label "Meta公司"@zh .

ex:DataScraping rdf:type airo:AITechnique ;
    rdfs:label "Data Scraping Technique"@en ;
    rdfs:label "数据抓取技术"@zh .

ex:UserDataCollectionWithoutConsent rdf:type airo:RiskSource ;
    rdfs:label "User Data Collection Without Consent Risk Source"@en ;
    rdfs:label "未经同意的用户数据收集"@zh .

ex:ArtistContentMisappropriation rdf:type airo:Impact ;
    rdfs:label "Artist Content Misappropriation Impact"@en ;
    rdfs:label "艺术家内容被挪用影响"@zh ;
    airo:hasImpactOnStakeholder ex:InstagramArtists .