@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutomatedHotelManagementSystem rdf:type airo:AISystem ;
    rdfs:label "Automated Hotel Management System"@en ;
    rdfs:label "自动化酒店管理系统"@zh ;
    airo:hasRisk ex:JobDisplacementRisk ;
    airo:hasStakeholder ex:HotelWorkers ;
    airo:isAppliedWithinDomain ex:HospitalityIndustryDomain .

ex:JobDisplacementRisk rdf:type airo:Risk ;
    rdfs:label "Job Displacement Risk"@en ;
    rdfs:label "工作替代风险"@zh ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Consequence of Job Displacement"@en ;
        rdfs:label "工作替代后果"@zh ;
        airo:hasImpact ex:EconomicHardshipForWorkers
    ] .

ex:HotelWorkers rdf:type airo:Stakeholder ;
    rdfs:label "Hotel Workers"@en ;
    rdfs:label "酒店员工"@zh .

ex:EconomicHardshipForWorkers rdf:type airo:Impact ;
    rdfs:label "Economic Hardship for Workers Impact"@en ;
    rdfs:label "员工经济困难的影响"@zh ;
    airo:hasImpactOnStakeholder ex:HotelWorkers .

ex:HospitalityIndustryDomain rdf:type airo:Domain ;
    rdfs:label "Hospitality Industry Domain"@en ;
    rdfs:label "酒店业领域"@zh .