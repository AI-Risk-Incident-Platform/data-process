@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RedditUserProfilingAI rdf:type airo:AISystem ;
    rdfs:label "Reddit User Profiling AI"@en ;
    rdfs:label "Reddit用户画像AI"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:RedditUsers ;
    airo:hasCapability ex:UserLocationAndIncomePrediction ;
    airo:isAppliedWithinDomain ex:SocialMediaAnalytics .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalPrivacyExposure .

ex:UserLocationAndIncomePrediction rdf:type airo:AICapability ;
    rdfs:label "User Location and Income Prediction Capability"@en ;
    rdfs:label "用户位置和收入预测能力"@zh .

ex:RedditUsers rdf:type airo:AISubject ;
    rdfs:label "Reddit Users"@en ;
    rdfs:label "Reddit用户"@zh .

ex:PersonalPrivacyExposure rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Exposure Impact"@en ;
    rdfs:label "个人隐私暴露影响"@zh ;
    airo:hasImpactOnStakeholder ex:RedditUsers .

ex:SocialMediaAnalytics rdf:type airo:Domain ;
    rdfs:label "Social Media Analytics Domain"@en ;
    rdfs:label "社交媒体分析领域"@zh .