@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClearviewAIFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Clearview AI Facial Recognition System"@en ;
    rdfs:label "Clearview AI面部识别系统"@zh ;
    airo:hasRisk ex:FacialRecognitionAccuracyRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:isDevelopedBy ex:ClearviewAI ;
    airo:hasPurpose ex:LawEnforcementSupport ;
    airo:hasStakeholder ex:ACLU ;
    airo:isAppliedWithinDomain ex:PublicSafety .

ex:FacialRecognitionAccuracyRisk rdf:type airo:Risk ;
    rdfs:label "Facial Recognition Accuracy Risk"@en ;
    rdfs:label "面部识别准确性风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation ;
    airo:isRiskSourceFor ex:WebScrapingAbuse .

ex:WebScrapingAbuse rdf:type airo:RiskSource ;
    rdfs:label "Web Scraping Abuse Risk Source"@en ;
    rdfs:label "网络爬虫滥用风险源"@zh .

ex:ACLU rdf:type airo:Stakeholder ;
    rdfs:label "American Civil Liberties Union"@en ;
    rdfs:label "美国公民自由联盟"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Privacy Violation Consequence"@en ;
    rdfs:label "隐私侵犯后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:ACLU .

ex:ClearviewAI rdf:type airo:AIOperator ;
    rdfs:label "Clearview AI Company"@en ;
    rdfs:label "Clearview AI公司"@zh .

ex:LawEnforcementSupport rdf:type airo:Purpose ;
    rdfs:label "Law Enforcement Support Purpose"@en ;
    rdfs:label "执法支持目的"@zh .

ex:PublicSafety rdf:type airo:Domain ;
    rdfs:label "Public Safety Domain"@en ;
    rdfs:label "公共安全领域"@zh .