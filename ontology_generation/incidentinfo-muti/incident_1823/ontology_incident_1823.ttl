@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaLlamaAI rdf:type airo:AISystem ;
    rdfs:label "Meta Llama AI"@en ;
    rdfs:label "Meta Llama AI"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDevelopedBy ex:Meta ;
    airo:hasStakeholder ex:CopyrightHolders .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalLiability ;
    airo:isRiskSourceFor ex:PiracyDatabase ;
    airo:hasRiskSource ex:PiracyDatabase .

ex:PiracyDatabase rdf:type airo:RiskSource ;
    rdfs:label "Piracy Database"@en ;
    rdfs:label "盗版数据库"@zh ;
    airo:isRiskSourceFor ex:CopyrightInfringementRisk .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@en ;
    rdfs:label "Meta"@zh .

ex:CopyrightHolders rdf:type airo:Stakeholder ;
    rdfs:label "Copyright Holders"@en ;
    rdfs:label "版权持有者"@zh .

ex:LegalLiability rdf:type airo:Consequence ;
    rdfs:label "Legal Liability"@en ;
    rdfs:label "法律责任"@zh ;
    airo:hasImpactOnStakeholder ex:CopyrightHolders .

ex:UnauthorizedDataUse rdf:type airo:Misuse ;
    rdfs:label "Unauthorized Data Use"@en ;
    rdfs:label "未经授权的数据使用"@zh ;
    airo:hasRiskSource ex:PiracyDatabase ;
    airo:hasRisk ex:CopyrightInfringementRisk .