@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GitHubCopilot rdf:type airo:AISystem ;
    rdfs:label "GitHub Copilot"@en ;
    rdfs:label "GitHub Copilot"@zh ;
    airo:hasRisk ex:CodeCopyrightInfringementRisk ;
    airo:isDevelopedBy ex:Microsoft ;
    airo:hasStakeholder ex:GitHubUsers ;
    airo:usesTechnique ex:CodeGenerationAI .

ex:CodeCopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Code Copyright Infringement Risk"@en ;
    rdfs:label "代码版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalActionConsequence ;
    airo:isRiskSourceFor ex:UnlicensedCodeUsage .

ex:GitHubUsers rdf:type airo:Stakeholder ;
    rdfs:label "GitHub Users"@en ;
    rdfs:label "GitHub用户"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Action Consequence"@en ;
    rdfs:label "法律诉讼后果"@zh ;
    airo:hasImpactOnStakeholder ex:GitHubUsers .

ex:UnlicensedCodeUsage rdf:type airo:RiskSource ;
    rdfs:label "Unlicensed Code Usage"@en ;
    rdfs:label "未经授权的代码使用"@zh .

ex:CodeGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Code Generation AI"@en ;
    rdfs:label "代码生成AI"@zh .