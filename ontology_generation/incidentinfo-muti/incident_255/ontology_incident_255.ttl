@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CruiseRobotaxis rdf:type airo:AISystem ;
    rdfs:label "Cruise自动驾驶出租车"@zh ;
    rdfs:label "Cruise Robotaxis"@en ;
    airo:hasRisk ex:TrafficDisruptionRisk ;
    airo:isDeployedBy ex:GeneralMotors ;
    airo:isAppliedWithinDomain ex:UrbanTransportation .

ex:TrafficDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "交通中断风险"@zh ;
    rdfs:label "Traffic Disruption Risk"@en ;
    airo:hasConsequence ex:StreetBlockage .

ex:StreetBlockage rdf:type airo:Consequence ;
    rdfs:label "街道堵塞后果"@zh ;
    rdfs:label "Street Blockage Consequence"@en ;
    airo:hasImpact ex:PublicInconvenience .

ex:GeneralMotors rdf:type airo:AIOperator ;
    rdfs:label "通用汽车"@zh ;
    rdfs:label "General Motors"@en .

ex:UrbanTransportation rdf:type airo:Domain ;
    rdfs:label "城市交通领域"@zh ;
    rdfs:label "Urban Transportation Domain"@en .

ex:PublicInconvenience rdf:type airo:Impact ;
    rdfs:label "公众不便影响"@zh ;
    rdfs:label "Public Inconvenience Impact"@en ;
    airo:hasImpactOnStakeholder ex:GeneralMotors .