@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FaceApp rdf:type airo:AISystem ;
    rdfs:label "FaceApp"@en ;
    rdfs:label "FaceApp"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:FaceAppUsers .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:DataStorageWithoutPermission .

ex:FaceAppUsers rdf:type airo:Stakeholder ;
    rdfs:label "FaceApp Users"@en ;
    rdfs:label "FaceApp用户"@zh .

ex:DataStorageWithoutPermission rdf:type airo:RiskSource ;
    rdfs:label "Data Storage Without Permission"@en ;
    rdfs:label "未经许可的数据存储"@zh ;
    airo:hasImpact ex:UserPrivacyConcerns .

ex:UserPrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "User Privacy Concerns"@en ;
    rdfs:label "用户隐私担忧"@zh ;
    airo:hasImpactOnStakeholder ex:FaceAppUsers .