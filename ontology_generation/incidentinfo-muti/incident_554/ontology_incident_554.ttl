@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:AcademicIntegrityRisk ;
    airo:isAppliedWithinDomain ex:AcademicResearch ;
    airo:hasPurpose ex:PeerReviewAutomation ;
    airo:hasStakeholder ex:AustralianResearchCouncil .

ex:AcademicIntegrityRisk rdf:type airo:Risk ;
    rdfs:label "Academic Integrity Risk"@en ;
    rdfs:label "学术诚信风险"@zh ;
    airo:hasConsequence ex:CompromisedResearchEvaluationQuality ;
    airo:isRiskSourceFor ex:UnauthorizedAIUseInAcademicProcesses .

ex:AcademicResearch rdf:type airo:Domain ;
    rdfs:label "Academic Research Domain"@en ;
    rdfs:label "学术研究领域"@zh .

ex:PeerReviewAutomation rdf:type airo:Purpose ;
    rdfs:label "Peer Review Automation Purpose"@en ;
    rdfs:label "同行评审自动化目的"@zh .

ex:AustralianResearchCouncil rdf:type airo:Stakeholder ;
    rdfs:label "Australian Research Council"@en ;
    rdfs:label "澳大利亚研究委员会"@zh .

ex:UnauthorizedAIUseInAcademicProcesses rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized AI Use in Academic Processes Risk Source"@en ;
    rdfs:label "学术流程中未经授权使用AI的风险源"@zh .

ex:CompromisedResearchEvaluationQuality rdf:type airo:Consequence ;
    rdfs:label "Compromised Research Evaluation Quality Consequence"@en ;
    rdfs:label "研究评估质量受损的后果"@zh .