@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Autopilot System"@en ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleMisuseRisk ;
    airo:hasStakeholder ex:AlbertaRCMP ;
    airo:hasAIUser ex:TeslaDriver ;
    airo:isAppliedWithinDomain ex:AutomotiveTransportation .

ex:AutonomousVehicleMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Misuse Risk"@en ;
    rdfs:label "自动驾驶汽车滥用风险"@zh ;
    airo:hasConsequence ex:TrafficSafetyViolation ;
    airo:isRiskSourceFor ex:SleepingWhileAutopilotEngaged .

ex:AlbertaRCMP rdf:type airo:Stakeholder ;
    rdfs:label "Alberta RCMP"@en ;
    rdfs:label "阿尔伯塔皇家骑警"@zh .

ex:TeslaDriver rdf:type airo:AIUser ;
    rdfs:label "Tesla Driver"@en ;
    rdfs:label "特斯拉驾驶员"@zh .

ex:TrafficSafetyViolation rdf:type airo:Consequence ;
    rdfs:label "Traffic Safety Violation Consequence"@en ;
    rdfs:label "交通安全违规后果"@zh .

ex:SleepingWhileAutopilotEngaged rdf:type airo:Misuse ;
    rdfs:label "Sleeping While Autopilot Engaged Misuse"@en ;
    rdfs:label "自动驾驶启用时睡觉的滥用行为"@zh .

ex:AutomotiveTransportation rdf:type airo:Domain ;
    rdfs:label "Automotive Transportation Domain"@en ;
    rdfs:label "汽车交通领域"@zh .