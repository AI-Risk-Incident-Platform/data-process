@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAI rdf:type airo:AISystem ;
    rdfs:label "Google AI"@en ;
    rdfs:label "谷歌AI"@zh ;
    airo:hasRisk ex:FalseInformationRisk ;
    airo:hasCapability ex:InformationRetrieval ;
    airo:isDeployedBy ex:Google ;
    airo:hasStakeholder ex:SuperBowlViewers ;
    airo:hasPurpose ex:Advertising ;
    airo:isAppliedWithinDomain ex:Marketing .

ex:FalseInformationRisk rdf:type airo:Risk ;
    rdfs:label "False Information Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:IncorrectData .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:BrandReputationDamage .

ex:InformationRetrieval rdf:type airo:AICapability ;
    rdfs:label "Information Retrieval Capability"@en ;
    rdfs:label "信息检索能力"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:SuperBowlViewers rdf:type airo:Stakeholder ;
    rdfs:label "Super Bowl Viewers"@en ;
    rdfs:label "超级碗观众"@zh .

ex:BrandReputationDamage rdf:type airo:Impact ;
    rdfs:label "Brand Reputation Damage Impact"@en ;
    rdfs:label "品牌声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:SuperBowlViewers .

ex:Advertising rdf:type airo:Purpose ;
    rdfs:label "Advertising Purpose"@en ;
    rdfs:label "广告宣传目的"@zh .

ex:Marketing rdf:type airo:Domain ;
    rdfs:label "Marketing Domain"@en ;
    rdfs:label "市场营销领域"@zh .

ex:IncorrectData rdf:type airo:RiskSource ;
    rdfs:label "Incorrect Data Risk Source"@en ;
    rdfs:label "错误数据风险源"@zh .