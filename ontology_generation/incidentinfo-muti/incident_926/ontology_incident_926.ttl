@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RussianDisinformationAI rdf:type airo:AISystem ;
    rdfs:label "Russian Disinformation AI"@en ;
    rdfs:label "俄罗斯虚假信息AI"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:usesTechnique ex:DisinformationGeneration ;
    airo:hasPurpose ex:InfluenceElectionOutcomes ;
    airo:hasStakeholder ex:GermanVoters ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:isRiskSourceFor ex:ForeignStateActors ;
    airo:hasConsequence ex:PublicConfusion ;
    airo:hasRiskControl ex:PlatformProtectionMeasures .

ex:GermanVoters rdf:type airo:Stakeholder ;
    rdfs:label "German Voters"@en ;
    rdfs:label "德国选民"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections"@en ;
    rdfs:label "政治选举"@zh .

ex:DemocraticProcessUndermining rdf:type airo:Impact ;
    rdfs:label "Democratic Process Undermining"@en ;
    rdfs:label "民主进程破坏"@zh ;
    airo:hasImpactOnStakeholder ex:GermanVoters .

ex:ForeignStateActors rdf:type airo:RiskSource ;
    rdfs:label "Foreign State Actors"@en ;
    rdfs:label "外国政府行为者"@zh .

ex:DisinformationGeneration rdf:type airo:AITechnique ;
    rdfs:label "Disinformation Generation Technique"@en ;
    rdfs:label "虚假信息生成技术"@zh .

ex:InfluenceElectionOutcomes rdf:type airo:Purpose ;
    rdfs:label "Influence Election Outcomes"@en ;
    rdfs:label "影响选举结果"@zh .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion"@en ;
    rdfs:label "公众困惑"@zh ;
    airo:hasImpact ex:DemocraticProcessUndermining .

ex:PlatformProtectionMeasures rdf:type airo:RiskControl ;
    rdfs:label "Platform Protection Measures"@en ;
    rdfs:label "平台保护措施"@zh ;
    airo:mitigatesRiskConcept ex:ElectionInterferenceRisk .