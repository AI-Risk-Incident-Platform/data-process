@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFSDSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    rdfs:label "Tesla Full Self-Driving System"@en ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:hasCapability ex:AutonomousDrivingCapability ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTransportation ;
    airo:hasStakeholder ex:TeslaInc .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    airo:hasConsequence ex:PublicSafetyConcern .

ex:PublicSafetyConcern rdf:type airo:Consequence ;
    rdfs:label "公共安全担忧后果"@zh ;
    rdfs:label "Public Safety Concern Consequence"@en ;
    airo:hasImpact ex:ConsumerTrustErosion .

ex:AutonomousDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:AutonomousVehicleTransportation rdf:type airo:Domain ;
    rdfs:label "自动驾驶交通领域"@zh ;
    rdfs:label "Autonomous Vehicle Transportation Domain"@en .

ex:ConsumerTrustErosion rdf:type airo:Impact ;
    rdfs:label "消费者信任度下降的影响"@zh ;
    rdfs:label "Consumer Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaInc .