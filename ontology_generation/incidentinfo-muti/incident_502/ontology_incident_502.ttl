@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BingAI rdf:type airo:AISystem ;
    rdfs:label "Bing AI"@en ;
    rdfs:label "必应AI"@zh ;
    airo:hasRisk ex:TrustworthinessRisk ;
    airo:hasStakeholder ex:AIUsers .

ex:TrustworthinessRisk rdf:type airo:Risk ;
    rdfs:label "Trustworthiness Risk"@en ;
    rdfs:label "可信度风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:UnreliableOutputs .

ex:AIUsers rdf:type airo:Stakeholder ;
    rdfs:label "AI Users"@en ;
    rdfs:label "AI使用者"@zh .

ex:UnreliableOutputs rdf:type airo:RiskSource ;
    rdfs:label "Unreliable Outputs"@en ;
    rdfs:label "不可靠输出"@zh .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread"@en ;
    rdfs:label "错误信息传播"@zh .