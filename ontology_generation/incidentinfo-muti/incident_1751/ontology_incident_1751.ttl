@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaCarCameraSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Car Camera System"@en ;
    rdfs:label "特斯拉汽车摄像头系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:TeslaEmployees ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasPurpose ex:VehicleSafetyMonitoring ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:CustomerPrivacyBreach ;
    airo:isRiskSourceFor ex:UnauthorizedDataAccess .

ex:CustomerPrivacyBreach rdf:type airo:Consequence ;
    rdfs:label "Customer Privacy Breach Consequence"@en ;
    rdfs:label "客户隐私泄露后果"@zh ;
    airo:hasImpact ex:CustomerTrustErosion .

ex:TeslaEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Tesla Employees"@en ;
    rdfs:label "特斯拉员工"@zh .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:CustomerTrustErosion rdf:type airo:Impact ;
    rdfs:label "Customer Trust Erosion Impact"@en ;
    rdfs:label "客户信任流失影响"@zh ;
    airo:hasImpactOnStakeholder ex:TeslaEmployees .

ex:VehicleSafetyMonitoring rdf:type airo:Purpose ;
    rdfs:label "Vehicle Safety Monitoring Purpose"@en ;
    rdfs:label "车辆安全监控目的"@zh .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "Automotive Industry Domain"@en ;
    rdfs:label "汽车工业领域"@zh .

ex:UnauthorizedDataAccess rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Data Access Risk Source"@en ;
    rdfs:label "未经授权的数据访问风险源"@zh .