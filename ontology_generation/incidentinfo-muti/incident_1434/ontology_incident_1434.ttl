@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PimEyesFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "PimEyes Facial Recognition System"@en ;
    rdfs:label "PimEyes面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:UKPolice ;
    airo:isDeployedBy ex:UKPolice ;
    airo:isProvidedBy ex:PimEyesProvider ;
    airo:hasCapability ex:FacialRecognitionSearch ;
    airo:isAppliedWithinDomain ex:LawEnforcement .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:MassSurveillanceConcerns .

ex:UKPolice rdf:type airo:Stakeholder ;
    rdfs:label "UK Police"@en ;
    rdfs:label "英国警方"@zh .

ex:PimEyesProvider rdf:type airo:AIOperator ;
    rdfs:label "PimEyes Provider"@en ;
    rdfs:label "PimEyes提供者"@zh .

ex:FacialRecognitionSearch rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition Search Capability"@en ;
    rdfs:label "面部识别搜索能力"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .

ex:MassSurveillanceConcerns rdf:type airo:Impact ;
    rdfs:label "Mass Surveillance Concerns Impact"@en ;
    rdfs:label "大规模监控担忧的影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKPolice .