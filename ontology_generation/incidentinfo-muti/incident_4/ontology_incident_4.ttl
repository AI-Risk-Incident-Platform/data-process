@base <https://w3id.org/airo#> .

ex:WordEmbeddingDebiasingSystem rdf:type airo:AISystem ;
    rdfs:label "Word Embedding Debiasing System"@en ;
    rdfs:label "词嵌入去偏系统"@zh ;
    airo:hasRisk ex:GenderBiasInWordEmbeddings ;
    airo:usesTechnique ex:WordEmbedding .

ex:GenderBiasInWordEmbeddings rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Word Embeddings Risk"@en ;
    rdfs:label "词嵌入中的性别偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:ReinforcementOfGenderStereotypes .

ex:WordEmbedding rdf:type airo:AITechnique ;
    rdfs:label "Word Embedding Technique"@en ;
    rdfs:label "词嵌入技术"@zh .

ex:ReinforcementOfGenderStereotypes rdf:type airo:Impact ;
    rdfs:label "Reinforcement of Gender Stereotypes Impact"@en ;
    rdfs:label "强化性别刻板印象的影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .