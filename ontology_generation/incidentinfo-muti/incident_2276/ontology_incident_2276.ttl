@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WalmartAntiShopliftingAI rdf:type airo:AISystem ;
    rdfs:label "Walmart Anti-Shoplifting AI System"@en ;
    rdfs:label "沃尔玛反盗窃AI系统"@zh ;
    airo:hasRisk ex:IneffectiveAntiTheftSystemRisk ;
    airo:hasStakeholder ex:WalmartEmployees .

ex:IneffectiveAntiTheftSystemRisk rdf:type airo:Risk ;
    rdfs:label "Ineffective Anti-Theft System Risk"@en ;
    rdfs:label "无效反盗窃系统风险"@zh ;
    airo:isRiskSourceFor ex:BiasInCashierlessTech ;
    airo:hasImpact ex:LossOfTrustInAISecurity .

ex:WalmartEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Walmart Employees"@en ;
    rdfs:label "沃尔玛员工"@zh .

ex:BiasInCashierlessTech rdf:type airo:RiskSource ;
    rdfs:label "Bias in Cashierless Tech"@en ;
    rdfs:label "无收银员技术中的偏见"@zh .

ex:LossOfTrustInAISecurity rdf:type airo:Impact ;
    rdfs:label "Loss of Trust in AI Security"@en ;
    rdfs:label "对AI安全性的信任丧失"@zh ;
    airo:hasImpactOnStakeholder ex:WalmartEmployees .