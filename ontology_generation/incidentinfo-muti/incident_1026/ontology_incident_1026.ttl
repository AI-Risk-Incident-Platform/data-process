@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VideoDoctoringAI rdf:type airo:AISystem ;
    rdfs:label "Video Doctoring AI"@en ;
    rdfs:label "视频篡改AI"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasStakeholder ex:NigerianPublic ;
    airo:isAppliedWithinDomain ex:PoliticalCommunication .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicConfusion .

ex:NigerianPublic rdf:type airo:Stakeholder ;
    rdfs:label "Nigerian Public"@en ;
    rdfs:label "尼日利亚公众"@zh .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion"@en ;
    rdfs:label "公众困惑"@zh ;
    airo:hasImpactOnStakeholder ex:NigerianPublic .

ex:PoliticalCommunication rdf:type airo:Domain ;
    rdfs:label "Political Communication"@en ;
    rdfs:label "政治传播"@zh .