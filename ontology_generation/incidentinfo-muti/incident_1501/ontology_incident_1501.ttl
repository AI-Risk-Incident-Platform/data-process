@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:FuneralAttendees ;
    airo:hasPurpose ex:SurveillancePurpose ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:LawEnforcement .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:DetentionConsequence ;
    airo:isRiskSourceFor ex:GovernmentSurveillance .

ex:LawEnforcement rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement"@en ;
    rdfs:label "执法部门"@zh .

ex:FuneralAttendees rdf:type airo:AISubject ;
    rdfs:label "Funeral Attendees"@en ;
    rdfs:label "葬礼参加者"@zh .

ex:HarassmentImpact rdf:type airo:Impact ;
    rdfs:label "Harassment Impact"@en ;
    rdfs:label "骚扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:FuneralAttendees .

ex:SurveillancePurpose rdf:type airo:Purpose ;
    rdfs:label "Surveillance Purpose"@en ;
    rdfs:label "监控目的"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:DetentionConsequence rdf:type airo:Consequence ;
    rdfs:label "Detention Consequence"@en ;
    rdfs:label "拘留后果"@zh ;
    airo:hasImpact ex:HarassmentImpact .

ex:GovernmentSurveillance rdf:type airo:RiskSource ;
    rdfs:label "Government Surveillance Risk Source"@en ;
    rdfs:label "政府监控风险源"@zh .