@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HomeValueAssessmentAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Home Value Assessment Algorithm"@en ;
    rdfs:label "房屋价值评估算法"@zh ;
    airo:hasRisk ex:AlgorithmicOpacityRisk ;
    airo:isDeployedBy ex:DutchCityGovernment ;
    airo:isProvidedBy ex:AlgorithmProvider ;
    airo:isAppliedWithinDomain ex:RealEstateValuation ;
    airo:hasStakeholder ex:DutchCityGovernment .

ex:AlgorithmicOpacityRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Opacity Risk"@en ;
    rdfs:label "算法不透明风险"@zh ;
    airo:isRiskSourceFor ex:LackOfAlgorithmicTransparency ;
    airo:hasConsequence ex:UnfairPropertyAssessments .

ex:DutchCityGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Dutch City Government"@en ;
    rdfs:label "荷兰市政府"@zh .

ex:AlgorithmProvider rdf:type airo:AIOperator ;
    rdfs:label "Algorithm Provider"@en ;
    rdfs:label "算法提供者"@zh .

ex:RealEstateValuation rdf:type airo:Domain ;
    rdfs:label "Real Estate Valuation Domain"@en ;
    rdfs:label "房地产评估领域"@zh .

ex:LackOfAlgorithmicTransparency rdf:type airo:RiskSource ;
    rdfs:label "Lack of Algorithmic Transparency Risk Source"@en ;
    rdfs:label "算法透明度缺失风险源"@zh ;
    airo:exploitsVulnerability ex:GovernmentRelianceOnOpaqueSystems .

ex:GovernmentRelianceOnOpaqueSystems rdf:type airo:Vulnerability ;
    rdfs:label "Government Reliance on Opaque Systems Vulnerability"@en ;
    rdfs:label "政府依赖不透明系统的脆弱性"@zh .

ex:UnfairPropertyAssessments rdf:type airo:Consequence ;
    rdfs:label "Unfair Property Assessments Consequence"@en ;
    rdfs:label "不公平房产评估后果"@zh ;
    airo:hasImpactOnStakeholder ex:DutchCityGovernment .