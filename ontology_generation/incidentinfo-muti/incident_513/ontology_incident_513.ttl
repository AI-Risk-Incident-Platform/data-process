@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:PrivacyDataLeakRisk ;
    airo:isDeployedBy ex:OpenAI ;
    airo:hasVulnerability ex:SystemBugVulnerability .

ex:PrivacyDataLeakRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Data Leak Risk"@en ;
    rdfs:label "隐私数据泄露风险"@zh ;
    airo:hasConsequence ex:UserConversationHistoryExposure ;
    airo:isRiskSourceFor ex:SoftwareBug .

ex:UserConversationHistoryExposure rdf:type airo:Consequence ;
    rdfs:label "User Conversation History Exposure Consequence"@en ;
    rdfs:label "用户对话历史暴露后果"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:OpenAI .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:SystemBugVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "System Bug Vulnerability"@en ;
    rdfs:label "系统漏洞脆弱性"@zh .

ex:SoftwareBug rdf:type airo:RiskSource ;
    rdfs:label "Software Bug Risk Source"@en ;
    rdfs:label "软件漏洞风险源"@zh .