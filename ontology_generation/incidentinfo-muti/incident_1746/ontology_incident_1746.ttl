@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAutocomplete rdf:type airo:AISystem ;
    rdfs:label "Google Autocomplete"@en ;
    rdfs:label "谷歌自动补全"@zh ;
    airo:hasRisk ex:ConspiracyTheoryPropagationRisk ;
    airo:hasCapability ex:SearchQueryPrediction ;
    airo:isDeployedBy ex:GoogleLLC ;
    airo:hasStakeholder ex:GeneralPublic ;
    airo:hasPurpose ex:SearchQueryAssistance ;
    airo:isAppliedWithinDomain ex:SearchEngineServices ;
    airo:usesTechnique ex:NaturalLanguageProcessing .

ex:ConspiracyTheoryPropagationRisk rdf:type airo:Risk ;
    rdfs:label "Conspiracy Theory Propagation Risk"@en ;
    rdfs:label "阴谋论传播风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInQuerySuggestions .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:PublicConfusionAndPanic .

ex:PublicConfusionAndPanic rdf:type airo:Impact ;
    rdfs:label "Public Confusion and Panic Impact"@en ;
    rdfs:label "公众困惑与恐慌影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:SearchQueryPrediction rdf:type airo:AICapability ;
    rdfs:label "Search Query Prediction Capability"@en ;
    rdfs:label "搜索查询预测能力"@zh .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌公司"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:SearchQueryAssistance rdf:type airo:Purpose ;
    rdfs:label "Search Query Assistance Purpose"@en ;
    rdfs:label "搜索查询辅助目的"@zh .

ex:SearchEngineServices rdf:type airo:Domain ;
    rdfs:label "Search Engine Services Domain"@en ;
    rdfs:label "搜索引擎服务领域"@zh .

ex:NaturalLanguageProcessing rdf:type airo:AITechnique ;
    rdfs:label "Natural Language Processing Technique"@en ;
    rdfs:label "自然语言处理技术"@zh .

ex:AlgorithmicBiasInQuerySuggestions rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Query Suggestions Risk Source"@en ;
    rdfs:label "查询建议中的算法偏见风险源"@zh .