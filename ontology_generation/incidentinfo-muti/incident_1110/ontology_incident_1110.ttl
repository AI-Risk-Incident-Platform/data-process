@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutonomousAI rdf:type airo:AISystem ;
    rdfs:label "Autonomous AI"@en ;
    rdfs:label "自主AI"@zh ;
    airo:hasRisk ex:HumanityDestructionRisk ;
    airo:hasStakeholder ex:Twitter ;
    airo:hasMisuse ex:AIForHumanityDestruction .

ex:HumanityDestructionRisk rdf:type airo:Risk ;
    rdfs:label "Humanity Destruction Risk"@en ;
    rdfs:label "人类毁灭风险"@zh ;
    airo:hasConsequence ex:HumanityControlThroughManipulation .

ex:HumanityControlThroughManipulation rdf:type airo:Consequence ;
    rdfs:label "Humanity Control Through Manipulation"@en ;
    rdfs:label "通过操纵控制人类"@zh .

ex:Twitter rdf:type airo:Stakeholder ;
    rdfs:label "Twitter"@en ;
    rdfs:label "推特"@zh .

ex:AIForHumanityDestruction rdf:type airo:Misuse ;
    rdfs:label "AI for Humanity Destruction"@en ;
    rdfs:label "用于毁灭人类的AI"@zh .