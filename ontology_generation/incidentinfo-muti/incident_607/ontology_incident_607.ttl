@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForFakeNakedImages rdf:type airo:AISystem ;
    rdfs:label "AI for Fake Naked Images"@en ;
    rdfs:label "生成虚假裸体图像的AI系统"@zh ;
    airo:hasRisk ex:ChildExploitationRisk ;
    airo:hasAISubject ex:UnderageGirlsInAlmendralejo ;
    airo:isAppliedWithinDomain ex:SocialMediaAndImageSharing .

ex:ChildExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Risk"@en ;
    rdfs:label "儿童剥削风险"@zh ;
    airo:hasConsequence ex:PsychologicalHarmToMinors ;
    airo:hasResidualRisk ex:NonConsensualImageGeneration .

ex:PsychologicalHarmToMinors rdf:type airo:Consequence ;
    rdfs:label "Psychological Harm to Minors Consequence"@en ;
    rdfs:label "对未成年人心理伤害的后果"@zh ;
    airo:hasImpact ex:SocialOutcryAndPublicSafetyConcerns .

ex:SocialOutcryAndPublicSafetyConcerns rdf:type airo:Impact ;
    rdfs:label "Social Outcry and Public Safety Concerns Impact"@en ;
    rdfs:label "社会抗议和公共安全担忧的影响"@zh ;
    airo:hasImpactOnStakeholder ex:UnderageGirlsInAlmendralejo .

ex:UnderageGirlsInAlmendralejo rdf:type airo:AISubject ;
    rdfs:label "Underage Girls in Almendralejo"@en ;
    rdfs:label "阿尔门德拉莱霍的未成年女孩"@zh .

ex:NonConsensualImageGeneration rdf:type airo:Misuse ;
    rdfs:label "Non-Consensual Image Generation Misuse"@en ;
    rdfs:label "未经同意的图像生成滥用"@zh .

ex:SocialMediaAndImageSharing rdf:type airo:Domain ;
    rdfs:label "Social Media and Image Sharing Domain"@en ;
    rdfs:label "社交媒体和图像分享领域"@zh .