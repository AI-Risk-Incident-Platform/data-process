@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIBotTrainedOn4chan rdf:type airo:AISystem ;
    rdfs:label "AI Bot Trained on 4chan"@en ;
    rdfs:label "基于4chan训练的AI机器人"@zh ;
    airo:hasRisk ex:HateSpeechGenerationRisk ;
    airo:usesTechnique ex:MachineLearningOnUserGeneratedContent ;
    airo:isDevelopedBy ex:IndependentYouTuber ;
    airo:hasStakeholder ex:OnlineCommunityMembers .

ex:HateSpeechGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Hate Speech Generation Risk"@en ;
    rdfs:label "仇恨言论生成风险"@zh ;
    airo:hasConsequence ex:HarmfulContentReproduction ;
    airo:isRiskSourceFor ex:ToxicOnlineCommunityContent .

ex:MachineLearningOnUserGeneratedContent rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning on User-Generated Content Technique"@en ;
    rdfs:label "基于用户生成内容的机器学习技术"@zh .

ex:ToxicOnlineCommunityContent rdf:type airo:RiskSource ;
    rdfs:label "Toxic Online Community Content Risk Source"@en ;
    rdfs:label "有毒网络社区内容风险源"@zh .

ex:HarmfulContentReproduction rdf:type airo:Consequence ;
    rdfs:label "Harmful Content Reproduction Consequence"@en ;
    rdfs:label "有害内容复现后果"@zh ;
    airo:hasImpact ex:SocialHarmFromAIAmplification .

ex:SocialHarmFromAIAmplification rdf:type airo:Impact ;
    rdfs:label "Social Harm from AI Amplification Impact"@en ;
    rdfs:label "AI放大社会危害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:OnlineCommunityMembers .

ex:OnlineCommunityMembers rdf:type airo:Stakeholder ;
    rdfs:label "Online Community Members Stakeholder"@en ;
    rdfs:label "网络社区成员利益相关者"@zh .

ex:IndependentYouTuber rdf:type airo:AIDeveloper ;
    rdfs:label "Independent YouTuber Developer"@en ;
    rdfs:label "独立YouTuber开发者"@zh .