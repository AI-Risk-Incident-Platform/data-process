@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TinyImagesAITrainingDataset rdf:type airo:AISystem ;
    rdfs:label "Tiny Images AI Training Dataset"@en ;
    rdfs:label "Tiny Images AI训练数据集"@zh ;
    airo:hasRisk ex:RacistAndMisogynisticContentRisk ;
    airo:usesTechnique ex:ImageRecognitionTraining ;
    airo:hasStakeholder ex:MITAndNYU .

ex:RacistAndMisogynisticContentRisk rdf:type airo:Risk ;
    rdfs:label "Racist and Misogynistic Content Risk"@en ;
    rdfs:label "种族主义和性别歧视内容风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:DiscriminatoryAIBehavior .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .

ex:DiscriminatoryAIBehavior rdf:type airo:Impact ;
    rdfs:label "Discriminatory AI Behavior"@en ;
    rdfs:label "歧视性AI行为"@zh ;
    airo:hasImpactOnStakeholder ex:MITAndNYU .

ex:MITAndNYU rdf:type airo:Stakeholder ;
    rdfs:label "MIT and NYU"@en ;
    rdfs:label "MIT和NYU"@zh .

ex:ImageRecognitionTraining rdf:type airo:AITechnique ;
    rdfs:label "Image Recognition Training"@en ;
    rdfs:label "图像识别训练"@zh .