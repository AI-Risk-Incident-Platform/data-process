@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:GeneralPublic ;
    airo:isDeployedBy ex:Paravision .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:UnauthorizedDataCollection ;
    airo:isRiskSourceFor ex:LackOfRegulation ;
    airo:hasRiskControl ex:AlgorithmDestruction .

ex:UnauthorizedDataCollection rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Data Collection Consequence"@en ;
    rdfs:label "未经授权的数据收集后果"@zh ;
    airo:hasImpact ex:PersonalPrivacyCompromise .

ex:PersonalPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Compromise Impact"@en ;
    rdfs:label "个人隐私受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:LackOfRegulation rdf:type airo:RiskSource ;
    rdfs:label "Lack of Regulation Risk Source"@en ;
    rdfs:label "缺乏监管的风险源"@zh .

ex:Paravision rdf:type airo:AIOperator ;
    rdfs:label "Paravision"@en ;
    rdfs:label "Paravision公司"@zh .

ex:UnauthorizedPhotoUsage rdf:type airo:Misuse ;
    rdfs:label "Unauthorized Photo Usage Misuse"@en ;
    rdfs:label "未经授权的照片使用滥用"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk .

ex:AlgorithmDestruction rdf:type airo:RiskControl ;
    rdfs:label "Algorithm Destruction Risk Control"@en ;
    rdfs:label "算法销毁管控"@zh ;
    airo:mitigatesRiskConcept ex:PrivacyViolationRisk .