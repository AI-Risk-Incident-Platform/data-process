@base <https://w3id.org/airo#> .

ex:AlexaAndRingSystem rdf:type airo:AISystem ;
    rdfs:label "Alexa and Ring Camera System"@en ;
    rdfs:label "Alexa和Ring摄像头系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:Amazon ;
    airo:hasStakeholder ex:FTCAndDOJ .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:ChildrenPrivacyViolation ;
    airo:isRiskSourceFor ex:VoiceRecordingRetention .

ex:ChildrenPrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Children Privacy Violation Consequence"@en ;
    rdfs:label "儿童隐私侵犯后果"@zh ;
    airo:hasImpact ex:ChildrenPrivacyImpact .

ex:ChildrenPrivacyImpact rdf:type airo:Impact ;
    rdfs:label "Children Privacy Impact"@en ;
    rdfs:label "儿童隐私影响"@zh ;
    airo:hasImpactOnStakeholder ex:FTCAndDOJ .

ex:FTCAndDOJ rdf:type airo:Stakeholder ;
    rdfs:label "FTC and DOJ"@en ;
    rdfs:label "FTC和DOJ"@zh .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:VoiceRecordingRetention rdf:type airo:RiskSource ;
    rdfs:label "Voice Recording Retention Risk Source"@en ;
    rdfs:label "语音记录保留"@zh .