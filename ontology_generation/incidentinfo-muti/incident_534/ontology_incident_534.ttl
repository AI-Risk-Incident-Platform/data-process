@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:FalseAccusationOfAcademicMisconduct ;
    airo:hasPurpose ex:AcademicWritingAssistance ;
    airo:hasStakeholder ex:CollegeStudents ;
    airo:isDeployedBy ex:CollegeInstructor ;
    airo:isAppliedWithinDomain ex:HigherEducation .

ex:FalseAccusationOfAcademicMisconduct rdf:type airo:Risk ;
    rdfs:label "False Accusation of Academic Misconduct Risk"@en ;
    rdfs:label "学术不端错误指控风险"@zh ;
    airo:hasConsequence ex:UnfairAcademicPenalties .

ex:CollegeStudents rdf:type airo:Stakeholder ;
    rdfs:label "College Students"@en ;
    rdfs:label "大学生"@zh .

ex:CollegeInstructor rdf:type airo:AIOperator ;
    rdfs:label "College Instructor"@en ;
    rdfs:label "大学教师"@zh .

ex:UnfairAcademicPenalties rdf:type airo:Consequence ;
    rdfs:label "Unfair Academic Penalties Consequence"@en ;
    rdfs:label "不公正学术处罚后果"@zh ;
    airo:hasImpact ex:StudentAcademicReputationDamage .

ex:StudentAcademicReputationDamage rdf:type airo:Impact ;
    rdfs:label "Student Academic Reputation Damage Impact"@en ;
    rdfs:label "学生学术声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:CollegeStudents .

ex:AcademicWritingAssistance rdf:type airo:Purpose ;
    rdfs:label "Academic Writing Assistance Purpose"@en ;
    rdfs:label "学术写作辅助目的"@zh .

ex:HigherEducation rdf:type airo:Domain ;
    rdfs:label "Higher Education Domain"@en ;
    rdfs:label "高等教育领域"@zh .