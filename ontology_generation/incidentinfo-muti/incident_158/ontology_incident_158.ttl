@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:hasPurpose ex:AutonomousDrivingAssistance ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:hasStakeholder ex:LegalSystem ;
    airo:hasAIOperator ex:TeslaDriver .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆致命风险"@zh ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    airo:hasConsequence ex:FatalTrafficAccident ;
    airo:isRiskSourceFor ex:HumanOverrelianceOnAutopilot ;
    airo:hasVulnerability ex:SystemLimitationsInEmergencySituations .

ex:FatalTrafficAccident rdf:type airo:Consequence ;
    rdfs:label "致命交通事故后果"@zh ;
    rdfs:label "Fatal Traffic Accident Consequence"@en ;
    airo:hasImpact ex:LossOfHumanLife .

ex:TeslaDriver rdf:type airo:AIOperator ;
    rdfs:label "特斯拉驾驶员"@zh ;
    rdfs:label "Tesla Driver"@en .

ex:LegalSystem rdf:type airo:Stakeholder ;
    rdfs:label "司法系统"@zh ;
    rdfs:label "Legal System"@en ;
    airo:hasImpactOnStakeholder ex:LossOfHumanLife .

ex:LossOfHumanLife rdf:type airo:Impact ;
    rdfs:label "人员死亡影响"@zh ;
    rdfs:label "Loss of Human Life Impact"@en .

ex:AutonomousDrivingAssistance rdf:type airo:Purpose ;
    rdfs:label "自动驾驶辅助目的"@zh ;
    rdfs:label "Autonomous Driving Assistance Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:HumanOverrelianceOnAutopilot rdf:type airo:RiskSource ;
    rdfs:label "人类过度依赖自动驾驶的风险源"@zh ;
    rdfs:label "Human Overreliance on Autopilot Risk Source"@en .

ex:SystemLimitationsInEmergencySituations rdf:type airo:Vulnerability ;
    rdfs:label "紧急情况下系统局限性的脆弱性"@zh ;
    rdfs:label "System Limitations in Emergency Situations Vulnerability"@en .