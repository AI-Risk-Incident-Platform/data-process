@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EsteeLauderAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Estee Lauder Algorithm"@en ;
    rdfs:label "雅诗兰黛算法"@zh ;
    airo:hasRisk ex:UnfairDismissalRisk ;
    airo:hasStakeholder ex:DismissedEmployees ;
    airo:isDeployedBy ex:EsteeLauderCompany ;
    airo:isAppliedWithinDomain ex:CorporateHumanResources .

ex:UnfairDismissalRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Dismissal Risk"@en ;
    rdfs:label "不公平解雇风险"@zh ;
    airo:hasConsequence ex:LegalSettlement ;
    airo:isRiskSourceFor ex:AutomatedDecisionMaking .

ex:LegalSettlement rdf:type airo:Consequence ;
    rdfs:label "Legal Settlement Consequence"@en ;
    rdfs:label "法律和解后果"@zh ;
    airo:hasImpact ex:FinancialCompensation .

ex:DismissedEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Dismissed Employees"@en ;
    rdfs:label "被解雇员工"@zh .

ex:EsteeLauderCompany rdf:type airo:AIOperator ;
    rdfs:label "Estee Lauder Company"@en ;
    rdfs:label "雅诗兰黛公司"@zh .

ex:FinancialCompensation rdf:type airo:Impact ;
    rdfs:label "Financial Compensation Impact"@en ;
    rdfs:label "经济补偿影响"@zh ;
    airo:hasImpactOnStakeholder ex:DismissedEmployees .

ex:CorporateHumanResources rdf:type airo:Domain ;
    rdfs:label "Corporate Human Resources Domain"@en ;
    rdfs:label "企业人力资源领域"@zh .

ex:AutomatedDecisionMaking rdf:type airo:RiskSource ;
    rdfs:label "Automated Decision Making Risk Source"@en ;
    rdfs:label "自动化决策风险源"@zh .