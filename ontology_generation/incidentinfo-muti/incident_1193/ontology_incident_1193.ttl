@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepSeekAI rdf:type airo:AISystem ;
    rdfs:label "DeepSeek AI"@en ;
    rdfs:label "DeepSeek AI"@zh ;
    airo:hasRisk ex:DataExposureRisk .

ex:DataExposureRisk rdf:type airo:Risk ;
    rdfs:label "Data Exposure Risk"@en ;
    rdfs:label "数据暴露风险"@zh ;
    airo:hasConsequence ex:DataLeakage ;
    airo:isRiskSourceFor ex:InsufficientSecurityMeasures .

ex:DataLeakage rdf:type airo:Consequence ;
    rdfs:label "Data Leakage"@en ;
    rdfs:label "数据泄露"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:InsufficientSecurityMeasures rdf:type airo:RiskSource ;
    rdfs:label "Insufficient Security Measures"@en ;
    rdfs:label "安全措施不足"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh .