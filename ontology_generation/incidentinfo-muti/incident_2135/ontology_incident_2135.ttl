@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Uber Facial Recognition System"@en ;
    rdfs:label "Uber面部识别系统"@zh ;
    airo:hasRisk ex:RacialBiasInFacialRecognition ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:UberDrivers ;
    airo:isDeployedBy ex:UberTechnologiesInc .

ex:RacialBiasInFacialRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Facial Recognition Risk"@en ;
    rdfs:label "面部识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:UnfairTerminationOfEmployment ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:UnfairTerminationOfEmployment rdf:type airo:Consequence ;
    rdfs:label "Unfair Termination of Employment Consequence"@en ;
    rdfs:label "不公平终止雇佣关系的后果"@zh ;
    airo:hasImpact ex:DiscriminationAgainstMinorityGroups .

ex:UberDrivers rdf:type airo:Stakeholder ;
    rdfs:label "Uber Drivers"@en ;
    rdfs:label "Uber司机"@zh .

ex:UberTechnologiesInc rdf:type airo:AIOperator ;
    rdfs:label "Uber Technologies Inc."@en ;
    rdfs:label "Uber科技公司"@zh .

ex:DiscriminationAgainstMinorityGroups rdf:type airo:Impact ;
    rdfs:label "Discrimination Against Minority Groups Impact"@en ;
    rdfs:label "对少数群体的歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:UberDrivers .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .