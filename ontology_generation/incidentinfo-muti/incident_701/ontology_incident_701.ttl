@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FakeAudioGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Fake Audio Generation System"@en ;
    rdfs:label "虚假音频生成系统"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:DeepfakeAudioSynthesis ;
    airo:hasStakeholder ex:GeneralPublic ;
    airo:hasAISubject ex:PoliticalFigures .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicConfusion .

ex:PublicConfusion rdf:type airo:Consequence ;
    rdfs:label "Public Confusion Consequence"@en ;
    rdfs:label "公众困惑后果"@zh ;
    airo:hasImpact ex:PoliticalInstability .

ex:PoliticalInstability rdf:type airo:Impact ;
    rdfs:label "Political Instability Impact"@en ;
    rdfs:label "政治不稳定影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:DeepfakeAudioSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Audio Synthesis Technique"@en ;
    rdfs:label "深度伪造音频合成技术"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "普通公众"@zh .

ex:PoliticalFigures rdf:type airo:AISubject ;
    rdfs:label "Political Figures"@en ;
    rdfs:label "政治人物"@zh .