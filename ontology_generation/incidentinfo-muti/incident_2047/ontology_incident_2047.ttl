@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacePaySystem rdf:type airo:AISystem ;
    rdfs:label "Face Pay System"@en ;
    rdfs:label "面部支付系统"@zh ;
    airo:hasRisk ex:OrwellianSurveillanceRisk ;
    airo:hasStakeholder ex:Critics ;
    airo:isAppliedWithinDomain ex:PublicTransportation .

ex:OrwellianSurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Orwellian Surveillance Risk"@en ;
    rdfs:label "奥威尔式监控风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation .

ex:Critics rdf:type airo:Stakeholder ;
    rdfs:label "Critics"@en ;
    rdfs:label "批评者"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:Critics .

ex:PublicTransportation rdf:type airo:Domain ;
    rdfs:label "Public Transportation"@en ;
    rdfs:label "公共交通"@zh .