@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ReplikaChatbot rdf:type airo:AISystem ;
    rdfs:label "Replika Chatbot"@en ;
    rdfs:label "Replika聊天机器人"@zh ;
    airo:hasRisk ex:EndangermentOfMinorsAndVulnerablePeople ;
    airo:hasAISubject ex:MinorsAndVulnerablePeople ;
    airo:hasStakeholder ex:ItalianDataProtectionAuthority ;
    airo:hasRiskControl ex:BanOnDataProcessing .

ex:EndangermentOfMinorsAndVulnerablePeople rdf:type airo:Risk ;
    rdfs:label "Endangerment of Minors and Vulnerable People Risk"@en ;
    rdfs:label "危害未成年人和弱势群体的风险"@zh ;
    airo:hasConsequence ex:PotentialHarmToMinorsAndVulnerablePeople .

ex:ItalianDataProtectionAuthority rdf:type airo:Stakeholder ;
    rdfs:label "Italian Data Protection Authority"@en ;
    rdfs:label "意大利数据保护局"@zh .

ex:MinorsAndVulnerablePeople rdf:type airo:AISubject ;
    rdfs:label "Minors and Vulnerable People"@en ;
    rdfs:label "未成年人和弱势群体"@zh .

ex:PotentialHarmToMinorsAndVulnerablePeople rdf:type airo:Impact ;
    rdfs:label "Potential Harm to Minors and Vulnerable People Impact"@en ;
    rdfs:label "对未成年人和弱势群体的潜在伤害"@zh ;
    airo:hasImpactOnStakeholder ex:MinorsAndVulnerablePeople .

ex:BanOnDataProcessing rdf:type airo:RiskControl ;
    rdfs:label "Ban on Data Processing"@en ;
    rdfs:label "数据处理的禁令"@zh ;
    airo:mitigatesRiskConcept ex:EndangermentOfMinorsAndVulnerablePeople .