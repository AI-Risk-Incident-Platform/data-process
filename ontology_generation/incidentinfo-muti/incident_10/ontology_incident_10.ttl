@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleImageSearch rdf:type airo:AISystem ;
    rdfs:label "Google Image Search"@en ;
    rdfs:label "谷歌图片搜索"@zh ;
    airo:hasRisk ex:GenderBiasInSearchResultsRisk ;
    airo:hasCapability ex:ImageRecognitionAndRankingCapability ;
    airo:usesTechnique ex:MachineLearningForImageClassificationTechnique ;
    airo:hasStakeholder ex:GoogleUsers .

ex:GenderBiasInSearchResultsRisk rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Search Results Risk"@en ;
    rdfs:label "搜索结果中的性别偏见风险"@zh ;
    airo:hasConsequence ex:ReinforcementOfGenderStereotypesConsequence ;
    airo:isRiskSourceFor ex:BiasedTrainingDataRiskSource .

ex:ReinforcementOfGenderStereotypesConsequence rdf:type airo:Consequence ;
    rdfs:label "Reinforcement of Gender Stereotypes Consequence"@en ;
    rdfs:label "强化性别刻板印象的后果"@zh ;
    airo:hasImpact ex:PerpetuationOfGenderInequalityImpact .

ex:PerpetuationOfGenderInequalityImpact rdf:type airo:Impact ;
    rdfs:label "Perpetuation of Gender Inequality Impact"@en ;
    rdfs:label "性别不平等持续的影响"@zh ;
    airo:hasImpactOnStakeholder ex:GoogleUsers .

ex:ImageRecognitionAndRankingCapability rdf:type airo:AICapability ;
    rdfs:label "Image Recognition and Ranking Capability"@en ;
    rdfs:label "图像识别与排名能力"@zh .

ex:MachineLearningForImageClassificationTechnique rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning for Image Classification Technique"@en ;
    rdfs:label "用于图像分类的机器学习技术"@zh .

ex:GoogleUsers rdf:type airo:Stakeholder ;
    rdfs:label "Google Users"@en ;
    rdfs:label "谷歌用户"@zh .

ex:BiasedTrainingDataRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .