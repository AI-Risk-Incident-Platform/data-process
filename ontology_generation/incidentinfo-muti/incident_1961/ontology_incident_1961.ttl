@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilot rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot"@en ;
    airo:hasRisk ex:MotorcyclistFatalityRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasStakeholder ex:MotorcycleAdvocates ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:hasPurpose ex:AutonomousDrivingAssistance .

ex:MotorcyclistFatalityRisk rdf:type airo:Risk ;
    rdfs:label "摩托车手死亡风险"@zh ;
    rdfs:label "Motorcyclist Fatality Risk"@en ;
    airo:hasConsequence ex:FatalMotorcycleCrash ;
    airo:isRiskSourceFor ex:AutonomousDrivingSystemFailure .

ex:FatalMotorcycleCrash rdf:type airo:Consequence ;
    rdfs:label "致命摩托车事故后果"@zh ;
    rdfs:label "Fatal Motorcycle Crash Consequence"@en ;
    airo:hasImpact ex:LossOfLife .

ex:LossOfLife rdf:type airo:Impact ;
    rdfs:label "生命损失影响"@zh ;
    rdfs:label "Loss of Life Impact"@en ;
    airo:hasImpactOnStakeholder ex:MotorcycleAdvocates .

ex:MotorcycleAdvocates rdf:type airo:Stakeholder ;
    rdfs:label "摩托车倡导者"@zh ;
    rdfs:label "Motorcycle Advocates"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:AutonomousDrivingSystemFailure rdf:type airo:RiskSource ;
    rdfs:label "自动驾驶系统故障风险源"@zh ;
    rdfs:label "Autonomous Driving System Failure Risk Source"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:AutonomousDrivingAssistance rdf:type airo:Purpose ;
    rdfs:label "自动驾驶辅助目的"@zh ;
    rdfs:label "Autonomous Driving Assistance Purpose"@en .