@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VolkswagenPlantRobot rdf:type airo:AISystem ;
    rdfs:label "Volkswagen Plant Robot"@en ;
    rdfs:label "大众工厂机器人"@zh ;
    airo:hasRisk ex:WorkerFatalityRisk ;
    airo:hasStakeholder ex:Volkswagen ;
    airo:isAppliedWithinDomain ex:AutomotiveManufacturing .

ex:WorkerFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Worker Fatality Risk"@en ;
    rdfs:label "工人死亡风险"@zh ;
    airo:hasConsequence ex:HumanFatality ;
    airo:isRiskSourceFor ex:RobotMalfunction .

ex:HumanFatality rdf:type airo:Consequence ;
    rdfs:label "Human Fatality Consequence"@en ;
    rdfs:label "人员死亡后果"@zh ;
    airo:hasImpact ex:WorkplaceSafetyCompromise .

ex:Volkswagen rdf:type airo:Stakeholder ;
    rdfs:label "Volkswagen"@en ;
    rdfs:label "大众汽车"@zh .

ex:WorkplaceSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Workplace Safety Compromise Impact"@en ;
    rdfs:label "工作场所安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:Volkswagen .

ex:AutomotiveManufacturing rdf:type airo:Domain ;
    rdfs:label "Automotive Manufacturing Domain"@en ;
    rdfs:label "汽车制造领域"@zh .

ex:RobotMalfunction rdf:type airo:RiskSource ;
    rdfs:label "Robot Malfunction Risk Source"@en ;
    rdfs:label "机器人故障风险源"@zh .