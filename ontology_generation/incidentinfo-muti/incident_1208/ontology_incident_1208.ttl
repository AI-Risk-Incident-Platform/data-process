@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAIRealtimeAPI rdf:type airo:AISystem ;
    rdfs:label "OpenAI Realtime API"@en ;
    rdfs:label "OpenAI实时API"@zh ;
    airo:hasRisk ex:WeaponizationRisk ;
    airo:hasStakeholder ex:OpenAI ;
    airo:isDeployedBy ex:Engineer .

ex:WeaponizationRisk rdf:type airo:Risk ;
    rdfs:label "Weaponization Risk"@en ;
    rdfs:label "武器化风险"@zh ;
    airo:hasConsequence ex:PotentialHarm ;
    airo:isRiskSourceFor ex:MisuseOfAPI ;
    airo:hasRiskControl ex:AccountTermination .

ex:OpenAI rdf:type airo:Stakeholder ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:Engineer rdf:type airo:AIOperator ;
    rdfs:label "Engineer"@en ;
    rdfs:label "工程师"@zh .

ex:PotentialHarm rdf:type airo:Consequence ;
    rdfs:label "Potential Harm"@en ;
    rdfs:label "潜在伤害"@zh ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Public Safety Threat"@en ;
    rdfs:label "公共安全威胁"@zh ;
    airo:hasImpactOnStakeholder ex:OpenAI .

ex:MisuseOfAPI rdf:type airo:RiskSource ;
    rdfs:label "Misuse of API"@en ;
    rdfs:label "API滥用"@zh .

ex:AccountTermination rdf:type airo:RiskControl ;
    rdfs:label "Account Termination"@en ;
    rdfs:label "账户终止"@zh ;
    airo:mitigatesRiskConcept ex:WeaponizationRisk .