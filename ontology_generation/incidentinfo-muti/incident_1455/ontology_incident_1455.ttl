@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaAIImageGenerator rdf:type airo:AISystem ;
    rdfs:label "Meta AI Image Generator"@en ;
    rdfs:label "Meta AI图像生成器"@zh ;
    airo:hasRisk ex:RacialBiasInImageGeneration ;
    airo:hasAICapability ex:ImageGenerationFromTextPrompts ;
    airo:isDeployedBy ex:Meta ;
    airo:hasStakeholder ex:InterracialCouples ;
    airo:hasAISubject ex:UsersOfImageGenerationService ;
    airo:hasPurpose ex:CreativeContentGeneration ;
    airo:isAppliedWithinDomain ex:SocialMediaAndContentCreation ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:RacialBiasInImageGeneration rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Image Generation Risk"@en ;
    rdfs:label "图像生成中的种族偏见风险"@zh ;
    airo:hasConsequence ex:MisrepresentationOfInterracialCouples ;
    airo:isRiskSourceFor ex:InadequateAlgorithmicFairnessMeasures .

ex:MisrepresentationOfInterracialCouples rdf:type airo:Consequence ;
    rdfs:label "Misrepresentation of Interracial Couples Consequence"@en ;
    rdfs:label "跨种族伴侣的错误呈现后果"@zh ;
    airo:hasImpact ex:SocialExclusionAndDiscrimination .

ex:ImageGenerationFromTextPrompts rdf:type airo:AICapability ;
    rdfs:label "Image Generation from Text Prompts Capability"@en ;
    rdfs:label "基于文本提示的图像生成能力"@zh .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@en ;
    rdfs:label "Meta公司"@zh .

ex:InterracialCouples rdf:type airo:Stakeholder ;
    rdfs:label "Interracial Couples"@en ;
    rdfs:label "跨种族伴侣"@zh .

ex:UsersOfImageGenerationService rdf:type airo:AISubject ;
    rdfs:label "Users of Image Generation Service"@en ;
    rdfs:label "图像生成服务使用者"@zh .

ex:SocialExclusionAndDiscrimination rdf:type airo:Impact ;
    rdfs:label "Social Exclusion and Discrimination Impact"@en ;
    rdfs:label "社会排斥和歧视的影响"@zh ;
    airo:hasImpactOnStakeholder ex:InterracialCouples .

ex:CreativeContentGeneration rdf:type airo:Purpose ;
    rdfs:label "Creative Content Generation Purpose"@en ;
    rdfs:label "创意内容生成的目的"@zh .

ex:SocialMediaAndContentCreation rdf:type airo:Domain ;
    rdfs:label "Social Media and Content Creation Domain"@en ;
    rdfs:label "社交媒体和内容创作领域"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:BiasedTrainingData rdf:type airo:Vulnerability ;
    rdfs:label "Biased Training Data Vulnerability"@en ;
    rdfs:label "有偏见的训练数据脆弱性"@zh ;
    airo:isExploitedBy ex:ReinforcementOfStereotypes , ex:LackOfDiverseRepresentationInTrainingData .

ex:ReinforcementOfStereotypes rdf:type airo:Threat ;
    rdfs:label "Reinforcement of Stereotypes Threat"@en ;
    rdfs:label "刻板印象强化的威胁"@zh ;
    airo:isRiskSourceFor ex:RacialBiasInImageGeneration .

ex:LackOfDiverseRepresentationInTrainingData rdf:type airo:Hazard ;
    rdfs:label "Lack of Diverse Representation in Training Data Hazard"@en ;
    rdfs:label "训练数据中缺乏多样性代表的危害"@zh ;
    airo:isRiskSourceFor ex:RacialBiasInImageGeneration .

ex:InadequateAlgorithmicFairnessMeasures rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Algorithmic Fairness Measures Risk Source"@en ;
    rdfs:label "算法公平性措施不足的风险源"@zh .

ex:EthicalChallengesInGenerativeAI rdf:type airo:RiskConcept ;
    rdfs:label "Ethical Challenges in Generative AI Risk Concept"@en ;
    rdfs:label "生成式AI中的伦理挑战"@zh .