@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleSafetyRisk ;
    airo:isDevelopedBy ex:TeslaInc ;
    airo:hasStakeholder ex:ParisTaxiCompany ;
    airo:isAppliedWithinDomain ex:TransportationServices ;
    airo:usesTechnique ex:AutonomousDrivingTechnology .

ex:AutonomousVehicleSafetyRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆安全风险"@zh ;
    rdfs:label "Autonomous Vehicle Safety Risk"@en ;
    airo:hasConsequence ex:MultipleInjuries ;
    airo:isRiskSourceFor ex:BrakingSystemFailure .

ex:MultipleInjuries rdf:type airo:Consequence ;
    rdfs:label "多人受伤后果"@zh ;
    rdfs:label "Multiple Injuries Consequence"@en ;
    airo:hasImpact ex:PublicSafetyConcern .

ex:BrakingSystemFailure rdf:type airo:RiskSource ;
    rdfs:label "制动系统故障风险源"@zh ;
    rdfs:label "Braking System Failure Risk Source"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:ParisTaxiCompany rdf:type airo:Stakeholder ;
    rdfs:label "巴黎出租车公司"@zh ;
    rdfs:label "Paris Taxi Company"@en ;
    airo:hasImpactOnStakeholder ex:PublicSafetyConcern .

ex:PublicSafetyConcern rdf:type airo:Impact ;
    rdfs:label "公共安全担忧影响"@zh ;
    rdfs:label "Public Safety Concern Impact"@en .

ex:TransportationServices rdf:type airo:Domain ;
    rdfs:label "交通运输服务领域"@zh ;
    rdfs:label "Transportation Services Domain"@en .

ex:AutonomousDrivingTechnology rdf:type airo:AITechnique ;
    rdfs:label "自动驾驶技术"@zh ;
    rdfs:label "Autonomous Driving Technology"@en .