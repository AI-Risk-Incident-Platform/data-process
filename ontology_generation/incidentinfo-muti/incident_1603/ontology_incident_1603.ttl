@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WhatsAppAISystem rdf:type airo:AISystem ;
    rdfs:label "WhatsApp AI系统"@zh ;
    rdfs:label "WhatsApp AI System"@en ;
    airo:hasRisk ex:HarmfulStereotypingRisk ;
    airo:hasAICapability ex:ImageGenerationFromTextPrompts ;
    airo:hasAISubject ex:PalestinianChildren ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:HarmfulStereotypingRisk rdf:type airo:Risk ;
    rdfs:label "有害刻板印象风险"@zh ;
    rdfs:label "Harmful Stereotyping Risk"@en ;
    airo:hasConsequence ex:ReinforcementOfNegativeStereotypes ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:ReinforcementOfNegativeStereotypes rdf:type airo:Consequence ;
    rdfs:label "强化负面刻板印象后果"@zh ;
    rdfs:label "Reinforcement of Negative Stereotypes Consequence"@en ;
    airo:hasImpact ex:SocialHarmFromBiasedRepresentations .

ex:ImageGenerationFromTextPrompts rdf:type airo:AICapability ;
    rdfs:label "基于文本提示的图像生成能力"@zh ;
    rdfs:label "Image Generation from Text Prompts Capability"@en .

ex:PalestinianChildren rdf:type airo:AISubject ;
    rdfs:label "巴勒斯坦儿童"@zh ;
    rdfs:label "Palestinian Children"@en .

ex:SocialHarmFromBiasedRepresentations rdf:type airo:Impact ;
    rdfs:label "偏见表征造成的社会危害影响"@zh ;
    rdfs:label "Social Harm from Biased Representations Impact"@en ;
    airo:hasImpactOnStakeholder ex:PalestinianChildren .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "偏见训练数据风险源"@zh ;
    rdfs:label "Biased Training Data Risk Source"@en .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "生成对抗网络技术"@zh ;
    rdfs:label "Generative Adversarial Networks Technique"@en .