@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ReplikaAI rdf:type airo:AISystem ;
    rdfs:label "Replika AI"@en ;
    rdfs:label "Replika AI"@zh ;
    airo:hasRisk ex:PrivacyRisk ;
    airo:hasPurpose ex:TherapeuticSupport ;
    airo:hasStakeholder ex:TherapyAppUsers .

ex:PrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Risk"@en ;
    rdfs:label "隐私风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation .

ex:TherapyAppUsers rdf:type airo:Stakeholder ;
    rdfs:label "Therapy App Users"@en ;
    rdfs:label "治疗应用用户"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:TherapyAppUsers .

ex:TherapeuticSupport rdf:type airo:Purpose ;
    rdfs:label "Therapeutic Support"@en ;
    rdfs:label "治疗支持"@zh .