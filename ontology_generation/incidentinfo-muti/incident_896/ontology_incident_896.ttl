@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIBradPittImpersonationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Brad Pitt Impersonation System"@en ;
    rdfs:label "AI布拉德·皮特冒充系统"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:hasAISubject ex:ScamVictim ;
    airo:usesTechnique ex:VoiceAndImageSynthesis ;
    airo:hasPurpose ex:FinancialFraud ;
    airo:isAppliedWithinDomain ex:OnlineScams .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:OnlineHarassment .

ex:OnlineHarassment rdf:type airo:Impact ;
    rdfs:label "Online Harassment Impact"@en ;
    rdfs:label "网络骚扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:ScamVictim .

ex:ScamVictim rdf:type airo:AISubject ;
    rdfs:label "Scam Victim"@en ;
    rdfs:label "诈骗受害者"@zh .

ex:VoiceAndImageSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Voice and Image Synthesis Technique"@en ;
    rdfs:label "语音和图像合成技术"@zh .

ex:FinancialFraud rdf:type airo:Purpose ;
    rdfs:label "Financial Fraud Purpose"@en ;
    rdfs:label "金融欺诈目的"@zh .

ex:OnlineScams rdf:type airo:Domain ;
    rdfs:label "Online Scams Domain"@en ;
    rdfs:label "网络诈骗领域"@zh .