@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SecurityRobot rdf:type airo:AISystem ;
    rdfs:label "Security Robot"@en ;
    rdfs:label "安保机器人"@zh ;
    airo:hasRisk ex:DehumanizationRisk ;
    airo:hasPurpose ex:DeterHomelessEncampments ;
    airo:hasStakeholder ex:HomelessPeople ;
    airo:isDeployedBy ex:AnimalShelter ;
    airo:isAppliedWithinDomain ex:PublicSpaceManagement .

ex:DehumanizationRisk rdf:type airo:Risk ;
    rdfs:label "Dehumanization Risk"@en ;
    rdfs:label "非人化风险"@zh ;
    airo:hasConsequence ex:PublicBacklash .

ex:HomelessPeople rdf:type airo:Stakeholder ;
    rdfs:label "Homeless People"@en ;
    rdfs:label "无家可归者"@zh .

ex:AnimalShelter rdf:type airo:AIOperator ;
    rdfs:label "Animal Shelter"@en ;
    rdfs:label "动物收容所"@zh .

ex:PublicBacklash rdf:type airo:Impact ;
    rdfs:label "Public Backlash Impact"@en ;
    rdfs:label "公众反弹影响"@zh ;
    airo:hasImpactOnStakeholder ex:HomelessPeople .

ex:DeterHomelessEncampments rdf:type airo:Purpose ;
    rdfs:label "Deter Homeless Encampments Purpose"@en ;
    rdfs:label "阻止无家可归者露营的目的"@zh .

ex:PublicSpaceManagement rdf:type airo:Domain ;
    rdfs:label "Public Space Management Domain"@en ;
    rdfs:label "公共空间管理领域"@zh .