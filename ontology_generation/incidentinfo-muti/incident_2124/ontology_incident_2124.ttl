@base <https://w3id.org/airo#> .

ex:AntiCheatingSoftware rdf:type airo:AISystem ;
    rdfs:label "Anti-Cheating Software"@en ;
    rdfs:label "反作弊软件"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasPurpose ex:AcademicIntegrityMonitoring ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:usesTechnique ex:RoomScanningTechnology ;
    airo:hasAISubject ex:Students ;
    airo:hasVulnerability ex:LackOfConsentMechanism .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:UnconstitutionalSurveillance ;
    airo:isRiskSourceFor ex:IntrusiveMonitoringPractice .

ex:UnconstitutionalSurveillance rdf:type airo:Consequence ;
    rdfs:label "Unconstitutional Surveillance Consequence"@en ;
    rdfs:label "违宪监控后果"@zh ;
    airo:hasImpact ex:ViolationOfBasicRights .

ex:Students rdf:type airo:AISubject ;
    rdfs:label "Students"@en ;
    rdfs:label "学生"@zh .

ex:ViolationOfBasicRights rdf:type airo:Impact ;
    rdfs:label "Violation of Basic Rights Impact"@en ;
    rdfs:label "基本权利侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:Students .

ex:AcademicIntegrityMonitoring rdf:type airo:Purpose ;
    rdfs:label "Academic Integrity Monitoring Purpose"@en ;
    rdfs:label "学术诚信监控目的"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .

ex:RoomScanningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Room Scanning Technology"@en ;
    rdfs:label "房间扫描技术"@zh .

ex:LackOfConsentMechanism rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Consent Mechanism Vulnerability"@en ;
    rdfs:label "缺乏同意机制的脆弱性"@zh .

ex:IntrusiveMonitoringPractice rdf:type airo:RiskSource ;
    rdfs:label "Intrusive Monitoring Practice Risk Source"@en ;
    rdfs:label "侵入性监控实践的风险源"@zh .