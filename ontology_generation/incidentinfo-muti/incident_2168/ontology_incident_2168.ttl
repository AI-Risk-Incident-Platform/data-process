@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaModelXAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Model X Autopilot System"@en ;
    rdfs:label "特斯拉Model X自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:hasStakeholder ex:SouthKoreanPolice ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTransportation .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    rdfs:label "自动驾驶车辆致命事故风险"@zh ;
    airo:hasConsequence ex:HumanFatalityImpact .

ex:SouthKoreanPolice rdf:type airo:Stakeholder ;
    rdfs:label "South Korean Police"@en ;
    rdfs:label "韩国警方"@zh .

ex:AutonomousVehicleTransportation rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Transportation Domain"@en ;
    rdfs:label "自动驾驶车辆运输领域"@zh .

ex:HumanFatalityImpact rdf:type airo:Impact ;
    rdfs:label "Human Fatality Impact"@en ;
    rdfs:label "人员死亡影响"@zh ;
    airo:hasImpactOnStakeholder ex:SouthKoreanPolice .