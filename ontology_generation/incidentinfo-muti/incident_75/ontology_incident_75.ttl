@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookTranslationService rdf:type airo:AISystem ;
    rdfs:label "Facebook Translation Service"@en ;
    rdfs:label "Facebook翻译服务"@zh ;
    airo:hasRisk ex:IncorrectTranslationRisk ;
    airo:hasAISubject ex:PalestinianMan ;
    airo:hasStakeholder ex:FacebookCompany ;
    airo:usesTechnique ex:MachineTranslation .

ex:IncorrectTranslationRisk rdf:type airo:Risk ;
    rdfs:label "Incorrect Translation Risk"@en ;
    rdfs:label "错误翻译风险"@zh ;
    airo:hasConsequence ex:WrongfulArrest .

ex:WrongfulArrest rdf:type airo:Consequence ;
    rdfs:label "Wrongful Arrest Consequence"@en ;
    rdfs:label "错误逮捕后果"@zh ;
    airo:hasImpact ex:HumanRightsViolation .

ex:PalestinianMan rdf:type airo:AISubject ;
    rdfs:label "Palestinian Man"@en ;
    rdfs:label "巴勒斯坦男子"@zh .

ex:HumanRightsViolation rdf:type airo:Impact ;
    rdfs:label "Human Rights Violation Impact"@en ;
    rdfs:label "人权侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:PalestinianMan .

ex:FacebookCompany rdf:type airo:Stakeholder ;
    rdfs:label "Facebook Company"@en ;
    rdfs:label "Facebook公司"@zh .

ex:MachineTranslation rdf:type airo:AITechnique ;
    rdfs:label "Machine Translation Technique"@en ;
    rdfs:label "机器翻译技术"@zh .