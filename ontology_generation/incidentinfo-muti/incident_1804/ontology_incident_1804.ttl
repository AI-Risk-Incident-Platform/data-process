@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MeetingScoreSystem rdf:type airo:AISystem ;
    rdfs:label "Meeting Score System"@en ;
    rdfs:label "会议评分系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:FacialExpressionAnalysis ;
    airo:isDeployedBy ex:Microsoft ;
    airo:hasPurpose ex:MeetingPerformanceEvaluation ;
    airo:isAppliedWithinDomain ex:WorkplaceProductivity .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:EmployeeStress .

ex:FacialExpressionAnalysis rdf:type airo:AITechnique ;
    rdfs:label "Facial Expression Analysis Technique"@en ;
    rdfs:label "面部表情分析技术"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:MeetingPerformanceEvaluation rdf:type airo:Purpose ;
    rdfs:label "Meeting Performance Evaluation Purpose"@en ;
    rdfs:label "会议表现评估目的"@zh .

ex:WorkplaceProductivity rdf:type airo:Domain ;
    rdfs:label "Workplace Productivity Domain"@en ;
    rdfs:label "职场生产力领域"@zh .

ex:EmployeeStress rdf:type airo:Consequence ;
    rdfs:label "Employee Stress Consequence"@en ;
    rdfs:label "员工压力后果"@zh ;
    airo:hasImpact ex:WorkplaceAnxiety .

ex:WorkplaceAnxiety rdf:type airo:Impact ;
    rdfs:label "Workplace Anxiety Impact"@en ;
    rdfs:label "职场焦虑影响"@zh .