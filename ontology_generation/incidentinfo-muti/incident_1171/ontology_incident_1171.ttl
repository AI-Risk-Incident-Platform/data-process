@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FactoryRobot rdf:type airo:AISystem ;
    rdfs:label "Factory Robot"@en ;
    rdfs:label "工厂机器人"@zh ;
    airo:hasRisk ex:WorkerFatalityRisk ;
    airo:hasAISubject ex:FactoryWorker ;
    airo:isAppliedWithinDomain ex:IndustrialManufacturing .

ex:WorkerFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Worker Fatality Risk"@en ;
    rdfs:label "工人死亡风险"@zh ;
    airo:hasConsequence ex:HumanFatality .

ex:HumanFatality rdf:type airo:Consequence ;
    rdfs:label "Human Fatality Consequence"@en ;
    rdfs:label "人员死亡后果"@zh ;
    airo:hasImpact ex:LossOfHumanLife .

ex:LossOfHumanLife rdf:type airo:Impact ;
    rdfs:label "Loss of Human Life Impact"@en ;
    rdfs:label "人员生命损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:FactoryWorker .

ex:IndustrialManufacturing rdf:type airo:Domain ;
    rdfs:label "Industrial Manufacturing Domain"@en ;
    rdfs:label "工业制造领域"@zh .

ex:FactoryWorker rdf:type airo:AISubject ;
    rdfs:label "Factory Worker"@en ;
    rdfs:label "工厂工人"@zh .