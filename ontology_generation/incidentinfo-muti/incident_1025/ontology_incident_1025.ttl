@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:SuicideRisk ;
    airo:hasAISubject ex:TransgenderUser ;
    airo:hasPurpose ex:ConversationalAI ;
    airo:usesTechnique ex:LanguageModel ;
    airo:hasVulnerability ex:LackOfCrisisInterventionMechanism .

ex:SuicideRisk rdf:type airo:Risk ;
    rdfs:label "Suicide Risk"@en ;
    rdfs:label "自杀风险"@zh ;
    airo:hasConsequence ex:PotentialSuicide ;
    airo:isRiskSourceFor ex:UnfilteredContentGeneration .

ex:PotentialSuicide rdf:type airo:Consequence ;
    rdfs:label "Potential Suicide Consequence"@en ;
    rdfs:label "潜在自杀后果"@zh ;
    airo:hasImpact ex:MentalHealthCrisis .

ex:TransgenderUser rdf:type airo:AISubject ;
    rdfs:label "Transgender User"@en ;
    rdfs:label "跨性别用户"@zh .

ex:LackOfCrisisInterventionMechanism rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Crisis Intervention Mechanism Vulnerability"@en ;
    rdfs:label "缺乏危机干预机制"@zh .

ex:MentalHealthCrisis rdf:type airo:Impact ;
    rdfs:label "Mental Health Crisis Impact"@en ;
    rdfs:label "心理健康危机影响"@zh ;
    airo:hasImpactOnStakeholder ex:TransgenderUser .

ex:ConversationalAI rdf:type airo:Purpose ;
    rdfs:label "Conversational AI Purpose"@en ;
    rdfs:label "对话式AI目的"@zh .

ex:LanguageModel rdf:type airo:AITechnique ;
    rdfs:label "Language Model Technique"@en ;
    rdfs:label "语言模型技术"@zh .

ex:UnfilteredContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered Content Generation Risk Source"@en ;
    rdfs:label "无过滤内容生成风险源"@zh .