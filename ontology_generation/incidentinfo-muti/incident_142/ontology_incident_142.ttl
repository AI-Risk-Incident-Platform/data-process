@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AgriculturalAIAllocationSystem rdf:type airo:AISystem ;
    rdfs:label "Agricultural AI Allocation System"@en ;
    rdfs:label "农业AI分配系统"@zh ;
    airo:hasRisk ex:AlgorithmicDecisionMakingRisk ;
    airo:hasStakeholder ex:IsraeliFarmer ;
    airo:isAppliedWithinDomain ex:Agriculture .

ex:AlgorithmicDecisionMakingRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Decision Making Risk"@en ;
    rdfs:label "算法决策风险"@zh ;
    airo:hasImpact ex:FarmerLivelihoodImpact .

ex:IsraeliFarmer rdf:type airo:Stakeholder ;
    rdfs:label "Israeli Farmer"@en ;
    rdfs:label "以色列农民"@zh .

ex:Agriculture rdf:type airo:Domain ;
    rdfs:label "Agriculture"@en ;
    rdfs:label "农业"@zh .

ex:FarmerLivelihoodImpact rdf:type airo:Impact ;
    rdfs:label "Farmer Livelihood Impact"@en ;
    rdfs:label "农民生计影响"@zh ;
    airo:hasImpactOnStakeholder ex:IsraeliFarmer .