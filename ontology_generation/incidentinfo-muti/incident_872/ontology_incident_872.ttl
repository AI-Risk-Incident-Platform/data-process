@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIInvestmentScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI投资诈骗系统"@zh ;
    rdfs:label "AI Investment Scam System"@en ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:SocialMediaTargeting ;
    airo:isAppliedWithinDomain ex:FinancialServices ;
    airo:hasStakeholder ex:GlobalVictims .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "金融欺诈风险"@zh ;
    rdfs:label "Financial Fraud Risk"@en ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:AIEnhancedScamming .

ex:GlobalVictims rdf:type airo:Stakeholder ;
    rdfs:label "全球受害者"@zh ;
    rdfs:label "Global Victims"@en .

ex:SocialMediaTargeting rdf:type airo:AITechnique ;
    rdfs:label "社交媒体定向技术"@zh ;
    rdfs:label "Social Media Targeting Technique"@en .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "金融服务领域"@zh ;
    rdfs:label "Financial Services Domain"@en .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "经济损失后果"@zh ;
    rdfs:label "Financial Loss Consequence"@en ;
    airo:hasImpact ex:VictimFinancialHardship .

ex:VictimFinancialHardship rdf:type airo:Impact ;
    rdfs:label "受害者财务困境影响"@zh ;
    rdfs:label "Victim Financial Hardship Impact"@en ;
    airo:hasImpactOnStakeholder ex:GlobalVictims .

ex:AIEnhancedScamming rdf:type airo:RiskSource ;
    rdfs:label "AI增强的诈骗手段风险源"@zh ;
    rdfs:label "AI-Enhanced Scamming Risk Source"@en .