@base <https://w3id.org/airo#> .

ex:FacialRecognitionService rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Service"@en ;
    rdfs:label "面部识别服务"@zh ;
    airo:hasAISubject ex:RussianPornActresses ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasConsequence ex:PersonalPrivacyExposure .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:WeaponizationOfFacialRecognition ;
    airo:hasConsequence ex:PersonalPrivacyExposure .

ex:RussianPornActresses rdf:type airo:AISubject ;
    rdfs:label "Russian Porn Actresses"@en ;
    rdfs:label "俄罗斯色情演员"@zh .

ex:PersonalPrivacyExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Privacy Exposure"@en ;
    rdfs:label "个人隐私暴露"@zh ;
    airo:hasImpact ex:SocialStigmaAndDiscrimination .

ex:SocialStigmaAndDiscrimination rdf:type airo:Impact ;
    rdfs:label "Social Stigma and Discrimination"@en ;
    rdfs:label "社会污名与歧视"@zh ;
    airo:hasImpactOnStakeholder ex:RussianPornActresses .

ex:WeaponizationOfFacialRecognition rdf:type airo:Misuse ;
    rdfs:label "Weaponization of Facial Recognition"@en ;
    rdfs:label "面部识别武器化"@zh ;
    airo:isRiskSourceFor ex:PrivacyViolationRisk .