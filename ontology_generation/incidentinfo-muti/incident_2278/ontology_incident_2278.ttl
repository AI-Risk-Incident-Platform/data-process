@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ComputerGeneratedPaperSystem rdf:type airo:AISystem ;
    rdfs:label "Computer Generated Paper System"@en ;
    rdfs:label "计算机生成论文系统"@zh ;
    airo:hasRisk ex:AcademicIntegrityRisk ;
    airo:hasStakeholder ex:SciencePublishers .

ex:AcademicIntegrityRisk rdf:type airo:Risk ;
    rdfs:label "Academic Integrity Risk"@en ;
    rdfs:label "学术诚信风险"@zh ;
    airo:hasConsequence ex:PaperWithdrawalConsequence ;
    airo:isRiskSourceFor ex:AutomatedContentGeneration .

ex:PaperWithdrawalConsequence rdf:type airo:Consequence ;
    rdfs:label "Paper Withdrawal Consequence"@en ;
    rdfs:label "论文撤回后果"@zh ;
    airo:hasImpactOnStakeholder ex:SciencePublishers .

ex:SciencePublishers rdf:type airo:Stakeholder ;
    rdfs:label "Science Publishers"@en ;
    rdfs:label "科学出版商"@zh .

ex:AutomatedContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Automated Content Generation Risk Source"@en ;
    rdfs:label "自动化内容生成风险源"@zh ;
    airo:isRiskSourceFor ex:AcademicIntegrityRisk .