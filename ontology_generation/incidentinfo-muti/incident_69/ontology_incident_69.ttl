@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:DrunkDrivingWithAutopilotRisk ;
    airo:hasAISubject ex:DrunkDriver ;
    airo:hasCapability ex:AutonomousDrivingCapability ;
    airo:hasPurpose ex:AssistedDriving ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionAndSensorFusion ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasStakeholder ex:SanFranciscoBayAreaPolice .

ex:DrunkDrivingWithAutopilotRisk rdf:type airo:Risk ;
    rdfs:label "醉酒驾驶使用自动驾驶风险"@zh ;
    rdfs:label "Drunk Driving with Autopilot Risk"@en ;
    airo:hasConsequence ex:PotentialTrafficAccident ;
    airo:isRiskSourceFor ex:LackOfDriverMonitoring ;
    airo:isRiskSourceFor ex:MisuseOfAutopilotByImpairedDrivers ;
    airo:isRiskSourceFor ex:UnattendedAutonomousVehicleOperation .

ex:PotentialTrafficAccident rdf:type airo:Consequence ;
    rdfs:label "潜在交通事故后果"@zh ;
    rdfs:label "Potential Traffic Accident Consequence"@en ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:AutonomousDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:SanFranciscoBayAreaPolice rdf:type airo:Stakeholder ;
    rdfs:label "旧金山湾区警察"@zh ;
    rdfs:label "San Francisco Bay Area Police"@en .

ex:DrunkDriver rdf:type airo:AISubject ;
    rdfs:label "醉酒司机"@zh ;
    rdfs:label "Drunk Driver"@en .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "公共安全威胁影响"@zh ;
    rdfs:label "Public Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:SanFranciscoBayAreaPolice .

ex:AssistedDriving rdf:type airo:Purpose ;
    rdfs:label "辅助驾驶目的"@zh ;
    rdfs:label "Assisted Driving Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVisionAndSensorFusion rdf:type airo:AITechnique ;
    rdfs:label "计算机视觉与传感器融合技术"@zh ;
    rdfs:label "Computer Vision and Sensor Fusion Technique"@en .

ex:LackOfDriverMonitoring rdf:type airo:RiskSource ;
    rdfs:label "缺乏驾驶员监控风险源"@zh ;
    rdfs:label "Lack of Driver Monitoring Risk Source"@en ;
    airo:exploitsVulnerability ex:HumanOverrideFailure .

ex:MisuseOfAutopilotByImpairedDrivers rdf:type airo:Threat ;
    rdfs:label "受损驾驶员误用自动驾驶威胁"@zh ;
    rdfs:label "Misuse of Autopilot by Impaired Drivers Threat"@en ;
    airo:exploitsVulnerability ex:HumanOverrideFailure .

ex:UnattendedAutonomousVehicleOperation rdf:type airo:Hazard ;
    rdfs:label "无人监管自动驾驶车辆操作危害"@zh ;
    rdfs:label "Unattended Autonomous Vehicle Operation Hazard"@en ;
    airo:exploitsVulnerability ex:HumanOverrideFailure .

ex:HumanOverrideFailure rdf:type airo:Vulnerability ;
    rdfs:label "人工干预失效脆弱性"@zh ;
    rdfs:label "Human Override Failure Vulnerability"@en .

ex:AutonomousVehicleSafetyConcerns rdf:type airo:RiskConcept ;
    rdfs:label "自动驾驶车辆安全问题"@zh ;
    rdfs:label "Autonomous Vehicle Safety Concerns Risk Concept"@en .