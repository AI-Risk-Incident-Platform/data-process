@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberAutonomousVehicle rdf:type airo:AISystem ;
    rdfs:label "Uber Autonomous Vehicle"@en ;
    rdfs:label "优步自动驾驶汽车"@zh ;
    airo:hasRisk ex:TrafficViolationRisk ;
    airo:isDeployedBy ex:Uber ;
    airo:isAppliedWithinDomain ex:AutonomousDriving .

ex:TrafficViolationRisk rdf:type airo:Risk ;
    rdfs:label "Traffic Violation Risk"@en ;
    rdfs:label "交通违规风险"@zh ;
    airo:isRiskSourceFor ex:TechnologyFailure ;
    airo:hasConsequence ex:TrafficSafetyThreat .

ex:TechnologyFailure rdf:type airo:RiskSource ;
    rdfs:label "Technology Failure"@en ;
    rdfs:label "技术故障"@zh .

ex:TrafficSafetyThreat rdf:type airo:Consequence ;
    rdfs:label "Traffic Safety Threat"@en ;
    rdfs:label "交通安全威胁"@zh .

ex:Uber rdf:type airo:AIOperator ;
    rdfs:label "Uber"@en ;
    rdfs:label "优步"@zh .

ex:AutonomousDriving rdf:type airo:Domain ;
    rdfs:label "Autonomous Driving"@en ;
    rdfs:label "自动驾驶"@zh .