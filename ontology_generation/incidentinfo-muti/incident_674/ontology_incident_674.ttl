@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForElectionDisinformation rdf:type airo:AISystem ;
    rdfs:label "AI for Election Disinformation"@en ;
    rdfs:label "选举虚假信息AI系统"@zh ;
    airo:hasRisk ex:ElectionDisinformationRisk ;
    airo:hasAICapability ex:ContentGenerationAndManipulation ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:hasStakeholder ex:GlobalDemocraticInstitutions .

ex:ElectionDisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Election Disinformation Risk"@en ;
    rdfs:label "选举虚假信息风险"@zh ;
    airo:hasConsequence ex:ManipulatedElectionOutcomes ;
    airo:isRiskSourceFor ex:MaliciousUseOfGenerativeAI .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:GlobalDemocraticInstitutions .

ex:ManipulatedElectionOutcomes rdf:type airo:Consequence ;
    rdfs:label "Manipulated Election Outcomes Consequence"@en ;
    rdfs:label "操纵选举结果后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:ContentGenerationAndManipulation rdf:type airo:AICapability ;
    rdfs:label "Content Generation and Manipulation Capability"@en ;
    rdfs:label "内容生成与操纵能力"@zh .

ex:MaliciousUseOfGenerativeAI rdf:type airo:RiskSource ;
    rdfs:label "Malicious Use of Generative AI Risk Source"@en ;
    rdfs:label "生成式AI的恶意使用风险源"@zh .

ex:GlobalDemocraticInstitutions rdf:type airo:Stakeholder ;
    rdfs:label "Global Democratic Institutions Stakeholder"@en ;
    rdfs:label "全球民主机构利益相关者"@zh .