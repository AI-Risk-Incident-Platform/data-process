@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Generation System"@en ;
    rdfs:label "深度伪造生成系统"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:DeepLearningBasedImageManipulation ;
    airo:hasPurpose ex:PoliticalCampaignInfluence ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:hasStakeholder ex:SocialMediaUsers ;
    airo:isDeployedBy ex:XPlatform .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:LackOfContentModeration ;
    airo:hasResidualRisk ex:PoliticalManipulationImpact .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:PoliticalManipulationImpact .

ex:DeepLearningBasedImageManipulation rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Image Manipulation Technique"@en ;
    rdfs:label "基于深度学习的图像操纵技术"@zh .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:XPlatform rdf:type airo:AIOperator ;
    rdfs:label "X Platform"@en ;
    rdfs:label "X平台"@zh .

ex:PoliticalManipulationImpact rdf:type airo:Impact ;
    rdfs:label "Political Manipulation Impact"@en ;
    rdfs:label "政治操纵影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .

ex:PoliticalCampaignInfluence rdf:type airo:Purpose ;
    rdfs:label "Political Campaign Influence Purpose"@en ;
    rdfs:label "政治竞选影响目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:LackOfContentModeration rdf:type airo:RiskSource ;
    rdfs:label "Lack of Content Moderation Risk Source"@en ;
    rdfs:label "内容审核缺失风险源"@zh .

ex:PoliticalDeepfakeMisuse rdf:type airo:Risk ;
    rdfs:label "Political Deepfake Misuse"@en ;
    rdfs:label "政治深度伪造滥用"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:LackOfContentModeration .