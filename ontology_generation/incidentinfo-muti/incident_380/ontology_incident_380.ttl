@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonWorkerTrackingSystem rdf:type airo:AISystem ;
    rdfs:label "Amazon Worker Tracking System"@en ;
    rdfs:label "亚马逊工人追踪系统"@zh ;
    airo:hasRisk ex:WorkerPrivacyViolationRisk ;
    airo:isDevelopedBy ex:Amazon ;
    airo:hasPurpose ex:WorkerProductivityMonitoring ;
    airo:isAppliedWithinDomain ex:WarehouseOperations .

ex:WorkerPrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Worker Privacy Violation Risk"@en ;
    rdfs:label "工人隐私侵犯风险"@zh ;
    airo:hasConsequence ex:WorkerStressAndBurnout .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:WorkerProductivityMonitoring rdf:type airo:Purpose ;
    rdfs:label "Worker Productivity Monitoring"@en ;
    rdfs:label "工人生产力监控"@zh .

ex:WarehouseOperations rdf:type airo:Domain ;
    rdfs:label "Warehouse Operations"@en ;
    rdfs:label "仓库运营"@zh .

ex:WorkerStressAndBurnout rdf:type airo:Impact ;
    rdfs:label "Worker Stress and Burnout"@en ;
    rdfs:label "工人压力与倦怠"@zh .