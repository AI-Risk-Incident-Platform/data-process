@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGaydarSystem rdf:type airo:AISystem ;
    rdfs:label "AI Gaydar System"@en ;
    rdfs:label "AI同性恋识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasAISubject ex:LGBTQIndividuals ;
    airo:hasStakeholder ex:AcademicResearchers .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasConsequence ex:DiscriminationImpact .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:LGBTQIndividuals rdf:type airo:AISubject ;
    rdfs:label "LGBTQ Individuals"@en ;
    rdfs:label "LGBTQ群体"@zh .

ex:DiscriminationImpact rdf:type airo:Impact ;
    rdfs:label "Discrimination Impact"@en ;
    rdfs:label "歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:AcademicResearchers , ex:LGBTQIndividuals .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .

ex:AcademicResearchers rdf:type airo:Stakeholder ;
    rdfs:label "Academic Researchers"@en ;
    rdfs:label "学术研究者"@zh .