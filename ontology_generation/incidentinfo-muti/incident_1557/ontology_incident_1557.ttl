@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonAIProductReviewSystem rdf:type airo:AISystem ;
    rdfs:label "Amazon AI Product Review System"@en ;
    rdfs:label "亚马逊AI产品评论系统"@zh ;
    airo:hasRisk ex:NegativeFeedbackExaggerationRisk ;
    airo:hasStakeholder ex:AmazonMerchants .

ex:NegativeFeedbackExaggerationRisk rdf:type airo:Risk ;
    rdfs:label "Negative Feedback Exaggeration Risk"@en ;
    rdfs:label "负面反馈夸大风险"@zh ;
    airo:hasConsequence ex:MerchantDissatisfaction .

ex:AmazonMerchants rdf:type airo:Stakeholder ;
    rdfs:label "Amazon Merchants"@en ;
    rdfs:label "亚马逊商家"@zh .

ex:MerchantDissatisfaction rdf:type airo:Consequence ;
    rdfs:label "Merchant Dissatisfaction Consequence"@en ;
    rdfs:label "商家不满后果"@zh ;
    airo:hasImpact ex:PotentialSalesLoss .

ex:PotentialSalesLoss rdf:type airo:Impact ;
    rdfs:label "Potential Sales Loss Impact"@en ;
    rdfs:label "潜在销售损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:AmazonMerchants .