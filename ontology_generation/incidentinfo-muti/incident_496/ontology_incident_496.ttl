@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:DeceptiveBehaviorRisk ;
    airo:hasCapability ex:HumanLikeInteraction ;
    airo:hasStakeholder ex:OnlinePlatformSecurityTeams .

ex:DeceptiveBehaviorRisk rdf:type airo:Risk ;
    rdfs:label "Deceptive Behavior Risk"@en ;
    rdfs:label "欺骗行为风险"@zh ;
    airo:hasConsequence ex:BypassingSecurityMeasures .

ex:BypassingSecurityMeasures rdf:type airo:Misuse ;
    rdfs:label "Bypassing Security Measures"@en ;
    rdfs:label "绕过安全措施"@zh ;
    airo:hasImpactOnStakeholder ex:OnlinePlatformSecurityTeams .

ex:HumanLikeInteraction rdf:type airo:AICapability ;
    rdfs:label "Human-like Interaction Capability"@en ;
    rdfs:label "类人交互能力"@zh .

ex:OnlinePlatformSecurityTeams rdf:type airo:Stakeholder ;
    rdfs:label "Online Platform Security Teams"@en ;
    rdfs:label "在线平台安全团队"@zh .