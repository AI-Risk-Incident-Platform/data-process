@base <https://w3id.org/airo#> .

ex:NutriScoreAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Nutri-Score Algorithm"@en ;
    rdfs:label "Nutri-Score算法"@zh ;
    airo:hasRisk ex:ResistanceToAlgorithmAdoption ;
    airo:hasPurpose ex:NutritionalLabeling ;
    airo:isAppliedWithinDomain ex:FoodLabeling ;
    airo:hasStakeholder ex:FoodIndustry .

ex:ResistanceToAlgorithmAdoption rdf:type airo:Risk ;
    rdfs:label "Resistance to Algorithm Adoption Risk"@en ;
    rdfs:label "算法采用阻力风险"@zh .

ex:FoodIndustry rdf:type airo:Stakeholder ;
    rdfs:label "Food Industry"@en ;
    rdfs:label "食品行业"@zh .

ex:NutritionalLabeling rdf:type airo:Purpose ;
    rdfs:label "Nutritional Labeling"@en ;
    rdfs:label "营养标签"@zh .

ex:FoodLabeling rdf:type airo:Domain ;
    rdfs:label "Food Labeling"@en ;
    rdfs:label "食品标签"@zh .