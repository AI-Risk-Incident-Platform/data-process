ex:VanderbiltUniversity rdf:type airo:Stakeholder ;
    rdfs:label "Vanderbilt University"@en ;
    rdfs:label "范德堡大学"@zh .

ex:VanderbiltUniversityStaff rdf:type airo:AIOperator ;
    rdfs:label "Vanderbilt University Staff"@en ;
    rdfs:label "范德堡大学工作人员"@zh .

ex:PublicOutcryAndReputationDamage rdf:type airo:Consequence ;
