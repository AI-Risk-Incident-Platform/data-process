ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:InappropriateUseOfAIInSensitiveSituations ;
    airo:hasStakeholder ex:VanderbiltUniversity ;
    airo:isDeployedBy ex:VanderbiltUniversityStaff ;
    airo:hasPurpose ex:AddressingTragicEventCommunication ;
    airo:isAppliedWithinDomain ex:HigherEducation .

ex:InappropriateUseOfAIInSensitiveSituations rdf:type airo:Risk ;
