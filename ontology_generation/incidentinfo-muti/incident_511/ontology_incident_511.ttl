@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:FalseAccusationRisk ;
    airo:hasAISubject ex:AfricanAmericanCitizen ;
    airo:hasStakeholder ex:LawEnforcementAgency ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:FalseAccusationRisk rdf:type airo:Risk ;
    rdfs:label "False Accusation Risk"@en ;
    rdfs:label "错误指控风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "名誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:AfricanAmericanCitizen .

ex:AfricanAmericanCitizen rdf:type airo:AISubject ;
    rdfs:label "African American Citizen"@en ;
    rdfs:label "非裔美国公民"@zh .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .

ex:LawEnforcementAgency rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement Agency"@en ;
    rdfs:label "执法机构"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .