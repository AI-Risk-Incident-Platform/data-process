@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MassSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Mass Surveillance System"@en ;
    rdfs:label "大规模监控系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:JournalistsAndStudents ;
    airo:isDeployedBy ex:NeusoftAndHuawei ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasPurpose ex:MonitoringSpecificGroups .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasImpact ex:ChillingEffectOnFreeSpeech .

ex:JournalistsAndStudents rdf:type airo:Stakeholder ;
    rdfs:label "Journalists and Students"@en ;
    rdfs:label "记者和学生"@zh .

ex:NeusoftAndHuawei rdf:type airo:AIOperator ;
    rdfs:label "Neusoft and Huawei"@en ;
    rdfs:label "东软和华为"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security"@en ;
    rdfs:label "公共安全"@zh .

ex:MonitoringSpecificGroups rdf:type airo:Purpose ;
    rdfs:label "Monitoring Specific Groups"@en ;
    rdfs:label "监控特定群体"@zh .

ex:ChillingEffectOnFreeSpeech rdf:type airo:Impact ;
    rdfs:label "Chilling Effect on Free Speech"@en ;
    rdfs:label "对言论自由的寒蝉效应"@zh ;
    airo:hasImpactOnStakeholder ex:JournalistsAndStudents .