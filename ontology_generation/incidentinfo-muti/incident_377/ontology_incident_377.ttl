@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleHomeMiniSpeaker rdf:type airo:AISystem ;
    rdfs:label "Google Home Mini Speaker"@en ;
    rdfs:label "Google Home Mini智能音箱"@zh ;
    airo:hasRisk ex:RacialSlurOutputRisk ;
    airo:isDevelopedBy ex:Google ;
    airo:hasStakeholder ex:Google .

ex:RacialSlurOutputRisk rdf:type airo:Risk ;
    rdfs:label "Racial Slur Output Risk"@en ;
    rdfs:label "种族歧视词汇输出风险"@zh ;
    airo:hasConsequence ex:RacialDiscriminationHarm .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:RacialDiscriminationHarm rdf:type airo:Consequence ;
    rdfs:label "Racial Discrimination Harm Consequence"@en ;
    rdfs:label "种族歧视伤害后果"@zh ;
    airo:hasImpact ex:SocialHarmToMinorityGroups .

ex:SocialHarmToMinorityGroups rdf:type airo:Impact ;
    rdfs:label "Social Harm to Minority Groups Impact"@en ;
    rdfs:label "对少数群体的社会伤害影响"@zh .