    rdfs:label "政治人物Orsi"@zh .

ex:ConversationRecreationAI rdf:type airo:AITechnique ;
    rdfs:label "Conversation Recreation AI Technique"@en ;
    rdfs:label "对话重建AI技术"@zh .

ex:PublicDeception rdf:type airo:Consequence ;
    rdfs:label "Public Deception Consequence"@en ;
    rdfs:label "公众欺骗后果"@zh ;
