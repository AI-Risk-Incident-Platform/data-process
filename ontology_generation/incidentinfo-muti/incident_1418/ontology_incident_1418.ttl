@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SoutheastAsiaLLMEcosystem rdf:type airo:AISystem ;
    rdfs:label "Southeast Asia's First LLM Ecosystem"@en ;
    rdfs:label "东南亚首个大型语言模型生态系统"@zh ;
    airo:hasRisk ex:UnauthorizedUseOfCopyrightedContentRisk ;
    airo:isDeployedBy ex:SingaporeGovernment ;
    airo:hasStakeholder ex:SingaporeWritersAndPublishers .

ex:UnauthorizedUseOfCopyrightedContentRisk rdf:type airo:Risk ;
    rdfs:label "Unauthorized Use of Copyrighted Content Risk"@en ;
    rdfs:label "未经授权使用受版权保护内容风险"@zh ;
    airo:hasConsequence ex:PotentialCopyrightInfringement ;
    airo:isRiskSourceFor ex:LackOfConsentForContentUsage .

ex:SingaporeWritersAndPublishers rdf:type airo:Stakeholder ;
    rdfs:label "Singapore Writers and Publishers"@en ;
    rdfs:label "新加坡作家和出版商"@zh .

ex:SingaporeGovernment rdf:type airo:AIDeployer ;
    rdfs:label "Singapore Government"@en ;
    rdfs:label "新加坡政府"@zh .

ex:LackOfConsentForContentUsage rdf:type airo:RiskSource ;
    rdfs:label "Lack of Consent for Content Usage"@en ;
    rdfs:label "缺乏内容使用同意"@zh .

ex:PotentialCopyrightInfringement rdf:type airo:Consequence ;
    rdfs:label "Potential Copyright Infringement Consequence"@en ;
    rdfs:label "潜在版权侵权后果"@zh .