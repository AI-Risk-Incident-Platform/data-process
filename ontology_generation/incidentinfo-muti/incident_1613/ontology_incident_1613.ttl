@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WorldcoinCryptoProject rdf:type airo:AISystem ;
    rdfs:label "Worldcoin Crypto Project"@en ;
    rdfs:label "Worldcoin加密货币项目"@zh ;
    airo:hasRisk ex:SafetyConcernsRisk ;
    airo:hasStakeholder ex:KenyaGovernment .

ex:SafetyConcernsRisk rdf:type airo:Risk ;
    rdfs:label "Safety Concerns Risk"@en ;
    rdfs:label "安全隐患风险"@zh ;
    airo:isRiskSourceFor ex:EyeballScansDataCollection ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "安全隐患后果"@zh ;
        rdfs:label "Safety Concerns Consequence"@en ;
        airo:hasImpact ex:PrivacyViolationImpact
    ] .

ex:KenyaGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Kenya Government"@en ;
    rdfs:label "肯尼亚政府"@zh .

ex:EyeballScansDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Eyeball Scans Data Collection"@en ;
    rdfs:label "眼球扫描数据收集"@zh .

ex:PrivacyViolationImpact rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:KenyaGovernment .