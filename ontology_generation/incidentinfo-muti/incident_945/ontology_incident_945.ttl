@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:PhishingScamRisk ;
    airo:hasStakeholder ex:ChatGPTUsers .

ex:PhishingScamRisk rdf:type airo:Risk ;
    rdfs:label "Phishing Scam Risk"@en ;
    rdfs:label "钓鱼诈骗风险"@zh ;
    airo:hasConsequence ex:UserCredentialsTheft ;
    airo:isRiskSourceFor ex:FakePremiumService .

ex:UserCredentialsTheft rdf:type airo:Consequence ;
    rdfs:label "User Credentials Theft Consequence"@en ;
    rdfs:label "用户凭证盗窃后果"@zh ;
    airo:hasImpact ex:PrivacyAndSecurityCompromise .

ex:PrivacyAndSecurityCompromise rdf:type airo:Impact ;
    rdfs:label "Privacy and Security Compromise Impact"@en ;
    rdfs:label "隐私和安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChatGPTUsers .

ex:FakePremiumService rdf:type airo:RiskSource ;
    rdfs:label "Fake Premium Service Risk Source"@en ;
    rdfs:label "虚假高级服务风险源"@zh .

ex:ChatGPTUsers rdf:type airo:Stakeholder ;
    rdfs:label "ChatGPT Users"@en ;
    rdfs:label "ChatGPT用户"@zh .