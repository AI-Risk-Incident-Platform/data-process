@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClearviewAIFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Clearview AI Facial Recognition System"@en ;
    rdfs:label "Clearview AI面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDevelopedBy ex:ClearviewAI ;
    airo:hasStakeholder ex:DutchRegulatoryAgency ;
    airo:usesTechnique ex:FacialRecognitionAlgorithm ;
    airo:hasPurpose ex:FacialRecognitionForLawEnforcement ;
    airo:isAppliedWithinDomain ex:LawEnforcement .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LegalPenalty ;
    airo:isRiskSourceFor ex:IllegalDataCollection .

ex:LegalPenalty rdf:type airo:Consequence ;
    rdfs:label "Legal Penalty Consequence"@en ;
    rdfs:label "法律处罚后果"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:ClearviewAI rdf:type airo:AIOperator ;
    rdfs:label "Clearview AI"@en ;
    rdfs:label "Clearview AI公司"@zh .

ex:DutchRegulatoryAgency rdf:type airo:Stakeholder ;
    rdfs:label "Dutch Regulatory Agency"@en ;
    rdfs:label "荷兰监管机构"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss Impact"@en ;
    rdfs:label "财务损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:ClearviewAI .

ex:FacialRecognitionForLawEnforcement rdf:type airo:Purpose ;
    rdfs:label "Facial Recognition for Law Enforcement Purpose"@en ;
    rdfs:label "执法面部识别目的"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .

ex:FacialRecognitionAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Algorithm Technique"@en ;
    rdfs:label "面部识别算法技术"@zh .

ex:IllegalDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Illegal Data Collection Risk Source"@en ;
    rdfs:label "非法数据收集风险源"@zh .