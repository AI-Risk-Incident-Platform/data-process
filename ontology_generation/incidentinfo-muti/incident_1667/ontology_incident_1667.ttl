@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIForTrainingPhotos rdf:type airo:AISystem ;
    rdfs:label "AI for Training Photos"@en ;
    rdfs:label "用于训练照片的AI"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:Photographer ;
    airo:isDeployedBy ex:Laion .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalDispute ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfPhotos .

ex:Photographer rdf:type airo:Stakeholder ;
    rdfs:label "Photographer"@en ;
    rdfs:label "摄影师"@zh .

ex:Laion rdf:type airo:AIOperator ;
    rdfs:label "Laion"@en ;
    rdfs:label "Laion"@zh .

ex:UnauthorizedUseOfPhotos rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Use of Photos"@en ;
    rdfs:label "未经授权使用照片"@zh .

ex:LegalDispute rdf:type airo:Consequence ;
    rdfs:label "Legal Dispute"@en ;
    rdfs:label "法律纠纷"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpactOnStakeholder ex:Photographer .