@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SaudiArabiaMaleRobot rdf:type airo:AISystem ;
    rdfs:label "Saudi Arabia Male Robot"@en ;
    rdfs:label "沙特阿拉伯男性机器人"@zh ;
    airo:hasRisk ex:InappropriatePhysicalContactRisk ;
    airo:hasAISubject ex:FemaleReporter ;
    airo:hasStakeholder ex:SaudiArabiaPublic .

ex:InappropriatePhysicalContactRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Physical Contact Risk"@en ;
    rdfs:label "不当身体接触风险"@zh ;
    airo:hasConsequence ex:PublicOutrage ;
    airo:isRiskSourceFor ex:UnintendedPhysicalInteraction .

ex:PublicOutrage rdf:type airo:Consequence ;
    rdfs:label "Public Outrage Consequence"@en ;
    rdfs:label "公众愤怒后果"@zh ;
    airo:hasImpact ex:SocialControversy .

ex:FemaleReporter rdf:type airo:AISubject ;
    rdfs:label "Female Reporter"@en ;
    rdfs:label "女性记者"@zh .

ex:SocialControversy rdf:type airo:Impact ;
    rdfs:label "Social Controversy Impact"@en ;
    rdfs:label "社会争议影响"@zh ;
    airo:hasImpactOnStakeholder ex:SaudiArabiaPublic .

ex:SaudiArabiaPublic rdf:type airo:Stakeholder ;
    rdfs:label "Saudi Arabia Public"@en ;
    rdfs:label "沙特阿拉伯公众"@zh .

ex:UnintendedPhysicalInteraction rdf:type airo:RiskSource ;
    rdfs:label "Unintended Physical Interaction Risk Source"@en ;
    rdfs:label "意外物理互动风险源"@zh .