@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PedestrianSpeedingReportingApp rdf:type airo:AISystem ;
    rdfs:label "Pedestrian Speeding Reporting App"@en ;
    rdfs:label "行人举报超速应用"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:Pedestrians ;
    airo:hasAISubject ex:Drivers ;
    airo:hasPurpose ex:TrafficViolationReporting .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:Pedestrians rdf:type airo:Stakeholder ;
    rdfs:label "Pedestrians"@en ;
    rdfs:label "行人"@zh .

ex:Drivers rdf:type airo:AISubject ;
    rdfs:label "Drivers"@en ;
    rdfs:label "驾驶员"@zh .

ex:TrafficViolationReporting rdf:type airo:Purpose ;
    rdfs:label "Traffic Violation Reporting"@en ;
    rdfs:label "交通违规举报"@zh .