@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:AcademicIntegrityRisk ;
    airo:hasPurpose ex:AcademicReportGeneration ;
    airo:hasStakeholder ex:AustralianResearchCouncil ;
    airo:isAppliedWithinDomain ex:AcademicResearch .

ex:AcademicIntegrityRisk rdf:type airo:Risk ;
    rdfs:label "Academic Integrity Risk"@en ;
    rdfs:label "学术诚信风险"@zh ;
    airo:hasConsequence ex:CompromisedPeerReviewProcess .

ex:AustralianResearchCouncil rdf:type airo:Stakeholder ;
    rdfs:label "Australian Research Council"@en ;
    rdfs:label "澳大利亚研究委员会"@zh .

ex:AcademicReportGeneration rdf:type airo:Purpose ;
    rdfs:label "Academic Report Generation"@en ;
    rdfs:label "学术报告生成"@zh .

ex:AcademicResearch rdf:type airo:Domain ;
    rdfs:label "Academic Research"@en ;
    rdfs:label "学术研究"@zh .

ex:CompromisedPeerReviewProcess rdf:type airo:Consequence ;
    rdfs:label "Compromised Peer Review Process"@en ;
    rdfs:label "同行评审过程受损"@zh ;
    airo:hasImpact ex:ReducedResearchQuality .

ex:ReducedResearchQuality rdf:type airo:Impact ;
    rdfs:label "Reduced Research Quality"@en ;
    rdfs:label "研究质量下降"@zh ;
    airo:hasImpactOnStakeholder ex:AustralianResearchCouncil .