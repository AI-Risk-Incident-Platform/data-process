@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:InstacartAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Instacart Algorithm"@en ;
    rdfs:label "Instacart算法"@zh ;
    airo:hasRisk ex:DriverExploitationRisk ;
    airo:isDevelopedBy ex:InstacartCompany ;
    airo:hasStakeholder ex:InstacartDrivers ;
    airo:hasPurpose ex:GigWorkManagement .

ex:DriverExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Driver Exploitation Risk"@en ;
    rdfs:label "司机剥削风险"@zh ;
    airo:hasConsequence ex:LowDriverCompensation ;
    airo:isRiskSourceFor ex:AlgorithmGlitch .

ex:LowDriverCompensation rdf:type airo:Consequence ;
    rdfs:label "Low Driver Compensation Consequence"@en ;
    rdfs:label "司机低报酬后果"@zh ;
    airo:hasImpact ex:WorkerStrikeImpact .

ex:InstacartDrivers rdf:type airo:Stakeholder ;
    rdfs:label "Instacart Drivers"@en ;
    rdfs:label "Instacart司机"@zh .

ex:WorkerStrikeImpact rdf:type airo:Impact ;
    rdfs:label "Worker Strike Impact"@en ;
    rdfs:label "工人罢工影响"@zh ;
    airo:hasImpactOnStakeholder ex:InstacartDrivers .

ex:AlgorithmGlitch rdf:type airo:RiskSource ;
    rdfs:label "Algorithm Glitch Risk Source"@en ;
    rdfs:label "算法故障风险源"@zh .

ex:InstacartCompany rdf:type airo:AIOperator ;
    rdfs:label "Instacart Company"@en ;
    rdfs:label "Instacart公司"@zh .

ex:GigWorkManagement rdf:type airo:Purpose ;
    rdfs:label "Gig Work Management Purpose"@en ;
    rdfs:label "零工管理目的"@zh .