@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "YouTube Content Moderation System"@en ;
    rdfs:label "YouTube内容审核系统"@zh ;
    airo:hasRisk ex:InappropriateContentDistributionRisk ;
    airo:usesTechnique ex:AutomatedContentFiltering ;
    airo:isDevelopedBy ex:GoogleLLC ;
    airo:hasStakeholder ex:YouTubeViewers .

ex:InappropriateContentDistributionRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Distribution Risk"@en ;
    rdfs:label "不当内容传播风险"@zh ;
    airo:hasConsequence ex:ExposureToHarmfulContent ;
    airo:isRiskSourceFor ex:ClickbaitThumbnails .

ex:ExposureToHarmfulContent rdf:type airo:Consequence ;
    rdfs:label "Exposure to Harmful Content Consequence"@en ;
    rdfs:label "接触有害内容后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToViewers .

ex:PsychologicalHarmToViewers rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Viewers Impact"@en ;
    rdfs:label "观众心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:YouTubeViewers .

ex:YouTubeViewers rdf:type airo:Stakeholder ;
    rdfs:label "YouTube Viewers"@en ;
    rdfs:label "YouTube观众"@zh .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌有限责任公司"@zh .

ex:ClickbaitThumbnails rdf:type airo:RiskSource ;
    rdfs:label "Clickbait Thumbnails Risk Source"@en ;
    rdfs:label "点击诱饵缩略图风险源"@zh .

ex:AutomatedContentFiltering rdf:type airo:AITechnique ;
    rdfs:label "Automated Content Filtering Technique"@en ;
    rdfs:label "自动化内容过滤技术"@zh .