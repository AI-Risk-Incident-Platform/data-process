@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DutchTaxAuthorityAlgorithm rdf:type airo:AISystem ;
    rdfs:label "荷兰税务局算法"@zh ;
    rdfs:label "Dutch Tax Authority Algorithm"@en ;
    airo:hasRisk ex:DiscriminatoryAlgorithmRisk ;
    airo:hasStakeholder ex:DutchTaxAuthority ;
    airo:isAppliedWithinDomain ex:TaxationAndSocialWelfare .

ex:DiscriminatoryAlgorithmRisk rdf:type airo:Risk ;
    rdfs:label "歧视性算法风险"@zh ;
    rdfs:label "Discriminatory Algorithm Risk"@en ;
    airo:hasConsequence ex:WrongfulAccusationOfFraud ;
    airo:isRiskSourceFor ex:BiasedAlgorithmDesign .

ex:WrongfulAccusationOfFraud rdf:type airo:Consequence ;
    rdfs:label "错误指控欺诈的后果"@zh ;
    rdfs:label "Wrongful Accusation of Fraud Consequence"@en ;
    airo:hasImpact ex:FinancialHardshipForFamilies .

ex:DutchTaxAuthority rdf:type airo:Stakeholder ;
    rdfs:label "荷兰税务局"@zh ;
    rdfs:label "Dutch Tax Authority"@en .

ex:FinancialHardshipForFamilies rdf:type airo:Impact ;
    rdfs:label "家庭经济困难的影响"@zh ;
    rdfs:label "Financial Hardship for Families Impact"@en ;
    airo:hasImpactOnStakeholder ex:DutchTaxAuthority .

ex:BiasedAlgorithmDesign rdf:type airo:RiskSource ;
    rdfs:label "偏见算法设计的风险源"@zh ;
    rdfs:label "Biased Algorithm Design Risk Source"@en .

ex:TaxationAndSocialWelfare rdf:type airo:Domain ;
    rdfs:label "税收和社会福利领域"@zh ;
    rdfs:label "Taxation and Social Welfare Domain"@en .