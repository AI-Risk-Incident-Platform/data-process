@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:SystematicTheftRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:Authors ;
    airo:isAppliedWithinDomain ex:CreativeWriting .

ex:SystematicTheftRisk rdf:type airo:Risk ;
    rdfs:label "Systematic Theft Risk"@en ;
    rdfs:label "系统性盗窃风险"@zh ;
    airo:hasConsequence ex:CopyrightInfringement ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfCopyrightedWorks .

ex:Authors rdf:type airo:Stakeholder ;
    rdfs:label "Authors"@en ;
    rdfs:label "作者"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:CopyrightInfringement rdf:type airo:Consequence ;
    rdfs:label "Copyright Infringement Consequence"@en ;
    rdfs:label "版权侵权后果"@zh ;
    airo:hasImpact ex:EconomicLossForAuthors .

ex:EconomicLossForAuthors rdf:type airo:Impact ;
    rdfs:label "Economic Loss for Authors Impact"@en ;
    rdfs:label "作者经济损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Authors .

ex:CreativeWriting rdf:type airo:Domain ;
    rdfs:label "Creative Writing Domain"@en ;
    rdfs:label "创意写作领域"@zh .

ex:UnauthorizedUseOfCopyrightedWorks rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Use of Copyrighted Works Risk Source"@en ;
    rdfs:label "未经授权使用受版权保护作品的风险源"@zh .