@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleMapsNavigationSystem rdf:type airo:AISystem ;
    rdfs:label "谷歌地图导航系统"@zh ;
    rdfs:label "Google Maps Navigation System"@en ;
    airo:hasRisk ex:NavigationErrorRisk ;
    airo:hasAIOperator ex:GoogleLLC ;
    airo:hasStakeholder ex:IndianRoadUsers ;
    airo:hasCapability ex:RoutePlanningAndGuidance ;
    airo:isAppliedWithinDomain ex:TransportationNavigation .

ex:NavigationErrorRisk rdf:type airo:Risk ;
    rdfs:label "导航错误风险"@zh ;
    rdfs:label "Navigation Error Risk"@en ;
    airo:hasConsequence ex:FatalCarAccident .

ex:FatalCarAccident rdf:type airo:Consequence ;
    rdfs:label "致命车祸后果"@zh ;
    rdfs:label "Fatal Car Accident Consequence"@en ;
    airo:hasImpact ex:LossOfLife .

ex:LossOfLife rdf:type airo:Impact ;
    rdfs:label "生命损失影响"@zh ;
    rdfs:label "Loss of Life Impact"@en ;
    airo:hasImpactOnStakeholder ex:IndianRoadUsers .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "谷歌有限责任公司"@zh ;
    rdfs:label "Google LLC"@en .

ex:IndianRoadUsers rdf:type airo:Stakeholder ;
    rdfs:label "印度道路使用者"@zh ;
    rdfs:label "Indian Road Users"@en .

ex:RoutePlanningAndGuidance rdf:type airo:AICapability ;
    rdfs:label "路线规划与引导能力"@zh ;
    rdfs:label "Route Planning and Guidance Capability"@en .

ex:TransportationNavigation rdf:type airo:Domain ;
    rdfs:label "交通导航领域"@zh ;
    rdfs:label "Transportation Navigation Domain"@en .