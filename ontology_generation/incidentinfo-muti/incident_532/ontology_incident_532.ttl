@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NJTransitStormModelingSystem rdf:type airo:AISystem ;
    rdfs:label "NJ Transit Storm Modeling System"@en ;
    rdfs:label "新泽西交通风暴建模系统"@zh ;
    airo:hasRisk ex:InaccurateStormModelingRisk ;
    airo:hasStakeholder ex:NJTransit .

ex:InaccurateStormModelingRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Storm Modeling Risk"@en ;
    rdfs:label "风暴建模不准确风险"@zh ;
    airo:hasConsequence ex:TransportationSystemFailure .

ex:NJTransit rdf:type airo:Stakeholder ;
    rdfs:label "NJ Transit"@en ;
    rdfs:label "新泽西交通"@zh .

ex:TransportationSystemFailure rdf:type airo:Consequence ;
    rdfs:label "Transportation System Failure Consequence"@en ;
    rdfs:label "交通系统故障后果"@zh ;
    airo:hasImpact ex:PublicTransportDisruption .

ex:PublicTransportDisruption rdf:type airo:Impact ;
    rdfs:label "Public Transport Disruption Impact"@en ;
    rdfs:label "公共交通中断影响"@zh ;
    airo:hasImpactOnStakeholder ex:NJTransit .