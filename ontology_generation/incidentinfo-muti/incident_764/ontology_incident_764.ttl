@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造图像生成系统"@zh ;
    rdfs:label "Deepfake Image Generation System"@en ;
    airo:hasRisk ex:MisinformationAndPublicDeceptionRisk ;
    airo:hasAISubject ex:CelebrityPublicFigure ;
    airo:hasPurpose ex:PoliticalEndorsementFabrication ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasStakeholder ex:PoliticalCampaignTeam .

ex:MisinformationAndPublicDeceptionRisk rdf:type airo:Risk ;
    rdfs:label "虚假信息和公众欺骗风险"@zh ;
    rdfs:label "Misinformation and Public Deception Risk"@en ;
    airo:hasConsequence ex:PoliticalMisinformationCampaign ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PoliticalCampaignTeam rdf:type airo:Stakeholder ;
    rdfs:label "政治竞选团队"@zh ;
    rdfs:label "Political Campaign Team"@en .

ex:CelebrityPublicFigure rdf:type airo:AISubject ;
    rdfs:label "名人公众人物"@zh ;
    rdfs:label "Celebrity Public Figure"@en .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "公众信任侵蚀"@zh ;
    rdfs:label "Public Trust Erosion"@en ;
    airo:hasImpactOnStakeholder ex:PoliticalCampaignTeam .

ex:PoliticalEndorsementFabrication rdf:type airo:Purpose ;
    rdfs:label "政治背书伪造"@zh ;
    rdfs:label "Political Endorsement Fabrication"@en .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "生成对抗网络"@zh ;
    rdfs:label "Generative Adversarial Networks"@en .

ex:PoliticalMisinformationCampaign rdf:type airo:Misuse ;
    rdfs:label "政治虚假信息活动"@zh ;
    rdfs:label "Political Misinformation Campaign"@en .