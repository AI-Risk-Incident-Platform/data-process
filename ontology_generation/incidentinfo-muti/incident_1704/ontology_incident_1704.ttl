@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystemAtMWC rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System at MWC"@en ;
    rdfs:label "MWC面部识别系统"@zh ;
    airo:hasRisk ex:GDPRInfringementRisk ;
    airo:hasStakeholder ex:GSMA ;
    airo:isAppliedWithinDomain ex:EventManagement .

ex:GDPRInfringementRisk rdf:type airo:Risk ;
    rdfs:label "GDPR Infringement Risk"@en ;
    rdfs:label "GDPR违规风险"@zh ;
    airo:hasConsequence ex:RegulatoryFine .

ex:GSMA rdf:type airo:Stakeholder ;
    rdfs:label "GSMA"@en ;
    rdfs:label "GSMA"@zh .

ex:RegulatoryFine rdf:type airo:Consequence ;
    rdfs:label "Regulatory Fine Consequence"@en ;
    rdfs:label "监管罚款后果"@zh ;
    airo:hasImpactOnStakeholder ex:GSMA .

ex:EventManagement rdf:type airo:Domain ;
    rdfs:label "Event Management Domain"@en ;
    rdfs:label "活动管理领域"@zh .