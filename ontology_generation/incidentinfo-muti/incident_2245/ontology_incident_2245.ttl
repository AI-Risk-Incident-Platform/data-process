@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutonomousVehicles rdf:type airo:AISystem ;
    rdfs:label "Autonomous Vehicles"@en ;
    rdfs:label "自动驾驶汽车"@zh ;
    airo:hasRisk ex:RoadSignSpoofingRisk ;
    airo:hasCapability ex:RoadSignRecognition .

ex:RoadSignSpoofingRisk rdf:type airo:Risk ;
    rdfs:label "Road Sign Spoofing Risk"@en ;
    rdfs:label "道路标志欺骗风险"@zh ;
    airo:hasConsequence ex:VehicleMisinterpretation ;
    airo:isRiskSourceFor ex:DroneProjectedRoadSigns .

ex:DroneProjectedRoadSigns rdf:type airo:RiskSource ;
    rdfs:label "Drone Projected Road Signs"@en ;
    rdfs:label "无人机投射道路标志"@zh ;
    airo:exploitsVulnerability ex:HumanPerceptionLimitation .

ex:VehicleMisinterpretation rdf:type airo:Consequence ;
    rdfs:label "Vehicle Misinterpretation Consequence"@en ;
    rdfs:label "车辆误判后果"@zh .

ex:RoadSignRecognition rdf:type airo:AICapability ;
    rdfs:label "Road Sign Recognition Capability"@en ;
    rdfs:label "道路标志识别能力"@zh .

ex:HumanPerceptionLimitation rdf:type airo:Vulnerability ;
    rdfs:label "Human Perception Limitation Vulnerability"@en ;
    rdfs:label "人类感知限制的脆弱性"@zh .

ex:DroneBasedSpoofingAttack rdf:type airo:Threat ;
    rdfs:label "Drone-based Spoofing Attack Threat"@en ;
    rdfs:label "基于无人机的欺骗攻击威胁"@zh ;
    airo:isRiskSourceFor ex:RoadSignSpoofingRisk .