@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPTNavigationApp rdf:type airo:AISystem ;
    rdfs:label "ChatGPT Navigation App"@en ;
    rdfs:label "ChatGPT导航应用"@zh ;
    airo:hasRisk ex:NavigationMisguidanceRisk ;
    airo:hasAIUser ex:TouristsUsingChatGPT .

ex:NavigationMisguidanceRisk rdf:type airo:Risk ;
    rdfs:label "Navigation Misguidance Risk"@en ;
    rdfs:label "导航误导风险"@zh ;
    airo:hasConsequence ex:MountainRescueOperation ;
    airo:hasStakeholder ex:TOPRRescueTeam ;
    airo:hasStakeholder ex:TouristsUsingChatGPT ;
    airo:hasSubRisk ex:InappropriateUseForMountainNavigation .

ex:MountainRescueOperation rdf:type airo:Consequence ;
    rdfs:label "Mountain Rescue Operation Consequence"@en ;
    rdfs:label "山地救援行动后果"@zh ;
    airo:hasImpactOnStakeholder ex:TOPRRescueTeam ;
    airo:hasImpactOnStakeholder ex:TouristsUsingChatGPT .

ex:TOPRRescueTeam rdf:type airo:Stakeholder ;
    rdfs:label "TOPR Rescue Team"@en ;
    rdfs:label "TOPR救援队"@zh .

ex:TouristsUsingChatGPT rdf:type airo:AIUser ;
    rdfs:label "Tourists Using ChatGPT"@en ;
    rdfs:label "使用ChatGPT的游客"@zh .

ex:InappropriateUseForMountainNavigation rdf:type airo:Misuse ;
    rdfs:label "Inappropriate Use for Mountain Navigation"@en ;
    rdfs:label "不恰当的山地导航使用"@zh .