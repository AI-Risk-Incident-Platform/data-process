@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:DataLeakRisk ;
    airo:hasStakeholder ex:SamsungEmployees ;
    airo:isDeployedBy ex:Samsung .

ex:DataLeakRisk rdf:type airo:Risk ;
    rdfs:label "Data Leak Risk"@en ;
    rdfs:label "数据泄露风险"@zh ;
    airo:hasConsequence ex:SensitiveCodeLeak ;
    airo:isRiskSourceFor ex:EmployeeMisuseOfAI ;
    airo:hasRiskControl ex:AIBanPolicy .

ex:SamsungEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Samsung Employees"@en ;
    rdfs:label "三星员工"@zh .

ex:Samsung rdf:type airo:AIOperator ;
    rdfs:label "Samsung"@en ;
    rdfs:label "三星"@zh .

ex:SensitiveCodeLeak rdf:type airo:Consequence ;
    rdfs:label "Sensitive Code Leak Consequence"@en ;
    rdfs:label "敏感代码泄露后果"@zh ;
    airo:hasImpactOnStakeholder ex:SamsungEmployees .

ex:EmployeeMisuseOfAI rdf:type airo:RiskSource ;
    rdfs:label "Employee Misuse of AI Risk Source"@en ;
    rdfs:label "员工滥用AI风险源"@zh .

ex:AIBanPolicy rdf:type airo:RiskControl ;
    rdfs:label "AI Ban Policy Risk Control"@en ;
    rdfs:label "AI禁令管控"@zh ;
    airo:mitigatesRiskConcept ex:DataLeakRisk .