@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAdDeliverySystem rdf:type airo:AISystem ;
    rdfs:label "Google Ad Delivery System"@en ;
    rdfs:label "谷歌广告投放系统"@zh ;
    airo:hasRisk ex:RacialBiasInAdDelivery ;
    airo:hasStakeholder ex:HarvardUniversityResearchers ;
    airo:isAppliedWithinDomain ex:OnlineAdvertising ;
    airo:isDeployedBy ex:GoogleLLC .

ex:RacialBiasInAdDelivery rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Ad Delivery Risk"@en ;
    rdfs:label "广告投放中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryAdTargeting ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInAdTargeting .

ex:HarvardUniversityResearchers rdf:type airo:Stakeholder ;
    rdfs:label "Harvard University Researchers"@en ;
    rdfs:label "哈佛大学研究人员"@zh .

ex:DiscriminatoryAdTargeting rdf:type airo:Impact ;
    rdfs:label "Discriminatory Ad Targeting Impact"@en ;
    rdfs:label "歧视性广告定向的影响"@zh .

ex:OnlineAdvertising rdf:type airo:Domain ;
    rdfs:label "Online Advertising Domain"@en ;
    rdfs:label "在线广告领域"@zh .

ex:AlgorithmicBiasInAdTargeting rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Ad Targeting Risk Source"@en ;
    rdfs:label "广告定向中的算法偏见风险源"@zh .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌有限责任公司"@zh .