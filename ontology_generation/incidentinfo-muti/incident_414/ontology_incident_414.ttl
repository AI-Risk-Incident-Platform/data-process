ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:RacismProblemInAI ;
    airo:hasCapability ex:NaturalLanguageProcessing .

ex:RacismProblemInAI rdf:type airo:Risk ;
    rdfs:label "Racism Problem in AI Risk"@en ;
    rdfs:label "AI中的种族主义问题风险"@zh ;
    airo:isRiskSourceFor ex:BiasInTrainingData ;
    airo:hasImpact ex:SocialDiscrimination .

ex:NaturalLanguageProcessing rdf:type airo:AICapability ;
    rdfs:label "Natural Language Processing Capability"@en ;
    rdfs:label "自然语言处理能力"@zh .

ex:BiasInTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Bias in Training Data Risk Source"@en ;
    rdfs:label "训练数据中的偏见风险源"@zh .

ex:SocialDiscrimination rdf:type airo:Impact ;
    rdfs:label "Social Discrimination Impact"@en ;
    rdfs:label "社会歧视影响"@zh .