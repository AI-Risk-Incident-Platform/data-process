@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasAICapability ex:SemiAutonomousDrivingCapability ;
    airo:hasPurpose ex:DriverAssistanceTechnology ;
    airo:hasStakeholder ex:PoliceDepartment ;
    airo:hasStakeholder ex:TeslaDriver ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    airo:hasConsequence ex:VehicleDamageAndSafetyHazard ;
    airo:isRiskSourceFor ex:DriverDistractionDuringAutopilot .

ex:VehicleDamageAndSafetyHazard rdf:type airo:Consequence ;
    rdfs:label "车辆损坏和安全危害后果"@zh ;
    rdfs:label "Vehicle Damage and Safety Hazard Consequence"@en ;
    airo:hasImpact ex:PublicSafetyCompromise .

ex:SemiAutonomousDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "半自动驾驶能力"@zh ;
    rdfs:label "Semi-Autonomous Driving Capability"@en .

ex:PoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "警察部门"@zh ;
    rdfs:label "Police Department"@en .

ex:TeslaDriver rdf:type airo:AIOperator ;
    rdfs:label "特斯拉驾驶员"@zh ;
    rdfs:label "Tesla Driver"@en .

ex:DriverDistractionDuringAutopilot rdf:type airo:Misuse ;
    rdfs:label "自动驾驶期间驾驶员分心滥用"@zh ;
    rdfs:label "Driver Distraction During Autopilot Misuse"@en .

ex:PublicSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "公共安全受损影响"@zh ;
    rdfs:label "Public Safety Compromise Impact"@en ;
    airo:hasImpactOnStakeholder ex:PoliceDepartment .

ex:DriverAssistanceTechnology rdf:type airo:Purpose ;
    rdfs:label "驾驶辅助技术目的"@zh ;
    rdfs:label "Driver Assistance Technology Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .