@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:StableDiffusion rdf:type airo:AISystem ;
    rdfs:label "Stable Diffusion"@en ;
    rdfs:label "Stable Diffusion"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDevelopedBy ex:StabilityAI ;
    airo:hasStakeholder ex:GettyImages ;
    airo:usesTechnique ex:DataScraping ;
    airo:isAppliedWithinDomain ex:ArtAndMedia .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalDispute .

ex:GettyImages rdf:type airo:Stakeholder ;
    rdfs:label "Getty Images"@en ;
    rdfs:label "盖蒂图片社"@zh .

ex:StabilityAI rdf:type airo:AIOperator ;
    rdfs:label "Stability AI"@en ;
    rdfs:label "Stability AI"@zh .

ex:LegalDispute rdf:type airo:Consequence ;
    rdfs:label "Legal Dispute"@en ;
    rdfs:label "法律纠纷"@zh ;
    airo:hasImpactOnStakeholder ex:GettyImages .

ex:ArtAndMedia rdf:type airo:Domain ;
    rdfs:label "Art and Media"@en ;
    rdfs:label "艺术与媒体"@zh .

ex:DataScraping rdf:type airo:AITechnique ;
    rdfs:label "Data Scraping"@en ;
    rdfs:label "数据抓取"@zh .