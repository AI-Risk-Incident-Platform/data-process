@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:hasCapability ex:RealisticVideoManipulation ;
    airo:hasPurpose ex:PoliticalMessageDissemination ;
    airo:hasStakeholder ex:PoliticalLeaders ;
    airo:isAppliedWithinDomain ex:PoliticalCommunication ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:ElectionOutcomeManipulation ;
    airo:isRiskSourceFor ex:MaliciousContentCreators .

ex:RealisticVideoManipulation rdf:type airo:AICapability ;
    rdfs:label "Realistic Video Manipulation Capability"@en ;
    rdfs:label "逼真视频篡改能力"@zh .

ex:PoliticalLeaders rdf:type airo:Stakeholder ;
    rdfs:label "Political Leaders"@en ;
    rdfs:label "政治领导人"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:PoliticalLeaders .

ex:PoliticalMessageDissemination rdf:type airo:Purpose ;
    rdfs:label "Political Message Dissemination Purpose"@en ;
    rdfs:label "政治信息传播目的"@zh .

ex:PoliticalCommunication rdf:type airo:Domain ;
    rdfs:label "Political Communication Domain"@en ;
    rdfs:label "政治传播领域"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:ElectionOutcomeManipulation rdf:type airo:Consequence ;
    rdfs:label "Election Outcome Manipulation Consequence"@en ;
    rdfs:label "选举结果操纵后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:MaliciousContentCreators rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creators Risk Source"@en ;
    rdfs:label "恶意内容创作者风险源"@zh .