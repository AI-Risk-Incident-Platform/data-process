@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIArtGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Art Generation System"@en ;
    rdfs:label "AI艺术生成系统"@zh ;
    airo:hasRisk ex:ArtisticCompetitionFairnessRisk ;
    airo:isAppliedWithinDomain ex:FineArtsCompetition ;
    airo:hasStakeholder ex:HumanArtists .

ex:ArtisticCompetitionFairnessRisk rdf:type airo:Risk ;
    rdfs:label "Artistic Competition Fairness Risk"@en ;
    rdfs:label "艺术竞赛公平性风险"@zh ;
    airo:hasImpact ex:ProfessionalRecognitionImpact .

ex:HumanArtists rdf:type airo:Stakeholder ;
    rdfs:label "Human Artists"@en ;
    rdfs:label "人类艺术家"@zh .

ex:ProfessionalRecognitionImpact rdf:type airo:Impact ;
    rdfs:label "Professional Recognition Impact"@en ;
    rdfs:label "专业认可影响"@zh ;
    airo:hasImpactOnStakeholder ex:HumanArtists .

ex:FineArtsCompetition rdf:type airo:Domain ;
    rdfs:label "Fine Arts Competition Domain"@en ;
    rdfs:label "美术竞赛领域"@zh .