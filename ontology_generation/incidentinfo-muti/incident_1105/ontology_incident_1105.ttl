@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlgorithmLendingPrograms rdf:type airo:AISystem ;
    rdfs:label "Algorithm Lending Programs"@en ;
    rdfs:label "算法贷款程序"@zh ;
    airo:hasRisk ex:DiscriminationAgainstMinoritiesRisk ;
    airo:hasStakeholder ex:MinorityLoanApplicants ;
    airo:hasPurpose ex:AutomatedLoanApproval ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:DiscriminationAgainstMinoritiesRisk rdf:type airo:Risk ;
    rdfs:label "Discrimination Against Minorities Risk"@en ;
    rdfs:label "对少数族裔的歧视风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasRiskControl ex:BiasReductionTechniques .

ex:MinorityLoanApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Minority Loan Applicants"@en ;
    rdfs:label "少数族裔贷款申请人"@zh .

ex:UnequalAccessToCredit rdf:type airo:Impact ;
    rdfs:label "Unequal Access to Credit Impact"@en ;
    rdfs:label "信贷获取不平等的影响"@zh ;
    airo:hasImpactOnStakeholder ex:MinorityLoanApplicants .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:BiasReductionTechniques rdf:type airo:RiskControl ;
    rdfs:label "Bias Reduction Techniques Risk Control"@en ;
    rdfs:label "偏见减少技术管控"@zh ;
    airo:mitigatesRiskConcept ex:DiscriminationAgainstMinoritiesRisk .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:AutomatedLoanApproval rdf:type airo:Purpose ;
    rdfs:label "Automated Loan Approval Purpose"@en ;
    rdfs:label "自动化贷款审批的目的"@zh .