@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIBookGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Book Generation System"@en ;
    rdfs:label "AI书籍生成系统"@zh ;
    airo:hasRisk ex:FalseInformationDisseminationRisk ;
    airo:hasPurpose ex:AutomatedBookCreation ;
    airo:isAppliedWithinDomain ex:PublishingIndustry ;
    airo:hasStakeholder ex:BuckinghamPalace .

ex:FalseInformationDisseminationRisk rdf:type airo:Risk ;
    rdfs:label "False Information Dissemination Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:UnverifiedContentGeneration .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:BuckinghamPalace .

ex:BuckinghamPalace rdf:type airo:Stakeholder ;
    rdfs:label "Buckingham Palace"@en ;
    rdfs:label "白金汉宫"@zh .

ex:UnverifiedContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unverified Content Generation Risk Source"@en ;
    rdfs:label "未经核实内容生成风险源"@zh .

ex:AutomatedBookCreation rdf:type airo:Purpose ;
    rdfs:label "Automated Book Creation Purpose"@en ;
    rdfs:label "自动化书籍创作目的"@zh .

ex:PublishingIndustry rdf:type airo:Domain ;
    rdfs:label "Publishing Industry Domain"@en ;
    rdfs:label "出版业领域"@zh .