@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutomatedTerminationSystem rdf:type airo:AISystem ;
    rdfs:label "Automated Termination System"@en ;
    rdfs:label "自动解雇系统"@zh ;
    airo:hasRisk ex:UnjustTerminationRisk ;
    airo:isDeployedBy ex:UnspecifiedCompany ;
    airo:hasAISubject ex:TerminatedSoftwareEngineer .

ex:UnjustTerminationRisk rdf:type airo:Risk ;
    rdfs:label "Unjust Termination Risk"@en ;
    rdfs:label "不公正解雇风险"@zh ;
    airo:hasConsequence ex:JobLossAndOfficeAccessDenial .

ex:UnspecifiedCompany rdf:type airo:AIOperator ;
    rdfs:label "Unspecified Company"@en ;
    rdfs:label "未指定公司"@zh .

ex:TerminatedSoftwareEngineer rdf:type airo:AISubject ;
    rdfs:label "Terminated Software Engineer"@en ;
    rdfs:label "被解雇的软件工程师"@zh .

ex:JobLossAndOfficeAccessDenial rdf:type airo:Consequence ;
    rdfs:label "Job Loss and Office Access Denial Consequence"@en ;
    rdfs:label "失业和办公室访问被拒后果"@zh .