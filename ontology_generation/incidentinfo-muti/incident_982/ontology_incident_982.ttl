@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:DigitalScamRisk ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasPurpose ex:FraudulentPromotion ;
    airo:hasAISubject ex:PublicFigures ;
    airo:hasStakeholder ex:SingaporeGovernment ;
    airo:isAppliedWithinDomain ex:DigitalSecurity .

ex:DigitalScamRisk rdf:type airo:Risk ;
    rdfs:label "Digital Scam Risk"@en ;
    rdfs:label "数字诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:MaliciousContentCreation .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "财务损失后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:SingaporeGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Singapore Government"@en ;
    rdfs:label "新加坡政府"@zh .

ex:PublicFigures rdf:type airo:AISubject ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:SingaporeGovernment .

ex:FraudulentPromotion rdf:type airo:Purpose ;
    rdfs:label "Fraudulent Promotion Purpose"@en ;
    rdfs:label "欺诈性宣传目的"@zh .

ex:DigitalSecurity rdf:type airo:Domain ;
    rdfs:label "Digital Security Domain"@en ;
    rdfs:label "数字安全领域"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:MaliciousContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creation Risk Source"@en ;
    rdfs:label "恶意内容创建风险源"@zh .