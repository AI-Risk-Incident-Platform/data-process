@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DUOFraudCheckSystem rdf:type airo:AISystem ;
    rdfs:label "DUO Fraud Check System"@en ;
    rdfs:label "DUO欺诈检查系统"@zh ;
    airo:hasRisk ex:DiscriminatoryAlgorithmRisk ;
    airo:hasAISubject ex:StudentsSubjectToFraudChecks ;
    airo:hasStakeholder ex:DutchGovernment .

ex:DiscriminatoryAlgorithmRisk rdf:type airo:Risk ;
    rdfs:label "Discriminatory Algorithm Risk"@en ;
    rdfs:label "歧视性算法风险"@zh ;
    airo:hasConsequence ex:UnjustFinancialImpact ;
    airo:isRiskSourceFor ex:BiasedAlgorithmDesign .

ex:UnjustFinancialImpact rdf:type airo:Consequence ;
    rdfs:label "Unjust Financial Impact Consequence"@en ;
    rdfs:label "不公正财务影响后果"@zh ;
    airo:hasImpact ex:FinancialHardshipForStudents .

ex:StudentsSubjectToFraudChecks rdf:type airo:AISubject ;
    rdfs:label "Students Subject to Fraud Checks"@en ;
    rdfs:label "接受欺诈检查的学生"@zh .

ex:DutchGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Dutch Government"@en ;
    rdfs:label "荷兰政府"@zh .

ex:FinancialHardshipForStudents rdf:type airo:Impact ;
    rdfs:label "Financial Hardship for Students Impact"@en ;
    rdfs:label "学生经济困难的影响"@zh ;
    airo:hasImpactOnStakeholder ex:StudentsSubjectToFraudChecks .

ex:BiasedAlgorithmDesign rdf:type airo:RiskSource ;
    rdfs:label "Biased Algorithm Design Risk Source"@en ;
    rdfs:label "偏见算法设计的风险源"@zh .