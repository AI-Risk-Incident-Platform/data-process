@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GammaAIPresentationTool rdf:type airo:AISystem ;
    rdfs:label "Gamma AI Presentation Tool"@en ;
    rdfs:label "Gamma AI演示工具"@zh ;
    airo:hasRisk ex:PhishingAttackRisk ;
    airo:hasCapability ex:PresentationGenerationCapability ;
    airo:isAppliedWithinDomain ex:CybersecurityDomain .

ex:PhishingAttackRisk rdf:type airo:Risk ;
    rdfs:label "Phishing Attack Risk"@en ;
    rdfs:label "钓鱼攻击风险"@zh ;
    airo:hasConsequence ex:SpoofedMicrosoftSharePointLogins ;
    airo:isRiskSourceFor ex:MultiStagePhishingAttack ;
    airo:hasResidualRisk ex:ToolAbuseForPhishing .

ex:SpoofedMicrosoftSharePointLogins rdf:type airo:Consequence ;
    rdfs:label "Spoofed Microsoft SharePoint Logins"@en ;
    rdfs:label "伪造微软SharePoint登录"@zh ;
    airo:hasImpact ex:CredentialTheftImpact .

ex:PresentationGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Presentation Generation Capability"@en ;
    rdfs:label "演示生成能力"@zh .

ex:MultiStagePhishingAttack rdf:type airo:RiskSource ;
    rdfs:label "Multi-Stage Phishing Attack"@en ;
    rdfs:label "多阶段钓鱼攻击"@zh .

ex:CredentialTheftImpact rdf:type airo:Impact ;
    rdfs:label "Credential Theft Impact"@en ;
    rdfs:label "凭证盗窃影响"@zh .

ex:CybersecurityDomain rdf:type airo:Domain ;
    rdfs:label "Cybersecurity Domain"@en ;
    rdfs:label "网络安全领域"@zh .

ex:ToolAbuseForPhishing rdf:type airo:Misuse ;
    rdfs:label "Tool Abuse for Phishing"@en ;
    rdfs:label "工具滥用进行钓鱼"@zh .