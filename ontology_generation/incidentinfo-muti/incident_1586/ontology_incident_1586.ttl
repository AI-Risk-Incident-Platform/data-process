@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:ReputationalHarmRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:FTC .

ex:ReputationalHarmRisk rdf:type airo:Risk ;
    rdfs:label "Reputational Harm Risk"@en ;
    rdfs:label "声誉损害风险"@zh ;
    airo:hasConsequence ex:DataLeakConsequence ;
    airo:isRiskSourceFor ex:InaccurateOutputs .

ex:DataLeakConsequence rdf:type airo:Consequence ;
    rdfs:label "Data Leak Consequence"@en ;
    rdfs:label "数据泄露后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:FTC rdf:type airo:Stakeholder ;
    rdfs:label "Federal Trade Commission"@en ;
    rdfs:label "联邦贸易委员会"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:FTC .

ex:InaccurateOutputs rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate Outputs Risk Source"@en ;
    rdfs:label "不准确输出风险源"@zh .