@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DWPBenefitFraudAlgorithm rdf:type airo:AISystem ;
    rdfs:label "DWP Benefit Fraud Algorithm"@en ;
    rdfs:label "DWP福利欺诈算法"@zh ;
    airo:hasRisk ex:DiscriminatoryTargetingRisk ;
    airo:isDeployedBy ex:DepartmentForWorkAndPensions ;
    airo:hasStakeholder ex:DisabledBenefitClaimants .

ex:DiscriminatoryTargetingRisk rdf:type airo:Risk ;
    rdfs:label "Discriminatory Targeting Risk"@en ;
    rdfs:label "歧视性目标定位风险"@zh ;
    airo:hasConsequence ex:UnjustBenefitDenial .

ex:DisabledBenefitClaimants rdf:type airo:Stakeholder ;
    rdfs:label "Disabled Benefit Claimants"@en ;
    rdfs:label "残疾福利申请人"@zh .

ex:DepartmentForWorkAndPensions rdf:type airo:AIOperator ;
    rdfs:label "Department for Work and Pensions"@en ;
    rdfs:label "工作和养老金部门"@zh .

ex:UnjustBenefitDenial rdf:type airo:Impact ;
    rdfs:label "Unjust Benefit Denial Impact"@en ;
    rdfs:label "不公正福利拒绝的影响"@zh ;
    airo:hasImpactOnStakeholder ex:DisabledBenefitClaimants .