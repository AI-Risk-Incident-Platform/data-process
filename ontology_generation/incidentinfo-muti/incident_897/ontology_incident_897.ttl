ex:AIPhoneScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI Phone Scam System"@en ;
    rdfs:label "AI电话诈骗系统"@zh ;
    airo:hasRisk ex:IdentityImpersonationRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasStakeholder ex:ThailandPrimeMinister .

ex:IdentityImpersonationRisk rdf:type airo:Risk ;
    rdfs:label "Identity Impersonation Risk"@en ;
    rdfs:label "身份冒充风险"@zh ;
    airo:hasConsequence ex:PublicFigureSecurityThreat .

ex:ThailandPrimeMinister rdf:type airo:Stakeholder ;
    rdfs:label "Thailand Prime Minister"@en ;
    rdfs:label "泰国总理"@zh .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:PublicFigureSecurityThreat rdf:type airo:Impact ;
    rdfs:label "Public Figure Security Threat"@en ;
    rdfs:label "公众人物安全威胁"@zh ;
    airo:hasImpactOnStakeholder ex:ThailandPrimeMinister .