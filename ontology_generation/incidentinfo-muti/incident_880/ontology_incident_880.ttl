ex:MetaAICharacterProfiles rdf:type airo:AISystem ;
    rdfs:label "Meta AI Character Profiles"@en ;
    rdfs:label "Meta AI角色档案"@zh ;
    airo:hasRisk ex:RacismAccusationsRisk ;
    airo:hasStakeholder ex:MetaCompany ;
    airo:hasAISubject ex:BlackAICharacter .

ex:RacismAccusationsRisk rdf:type airo:Risk ;
    rdfs:label "Racism Accusations Risk"@en ;
    rdfs:label "种族主义指控风险"@zh ;
    airo:hasConsequence ex:PublicBacklash .

ex:MetaCompany rdf:type airo:Stakeholder ;
    rdfs:label "Meta Company"@en ;
    rdfs:label "Meta公司"@zh .

ex:BlackAICharacter rdf:type airo:AISubject ;
    rdfs:label "Black AI Character"@en ;
    rdfs:label "黑人AI角色"@zh .

ex:PublicBacklash rdf:type airo:Consequence ;
    rdfs:label "Public Backlash"@en ;
    rdfs:label "公众强烈反对"@zh .