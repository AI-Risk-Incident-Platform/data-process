@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:ChinaStateTV ;
    airo:isDeployedBy ex:KohlerAndBMW .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasImpact ex:ConsumerPrivacyConcerns .

ex:ChinaStateTV rdf:type airo:Stakeholder ;
    rdfs:label "China State TV"@en ;
    rdfs:label "中国中央电视台"@zh .

ex:KohlerAndBMW rdf:type airo:AIOperator ;
    rdfs:label "Kohler and BMW"@en ;
    rdfs:label "科勒和宝马"@zh .

ex:ConsumerPrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "Consumer Privacy Concerns"@en ;
    rdfs:label "消费者隐私担忧"@zh ;
    airo:hasImpactOnStakeholder ex:ChinaStateTV .