@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:usesTechnique ex:DeepLearningBasedSynthesis ;
    airo:isAppliedWithinDomain ex:PoliticalCampaigns ;
    airo:hasStakeholder ex:PoliticalParties .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PoliticalCampaigns rdf:type airo:Domain ;
    rdfs:label "Political Campaigns"@en ;
    rdfs:label "政治竞选活动"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:PoliticalParties .

ex:DeepLearningBasedSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Synthesis"@en ;
    rdfs:label "基于深度学习的合成技术"@zh .

ex:PoliticalParties rdf:type airo:Stakeholder ;
    rdfs:label "Political Parties"@en ;
    rdfs:label "政党"@zh .