@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoOfYouTubeCEO rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video of YouTube CEO"@en ;
    rdfs:label "YouTube CEO的深度伪造视频"@zh ;
    airo:hasRisk ex:PhishingAttackRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:YouTubeCreators .

ex:PhishingAttackRisk rdf:type airo:Risk ;
    rdfs:label "Phishing Attack Risk"@en ;
    rdfs:label "钓鱼攻击风险"@zh ;
    airo:hasConsequence ex:CredentialTheft .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:YouTubeCreators rdf:type airo:Stakeholder ;
    rdfs:label "YouTube Creators"@en ;
    rdfs:label "YouTube创作者"@zh .

ex:CredentialTheft rdf:type airo:Consequence ;
    rdfs:label "Credential Theft Consequence"@en ;
    rdfs:label "凭证盗窃后果"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss Impact"@en ;
    rdfs:label "财务损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:YouTubeCreators .