@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberEatsAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Uber Eats Algorithm"@en ;
    rdfs:label "Uber Eats算法"@zh ;
    airo:hasRisk ex:WageReductionRisk ;
    airo:hasAISubject ex:CouriersWithoutCars ;
    airo:isDevelopedBy ex:UberTechnologies .

ex:WageReductionRisk rdf:type airo:Risk ;
    rdfs:label "Wage Reduction Risk"@en ;
    rdfs:label "工资减少风险"@zh ;
    airo:hasConsequence ex:DwindlingWages .

ex:CouriersWithoutCars rdf:type airo:AISubject ;
    rdfs:label "Couriers Without Cars"@en ;
    rdfs:label "无车配送员"@zh .

ex:DwindlingWages rdf:type airo:Consequence ;
    rdfs:label "Dwindling Wages Consequence"@en ;
    rdfs:label "工资减少后果"@zh .

ex:UberTechnologies rdf:type airo:AIOperator ;
    rdfs:label "Uber Technologies"@en ;
    rdfs:label "优步科技"@zh .