@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AICameraSystem rdf:type airo:AISystem ;
    rdfs:label "AI Camera System"@en ;
    rdfs:label "AI摄像头系统"@zh ;
    airo:hasRisk ex:InaccurateSpeedDetectionRisk ;
    airo:isDeployedBy ex:Keltron ;
    airo:hasStakeholder ex:KeralaGovernment ;
    airo:isAppliedWithinDomain ex:TrafficMonitoring .

ex:InaccurateSpeedDetectionRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Speed Detection Risk"@en ;
    rdfs:label "速度检测不准确风险"@zh ;
    airo:hasConsequence ex:UnjustFines ;
    airo:isRiskSourceFor ex:FaultyAlgorithm .

ex:UnjustFines rdf:type airo:Consequence ;
    rdfs:label "Unjust Fines Consequence"@en ;
    rdfs:label "不公正罚款后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:KeralaGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Kerala Government"@en ;
    rdfs:label "喀拉拉邦政府"@zh .

ex:Keltron rdf:type airo:AIOperator ;
    rdfs:label "Keltron"@en ;
    rdfs:label "Keltron公司"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任丧失影响"@zh ;
    airo:hasImpactOnStakeholder ex:KeralaGovernment .

ex:TrafficMonitoring rdf:type airo:Domain ;
    rdfs:label "Traffic Monitoring Domain"@en ;
    rdfs:label "交通监控领域"@zh .

ex:FaultyAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Faulty Algorithm Risk Source"@en ;
    rdfs:label "故障算法风险源"@zh .