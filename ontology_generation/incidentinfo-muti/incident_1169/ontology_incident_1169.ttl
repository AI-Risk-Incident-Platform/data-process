@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookAdTargetingSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Ad Targeting System"@en ;
    rdfs:label "Facebook广告定向系统"@zh ;
    airo:hasRisk ex:DiscriminationInAdTargeting ;
    airo:hasStakeholder ex:Advertisers ;
    airo:hasAISubject ex:TargetedUsers .

ex:DiscriminationInAdTargeting rdf:type airo:Risk ;
    rdfs:label "Discrimination in Ad Targeting Risk"@en ;
    rdfs:label "广告定向中的歧视风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInTargeting ;
    airo:hasImpact ex:UnfairTreatmentOfUsers .

ex:Advertisers rdf:type airo:Stakeholder ;
    rdfs:label "Advertisers"@en ;
    rdfs:label "广告商"@zh .

ex:TargetedUsers rdf:type airo:AISubject ;
    rdfs:label "Targeted Users"@en ;
    rdfs:label "被定向用户"@zh .

ex:AlgorithmicBiasInTargeting rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Targeting Risk Source"@en ;
    rdfs:label "定向算法偏见风险源"@zh .

ex:UnfairTreatmentOfUsers rdf:type airo:Impact ;
    rdfs:label "Unfair Treatment of Users Impact"@en ;
    rdfs:label "用户受到不公平待遇的影响"@zh ;
    airo:hasImpactOnStakeholder ex:TargetedUsers .