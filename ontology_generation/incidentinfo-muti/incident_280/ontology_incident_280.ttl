@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TikTokRecommendationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "TikTok Recommendation Algorithm"@en ;
    rdfs:label "TikTok推荐算法"@zh ;
    airo:hasRisk ex:HarmfulContentPromotionRisk ;
    airo:hasCapability ex:ContentPersonalizationAlgorithm ;
    airo:isDevelopedBy ex:ByteDance ;
    airo:hasPurpose ex:UserEngagementMaximization ;
    airo:hasStakeholder ex:TikTokUsersWithMentalHealthIssues ;
    airo:hasAISubject ex:VulnerableTeenagers ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatform ;
    airo:usesTechnique ex:DeepLearningBasedRecommendation .

ex:HarmfulContentPromotionRisk rdf:type airo:Risk ;
    rdfs:label "Harmful Content Promotion Risk"@en ;
    rdfs:label "有害内容推广风险"@zh ;
    airo:hasConsequence ex:MentalHealthDeterioration ;
    airo:isRiskSourceFor ex:InadequateContentModeration .

ex:MentalHealthDeterioration rdf:type airo:Consequence ;
    rdfs:label "Mental Health Deterioration Consequence"@en ;
    rdfs:label "心理健康恶化后果"@zh ;
    airo:hasImpact ex:IncreasedSelfHarmBehavior .

ex:ContentPersonalizationAlgorithm rdf:type airo:AICapability ;
    rdfs:label "Content Personalization Algorithm Capability"@en ;
    rdfs:label "内容个性化算法能力"@zh .

ex:ByteDance rdf:type airo:AIOperator ;
    rdfs:label "ByteDance"@en ;
    rdfs:label "字节跳动"@zh .

ex:TikTokUsersWithMentalHealthIssues rdf:type airo:Stakeholder ;
    rdfs:label "TikTok Users with Mental Health Issues Stakeholder"@en ;
    rdfs:label "有心理健康问题的TikTok用户"@zh .

ex:VulnerableTeenagers rdf:type airo:AISubject ;
    rdfs:label "Vulnerable Teenagers Subject"@en ;
    rdfs:label "易受影响的青少年"@zh .

ex:IncreasedSelfHarmBehavior rdf:type airo:Impact ;
    rdfs:label "Increased Self-Harm Behavior Impact"@en ;
    rdfs:label "自残行为增加的影响"@zh ;
    airo:hasImpactOnStakeholder ex:TikTokUsersWithMentalHealthIssues .

ex:UserEngagementMaximization rdf:type airo:Purpose ;
    rdfs:label "User Engagement Maximization Purpose"@en ;
    rdfs:label "用户参与度最大化的目的"@zh .

ex:SocialMediaPlatform rdf:type airo:Domain ;
    rdfs:label "Social Media Platform Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:DeepLearningBasedRecommendation rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Recommendation Technique"@en ;
    rdfs:label "基于深度学习的推荐技术"@zh .

ex:InadequateContentModeration rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Content Moderation Risk Source"@en ;
    rdfs:label "内容审核不足的风险源"@zh ;
    airo:exploitsVulnerability ex:AlgorithmicBiasTowardsExtremeContent ;
    airo:isThreatenedBy ex:ProliferationOfHarmfulContent ;
    airo:isHazardousTo ex:UnmoderatedUserGeneratedContent .

ex:AlgorithmicBiasTowardsExtremeContent rdf:type airo:Vulnerability ;
    rdfs:label "Algorithmic Bias Towards Extreme Content Vulnerability"@en ;
    rdfs:label "算法偏向极端内容的脆弱性"@zh .

ex:ProliferationOfHarmfulContent rdf:type airo:Threat ;
    rdfs:label "Proliferation of Harmful Content Threat"@en ;
    rdfs:label "有害内容扩散的威胁"@zh .

ex:UnmoderatedUserGeneratedContent rdf:type airo:Hazard ;
    rdfs:label "Unmoderated User Generated Content Hazard"@en ;
    rdfs:label "未经审核的用户生成内容危害"@zh .

ex:UserEngagementMaximization rdf:type airo:Purpose ;
    rdfs:label "User Engagement Maximization Purpose"@en ;
    rdfs:label "用户参与度最大化的目的"@zh .

ex:AlgorithmicAmplificationOfHarmfulBehavior rdf:type airo:RiskConcept ;
    rdfs:label "Algorithmic Amplification of Harmful Behavior Risk Concept"@en ;
    rdfs:label "有害行为的算法放大"@zh .