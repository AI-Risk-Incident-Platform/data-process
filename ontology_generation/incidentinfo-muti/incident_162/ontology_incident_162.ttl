@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonDeliveryDriverManagementSystem rdf:type airo:AISystem ;
    rdfs:label "Amazon Delivery Driver Management System"@en ;
    rdfs:label "亚马逊配送司机管理系统"@zh ;
    airo:hasRisk ex:DriverMicromanagementRisk ;
    airo:hasAISubject ex:DeliveryDrivers ;
    airo:hasPurpose ex:DeliveryDriverManagement ;
    airo:isAppliedWithinDomain ex:LogisticsAndDelivery ;
    airo:isDeployedBy ex:Amazon .

ex:DriverMicromanagementRisk rdf:type airo:Risk ;
    rdfs:label "Driver Micromanagement Risk"@en ;
    rdfs:label "司机微观管理风险"@zh ;
    airo:hasConsequence ex:VehicleCrash .

ex:VehicleCrash rdf:type airo:Consequence ;
    rdfs:label "Vehicle Crash Consequence"@en ;
    rdfs:label "车辆碰撞后果"@zh ;
    airo:hasImpact ex:RoadSafetyCompromise .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:DeliveryDrivers rdf:type airo:AISubject ;
    rdfs:label "Delivery Drivers"@en ;
    rdfs:label "配送司机"@zh .

ex:DeliveryDriverManagement rdf:type airo:Purpose ;
    rdfs:label "Delivery Driver Management Purpose"@en ;
    rdfs:label "配送司机管理目的"@zh .

ex:LogisticsAndDelivery rdf:type airo:Domain ;
    rdfs:label "Logistics and Delivery Domain"@en ;
    rdfs:label "物流配送领域"@zh .

ex:RoadSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Road Safety Compromise Impact"@en ;
    rdfs:label "道路安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:DeliveryDrivers .