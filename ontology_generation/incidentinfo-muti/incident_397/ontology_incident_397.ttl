@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GmailEmailFilteringSystem rdf:type airo:AISystem ;
    rdfs:label "Gmail Email Filtering System"@en ;
    rdfs:label "Gmail邮件过滤系统"@zh ;
    airo:hasRisk ex:PoliticalEmailBiasRisk ;
    airo:hasStakeholder ex:PoliticalCampaigns ;
    airo:isDevelopedBy ex:Google ;
    airo:hasPurpose ex:EmailClassification ;
    airo:isAppliedWithinDomain ex:EmailServices ;
    airo:usesTechnique ex:MachineLearningFiltering .

ex:PoliticalEmailBiasRisk rdf:type airo:Risk ;
    rdfs:label "Political Email Bias Risk"@en ;
    rdfs:label "政治邮件偏见风险"@zh ;
    airo:hasConsequence ex:UnequalEmailDelivery ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:PoliticalCampaigns rdf:type airo:Stakeholder ;
    rdfs:label "Political Campaigns"@en ;
    rdfs:label "政治竞选活动"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:EmailClassification rdf:type airo:Purpose ;
    rdfs:label "Email Classification"@en ;
    rdfs:label "邮件分类"@zh .

ex:EmailServices rdf:type airo:Domain ;
    rdfs:label "Email Services"@en ;
    rdfs:label "电子邮件服务"@zh .

ex:MachineLearningFiltering rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning Filtering"@en ;
    rdfs:label "机器学习过滤"@zh .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias"@en ;
    rdfs:label "算法偏见"@zh .

ex:UnequalEmailDelivery rdf:type airo:Consequence ;
    rdfs:label "Unequal Email Delivery"@en ;
    rdfs:label "邮件投递不平等"@zh ;
    airo:hasImpact ex:PoliticalInfluence .

ex:PoliticalInfluence rdf:type airo:Impact ;
    rdfs:label "Political Influence"@en ;
    rdfs:label "政治影响"@zh ;
    airo:hasImpactOnStakeholder ex:PoliticalCampaigns .