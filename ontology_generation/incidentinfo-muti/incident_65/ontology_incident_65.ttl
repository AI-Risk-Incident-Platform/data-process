@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:StoreCustomerServiceRobot rdf:type airo:AISystem ;
    rdfs:label "Store Customer Service Robot"@en ;
    rdfs:label "商店客户服务机器人"@zh ;
    airo:hasRisk ex:CustomerFearRisk ;
    airo:isDeployedBy ex:StoreManagement ;
    airo:hasStakeholder ex:StoreCustomers ;
    airo:hasPurpose ex:CustomerAssistance ;
    airo:isAppliedWithinDomain ex:RetailCustomerService .

ex:CustomerFearRisk rdf:type airo:Risk ;
    rdfs:label "Customer Fear Risk"@en ;
    rdfs:label "顾客恐惧风险"@zh ;
    airo:hasImpact ex:NegativeCustomerExperience .

ex:StoreCustomers rdf:type airo:Stakeholder ;
    rdfs:label "Store Customers"@en ;
    rdfs:label "商店顾客"@zh .

ex:StoreManagement rdf:type airo:AIOperator ;
    rdfs:label "Store Management"@en ;
    rdfs:label "商店管理层"@zh .

ex:NegativeCustomerExperience rdf:type airo:Impact ;
    rdfs:label "Negative Customer Experience"@en ;
    rdfs:label "负面顾客体验"@zh ;
    airo:hasImpactOnStakeholder ex:StoreCustomers .

ex:RetailCustomerService rdf:type airo:Domain ;
    rdfs:label "Retail Customer Service"@en ;
    rdfs:label "零售客户服务"@zh .

ex:CustomerAssistance rdf:type airo:Purpose ;
    rdfs:label "Customer Assistance"@en ;
    rdfs:label "顾客协助"@zh .