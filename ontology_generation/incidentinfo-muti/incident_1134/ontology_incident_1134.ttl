@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeacherEvaluationSystem rdf:type airo:AISystem ;
    rdfs:label "Teacher Evaluation System"@en ;
    rdfs:label "教师评估系统"@zh ;
    airo:hasRisk ex:LackOfTransparencyRisk ;
    airo:hasStakeholder ex:HoustonTeachers ;
    airo:isDeployedBy ex:HoustonFederationOfTeachers .

ex:LackOfTransparencyRisk rdf:type airo:Risk ;
    rdfs:label "Lack of Transparency Risk"@en ;
    rdfs:label "缺乏透明度风险"@zh ;
    airo:hasConsequence ex:LegalChallenge .

ex:HoustonTeachers rdf:type airo:Stakeholder ;
    rdfs:label "Houston Teachers"@en ;
    rdfs:label "休斯顿教师"@zh .

ex:HoustonFederationOfTeachers rdf:type airo:AIOperator ;
    rdfs:label "Houston Federation of Teachers"@en ;
    rdfs:label "休斯顿教师联合会"@zh .

ex:LegalChallenge rdf:type airo:Consequence ;
    rdfs:label "Legal Challenge Consequence"@en ;
    rdfs:label "法律挑战后果"@zh ;
    airo:hasImpactOnStakeholder ex:HoustonTeachers .