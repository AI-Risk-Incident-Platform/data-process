@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonAlexa rdf:type airo:AISystem ;
    rdfs:label "Amazon Alexa"@en ;
    rdfs:label "亚马逊Alexa"@zh ;
    airo:hasRisk ex:ChildrenMisuseRisk ;
    airo:hasStakeholder ex:Children ;
    airo:isAppliedWithinDomain ex:SmartHome .

ex:ChildrenMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Children Misuse Risk"@en ;
    rdfs:label "儿童误用风险"@zh ;
    airo:hasConsequence ex:InappropriateInteractionByChildren .

ex:Children rdf:type airo:Stakeholder ;
    rdfs:label "Children"@en ;
    rdfs:label "儿童"@zh .

ex:InappropriateInteractionByChildren rdf:type airo:Consequence ;
    rdfs:label "Inappropriate Interaction by Children"@en ;
    rdfs:label "儿童不当互动"@zh ;
    airo:hasImpactOnStakeholder ex:Children .

ex:SmartHome rdf:type airo:Domain ;
    rdfs:label "Smart Home"@en ;
    rdfs:label "智能家居"@zh .