@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GPT3LanguageModel rdf:type airo:AISystem ;
    rdfs:label "GPT-3 Language Model"@en ;
    rdfs:label "GPT-3语言模型"@zh ;
    airo:hasRisk ex:AntiMuslimBiasRisk ;
    airo:hasAICapability ex:NaturalLanguageGeneration ;
    airo:usesTechnique ex:TransformerBasedModel ;
    airo:hasStakeholder ex:MuslimCommunity .

ex:AntiMuslimBiasRisk rdf:type airo:Risk ;
    rdfs:label "Anti-Muslim Bias Risk"@en ;
    rdfs:label "反穆斯林偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:DiscriminatoryContentGeneration .

ex:NaturalLanguageGeneration rdf:type airo:AICapability ;
    rdfs:label "Natural Language Generation Capability"@en ;
    rdfs:label "自然语言生成能力"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "偏见训练数据风险源"@zh .

ex:DiscriminatoryContentGeneration rdf:type airo:Impact ;
    rdfs:label "Discriminatory Content Generation Impact"@en ;
    rdfs:label "歧视性内容生成影响"@zh ;
    airo:hasImpactOnStakeholder ex:MuslimCommunity .

ex:MuslimCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Muslim Community Stakeholder"@en ;
    rdfs:label "穆斯林社区利益相关者"@zh .

ex:TransformerBasedModel rdf:type airo:AITechnique ;
    rdfs:label "Transformer-Based Model Technique"@en ;
    rdfs:label "基于Transformer的模型技术"@zh .