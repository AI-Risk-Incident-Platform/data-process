@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AnthropicAI rdf:type airo:AISystem ;
    rdfs:label "Anthropic AI"@en ;
    rdfs:label "Anthropic AI"@zh ;
    airo:hasRisk ex:DataScrapingRisk ;
    airo:hasStakeholder ex:iFixit .

ex:DataScrapingRisk rdf:type airo:Risk ;
    rdfs:label "Data Scraping Risk"@en ;
    rdfs:label "数据抓取风险"@zh ;
    airo:hasConsequence ex:WebsiteOverload ;
    airo:isRiskSourceFor ex:WebScraping .

ex:WebScraping rdf:type airo:RiskSource ;
    rdfs:label "Web Scraping"@en ;
    rdfs:label "网络抓取"@zh .

ex:iFixit rdf:type airo:Stakeholder ;
    rdfs:label "iFixit"@en ;
    rdfs:label "iFixit"@zh .

ex:WebsiteOverload rdf:type airo:Consequence ;
    rdfs:label "Website Overload"@en ;
    rdfs:label "网站过载"@zh ;
    airo:hasImpact ex:ServiceDisruption .

ex:ServiceDisruption rdf:type airo:Impact ;
    rdfs:label "Service Disruption"@en ;
    rdfs:label "服务中断"@zh ;
    airo:hasImpactOnStakeholder ex:iFixit .