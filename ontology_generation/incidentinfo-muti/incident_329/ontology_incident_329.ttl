@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleSearchAlgorithm rdf:type airo:AISystem ;
    rdfs:label "谷歌搜索算法"@zh ;
    rdfs:label "Google Search Algorithm"@en ;
    airo:hasRisk ex:AlgorithmicBiasInSearchResults ;
    airo:hasAISubject ex:SearchEngineUsers ;
    airo:hasStakeholder ex:GoogleCompany ;
    airo:hasAICapability ex:ContentRankingAlgorithm .

ex:AlgorithmicBiasInSearchResults rdf:type airo:Risk ;
    rdfs:label "搜索结果中的算法偏见风险"@zh ;
    rdfs:label "Algorithmic Bias in Search Results Risk"@en ;
    airo:hasConsequence ex:RacialStereotypingInSearchResults ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:RacialStereotypingInSearchResults rdf:type airo:Consequence ;
    rdfs:label "搜索结果中的种族刻板印象后果"@zh ;
    rdfs:label "Racial Stereotyping in Search Results Consequence"@en ;
    airo:hasImpact ex:SocialDiscriminationImpact .

ex:SocialDiscriminationImpact rdf:type airo:Impact ;
    rdfs:label "社会歧视影响"@zh ;
    rdfs:label "Social Discrimination Impact"@en ;
    airo:hasImpactOnStakeholder ex:SearchEngineUsers .

ex:ContentRankingAlgorithm rdf:type airo:AICapability ;
    rdfs:label "内容排名算法能力"@zh ;
    rdfs:label "Content Ranking Algorithm Capability"@en .

ex:GoogleCompany rdf:type airo:Stakeholder ;
    rdfs:label "谷歌公司"@zh ;
    rdfs:label "Google Company"@en .

ex:SearchEngineUsers rdf:type airo:AISubject ;
    rdfs:label "搜索引擎用户"@zh ;
    rdfs:label "Search Engine Users"@en .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "有偏见的训练数据风险源"@zh ;
    rdfs:label "Biased Training Data Risk Source"@en .