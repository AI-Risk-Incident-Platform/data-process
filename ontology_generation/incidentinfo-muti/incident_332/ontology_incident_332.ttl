@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UKVisaStreamingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "UK Visa Streaming Algorithm"@en ;
    rdfs:label "英国签证流算法"@zh ;
    airo:hasRisk ex:BiasInVisaGrantingSystem ;
    airo:isDeployedBy ex:UKHomeOffice ;
    airo:hasStakeholder ex:RightsGroups ;
    airo:hasPurpose ex:StreamVisaApplications ;
    airo:isAppliedWithinDomain ex:ImmigrationServices .

ex:BiasInVisaGrantingSystem rdf:type airo:Risk ;
    rdfs:label "Bias in Visa Granting System Risk"@en ;
    rdfs:label "签证发放系统中的偏见风险"@zh ;
    airo:hasConsequence ex:UnfairTreatmentOfApplicants ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:RightsGroups rdf:type airo:Stakeholder ;
    rdfs:label "Rights Groups"@en ;
    rdfs:label "权利组织"@zh .

ex:UKHomeOffice rdf:type airo:AIOperator ;
    rdfs:label "UK Home Office"@en ;
    rdfs:label "英国内政部"@zh .

ex:DiscriminatoryVisaDecisions rdf:type airo:Impact ;
    rdfs:label "Discriminatory Visa Decisions Impact"@en ;
    rdfs:label "歧视性签证决策的影响"@zh ;
    airo:hasImpactOnStakeholder ex:RightsGroups .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .

ex:UnfairTreatmentOfApplicants rdf:type airo:Consequence ;
    rdfs:label "Unfair Treatment of Applicants Consequence"@en ;
    rdfs:label "申请人受到不公平待遇的后果"@zh ;
    airo:hasImpact ex:DiscriminatoryVisaDecisions .

ex:StreamVisaApplications rdf:type airo:Purpose ;
    rdfs:label "Stream Visa Applications Purpose"@en ;
    rdfs:label "分流签证申请的目的"@zh .

ex:ImmigrationServices rdf:type airo:Domain ;
    rdfs:label "Immigration Services Domain"@en ;
    rdfs:label "移民服务领域"@zh .