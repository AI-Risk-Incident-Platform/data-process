@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MicrosoftAI rdf:type airo:AISystem ;
    rdfs:label "Microsoft AI Tools"@en ;
    rdfs:label "微软AI工具"@zh ;
    airo:hasRisk ex:CyberAttackRisk ;
    airo:isDevelopedBy ex:Microsoft ;
    airo:hasStakeholder ex:HackersFromChinaRussiaIran .

ex:CyberAttackRisk rdf:type airo:Risk ;
    rdfs:label "Cyber Attack Risk"@en ;
    rdfs:label "网络攻击风险"@zh ;
    airo:isRiskSourceFor ex:HackerMisuseOfAI .

ex:HackersFromChinaRussiaIran rdf:type airo:Stakeholder ;
    rdfs:label "Hackers from China, Russia and Iran"@en ;
    rdfs:label "来自中国、俄罗斯和伊朗的黑客"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:HackerMisuseOfAI rdf:type airo:RiskSource ;
    rdfs:label "Hacker Misuse of AI"@en ;
    rdfs:label "黑客滥用AI"@zh .