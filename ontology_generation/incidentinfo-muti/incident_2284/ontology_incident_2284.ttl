ex:DeepNudeApp rdf:type airo:AISystem ;
    rdfs:label "DeepNude App"@en ;
    rdfs:label "DeepNude应用"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:DeepfakeImageGeneration ;
    airo:hasPurpose ex:NonConsensualNudityGeneration ;
    airo:hasStakeholder ex:WomenSubjects ;
    airo:isAppliedWithinDomain ex:ImageProcessingApplications ;
    airo:hasRisk ex:NonConsensualImageCreationMisuse .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
