ex:NonConsensualNudityGeneration rdf:type airo:Purpose ;
    rdfs:label "Non-Consensual Nudity Generation Purpose"@en ;
    rdfs:label "非自愿裸体生成目的"@zh .

ex:MaliciousUseOfAI rdf:type airo:RiskSource ;
