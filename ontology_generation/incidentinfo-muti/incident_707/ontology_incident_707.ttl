@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:LegalMisinformationRisk ;
    airo:hasAICapability ex:TextGeneration ;
    airo:hasPurpose ex:LegalResearchAssistance ;
    airo:hasStakeholder ex:LegalProfessionals ;
    airo:isAppliedWithinDomain ex:LegalServices .

ex:LegalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Misinformation Risk"@en ;
    rdfs:label "法律错误信息风险"@zh ;
    airo:hasConsequence ex:FalseLegalCitations ;
    airo:isRiskSourceFor ex:UnverifiedAIOutputs .

ex:FalseLegalCitations rdf:type airo:Consequence ;
    rdfs:label "False Legal Citations Consequence"@en ;
    rdfs:label "虚假法律引用后果"@zh ;
    airo:hasImpact ex:JudicialSystemIntegrityCompromise .

ex:TextGeneration rdf:type airo:AICapability ;
    rdfs:label "Text Generation Capability"@en ;
    rdfs:label "文本生成能力"@zh .

ex:LegalProfessionals rdf:type airo:Stakeholder ;
    rdfs:label "Legal Professionals"@en ;
    rdfs:label "法律专业人士"@zh .

ex:JudicialSystemIntegrityCompromise rdf:type airo:Impact ;
    rdfs:label "Judicial System Integrity Compromise Impact"@en ;
    rdfs:label "司法系统完整性受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:LegalProfessionals .

ex:LegalResearchAssistance rdf:type airo:Purpose ;
    rdfs:label "Legal Research Assistance Purpose"@en ;
    rdfs:label "法律研究辅助目的"@zh .

ex:LegalServices rdf:type airo:Domain ;
    rdfs:label "Legal Services Domain"@en ;
    rdfs:label "法律服务领域"@zh .

ex:UnverifiedAIOutputs rdf:type airo:RiskSource ;
    rdfs:label "Unverified AI Outputs Risk Source"@en ;
    rdfs:label "未经验证的AI输出风险源"@zh ;
    airo:exploitsVulnerability ex:OverrelianceOnAIWithoutVerification .

ex:OverrelianceOnAIWithoutVerification rdf:type airo:Vulnerability ;
    rdfs:label "Overreliance on AI Without Verification Vulnerability"@en ;
    rdfs:label "未经核实的AI过度依赖脆弱性"@zh .