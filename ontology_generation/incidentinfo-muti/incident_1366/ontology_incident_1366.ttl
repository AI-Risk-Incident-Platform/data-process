@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AITrainingDataset rdf:type airo:AISystem ;
    rdfs:label "AI Training Dataset"@en ;
    rdfs:label "AI训练数据集"@zh ;
    airo:hasRisk ex:DeepfakeRisk ;
    airo:hasAISubject ex:AustralianChildren ;
    airo:hasStakeholder ex:HumanRightsGroup .

ex:DeepfakeRisk rdf:type airo:Risk ;
    rdfs:label "Deepfake Risk"@en ;
    rdfs:label "深度伪造风险"@zh ;
    airo:hasConsequence ex:PotentialDeepfakeCreation ;
    airo:isRiskSourceFor ex:UnauthorizedDataCollection .

ex:AustralianChildren rdf:type airo:AISubject ;
    rdfs:label "Australian Children"@en ;
    rdfs:label "澳大利亚儿童"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation"@en ;
    rdfs:label "隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:HumanRightsGroup .

ex:HumanRightsGroup rdf:type airo:Stakeholder ;
    rdfs:label "Human Rights Group"@en ;
    rdfs:label "人权组织"@zh .

ex:UnauthorizedDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Data Collection"@en ;
    rdfs:label "未经授权的数据收集"@zh .

ex:PotentialDeepfakeCreation rdf:type airo:Consequence ;
    rdfs:label "Potential Deepfake Creation"@en ;
    rdfs:label "潜在的深度伪造内容制作"@zh ;
    airo:hasImpact ex:PrivacyViolation .