@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIModificationTool rdf:type airo:AISystem ;
    rdfs:label "AI Modification Tool"@en ;
    rdfs:label "AI修改工具"@zh ;
    airo:hasRisk ex:RacialBiasInAI ;
    airo:hasStakeholder ex:AIScientist ;
    airo:isAppliedWithinDomain ex:EntertainmentIndustry .

ex:RacialBiasInAI rdf:type airo:Risk ;
    rdfs:label "Racial Bias in AI Risk"@en ;
    rdfs:label "AI中的种族偏见风险"@zh ;
    airo:hasImpact ex:CulturalMisrepresentation .

ex:AIScientist rdf:type airo:Stakeholder ;
    rdfs:label "AI Scientist"@en ;
    rdfs:label "AI科学家"@zh .

ex:CulturalMisrepresentation rdf:type airo:Impact ;
    rdfs:label "Cultural Misrepresentation Impact"@en ;
    rdfs:label "文化误传的影响"@zh ;
    airo:hasImpactOnStakeholder ex:AIScientist .

ex:EntertainmentIndustry rdf:type airo:Domain ;
    rdfs:label "Entertainment Industry Domain"@en ;
    rdfs:label "娱乐产业领域"@zh .