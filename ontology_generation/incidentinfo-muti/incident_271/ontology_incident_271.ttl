@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AppStoreRankingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "App Store Ranking Algorithm"@en ;
    rdfs:label "App Store排名算法"@zh ;
    airo:hasRisk ex:AppRankingFluctuationRisk ;
    airo:isDeployedBy ex:AppleInc ;
    airo:isAppliedWithinDomain ex:MobileApplicationMarket .

ex:AppRankingFluctuationRisk rdf:type airo:Risk ;
    rdfs:label "App Ranking Fluctuation Risk"@en ;
    rdfs:label "应用排名波动风险"@zh ;
    airo:hasImpact ex:ChineseAppsMarketPositionImpact .

ex:AppleInc rdf:type airo:AIOperator ;
    rdfs:label "Apple Inc."@en ;
    rdfs:label "苹果公司"@zh .

ex:ChineseAppsMarketPositionImpact rdf:type airo:Impact ;
    rdfs:label "Chinese Apps Market Position Impact"@en ;
    rdfs:label "中国应用市场地位影响"@zh .

ex:MobileApplicationMarket rdf:type airo:Domain ;
    rdfs:label "Mobile Application Market Domain"@en ;
    rdfs:label "移动应用市场领域"@zh .