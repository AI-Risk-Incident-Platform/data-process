@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIvoiceCloningSystem rdf:type airo:AISystem ;
    rdfs:label "AI Voice Cloning System"@en ;
    rdfs:label "AI语音克隆系统"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:hasAICapability ex:VoiceImpersonation ;
    airo:hasAISubject ex:PublicFigures ;
    airo:hasPurpose ex:DeceptiveFinancialGain ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:usesTechnique ex:DeepfakeVoiceSynthesis ;
    airo:hasVulnerability ex:PublicFigureVoiceAvailability ;
    airo:hasStakeholder ex:ScamVictims .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:MaliciousUseOfAIVoiceCloning .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:TrustErosion .

ex:VoiceImpersonation rdf:type airo:AICapability ;
    rdfs:label "Voice Impersonation Capability"@en ;
    rdfs:label "语音模仿能力"@zh .

ex:ScamVictims rdf:type airo:Stakeholder ;
    rdfs:label "Scam Victims"@en ;
    rdfs:label "诈骗受害者"@zh .

ex:PublicFigures rdf:type airo:AISubject ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:TrustErosion rdf:type airo:Impact ;
    rdfs:label "Trust Erosion Impact"@en ;
    rdfs:label "信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:ScamVictims .

ex:DeceptiveFinancialGain rdf:type airo:Purpose ;
    rdfs:label "Deceptive Financial Gain Purpose"@en ;
    rdfs:label "欺骗性财务收益目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:DeepfakeVoiceSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Voice Synthesis Technique"@en ;
    rdfs:label "深度伪造语音合成技术"@zh .

ex:PublicFigureVoiceAvailability rdf:type airo:Vulnerability ;
    rdfs:label "Public Figure Voice Availability Vulnerability"@en ;
    rdfs:label "公众人物语音可获取性漏洞"@zh .

ex:AIpoweredSocialEngineering rdf:type airo:Threat ;
    rdfs:label "AI-powered Social Engineering Threat"@en ;
    rdfs:label "AI驱动的社会工程威胁"@zh ;
    airo:isRiskSourceFor ex:FinancialScamRisk .

ex:UnregulatedVoiceCloningTechnology rdf:type airo:Hazard ;
    rdfs:label "Unregulated Voice Cloning Technology Hazard"@en ;
    rdfs:label "未受监管的语音克隆技术危害"@zh ;
    airo:isRiskSourceFor ex:FinancialScamRisk .

ex:MaliciousUseOfAIVoiceCloning rdf:type airo:RiskSource ;
    rdfs:label "Malicious Use of AI Voice Cloning Risk Source"@en ;
    rdfs:label "恶意使用AI语音克隆的风险源"@zh .

ex:EmergingAIFraudTechniques rdf:type airo:RiskConcept ;
    rdfs:label "Emerging AI Fraud Techniques Risk Concept"@en ;
    rdfs:label "新兴AI欺诈技术风险概念"@zh .