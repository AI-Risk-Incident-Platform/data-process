@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NYPDFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "NYPD Facial Recognition System"@en ;
    rdfs:label "纽约警察局面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:NYPD ;
    airo:isAppliedWithinDomain ex:LawEnforcement ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:BlackLivesMatterActivist .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasImpact ex:CivilLibertiesViolation .

ex:BlackLivesMatterActivist rdf:type airo:Stakeholder ;
    rdfs:label "Black Lives Matter Activist"@en ;
    rdfs:label "黑人的命也是命活动家"@zh .

ex:NYPD rdf:type airo:AIOperator ;
    rdfs:label "NYPD"@en ;
    rdfs:label "纽约警察局"@zh .

ex:CivilLibertiesViolation rdf:type airo:Impact ;
    rdfs:label "Civil Liberties Violation Impact"@en ;
    rdfs:label "公民自由侵犯的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackLivesMatterActivist .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .