@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WorldBankCashTransferAlgorithm rdf:type airo:AISystem ;
    rdfs:label "World Bank Cash Transfer Algorithm"@en ;
    rdfs:label "世界银行现金转移算法"@zh ;
    airo:hasRisk ex:InaccurateWelfareTargetingRisk ;
    airo:hasAISubject ex:PoorPopulationInJordan ;
    airo:hasStakeholder ex:WorldBank .

ex:InaccurateWelfareTargetingRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Welfare Targeting Risk"@en ;
    rdfs:label "福利目标定位不准确风险"@zh ;
    airo:hasConsequence ex:NeedyPeopleDeniedSupport ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInTargeting .

ex:NeedyPeopleDeniedSupport rdf:type airo:Consequence ;
    rdfs:label "Needy People Denied Support"@en ;
    rdfs:label "有需要的人被拒绝支持"@zh ;
    airo:hasImpact ex:IncreasedPovertyLevels .

ex:WorldBank rdf:type airo:Stakeholder ;
    rdfs:label "World Bank"@en ;
    rdfs:label "世界银行"@zh .

ex:PoorPopulationInJordan rdf:type airo:AISubject ;
    rdfs:label "Poor Population in Jordan"@en ;
    rdfs:label "约旦贫困人口"@zh .

ex:IncreasedPovertyLevels rdf:type airo:Impact ;
    rdfs:label "Increased Poverty Levels"@en ;
    rdfs:label "贫困水平加剧"@zh ;
    airo:hasImpactOnStakeholder ex:WorldBank ;
    airo:hasImpactOnStakeholder ex:PoorPopulationInJordan .

ex:AlgorithmicBiasInTargeting rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Targeting"@en ;
    rdfs:label "目标定位中的算法偏见"@zh .