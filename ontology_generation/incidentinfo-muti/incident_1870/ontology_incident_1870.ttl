@base <https://w3id.org/airo#> .

ex:AmazonAI rdf:type airo:AISystem ;
    rdfs:label "Amazon AI System"@en ;
    rdfs:label "亚马逊AI系统"@zh ;
    airo:hasRisk ex:CompetitionRisk ;
    airo:hasStakeholder ex:RobertHammond ;
    airo:isAppliedWithinDomain ex:ECommerce .

ex:CompetitionRisk rdf:type airo:Risk ;
    rdfs:label "Competition Risk"@en ;
    rdfs:label "竞争风险"@zh ;
    airo:hasConsequence ex:LegalAction .

ex:RobertHammond rdf:type airo:Stakeholder ;
    rdfs:label "Robert Hammond"@en ;
    rdfs:label "罗伯特·哈蒙德"@zh .

ex:ECommerce rdf:type airo:Domain ;
    rdfs:label "E-Commerce"@en ;
    rdfs:label "电子商务"@zh .

ex:LegalAction rdf:type airo:Consequence ;
    rdfs:label "Legal Action"@en ;
    rdfs:label "法律诉讼"@zh .