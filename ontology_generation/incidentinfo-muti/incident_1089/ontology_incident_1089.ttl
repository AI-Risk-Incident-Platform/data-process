@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RobotStaffInJapaneseHotel rdf:type airo:AISystem ;
    rdfs:label "Robot Staff in Japanese Hotel"@en ;
    rdfs:label "日本酒店机器人员工"@zh ;
    airo:hasRisk ex:JobDisplacementRisk ;
    airo:hasStakeholder ex:ChineseNetizens ;
    airo:isDeployedBy ex:JapaneseHotel .

ex:JobDisplacementRisk rdf:type airo:Risk ;
    rdfs:label "Job Displacement Risk"@en ;
    rdfs:label "工作替代风险"@zh ;
    airo:hasImpact ex:PublicReactionToAutomation .

ex:ChineseNetizens rdf:type airo:Stakeholder ;
    rdfs:label "Chinese Netizens"@en ;
    rdfs:label "中国网民"@zh .

ex:JapaneseHotel rdf:type airo:AIOperator ;
    rdfs:label "Japanese Hotel"@en ;
    rdfs:label "日本酒店"@zh .

ex:PublicReactionToAutomation rdf:type airo:Impact ;
    rdfs:label "Public Reaction to Automation"@en ;
    rdfs:label "公众对自动化的反应"@zh ;
    airo:hasImpactOnStakeholder ex:ChineseNetizens .