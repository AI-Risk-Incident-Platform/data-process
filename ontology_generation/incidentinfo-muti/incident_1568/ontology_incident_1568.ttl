@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAI rdf:type airo:AISystem ;
    rdfs:label "Generative AI"@en ;
    rdfs:label "生成式AI"@zh ;
    airo:hasRisk ex:CarbonFootprintRisk ;
    airo:isAppliedWithinDomain ex:FinancialInstitutions .

ex:CarbonFootprintRisk rdf:type airo:Risk ;
    rdfs:label "Carbon Footprint Risk"@en ;
    rdfs:label "碳足迹风险"@zh ;
    airo:isRiskSourceFor ex:EnergyIntensiveTraining ;
    airo:hasConsequence ex:EnvironmentalImpact .

ex:EnvironmentalImpact rdf:type airo:Consequence ;
    rdfs:label "Environmental Impact Consequence"@en ;
    rdfs:label "环境影响后果"@zh ;
    airo:hasImpact ex:ClimateChangeImpact .

ex:FinancialInstitutions rdf:type airo:Domain ;
    rdfs:label "Financial Institutions Domain"@en ;
    rdfs:label "金融机构领域"@zh .

ex:ClimateChangeImpact rdf:type airo:Impact ;
    rdfs:label "Climate Change Impact"@en ;
    rdfs:label "气候变化影响"@zh .

ex:EnergyIntensiveTraining rdf:type airo:RiskSource ;
    rdfs:label "Energy Intensive Training Risk Source"@en ;
    rdfs:label "能源密集型训练风险源"@zh .