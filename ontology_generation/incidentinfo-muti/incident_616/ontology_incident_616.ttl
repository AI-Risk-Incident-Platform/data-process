@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RiteAidFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Rite Aid Facial Recognition System"@en ;
    rdfs:label "Rite Aid面部识别系统"@zh ;
    airo:hasRisk ex:RacialBiasInFacialRecognitionRisk ;
    airo:isDeployedBy ex:RiteAid .

ex:RacialBiasInFacialRecognitionRisk rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Facial Recognition Risk"@en ;
    rdfs:label "面部识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:MisidentificationOfMinorities ;
    airo:isRiskSourceFor ex:RecklessUseOfFacialRecognition ;
    airo:hasRiskControl ex:FiveYearBanOnFacialRecognition .

ex:MisidentificationOfMinorities rdf:type airo:Consequence ;
    rdfs:label "Misidentification of Minorities Consequence"@en ;
    rdfs:label "少数族裔误认后果"@zh ;
    airo:hasImpact ex:DiscriminationAgainstMinorities .

ex:DiscriminationAgainstMinorities rdf:type airo:Impact ;
    rdfs:label "Discrimination Against Minorities Impact"@en ;
    rdfs:label "对少数族裔的歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:RiteAid .

ex:RiteAid rdf:type airo:AIOperator ;
    rdfs:label "Rite Aid"@en ;
    rdfs:label "Rite Aid"@zh .

ex:FiveYearBanOnFacialRecognition rdf:type airo:RiskControl ;
    rdfs:label "Five-Year Ban on Facial Recognition RiskControl"@en ;
    rdfs:label "五年面部识别禁令管控"@zh ;
    airo:mitigatesRiskConcept ex:RacialBiasInFacialRecognitionRisk .

ex:RecklessUseOfFacialRecognition rdf:type airo:RiskSource ;
    rdfs:label "Reckless Use of Facial Recognition Risk Source"@en ;
    rdfs:label "鲁莽使用面部识别的风险源"@zh .