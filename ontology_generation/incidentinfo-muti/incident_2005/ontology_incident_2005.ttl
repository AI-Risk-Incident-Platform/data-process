@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:FacialSynthesisTechnology ;
    airo:hasPurpose ex:DeceptionForFinancialGain ;
    airo:hasAISubject ex:CryptocurrencyInvestors ;
    airo:hasStakeholder ex:BinanceExecutive ;
    airo:isAppliedWithinDomain ex:CryptocurrencyIndustryDomain .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "金融欺诈风险"@zh ;
    rdfs:label "Financial Fraud Risk"@en ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:MaliciousUseOfDeepfake .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "经济损失后果"@zh ;
    rdfs:label "Financial Loss Consequence"@en ;
    airo:hasImpact ex:ReputationDamage .

ex:BinanceExecutive rdf:type airo:Stakeholder ;
    rdfs:label "币安高管"@zh ;
    rdfs:label "Binance Executive"@en .

ex:CryptocurrencyInvestors rdf:type airo:AISubject ;
    rdfs:label "加密货币投资者"@zh ;
    rdfs:label "Cryptocurrency Investors"@en .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "声誉损害影响"@zh ;
    rdfs:label "Reputation Damage Impact"@en ;
    airo:hasImpactOnStakeholder ex:BinanceExecutive .

ex:DeceptionForFinancialGain rdf:type airo:Purpose ;
    rdfs:label "欺骗以获取经济利益的目的"@zh ;
    rdfs:label "Deception for Financial Gain Purpose"@en .

ex:CryptocurrencyIndustryDomain rdf:type airo:Domain ;
    rdfs:label "加密货币行业领域"@zh ;
    rdfs:label "Cryptocurrency Industry Domain"@en .

ex:FacialSynthesisTechnology rdf:type airo:AITechnique ;
    rdfs:label "面部合成技术"@zh ;
    rdfs:label "Facial Synthesis Technology"@en .

ex:MaliciousUseOfDeepfake rdf:type airo:RiskSource ;
    rdfs:label "恶意使用深度伪造的风险源"@zh ;
    rdfs:label "Malicious Use of Deepfake Risk Source"@en .