@base <https://w3id.org/airo#> .

ex:BytedanceCensorshipSystem rdf:type airo:AISystem ;
    rdfs:label "Bytedance Censorship System"@en ;
    rdfs:label "字节跳动内容审查系统"@zh ;
    airo:hasRisk ex:ContentCensorshipRisk ;
    airo:usesTechnique ex:DifferentiableSimulator ;
    airo:hasCapability ex:ClothesFoldingCapability ;
    airo:isProvidedBy ex:Bytedance .

ex:ContentCensorshipRisk rdf:type airo:Risk ;
    rdfs:label "Content Censorship Risk"@en ;
    rdfs:label "内容审查风险"@zh .

ex:DifferentiableSimulator rdf:type airo:AITechnique ;
    rdfs:label "Differentiable Simulator Technique"@en ;
    rdfs:label "可微分模拟器技术"@zh .

ex:ClothesFoldingCapability rdf:type airo:AICapability ;
    rdfs:label "Clothes Folding Capability"@en ;
    rdfs:label "衣物折叠能力"@zh .

ex:Bytedance rdf:type airo:AIProvider ;
    rdfs:label "Bytedance"@en ;
    rdfs:label "字节跳动"@zh .