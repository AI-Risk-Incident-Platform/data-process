@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVoiceCloningSystem rdf:type airo:AISystem ;
    rdfs:label "AI Voice Cloning System"@en ;
    rdfs:label "AI语音克隆系统"@zh ;
    airo:hasRisk ex:RealityDistortionRisk ;
    airo:hasCapability ex:VoiceSynthesisAndCloning ;
    airo:hasStakeholder ex:PoliticiansAndCelebrities .

ex:RealityDistortionRisk rdf:type airo:Risk ;
    rdfs:label "Reality Distortion Risk"@en ;
    rdfs:label "现实扭曲风险"@zh ;
    airo:hasConsequence ex:PublicTrustErosion .

ex:VoiceSynthesisAndCloning rdf:type airo:AICapability ;
    rdfs:label "Voice Synthesis and Cloning Capability"@en ;
    rdfs:label "语音合成与克隆能力"@zh .

ex:PoliticiansAndCelebrities rdf:type airo:Stakeholder ;
    rdfs:label "Politicians and Celebrities"@en ;
    rdfs:label "政治人物与名人"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:PoliticiansAndCelebrities .