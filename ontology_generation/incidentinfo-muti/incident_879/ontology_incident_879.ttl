@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenAIForDisinformation rdf:type airo:AISystem ;
    rdfs:label "Gen AI for Disinformation"@en ;
    rdfs:label "用于虚假信息传播的生成式AI"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:hasPurpose ex:PoliticalInfluence ;
    airo:hasStakeholder ex:USGovernment ;
    airo:isDeployedBy ex:RussianEntities ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:isRiskSourceFor ex:ForeignStateSponsoredDisinformation ;
    airo:hasConsequence ex:DemocraticProcessUndermining .

ex:USGovernment rdf:type airo:Stakeholder ;
    rdfs:label "US Government"@en ;
    rdfs:label "美国政府"@zh .

ex:RussianEntities rdf:type airo:AIOperator ;
    rdfs:label "Russian Entities"@en ;
    rdfs:label "俄罗斯实体"@zh .

ex:PublicOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "Public Opinion Manipulation Impact"@en ;
    rdfs:label "舆论操纵影响"@zh ;
    airo:hasImpactOnStakeholder ex:USGovernment .

ex:PoliticalInfluence rdf:type airo:Purpose ;
    rdfs:label "Political Influence Purpose"@en ;
    rdfs:label "政治影响目的"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:DemocraticProcessUndermining rdf:type airo:Consequence ;
    rdfs:label "Democratic Process Undermining Consequence"@en ;
    rdfs:label "民主进程破坏后果"@zh ;
    airo:hasImpact ex:PublicOpinionManipulation .

ex:ForeignStateSponsoredDisinformation rdf:type airo:RiskSource ;
    rdfs:label "Foreign State-Sponsored Disinformation Risk Source"@en ;
    rdfs:label "外国政府支持的虚假信息风险源"@zh .