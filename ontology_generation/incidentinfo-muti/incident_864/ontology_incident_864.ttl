@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TikTokContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "TikTok Content Moderation System"@en ;
    rdfs:label "TikTok内容审核系统"@zh ;
    airo:hasRisk ex:HarmfulContentExposureRisk ;
    airo:hasStakeholder ex:FrenchFamilies ;
    airo:isDeployedBy ex:TikTokCompany .

ex:HarmfulContentExposureRisk rdf:type airo:Risk ;
    rdfs:label "Harmful Content Exposure Risk"@en ;
    rdfs:label "有害内容暴露风险"@zh ;
    airo:hasConsequence ex:SuicideCases ;
    airo:isRiskSourceFor ex:InadequateContentModeration .

ex:SuicideCases rdf:type airo:Consequence ;
    rdfs:label "Suicide Cases Consequence"@en ;
    rdfs:label "自杀案例后果"@zh ;
    airo:hasImpact ex:MentalHealthDamage .

ex:FrenchFamilies rdf:type airo:Stakeholder ;
    rdfs:label "French Families"@en ;
    rdfs:label "法国家庭"@zh .

ex:TikTokCompany rdf:type airo:AIOperator ;
    rdfs:label "TikTok Company"@en ;
    rdfs:label "TikTok公司"@zh .

ex:MentalHealthDamage rdf:type airo:Impact ;
    rdfs:label "Mental Health Damage Impact"@en ;
    rdfs:label "心理健康损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:FrenchFamilies .

ex:InadequateContentModeration rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Content Moderation Risk Source"@en ;
    rdfs:label "内容审核不足风险源"@zh .