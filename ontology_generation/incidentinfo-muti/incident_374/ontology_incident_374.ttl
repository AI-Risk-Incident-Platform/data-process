@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HuaweiFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Huawei Facial Recognition System"@en ;
    rdfs:label "华为面部识别系统"@zh ;
    airo:hasRisk ex:SurveillanceAndCensorshipRisk ;
    airo:hasPurpose ex:CrackDownOnDissent ;
    airo:hasStakeholder ex:UgandaGovernment ;
    airo:isDeployedBy ex:UgandaGovernment ;
    airo:isProvidedBy ex:HuaweiTechnologies ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:SurveillanceAndCensorshipRisk rdf:type airo:Risk ;
    rdfs:label "Surveillance and Censorship Risk"@en ;
    rdfs:label "监控与审查风险"@zh ;
    airo:hasConsequence ex:SuppressionOfPoliticalDissent .

ex:UgandaGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Uganda Government"@en ;
    rdfs:label "乌干达政府"@zh .

ex:HuaweiTechnologies rdf:type airo:AIProvider ;
    rdfs:label "Huawei Technologies"@en ;
    rdfs:label "华为技术有限公司"@zh .

ex:ThreatToInternetFreedomAndDemocracy rdf:type airo:Impact ;
    rdfs:label "Threat to Internet Freedom and Democracy"@en ;
    rdfs:label "对互联网自由和民主的威胁"@zh ;
    airo:hasImpactOnStakeholder ex:UgandaGovernment .

ex:CrackDownOnDissent rdf:type airo:Purpose ;
    rdfs:label "Crack Down on Dissent"@en ;
    rdfs:label "镇压异议"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security"@en ;
    rdfs:label "公共安全"@zh .

ex:SuppressionOfPoliticalDissent rdf:type airo:Consequence ;
    rdfs:label "Suppression of Political Dissent"@en ;
    rdfs:label "政治异议压制"@zh ;
    airo:hasImpact ex:ThreatToInternetFreedomAndDemocracy .