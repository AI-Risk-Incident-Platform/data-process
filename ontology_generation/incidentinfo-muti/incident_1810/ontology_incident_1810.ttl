@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SupermarketSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Supermarket Surveillance System"@en ;
    rdfs:label "超市监控系统"@zh ;
    airo:hasRisk ex:PrivacyInvasionRisk ;
    airo:hasPurpose ex:SecurityMonitoring ;
    airo:isAppliedWithinDomain ex:RetailSecurity ;
    airo:hasStakeholder ex:SupermarketCustomers ;
    airo:isDeployedBy ex:JumboSupermarket .

ex:PrivacyInvasionRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Invasion Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:SupermarketCustomers rdf:type airo:Stakeholder ;
    rdfs:label "Supermarket Customers"@en ;
    rdfs:label "超市顾客"@zh .

ex:JumboSupermarket rdf:type airo:AIOperator ;
    rdfs:label "Jumbo Supermarket"@en ;
    rdfs:label "Jumbo超市"@zh .

ex:SecurityMonitoring rdf:type airo:Purpose ;
    rdfs:label "Security Monitoring"@en ;
    rdfs:label "安全监控"@zh .

ex:RetailSecurity rdf:type airo:Domain ;
    rdfs:label "Retail Security"@en ;
    rdfs:label "零售安全"@zh .