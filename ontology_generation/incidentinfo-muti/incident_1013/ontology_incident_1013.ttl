@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAIImageGenerator rdf:type airo:AISystem ;
    rdfs:label "OpenAI Image Generator"@en ;
    rdfs:label "OpenAI图像生成器"@zh ;
    airo:hasRisk ex:FraudulentDocumentCreationRisk ;
    airo:hasCapability ex:RealisticImageGeneration ;
    airo:isProvidedBy ex:OpenAI ;
    airo:hasPurpose ex:CreativeContentGeneration ;
    airo:isAppliedWithinDomain ex:DigitalContentCreation .

ex:FraudulentDocumentCreationRisk rdf:type airo:Risk ;
    rdfs:label "Fraudulent Document Creation Risk"@en ;
    rdfs:label "伪造文件创建风险"@zh ;
    airo:hasConsequence ex:DocumentFraud ;
    airo:isRiskSourceFor ex:HighFidelityImageGenerationCapability .

ex:HighFidelityImageGenerationCapability rdf:type airo:RiskSource ;
    rdfs:label "High-Fidelity Image Generation Capability"@en ;
    rdfs:label "高保真图像生成能力"@zh .

ex:DocumentFraud rdf:type airo:Consequence ;
    rdfs:label "Document Fraud Consequence"@en ;
    rdfs:label "文件欺诈后果"@zh .

ex:RealisticImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Realistic Image Generation Capability"@en ;
    rdfs:label "逼真图像生成能力"@zh .

ex:OpenAI rdf:type airo:AIProvider ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:CreativeContentGeneration rdf:type airo:Purpose ;
    rdfs:label "Creative Content Generation Purpose"@en ;
    rdfs:label "创意内容生成目的"@zh .

ex:DigitalContentCreation rdf:type airo:Domain ;
    rdfs:label "Digital Content Creation Domain"@en ;
    rdfs:label "数字内容创作领域"@zh .