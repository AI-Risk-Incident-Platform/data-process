@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedImages rdf:type airo:AISystem ;
    rdfs:label "AI Generated Images"@en ;
    rdfs:label "AI生成图像"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasAISubject ex:PoliticalFigures ;
    airo:hasStakeholder ex:SocialMediaUsers ;
    airo:hasPurpose ex:PoliticalInfluence ;
    airo:isAppliedWithinDomain ex:PoliticalCampaign ;
    airo:usesTechnique ex:ImageGeneration .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:ElectionInterference ;
    airo:isRiskSourceFor ex:FakeContentCreation .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:PoliticalFigures rdf:type airo:AISubject ;
    rdfs:label "Political Figures"@en ;
    rdfs:label "政治人物"@zh .

ex:PublicDeception rdf:type airo:Impact ;
    rdfs:label "Public Deception"@en ;
    rdfs:label "公众欺骗"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .

ex:PoliticalInfluence rdf:type airo:Purpose ;
    rdfs:label "Political Influence"@en ;
    rdfs:label "政治影响"@zh .

ex:PoliticalCampaign rdf:type airo:Domain ;
    rdfs:label "Political Campaign"@en ;
    rdfs:label "政治竞选"@zh .

ex:ImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Image Generation"@en ;
    rdfs:label "图像生成"@zh .

ex:FakeContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Fake Content Creation"@en ;
    rdfs:label "虚假内容创作"@zh .

ex:ElectionInterference rdf:type airo:Consequence ;
    rdfs:label "Election Interference"@en ;
    rdfs:label "选举干预"@zh ;
    airo:hasImpact ex:PublicDeception .