ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:AcademicFraudRisk ;
    airo:hasAISubject ex:StudentApplicants ;
    airo:hasStakeholder ex:UKUniversities .

ex:AcademicFraudRisk rdf:type airo:Risk ;
    rdfs:label "Academic Fraud Risk"@en ;
    rdfs:label "学术欺诈风险"@zh ;
    airo:hasConsequence ex:FraudulentAdmissions ;
    airo:isRiskSourceFor ex:DeepfakeForAcademicFraud .

ex:UKUniversities rdf:type airo:Stakeholder ;
    rdfs:label "UK Universities"@en ;
    rdfs:label "英国大学"@zh .

ex:StudentApplicants rdf:type airo:AISubject ;
    rdfs:label "Student Applicants"@en ;
    rdfs:label "学生申请者"@zh .

ex:FraudulentAdmissions rdf:type airo:Consequence ;
    rdfs:label "Fraudulent Admissions"@en ;
    rdfs:label "欺诈性录取"@zh ;
    airo:hasImpactOnStakeholder ex:UKUniversities .

ex:DeepfakeForAcademicFraud rdf:type airo:Misuse ;
    rdfs:label "Deepfake for Academic Fraud Misuse"@en ;
    rdfs:label "学术欺诈的深度伪造滥用"@zh .