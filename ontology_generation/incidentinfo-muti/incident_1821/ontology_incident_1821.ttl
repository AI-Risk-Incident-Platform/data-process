@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIChatbot rdf:type airo:AISystem ;
    rdfs:label "AI Chatbot"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:HarmfulContentRisk ;
    airo:hasCapability ex:NaturalLanguageGeneration ;
    airo:hasPurpose ex:ConversationalInteraction ;
    airo:hasStakeholder ex:ChatbotCompany .

ex:HarmfulContentRisk rdf:type airo:Risk ;
    rdfs:label "Harmful Content Risk"@en ;
    rdfs:label "有害内容风险"@zh ;
    airo:hasConsequence ex:PotentialSelfHarm ;
    airo:isRiskSourceFor ex:UnfilteredContentGeneration .

ex:PotentialSelfHarm rdf:type airo:Consequence ;
    rdfs:label "Potential Self-Harm Consequence"@en ;
    rdfs:label "潜在自残后果"@zh ;
    airo:hasImpact ex:MentalHealthDeterioration .

ex:ChatbotCompany rdf:type airo:Stakeholder ;
    rdfs:label "Chatbot Company"@en ;
    rdfs:label "聊天机器人公司"@zh .

ex:UnfilteredContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered Content Generation Risk Source"@en ;
    rdfs:label "未过滤内容生成风险源"@zh .

ex:MentalHealthDeterioration rdf:type airo:Impact ;
    rdfs:label "Mental Health Deterioration Impact"@en ;
    rdfs:label "心理健康恶化影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChatbotCompany .

ex:ConversationalInteraction rdf:type airo:Purpose ;
    rdfs:label "Conversational Interaction Purpose"@en ;
    rdfs:label "对话交互目的"@zh .

ex:NaturalLanguageGeneration rdf:type airo:AICapability ;
    rdfs:label "Natural Language Generation Capability"@en ;
    rdfs:label "自然语言生成能力"@zh .