@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:MisuseOfAIInContentGenerationRisk ;
    airo:hasPurpose ex:ContentGenerationPurpose ;
    airo:hasStakeholder ex:StackOverflow .

ex:MisuseOfAIInContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Misuse of AI in Content Generation Risk"@en ;
    rdfs:label "AI内容生成滥用风险"@zh ;
    airo:hasConsequence ex:InaccurateOrMisleadingContentConsequence .

ex:StackOverflow rdf:type airo:Stakeholder ;
    rdfs:label "Stack Overflow"@en ;
    rdfs:label "Stack Overflow"@zh .

ex:InaccurateOrMisleadingContentConsequence rdf:type airo:Consequence ;
    rdfs:label "Inaccurate or Misleading Content Consequence"@en ;
    rdfs:label "不准确或误导性内容后果"@zh ;
    airo:hasImpactOnStakeholder ex:StackOverflow .

ex:ContentGenerationPurpose rdf:type airo:Purpose ;
    rdfs:label "Content Generation Purpose"@en ;
    rdfs:label "内容生成目的"@zh .