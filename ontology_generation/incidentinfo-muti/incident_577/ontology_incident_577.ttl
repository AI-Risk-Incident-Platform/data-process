@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookAIGeneratedStickers rdf:type airo:AISystem ;
    rdfs:label "Facebook AI-Generated Stickers"@en ;
    rdfs:label "Facebook AI生成贴纸"@zh ;
    airo:hasRisk ex:InappropriateContentGenerationRisk ;
    airo:hasAICapability ex:ImageGenerationCapability ;
    airo:isProvidedBy ex:Facebook ;
    airo:hasPurpose ex:SocialMediaContentEnhancement ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms .

ex:InappropriateContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Generation Risk"@en ;
    rdfs:label "不当内容生成风险"@zh ;
    airo:isRiskSourceFor ex:UnfilteredContentGeneration ;
    airo:hasConsequence ex:OffensiveContentCreation .

ex:OffensiveContentCreation rdf:type airo:Consequence ;
    rdfs:label "Offensive Content Creation Consequence"@en ;
    rdfs:label "冒犯性内容创建后果"@zh ;
    airo:hasImpact ex:UserExperienceDegradation .

ex:ImageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Image Generation Capability"@en ;
    rdfs:label "图像生成能力"@zh .

ex:Facebook rdf:type airo:AIProvider ;
    rdfs:label "Facebook"@en ;
    rdfs:label "Facebook"@zh .

ex:UserExperienceDegradation rdf:type airo:Impact ;
    rdfs:label "User Experience Degradation Impact"@en ;
    rdfs:label "用户体验下降影响"@zh .

ex:SocialMediaContentEnhancement rdf:type airo:Purpose ;
    rdfs:label "Social Media Content Enhancement Purpose"@en ;
    rdfs:label "社交媒体内容增强目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:UnfilteredContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered Content Generation Risk Source"@en ;
    rdfs:label "未过滤内容生成风险源"@zh .