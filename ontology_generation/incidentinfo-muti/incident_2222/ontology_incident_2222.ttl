ex:DeepfakeJournalist rdf:type airo:AISystem ;
    rdfs:label "Deepfake Journalist"@en ;
    rdfs:label "深度伪造记者"@zh ;
    airo:hasRisk ex:MisinformationPropagationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology .

ex:MisinformationPropagationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Propagation Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:MediaConsumers .

ex:MediaConsumers rdf:type airo:Stakeholder ;
    rdfs:label "Media Consumers"@en ;
    rdfs:label "媒体消费者"@zh .