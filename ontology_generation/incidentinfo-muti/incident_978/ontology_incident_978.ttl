@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedMusicSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Music System"@en ;
    rdfs:label "AI生成音乐系统"@zh ;
    airo:hasRisk ex:UnauthorizedUseOfCelebrityVoice ;
    airo:hasPurpose ex:MusicCreation ;
    airo:hasStakeholder ex:CelineDion ;
    airo:isAppliedWithinDomain ex:MusicIndustry ;
    airo:usesTechnique ex:VoiceSynthesisTechnology .

ex:UnauthorizedUseOfCelebrityVoice rdf:type airo:Risk ;
    rdfs:label "Unauthorized Use of Celebrity Voice Risk"@en ;
    rdfs:label "未经授权使用名人声音风险"@zh ;
    airo:hasConsequence ex:ViolationOfCelebrityRights .

ex:CelineDion rdf:type airo:Stakeholder ;
    rdfs:label "Céline Dion"@en ;
    rdfs:label "席琳·迪翁"@zh .

ex:ViolationOfCelebrityRights rdf:type airo:Consequence ;
    rdfs:label "Violation of Celebrity Rights Consequence"@en ;
    rdfs:label "侵犯名人权利后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:CelineDion .

ex:MusicCreation rdf:type airo:Purpose ;
    rdfs:label "Music Creation Purpose"@en ;
    rdfs:label "音乐创作目的"@zh .

ex:MusicIndustry rdf:type airo:Domain ;
    rdfs:label "Music Industry Domain"@en ;
    rdfs:label "音乐产业领域"@zh .

ex:VoiceSynthesisTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Synthesis Technology"@en ;
    rdfs:label "语音合成技术"@zh .