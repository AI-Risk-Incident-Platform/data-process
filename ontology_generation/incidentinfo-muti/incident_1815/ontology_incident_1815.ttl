ex:DystopianAIStartup rdf:type airo:AISystem ;
    rdfs:label "Dystopian AI Startup"@en ;
    rdfs:label "反乌托邦AI初创公司"@zh ;
    airo:hasRisk ex:PromotingSlaveryRisk ;
    airo:hasStakeholder ex:IndianFounders ;
    airo:hasImpactOnStakeholder ex:GlobalOutrage ;
    airo:hasMisuse ex:SlaveryPromotion .

ex:PromotingSlaveryRisk rdf:type airo:Risk ;
    rdfs:label "Promoting Slavery Risk"@en ;
    rdfs:label "宣扬奴隶制风险"@zh ;
    airo:hasConsequence ex:GlobalOutrage .

ex:IndianFounders rdf:type airo:Stakeholder ;
    rdfs:label "Indian Founders"@en ;
    rdfs:label "印度创始人"@zh .

ex:GlobalOutrage rdf:type airo:Impact ;
    rdfs:label "Global Outrage Impact"@en ;
    rdfs:label "全球愤怒影响"@zh .

ex:SlaveryPromotion rdf:type airo:Misuse ;
    rdfs:label "Slavery Promotion Misuse"@en ;
    rdfs:label "奴隶制宣传滥用"@zh .