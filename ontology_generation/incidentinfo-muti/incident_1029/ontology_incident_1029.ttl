ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:DeepLearningBasedVideoManipulation .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasConsequence ex:PoliticalManipulationConsequence .

ex:DeepLearningBasedVideoManipulation rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Video Manipulation Technique"@en ;
    rdfs:label "基于深度学习的视频操纵技术"@zh .

ex:PublicMisinformationImpact rdf:type airo:Impact ;
    rdfs:label "Public Misinformation Impact"@en ;
    rdfs:label "公众误导影响"@zh .

ex:PoliticalManipulationConsequence rdf:type airo:Consequence ;
    rdfs:label "Political Manipulation Consequence"@en ;
    rdfs:label "政治操纵后果"@zh ;
    airo:hasImpact ex:PublicMisinformationImpact .