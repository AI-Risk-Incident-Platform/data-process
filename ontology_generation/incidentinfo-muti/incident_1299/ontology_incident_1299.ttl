ex:AItextbookProgram rdf:type airo:AISystem ;
    rdfs:label "AI Textbook Program"@en ;
    rdfs:label "AI教科书项目"@zh ;
    airo:hasRisk ex:ParentalConcernsOverAIEducation ;
    airo:hasStakeholder ex:SouthKoreanParents ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:hasPurpose ex:EducationalContentDelivery .

ex:ParentalConcernsOverAIEducation rdf:type airo:Risk ;
    rdfs:label "Parental Concerns Over AI Education Risk"@en ;
    rdfs:label "家长对AI教育的担忧风险"@zh .

ex:SouthKoreanParents rdf:type airo:Stakeholder ;
    rdfs:label "South Korean Parents"@en ;
    rdfs:label "韩国家长"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .

ex:EducationalContentDelivery rdf:type airo:Purpose ;
    rdfs:label "Educational Content Delivery Purpose"@en ;
    rdfs:label "教育内容传递的目的"@zh .