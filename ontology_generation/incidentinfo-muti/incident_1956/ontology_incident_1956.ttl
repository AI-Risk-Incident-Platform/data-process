@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:LanguageBiasInAIResponsesRisk ;
    airo:hasCapability ex:MultilingualTextGenerationCapability .

ex:LanguageBiasInAIResponsesRisk rdf:type airo:Risk ;
    rdfs:label "Language Bias in AI Responses Risk"@en ;
    rdfs:label "AI回答中的语言偏见风险"@zh ;
    airo:isRiskSourceFor ex:TrainingDataImbalanceRiskSource ;
    airo:hasImpact ex:MisinformationSpreadImpact .

ex:MultilingualTextGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Multilingual Text Generation Capability"@en ;
    rdfs:label "多语言文本生成能力"@zh .

ex:TrainingDataImbalanceRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Training Data Imbalance Risk Source"@en ;
    rdfs:label "训练数据不平衡风险源"@zh .

ex:MisinformationSpreadImpact rdf:type airo:Impact ;
    rdfs:label "Misinformation Spread Impact"@en ;
    rdfs:label "错误信息传播的影响"@zh .