ex:DeepfakeVideo rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video"@en ;
    rdfs:label "深度伪造视频"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:BelgianPoliticalParty .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicDeception .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:BelgianPoliticalParty rdf:type airo:Stakeholder ;
    rdfs:label "Belgian Political Party"@en ;
    rdfs:label "比利时政党"@zh .

ex:PublicDeception rdf:type airo:Impact ;
    rdfs:label "Public Deception"@en ;
    rdfs:label "公众欺骗"@zh ;
    airo:hasImpactOnStakeholder ex:BelgianPoliticalParty .