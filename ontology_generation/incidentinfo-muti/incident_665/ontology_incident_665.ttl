@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SupermarketFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Supermarket Facial Recognition System"@en ;
    rdfs:label "超市面部识别系统"@zh ;
    airo:hasRisk ex:DiscriminationRisk ;
    airo:hasAISubject ex:MaoriWoman ;
    airo:hasStakeholder ex:Supermarket .

ex:DiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Discrimination Risk"@en ;
    rdfs:label "歧视风险"@zh ;
    airo:hasConsequence ex:FalseAccusation ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:FalseAccusation rdf:type airo:Consequence ;
    rdfs:label "False Accusation Consequence"@en ;
    rdfs:label "错误指控后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:MaoriWoman rdf:type airo:AISubject ;
    rdfs:label "Maori Woman"@en ;
    rdfs:label "毛利女性"@zh .

ex:Supermarket rdf:type airo:Stakeholder ;
    rdfs:label "Supermarket"@en ;
    rdfs:label "超市"@zh .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:MaoriWoman .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .