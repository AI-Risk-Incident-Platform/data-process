@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionWebsite rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Website"@en ;
    rdfs:label "面部识别网站"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:GeneralPublic .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:UnauthorizedSurveillance .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:UnauthorizedSurveillance rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Surveillance"@en ;
    rdfs:label "未经授权的监控"@zh ;
    airo:hasImpact ex:PersonalSafetyThreat .

ex:PersonalSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Personal Safety Threat"@en ;
    rdfs:label "人身安全威胁"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .