@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPTMacApp rdf:type airo:AISystem ;
    rdfs:label "ChatGPT Mac App"@en ;
    rdfs:label "ChatGPT Mac应用"@zh ;
    airo:hasRisk ex:DataPrivacyRisk ;
    airo:isProvidedBy ex:OpenAI ;
    airo:hasAISubject ex:ChatGPTUsers .

ex:DataPrivacyRisk rdf:type airo:Risk ;
    rdfs:label "Data Privacy Risk"@en ;
    rdfs:label "数据隐私风险"@zh ;
    airo:hasConsequence ex:UnauthorizedDataAccess ;
    airo:isRiskSourceFor ex:PlainTextStorage .

ex:PlainTextStorage rdf:type airo:RiskSource ;
    rdfs:label "Plain Text Storage"@en ;
    rdfs:label "明文存储"@zh .

ex:UnauthorizedDataAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Data Access"@en ;
    rdfs:label "未经授权的数据访问"@zh ;
    airo:hasImpact ex:UserPrivacyCompromise .

ex:UserPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "User Privacy Compromise"@en ;
    rdfs:label "用户隐私泄露"@zh ;
    airo:hasImpactOnStakeholder ex:ChatGPTUsers .

ex:OpenAI rdf:type airo:AIProvider ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:ChatGPTUsers rdf:type airo:AISubject ;
    rdfs:label "ChatGPT Users"@en ;
    rdfs:label "ChatGPT用户"@zh .