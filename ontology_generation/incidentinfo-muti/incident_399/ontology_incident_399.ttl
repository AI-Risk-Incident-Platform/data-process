@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SchufaCreditScoringSystem rdf:type airo:AISystem ;
    rdfs:label "Schufa Credit Scoring System"@en ;
    rdfs:label "Schufa信用评分系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInCreditScoring ;
    airo:hasCapability ex:CreditworthinessPrediction ;
    airo:hasStakeholder ex:GermanConsumers ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:AlgorithmicBiasInCreditScoring rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Credit Scoring Risk"@en ;
    rdfs:label "信用评分中的算法偏见风险"@zh ;
    airo:hasConsequence ex:UnfairAccessToFinancialServices ;
    airo:isRiskSourceFor ex:OpaqueScoringAlgorithm .

ex:GermanConsumers rdf:type airo:Stakeholder ;
    rdfs:label "German Consumers"@en ;
    rdfs:label "德国消费者"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:CreditworthinessPrediction rdf:type airo:AICapability ;
    rdfs:label "Creditworthiness Prediction Capability"@en ;
    rdfs:label "信用度预测能力"@zh .

ex:UnfairAccessToFinancialServices rdf:type airo:Impact ;
    rdfs:label "Unfair Access to Financial Services Impact"@en ;
    rdfs:label "不公平获取金融服务的影响"@zh ;
    airo:hasImpactOnStakeholder ex:GermanConsumers .

ex:OpaqueScoringAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Opaque Scoring Algorithm Risk Source"@en ;
    rdfs:label "不透明的评分算法风险源"@zh .