@base <https://w3id.org/airo#> .

ex:FakeBidenRobocallSystem rdf:type airo:AISystem ;
    rdfs:label "Fake Biden Robocall System"@en ;
    rdfs:label "伪造拜登语音电话系统"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:hasPurpose ex:MisleadingVoters ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:hasStakeholder ex:NewHampshireDemocrats .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasConsequence ex:VoterSuppression ;
    airo:isRiskSourceFor ex:PoliticalDisinformation .

ex:VoterSuppression rdf:type airo:Consequence ;
    rdfs:label "Voter Suppression Consequence"@en ;
    rdfs:label "选民压制后果"@zh ;
    airo:hasImpact ex:UnderminedDemocraticProcess .

ex:NewHampshireDemocrats rdf:type airo:Stakeholder ;
    rdfs:label "New Hampshire Democrats"@en ;
    rdfs:label "新罕布什尔州民主党人"@zh .

ex:UnderminedDemocraticProcess rdf:type airo:Impact ;
    rdfs:label "Undermined Democratic Process Impact"@en ;
    rdfs:label "破坏民主进程的影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewHampshireDemocrats .

ex:MisleadingVoters rdf:type airo:Purpose ;
    rdfs:label "Misleading Voters Purpose"@en ;
    rdfs:label "误导选民的目的"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:PoliticalDisinformation rdf:type airo:Misuse ;
    rdfs:label "Political Disinformation Misuse"@en ;
    rdfs:label "政治虚假信息滥用"@zh .