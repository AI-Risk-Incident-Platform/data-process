@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GPTStore rdf:type airo:AISystem ;
    rdfs:label "GPT Store"@en ;
    rdfs:label "GPT商店"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:OpenAI .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:isRiskSourceFor ex:ContentGenerationWithoutProperLicensing ;
    airo:hasConsequence ex:LegalComplaints .

ex:OpenAI rdf:type airo:Stakeholder ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:ContentGenerationWithoutProperLicensing rdf:type airo:RiskSource ;
    rdfs:label "Content Generation Without Proper Licensing"@en ;
    rdfs:label "未经适当许可的内容生成"@zh .

ex:LegalComplaints rdf:type airo:Consequence ;
    rdfs:label "Legal Complaints"@en ;
    rdfs:label "法律投诉"@zh .