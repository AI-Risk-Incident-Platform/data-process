@base <https://w3id.org/airo#> .

ex:TweetErinInTheMorn rdf:type airo:AISystem ;
    rdfs:label "Tweet @ErinInTheMorn"@en ;
    rdfs:label "推特账号@ErinInTheMorn"@zh ;
    airo:hasRisk ex:SocialMediaMisinformationRisk ;
    airo:hasAISubject ex:PublicOpinion ;
    airo:hasStakeholder ex:SocialMediaUsers .

ex:SocialMediaMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Social Media Misinformation Risk"@en ;
    rdfs:label "社交媒体虚假信息风险"@zh ;
    airo:hasImpact ex:PublicConfusion .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:PublicOpinion rdf:type airo:AISubject ;
    rdfs:label "Public Opinion"@en ;
    rdfs:label "公众舆论"@zh .

ex:PublicConfusion rdf:type airo:Impact ;
    rdfs:label "Public Confusion"@en ;
    rdfs:label "公众困惑"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .