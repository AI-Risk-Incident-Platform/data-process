@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WhisperTranscriptionTool rdf:type airo:AISystem ;
    rdfs:label "Whisper转录工具"@zh ;
    rdfs:label "Whisper Transcription Tool"@en ;
    airo:hasRisk ex:HallucinationRisk ;
    airo:hasCapability ex:SpeechToTextConversion ;
    airo:isDeployedBy ex:OpenAI ;
    airo:isAppliedWithinDomain ex:Healthcare .

ex:HallucinationRisk rdf:type airo:Risk ;
    rdfs:label "幻觉风险"@zh ;
    rdfs:label "Hallucination Risk"@en ;
    airo:hasConsequence ex:InaccurateMedicalRecords ;
    airo:isRiskSourceFor ex:AIHallucination .

ex:InaccurateMedicalRecords rdf:type airo:Consequence ;
    rdfs:label "不准确的医疗记录后果"@zh ;
    rdfs:label "Inaccurate Medical Records Consequence"@en ;
    airo:hasImpact ex:PatientSafetyThreat .

ex:SpeechToTextConversion rdf:type airo:AICapability ;
    rdfs:label "语音转文本能力"@zh ;
    rdfs:label "Speech-to-Text Conversion Capability"@en .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@zh ;
    rdfs:label "OpenAI"@en .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "医疗保健领域"@zh ;
    rdfs:label "Healthcare Domain"@en .

ex:PatientSafetyThreat rdf:type airo:Impact ;
    rdfs:label "患者安全威胁影响"@zh ;
    rdfs:label "Patient Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:OpenAI .

ex:AIHallucination rdf:type airo:RiskSource ;
    rdfs:label "AI幻觉风险源"@zh ;
    rdfs:label "AI Hallucination Risk Source"@en .