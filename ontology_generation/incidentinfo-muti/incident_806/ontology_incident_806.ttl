@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HitlerSpeechesAIGeneratedContent rdf:type airo:AISystem ;
    rdfs:label "希特勒演讲AI生成内容"@zh ;
    rdfs:label "Hitler Speeches AI Generated Content"@en ;
    airo:hasRisk ex:HistoricalFigureMisrepresentationRisk ;
    airo:hasStakeholder ex:TikTokPlatform ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms .

ex:HistoricalFigureMisrepresentationRisk rdf:type airo:Risk ;
    rdfs:label "历史人物错误表述风险"@zh ;
    rdfs:label "Historical Figure Misrepresentation Risk"@en ;
    airo:isRiskSourceFor ex:AIgeneratedContentCreation ;
    airo:hasConsequence ex:HistoricalDistortion .

ex:TikTokPlatform rdf:type airo:Stakeholder ;
    rdfs:label "TikTok平台"@zh ;
    rdfs:label "TikTok Platform"@en .

ex:HistoricalDistortion rdf:type airo:Consequence ;
    rdfs:label "历史扭曲后果"@zh ;
    rdfs:label "Historical Distortion Consequence"@en ;
    airo:hasImpact ex:SocialHarm .

ex:SocialHarm rdf:type airo:Impact ;
    rdfs:label "社会危害影响"@zh ;
    rdfs:label "Social Harm Impact"@en ;
    airo:hasImpactOnStakeholder ex:TikTokPlatform .

ex:AIgeneratedContentCreation rdf:type airo:RiskSource ;
    rdfs:label "AI生成内容创作风险源"@zh ;
    rdfs:label "AI-generated Content Creation Risk Source"@en .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "社交媒体平台领域"@zh ;
    rdfs:label "Social Media Platforms Domain"@en .