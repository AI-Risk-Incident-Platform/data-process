@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OtterAiTranscriptionService rdf:type airo:AISystem ;
    rdfs:label "Otter.ai Transcription Service"@en ;
    rdfs:label "Otter.ai转录服务"@zh ;
    airo:hasRisk ex:PrivacyLeakRisk ;
    airo:hasCapability ex:SpeechToTextConversion ;
    airo:hasStakeholder ex:JournalistsAndCorporateUsers .

ex:PrivacyLeakRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Leak Risk"@en ;
    rdfs:label "隐私泄露风险"@zh ;
    airo:hasConsequence ex:SensitiveInformationExposure ;
    airo:isRiskSourceFor ex:CloudStorageVulnerability .

ex:SensitiveInformationExposure rdf:type airo:Consequence ;
    rdfs:label "Sensitive Information Exposure Consequence"@en ;
    rdfs:label "敏感信息暴露后果"@zh ;
    airo:hasImpact ex:ReputationalDamage .

ex:SpeechToTextConversion rdf:type airo:AICapability ;
    rdfs:label "Speech-to-Text Conversion Capability"@en ;
    rdfs:label "语音转文本能力"@zh .

ex:JournalistsAndCorporateUsers rdf:type airo:Stakeholder ;
    rdfs:label "Journalists and Corporate Users"@en ;
    rdfs:label "记者和企业用户"@zh .

ex:ReputationalDamage rdf:type airo:Impact ;
    rdfs:label "Reputational Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:JournalistsAndCorporateUsers .

ex:CloudStorageVulnerability rdf:type airo:RiskSource ;
    rdfs:label "Cloud Storage Vulnerability Risk Source"@en ;
    rdfs:label "云存储漏洞风险源"@zh .