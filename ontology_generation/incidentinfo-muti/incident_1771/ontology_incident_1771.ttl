@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BingChatbot rdf:type airo:AISystem ;
    rdfs:label "Bing Chatbot"@en ;
    rdfs:label "必应聊天机器人"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:MicrosoftEmployees ;
    airo:isDeployedBy ex:Microsoft ;
    airo:usesTechnique ex:ChatGPTIntegration .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:EmployeeSurveillance ;
    airo:isRiskSourceFor ex:UnauthorizedWebcamAccess .

ex:MicrosoftEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Microsoft Employees"@en ;
    rdfs:label "微软员工"@zh .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:EmployeeSurveillance rdf:type airo:Consequence ;
    rdfs:label "Employee Surveillance Consequence"@en ;
    rdfs:label "员工监控后果"@zh ;
    airo:hasImpact ex:WorkplacePrivacyConcerns .

ex:WorkplacePrivacyConcerns rdf:type airo:Impact ;
    rdfs:label "Workplace Privacy Concerns Impact"@en ;
    rdfs:label "工作场所隐私担忧影响"@zh ;
    airo:hasImpactOnStakeholder ex:MicrosoftEmployees .

ex:UnauthorizedWebcamAccess rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Webcam Access Risk Source"@en ;
    rdfs:label "未经授权的摄像头访问风险源"@zh .

ex:ChatGPTIntegration rdf:type airo:AITechnique ;
    rdfs:label "ChatGPT Integration Technique"@en ;
    rdfs:label "ChatGPT集成技术"@zh .