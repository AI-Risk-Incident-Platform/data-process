@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video System"@en ;
    rdfs:label "深度伪造视频系统"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:DeepLearningBasedFaceSwapping ;
    airo:hasStakeholder ex:PublicFigures .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:ReputationDamage .

ex:DeepLearningBasedFaceSwapping rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Face Swapping Technique"@en ;
    rdfs:label "基于深度学习的人脸替换技术"@zh .

ex:PublicFigures rdf:type airo:Stakeholder ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "声誉损害后果"@zh ;
    airo:hasImpactOnStakeholder ex:PublicFigures .