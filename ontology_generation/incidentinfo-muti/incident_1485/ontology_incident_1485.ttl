@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PhotoEditingAI rdf:type airo:AISystem ;
    rdfs:label "Photo Editing AI"@en ;
    rdfs:label "照片编辑AI"@zh ;
    airo:hasRisk ex:MisrepresentationRisk ;
    airo:hasPurpose ex:ImageEnhancement ;
    airo:hasStakeholder ex:PublicFigures .

ex:MisrepresentationRisk rdf:type airo:Risk ;
    rdfs:label "Misrepresentation Risk"@en ;
    rdfs:label "虚假呈现风险"@zh ;
    airo:hasConsequence ex:PublicTrustErosion .

ex:PublicFigures rdf:type airo:Stakeholder ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:PublicTrustErosion rdf:type airo:Consequence ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:PublicFigures .

ex:ImageEnhancement rdf:type airo:Purpose ;
    rdfs:label "Image Enhancement"@en ;
    rdfs:label "图像增强"@zh .