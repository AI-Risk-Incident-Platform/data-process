@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSmartLockers rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Smart Lockers"@en ;
    rdfs:label "面部识别智能储物柜"@zh ;
    airo:hasRisk ex:SecurityVulnerabilityRisk ;
    airo:hasStakeholder ex:FourthGradeStudents .

ex:SecurityVulnerabilityRisk rdf:type airo:Risk ;
    rdfs:label "Security Vulnerability Risk"@en ;
    rdfs:label "安全漏洞风险"@zh ;
    airo:hasConsequence ex:UnauthorizedAccess ;
    airo:isRiskSourceFor ex:InadequateSecurityMeasures .

ex:FourthGradeStudents rdf:type airo:Stakeholder ;
    rdfs:label "Fourth Grade Students"@en ;
    rdfs:label "四年级学生"@zh .

ex:InadequateSecurityMeasures rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Security Measures"@en ;
    rdfs:label "安全措施不足"@zh .

ex:UnauthorizedAccess rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Access"@en ;
    rdfs:label "未经授权访问"@zh ;
    airo:hasImpact ex:PrivacyAndSecurityCompromise .

ex:PrivacyAndSecurityCompromise rdf:type airo:Impact ;
    rdfs:label "Privacy and Security Compromise"@en ;
    rdfs:label "隐私和安全受损"@zh ;
    airo:hasImpactOnStakeholder ex:FourthGradeStudents .