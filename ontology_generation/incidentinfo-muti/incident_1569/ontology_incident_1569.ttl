ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:GenderBiasInRecommendationLettersRisk ;
    airo:hasCapability ex:NaturalLanguageGenerationCapability ;
    airo:usesTechnique ex:LargeLanguageModelTechnique .

ex:GenderBiasInRecommendationLettersRisk rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Recommendation Letters Risk"@en ;
    rdfs:label "推荐信中的性别偏见风险"@zh ;
    airo:hasConsequence ex:GenderDiscriminationImpact ;
    airo:isRiskSourceFor ex:BiasedTrainingDataRiskSource .

ex:NaturalLanguageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Natural Language Generation Capability"@en ;
    rdfs:label "自然语言生成能力"@zh .

ex:LargeLanguageModelTechnique rdf:type airo:AITechnique ;
    rdfs:label "Large Language Model Technique"@en ;
    rdfs:label "大语言模型技术"@zh .

ex:GenderDiscriminationImpact rdf:type airo:Impact ;
    rdfs:label "Gender Discrimination Impact"@en ;
    rdfs:label "性别歧视影响"@zh .

ex:BiasedTrainingDataRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .