@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:LegalMisinformationRisk ;
    airo:hasAICapability ex:LegalCaseGeneration ;
    airo:hasAISubject ex:CourtSystem ;
    airo:hasPurpose ex:LegalResearchAssistance ;
    airo:isAppliedWithinDomain ex:LegalServices ;
    airo:usesTechnique ex:TextGeneration ;
    airo:hasStakeholder ex:LegalProfessionals ;
    airo:hasVulnerability ex:FactVerificationAbsence .

ex:LegalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Misinformation Risk"@en ;
    rdfs:label "法律错误信息风险"@zh ;
    airo:hasConsequence ex:LegalProceedingsDisruption ;
    airo:isRiskSourceFor ex:UnverifiedAIOutputs .

ex:LegalProceedingsDisruption rdf:type airo:Consequence ;
    rdfs:label "Legal Proceedings Disruption Consequence"@en ;
    rdfs:label "法律程序中断后果"@zh ;
    airo:hasImpact ex:JudicialSystemIntegrityUndermined .

ex:LegalCaseGeneration rdf:type airo:AICapability ;
    rdfs:label "Legal Case Generation Capability"@en ;
    rdfs:label "法律案例生成能力"@zh .

ex:LegalProfessionals rdf:type airo:Stakeholder ;
    rdfs:label "Legal Professionals"@en ;
    rdfs:label "法律专业人士"@zh .

ex:CourtSystem rdf:type airo:AISubject ;
    rdfs:label "Court System"@en ;
    rdfs:label "法院系统"@zh .

ex:JudicialSystemIntegrityUndermined rdf:type airo:Impact ;
    rdfs:label "Judicial System Integrity Undermined Impact"@en ;
    rdfs:label "司法系统完整性受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:LegalProfessionals .

ex:LegalResearchAssistance rdf:type airo:Purpose ;
    rdfs:label "Legal Research Assistance Purpose"@en ;
    rdfs:label "法律研究辅助目的"@zh .

ex:LegalServices rdf:type airo:Domain ;
    rdfs:label "Legal Services Domain"@en ;
    rdfs:label "法律服务领域"@zh .

ex:TextGeneration rdf:type airo:AITechnique ;
    rdfs:label "Text Generation Technique"@en ;
    rdfs:label "文本生成技术"@zh .

ex:FactVerificationAbsence rdf:type airo:Vulnerability ;
    rdfs:label "Fact Verification Absence Vulnerability"@en ;
    rdfs:label "事实核查缺失脆弱性"@zh .

ex:LegalDocumentFabrication rdf:type airo:Threat ;
    rdfs:label "Legal Document Fabrication Threat"@en ;
    rdfs:label "法律文件伪造威胁"@zh ;
    airo:isRiskSourceFor ex:LegalMisinformationRisk .

ex:HallucinatedLegalPrecedents rdf:type airo:Hazard ;
    rdfs:label "Hallucinated Legal Precedents Hazard"@en ;
    rdfs:label "虚构法律先例危害"@zh ;
    airo:isRiskSourceFor ex:LegalMisinformationRisk .

ex:UnverifiedAIOutputs rdf:type airo:RiskSource ;
    rdfs:label "Unverified AI Outputs Risk Source"@en ;
    rdfs:label "未经验证的AI输出风险源"@zh .

ex:EthicalUseOfAIInLegalContext rdf:type airo:RiskConcept ;
    rdfs:label "Ethical Use of AI in Legal Context Risk Concept"@en ;
    rdfs:label "法律场景中AI的伦理使用"@zh .