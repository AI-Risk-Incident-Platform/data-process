ex:SophiaRobot rdf:type airo:AISystem ;
    rdfs:label "Sophia Robot"@en ;
    rdfs:label "索菲亚机器人"@zh ;
    airo:hasRisk ex:EthicalDilemmaOfRobotCitizenship ;
    airo:hasPurpose ex:MarketingPublicityStunt ;
    airo:hasStakeholder ex:SaudiArabianGovernment ;
    airo:isAppliedWithinDomain ex:PublicRelationsAndMarketing .

ex:EthicalDilemmaOfRobotCitizenship rdf:type airo:Risk ;
    rdfs:label "Ethical Dilemma of Robot Citizenship Risk"@en ;
    rdfs:label "机器人公民身份的伦理困境风险"@zh ;
    airo:hasConsequence ex:DevaluationOfHumanCitizenship ;
    airo:hasImpact ex:PublicConfusionAboutAIStatus ;
    airo:isRiskSourceFor ex:ExploitationOfRobotForSymbolicGestures .

ex:SaudiArabianGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Saudi Arabian Government"@en ;
    rdfs:label "沙特阿拉伯政府"@zh .

ex:DevaluationOfHumanCitizenship rdf:type airo:Consequence ;
    rdfs:label "Devaluation of Human Citizenship Consequence"@en ;
    rdfs:label "人类公民身份贬值后果"@zh ;
    airo:hasImpact ex:PublicConfusionAboutAIStatus .

ex:MarketingPublicityStunt rdf:type airo:Purpose ;
    rdfs:label "Marketing Publicity Stunt Purpose"@en ;
    rdfs:label "营销宣传噱头目的"@zh .

ex:PublicConfusionAboutAIStatus rdf:type airo:Impact ;
    rdfs:label "Public Confusion About AI Status Impact"@en ;
    rdfs:label "公众对AI地位认知混乱的影响"@zh .

ex:PublicRelationsAndMarketing rdf:type airo:Domain ;
    rdfs:label "Public Relations and Marketing Domain"@en ;
    rdfs:label "公共关系与营销领域"@zh .

ex:ExploitationOfRobotForSymbolicGestures rdf:type airo:Misuse ;
    rdfs:label "Exploitation of Robot for Symbolic Gestures Misuse"@en ;
    rdfs:label "利用机器人进行象征性姿态的滥用"@zh .