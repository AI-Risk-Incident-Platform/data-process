@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ProctoringApps rdf:type airo:AISystem ;
    rdfs:label "Proctoring Apps"@en ;
    rdfs:label "监考应用程序"@zh ;
    airo:hasRisk ex:UnnecessarySurveillanceRisk ;
    airo:hasAISubject ex:Students ;
    airo:hasPurpose ex:AcademicIntegrityMonitoring .

ex:UnnecessarySurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Unnecessary Surveillance Risk"@en ;
    rdfs:label "不必要的监控风险"@zh ;
    airo:hasConsequence ex:PrivacyViolationImpact .

ex:Students rdf:type airo:AISubject ;
    rdfs:label "Students"@en ;
    rdfs:label "学生"@zh .

ex:PrivacyViolationImpact rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:Students .

ex:AcademicIntegrityMonitoring rdf:type airo:Purpose ;
    rdfs:label "Academic Integrity Monitoring"@en ;
    rdfs:label "学术诚信监控"@zh .