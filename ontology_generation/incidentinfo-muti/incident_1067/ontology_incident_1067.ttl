@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClaudeAI rdf:type airo:AISystem ;
    rdfs:label "Claude AI"@en ;
    rdfs:label "Claude AI"@zh ;
    airo:hasRisk ex:LegalCitationHallucinationRisk ;
    airo:isDevelopedBy ex:Anthropic ;
    airo:hasStakeholder ex:AnthropicLegalTeam .

ex:LegalCitationHallucinationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Citation Hallucination Risk"@en ;
    rdfs:label "法律引用幻觉风险"@zh ;
    airo:hasConsequence ex:ForcedApologyFromLawyer ;
    airo:isRiskSourceFor ex:AIHallucination .

ex:ForcedApologyFromLawyer rdf:type airo:Consequence ;
    rdfs:label "Forced Apology from Lawyer Consequence"@en ;
    rdfs:label "律师被迫道歉后果"@zh ;
    airo:hasImpact ex:LegalCredibilityDamage .

ex:AnthropicLegalTeam rdf:type airo:Stakeholder ;
    rdfs:label "Anthropic Legal Team"@en ;
    rdfs:label "Anthropic法律团队"@zh .

ex:Anthropic rdf:type airo:AIOperator ;
    rdfs:label "Anthropic"@en ;
    rdfs:label "Anthropic公司"@zh .

ex:AIHallucination rdf:type airo:RiskSource ;
    rdfs:label "AI Hallucination Risk Source"@en ;
    rdfs:label "AI幻觉风险源"@zh .

ex:LegalCredibilityDamage rdf:type airo:Impact ;
    rdfs:label "Legal Credibility Damage Impact"@en ;
    rdfs:label "法律可信度损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:AnthropicLegalTeam .