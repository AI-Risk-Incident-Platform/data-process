ex:AlgorithmicOppressionSystem rdf:type airo:AISystem ;
    rdfs:label "Algorithmic Oppression System"@en ;
    rdfs:label "算法压迫系统"@zh ;
    airo:hasRisk ex:AlgorithmicOppressionRisk ;
    airo:hasAISubject ex:MarginalizedCommunities ;
    airo:hasStakeholder ex:SafiyaNoble .

ex:AlgorithmicOppressionRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Oppression Risk"@en ;
    rdfs:label "算法压迫风险"@zh ;
    airo:hasImpact ex:SocialOppressionImpact .

ex:SafiyaNoble rdf:type airo:Stakeholder ;
    rdfs:label "Safiya Noble"@en ;
    rdfs:label "萨菲亚·诺布尔"@zh .

ex:SocialOppressionImpact rdf:type airo:Impact ;
    rdfs:label "Social Oppression Impact"@en ;
    rdfs:label "社会压迫影响"@zh ;
    airo:hasImpactOnStakeholder ex:SafiyaNoble ;
    airo:hasImpactOnStakeholder ex:MarginalizedCommunities .

ex:MarginalizedCommunities rdf:type airo:AISubject ;
    rdfs:label "Marginalized Communities"@en ;
    rdfs:label "边缘化群体"@zh .