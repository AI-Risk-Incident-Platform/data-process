@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAI rdf:type airo:AISystem ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh ;
    airo:hasRisk ex:CovertCampaignsRisk ;
    airo:hasPurpose ex:InfluenceElections ;
    airo:hasStakeholder ex:China ;
    airo:isAppliedWithinDomain ex:PoliticalCampaigns .

ex:CovertCampaignsRisk rdf:type airo:Risk ;
    rdfs:label "Covert Campaigns Risk"@en ;
    rdfs:label "隐蔽宣传活动风险"@zh ;
    airo:isRiskSourceFor ex:MisuseOfAIForCovertCampaigns ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "产生后果"@zh ;
        rdfs:label "Result in Consequences"@en ;
        airo:hasImpact ex:DisinformationImpact
    ] .

ex:China rdf:type airo:Stakeholder ;
    rdfs:label "China"@en ;
    rdfs:label "中国"@zh .

ex:MisuseOfAIForCovertCampaigns rdf:type airo:RiskSource ;
    rdfs:label "Misuse of AI for Covert Campaigns"@en ;
    rdfs:label "滥用AI进行隐蔽宣传活动"@zh .

ex:DisinformationImpact rdf:type airo:Impact ;
    rdfs:label "Disinformation Impact"@en ;
    rdfs:label "虚假信息影响"@zh ;
    airo:hasImpactOnStakeholder ex:China .

ex:PoliticalCampaigns rdf:type airo:Domain ;
    rdfs:label "Political Campaigns"@en ;
    rdfs:label "政治竞选"@zh .

ex:InfluenceElections rdf:type airo:Purpose ;
    rdfs:label "Influence Elections"@en ;
    rdfs:label "影响选举"@zh .