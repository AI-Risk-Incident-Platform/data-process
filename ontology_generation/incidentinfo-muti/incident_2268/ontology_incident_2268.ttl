@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystemForMetroSecurity rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System for Metro Security"@en ;
    rdfs:label "地铁安全面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isAppliedWithinDomain ex:PublicTransportationSecurity ;
    airo:hasPurpose ex:EnhancingMetroSecurity ;
    airo:hasCapability ex:RealTimeFacialIdentification .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:PublicTransportationSecurity rdf:type airo:Domain ;
    rdfs:label "Public Transportation Security Domain"@en ;
    rdfs:label "公共交通安保领域"@zh .

ex:EnhancingMetroSecurity rdf:type airo:Purpose ;
    rdfs:label "Enhancing Metro Security Purpose"@en ;
    rdfs:label "加强地铁安全的目的"@zh .

ex:RealTimeFacialIdentification rdf:type airo:AICapability ;
    rdfs:label "Real-Time Facial Identification Capability"@en ;
    rdfs:label "实时面部识别能力"@zh .