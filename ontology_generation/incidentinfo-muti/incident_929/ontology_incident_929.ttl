@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BPFuelTheftDetectionSystem rdf:type airo:AISystem ;
    rdfs:label "BP Fuel Theft Detection System"@en ;
    rdfs:label "BP燃油盗窃检测系统"@zh ;
    airo:hasRisk ex:IncorrectFuelTheftIdentificationRisk ;
    airo:isDevelopedBy ex:BPCompany ;
    airo:hasStakeholder ex:AucklandMan ;
    airo:hasAISubject ex:AucklandMan ;
    airo:isAppliedWithinDomain ex:RetailFuelIndustry .

ex:IncorrectFuelTheftIdentificationRisk rdf:type airo:Risk ;
    rdfs:label "Incorrect Fuel Theft Identification Risk"@en ;
    rdfs:label "燃油盗窃错误识别风险"@zh ;
    airo:hasConsequence ex:WrongfulAccusationConsequence ;
    airo:isRiskSourceFor ex:VehicleIdentificationError .

ex:WrongfulAccusationConsequence rdf:type airo:Consequence ;
    rdfs:label "Wrongful Accusation Consequence"@en ;
    rdfs:label "错误指控后果"@zh ;
    airo:hasImpact ex:LegalAndReputationalDamageImpact .

ex:AucklandMan rdf:type airo:AISubject ;
    rdfs:label "Auckland Man"@en ;
    rdfs:label "奥克兰男子"@zh .

ex:LegalAndReputationalDamageImpact rdf:type airo:Impact ;
    rdfs:label "Legal and Reputational Damage Impact"@en ;
    rdfs:label "法律和声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:AucklandMan .

ex:VehicleIdentificationError rdf:type airo:RiskSource ;
    rdfs:label "Vehicle Identification Error Risk Source"@en ;
    rdfs:label "车辆识别错误风险源"@zh .

ex:BPCompany rdf:type airo:AIOperator ;
    rdfs:label "BP Company"@en ;
    rdfs:label "BP公司"@zh .

ex:RetailFuelIndustry rdf:type airo:Domain ;
    rdfs:label "Retail Fuel Industry Domain"@en ;
    rdfs:label "零售燃油行业领域"@zh .