@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DartmouthMedicalSchoolCheatingDetectionSystem rdf:type airo:AISystem ;
    rdfs:label "Dartmouth Medical School Cheating Detection System"@en ;
    rdfs:label "达特茅斯医学院作弊检测系统"@zh ;
    airo:hasRisk ex:AcademicIntegrityRisk ;
    airo:hasStakeholder ex:DartmouthMedicalSchoolStudents ;
    airo:isDeployedBy ex:DartmouthMedicalSchoolAdministration .

ex:AcademicIntegrityRisk rdf:type airo:Risk ;
    rdfs:label "Academic Integrity Risk"@en ;
    rdfs:label "学术诚信风险"@zh ;
    airo:hasConsequence ex:StudentDisciplinaryActions .

ex:DartmouthMedicalSchoolStudents rdf:type airo:Stakeholder ;
    rdfs:label "Dartmouth Medical School Students"@en ;
    rdfs:label "达特茅斯医学院学生"@zh .

ex:DartmouthMedicalSchoolAdministration rdf:type airo:AIOperator ;
    rdfs:label "Dartmouth Medical School Administration"@en ;
    rdfs:label "达特茅斯医学院管理部门"@zh .

ex:StudentDisciplinaryActions rdf:type airo:Consequence ;
    rdfs:label "Student Disciplinary Actions Consequence"@en ;
    rdfs:label "学生纪律处分后果"@zh ;
    airo:hasImpactOnStakeholder ex:DartmouthMedicalSchoolStudents .