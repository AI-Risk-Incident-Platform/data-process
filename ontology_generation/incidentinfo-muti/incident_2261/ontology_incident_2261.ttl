@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetPoliceFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Met Police Facial Recognition System"@en ;
    rdfs:label "伦敦警察面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:MetropolitanPoliceService ;
    airo:hasAISubject ex:LondonShoppers ;
    airo:hasCapability ex:RealTimeFacialRecognition ;
    airo:isAppliedWithinDomain ex:PublicSurveillance ;
    airo:hasStakeholder ex:MetropolitanPoliceService , ex:LondonShoppers .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PublicDistrust ;
    airo:isRiskSourceFor ex:LackOfPublicConsent ;
    airo:hasVulnerability ex:InaccurateRecognition .

ex:MetropolitanPoliceService rdf:type airo:AIOperator ;
    rdfs:label "Metropolitan Police Service"@en ;
    rdfs:label "伦敦警察厅"@zh .

ex:LondonShoppers rdf:type airo:AISubject ;
    rdfs:label "London Shoppers"@en ;
    rdfs:label "伦敦购物者"@zh .

ex:PublicDistrust rdf:type airo:Consequence ;
    rdfs:label "Public Distrust Consequence"@en ;
    rdfs:label "公众不信任后果"@zh ;
    airo:hasImpact ex:CivilLibertiesErosion .

ex:RealTimeFacialRecognition rdf:type airo:AICapability ;
    rdfs:label "Real-Time Facial Recognition Capability"@en ;
    rdfs:label "实时面部识别能力"@zh .

ex:PublicSurveillance rdf:type airo:Domain ;
    rdfs:label "Public Surveillance Domain"@en ;
    rdfs:label "公共监控领域"@zh .

ex:CivilLibertiesErosion rdf:type airo:Impact ;
    rdfs:label "Civil Liberties Erosion Impact"@en ;
    rdfs:label "公民自由侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:LondonShoppers .

ex:LackOfPublicConsent rdf:type airo:RiskSource ;
    rdfs:label "Lack of Public Consent Risk Source"@en ;
    rdfs:label "缺乏公众同意的风险源"@zh .

ex:InaccurateRecognition rdf:type airo:Vulnerability ;
    rdfs:label "Inaccurate Recognition Vulnerability"@en ;
    rdfs:label "识别不准确的脆弱性"@zh .