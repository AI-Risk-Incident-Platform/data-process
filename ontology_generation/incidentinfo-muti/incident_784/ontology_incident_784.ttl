@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeContentRecommendationSystem rdf:type airo:AISystem ;
    rdfs:label "YouTube Content Recommendation System"@en ;
    rdfs:label "YouTube内容推荐系统"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:hasStakeholder ex:YouTubeViewers ;
    airo:isDeployedBy ex:YouTube .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:FakeEndorsement .

ex:YouTubeViewers rdf:type airo:Stakeholder ;
    rdfs:label "YouTube Viewers"@en ;
    rdfs:label "YouTube观众"@zh .

ex:YouTube rdf:type airo:AIOperator ;
    rdfs:label "YouTube"@en ;
    rdfs:label "YouTube"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:TrustErosion .

ex:FakeEndorsement rdf:type airo:RiskSource ;
    rdfs:label "Fake Endorsement Risk Source"@en ;
    rdfs:label "虚假代言风险源"@zh .

ex:TrustErosion rdf:type airo:Impact ;
    rdfs:label "Trust Erosion Impact"@en ;
    rdfs:label "信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:YouTubeViewers .