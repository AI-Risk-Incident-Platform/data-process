@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChinaAIAlgorithmTrainingSystem rdf:type airo:AISystem ;
    rdfs:label "China AI Algorithm Training System"@en ;
    rdfs:label "中国AI算法训练系统"@zh ;
    airo:hasRisk ex:ExploitationOfStudentLaborRisk ;
    airo:hasStakeholder ex:StudentInterns ;
    airo:isDeployedBy ex:ChinaAICompanies .

ex:ExploitationOfStudentLaborRisk rdf:type airo:Risk ;
    rdfs:label "Exploitation of Student Labor Risk"@en ;
    rdfs:label "学生劳动剥削风险"@zh ;
    airo:isRiskSourceFor ex:LaborExploitationPractices ;
    airo:hasImpact ex:HumanRightsViolations .

ex:StudentInterns rdf:type airo:Stakeholder ;
    rdfs:label "Student Interns"@en ;
    rdfs:label "学生实习生"@zh .

ex:ChinaAICompanies rdf:type airo:AIOperator ;
    rdfs:label "China AI Companies"@en ;
    rdfs:label "中国AI公司"@zh .

ex:HumanRightsViolations rdf:type airo:Impact ;
    rdfs:label "Human Rights Violations Impact"@en ;
    rdfs:label "人权侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:StudentInterns .

ex:LaborExploitationPractices rdf:type airo:RiskSource ;
    rdfs:label "Labor Exploitation Practices Risk Source"@en ;
    rdfs:label "劳动剥削行为风险源"@zh .