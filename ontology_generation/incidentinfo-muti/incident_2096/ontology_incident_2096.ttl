@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BurgerFlippingRobot rdf:type airo:AISystem ;
    rdfs:label "Burger Flipping Robot"@en ;
    rdfs:label "汉堡翻转机器人"@zh ;
    airo:hasRisk ex:OperationalFailureRisk ;
    airo:hasStakeholder ex:BurgerChain .

ex:OperationalFailureRisk rdf:type airo:Risk ;
    rdfs:label "Operational Failure Risk"@en ;
    rdfs:label "操作失败风险"@zh ;
    airo:hasConsequence ex:JobTermination .

ex:JobTermination rdf:type airo:Consequence ;
    rdfs:label "Job Termination Consequence"@en ;
    rdfs:label "工作终止后果"@zh ;
    airo:hasImpact ex:OperationalDisruption .

ex:OperationalDisruption rdf:type airo:Impact ;
    rdfs:label "Operational Disruption Impact"@en ;
    rdfs:label "运营中断影响"@zh ;
    airo:hasImpactOnStakeholder ex:BurgerChain .

ex:BurgerChain rdf:type airo:Stakeholder ;
    rdfs:label "Burger Chain"@en ;
    rdfs:label "汉堡连锁店"@zh .