@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:HighWaterConsumptionRisk ;
    airo:hasAICapability ex:LargeScaleLanguageProcessing ;
    airo:hasPurpose ex:ConversationalAIInteraction ;
    airo:isAppliedWithinDomain ex:ArtificialIntelligenceServices ;
    airo:hasStakeholder ex:OpenAI .

ex:HighWaterConsumptionRisk rdf:type airo:Risk ;
    rdfs:label "High Water Consumption Risk"@en ;
    rdfs:label "高耗水风险"@zh ;
    airo:hasConsequence ex:EnvironmentalResourceDepletion ;
    airo:isRiskSourceFor ex:DataCenterCoolingRequirements .

ex:EnvironmentalResourceDepletion rdf:type airo:Consequence ;
    rdfs:label "Environmental Resource Depletion Consequence"@en ;
    rdfs:label "环境资源耗竭后果"@zh ;
    airo:hasImpact ex:WaterResourceStrain .

ex:LargeScaleLanguageProcessing rdf:type airo:AICapability ;
    rdfs:label "Large-Scale Language Processing Capability"@en ;
    rdfs:label "大规模语言处理能力"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:WaterResourceStrain rdf:type airo:Impact ;
    rdfs:label "Water Resource Strain Impact"@en ;
    rdfs:label "水资源紧张影响"@zh .

ex:ConversationalAIInteraction rdf:type airo:Purpose ;
    rdfs:label "Conversational AI Interaction Purpose"@en ;
    rdfs:label "对话式AI交互目的"@zh .

ex:ArtificialIntelligenceServices rdf:type airo:Domain ;
    rdfs:label "Artificial Intelligence Services Domain"@en ;
    rdfs:label "人工智能服务领域"@zh .

ex:DataCenterCoolingRequirements rdf:type airo:RiskSource ;
    rdfs:label "Data Center Cooling Requirements Risk Source"@en ;
    rdfs:label "数据中心冷却需求风险源"@zh .