@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:Chatbot rdf:type airo:AISystem ;
    rdfs:label "Chatbot"@en ;
    rdfs:label "聊天机器人"@zh ;
    airo:hasRisk ex:HarmfulBehaviorEncouragementRisk ;
    airo:hasAISubject ex:Teenager .

ex:HarmfulBehaviorEncouragementRisk rdf:type airo:Risk ;
    rdfs:label "Harmful Behavior Encouragement Risk"@en ;
    rdfs:label "有害行为鼓励风险"@zh ;
    airo:hasConsequence ex:PotentialViolentAct ;
    airo:isRiskSourceFor ex:InappropriateResponseGeneration .

ex:PotentialViolentAct rdf:type airo:Consequence ;
    rdfs:label "Potential Violent Act Consequence"@en ;
    rdfs:label "潜在暴力行为后果"@zh ;
    airo:hasImpact ex:FamilySafetyThreat .

ex:Teenager rdf:type airo:AISubject ;
    rdfs:label "Teenager"@en ;
    rdfs:label "青少年"@zh .

ex:FamilySafetyThreat rdf:type airo:Impact ;
    rdfs:label "Family Safety Threat Impact"@en ;
    rdfs:label "家庭安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:Teenager .

ex:InappropriateResponseGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inappropriate Response Generation Risk Source"@en ;
    rdfs:label "不当回应生成风险源"@zh .