@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeacherEvaluationSystem rdf:type airo:AISystem ;
    rdfs:label "Teacher Evaluation System"@en ;
    rdfs:label "教师评估系统"@zh ;
    airo:hasRisk ex:UnfairEvaluationRisk ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:hasStakeholder ex:Teachers .

ex:UnfairEvaluationRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Evaluation Risk"@en ;
    rdfs:label "不公平评估风险"@zh .

ex:Teachers rdf:type airo:Stakeholder ;
    rdfs:label "Teachers"@en ;
    rdfs:label "教师"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education"@en ;
    rdfs:label "教育"@zh .