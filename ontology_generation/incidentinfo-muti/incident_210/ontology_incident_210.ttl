@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaSecuritySystem rdf:type airo:AISystem ;
    rdfs:label "Meta Security System"@en ;
    rdfs:label "Meta安全系统"@zh ;
    airo:hasRisk ex:SocialMediaHackingRisk ;
    airo:hasStakeholder ex:MetaCompany ;
    airo:isDeployedBy ex:MetaSecurityTeam .

ex:SocialMediaHackingRisk rdf:type airo:Risk ;
    rdfs:label "Social Media Hacking Risk"@en ;
    rdfs:label "社交媒体黑客风险"@zh ;
    airo:hasConsequence ex:DisinformationSpread ;
    airo:isRiskSourceFor ex:ProRussianHackers .

ex:DisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Disinformation Spread Consequence"@en ;
    rdfs:label "虚假信息传播后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:MetaCompany rdf:type airo:Stakeholder ;
    rdfs:label "Meta Company"@en ;
    rdfs:label "Meta公司"@zh .

ex:MetaSecurityTeam rdf:type airo:AIOperator ;
    rdfs:label "Meta Security Team"@en ;
    rdfs:label "Meta安全团队"@zh .

ex:ProRussianHackers rdf:type airo:Threat ;
    rdfs:label "Pro-Russian Hackers Threat"@en ;
    rdfs:label "亲俄黑客威胁"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:MetaCompany .