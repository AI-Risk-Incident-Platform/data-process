@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TwitterAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Twitter Algorithm"@en ;
    rdfs:label "推特算法"@zh ;
    airo:hasRisk ex:AlgorithmicBiasRisk ;
    airo:hasStakeholder ex:RightwingPoliticians .

ex:AlgorithmicBiasRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias Risk"@en ;
    rdfs:label "算法偏见风险"@zh ;
    airo:hasConsequence ex:PoliticalAmplification .

ex:RightwingPoliticians rdf:type airo:Stakeholder ;
    rdfs:label "Rightwing Politicians"@en ;
    rdfs:label "右翼政客"@zh .

ex:PoliticalAmplification rdf:type airo:Consequence ;
    rdfs:label "Political Amplification"@en ;
    rdfs:label "政治内容放大"@zh ;
    airo:hasImpact ex:PublicDiscourseDistortion .

ex:PublicDiscourseDistortion rdf:type airo:Impact ;
    rdfs:label "Public Discourse Distortion"@en ;
    rdfs:label "公共话语扭曲"@zh ;
    airo:hasImpactOnStakeholder ex:RightwingPoliticians .