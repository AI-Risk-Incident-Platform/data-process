    rdfs:label "深度伪造AI生成的帕希尼扬图像"@zh ;
    airo:hasRisk ex:OnlineScamRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:FraudulentAdvertising ;
    airo:hasStakeholder ex:Authorities .

ex:OnlineScamRisk rdf:type airo:Risk ;
