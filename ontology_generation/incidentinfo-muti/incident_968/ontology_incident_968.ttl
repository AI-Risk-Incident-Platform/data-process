ex:Authorities rdf:type airo:Stakeholder ;
    rdfs:label "Authorities"@en ;
    rdfs:label "当局"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:FraudulentAdvertising rdf:type airo:Purpose ;
    rdfs:label "Fraudulent Advertising"@en ;
    rdfs:label "欺诈性广告"@zh .

ex:PublicDeception rdf:type airo:Consequence ;
    rdfs:label "Public Deception"@en ;
