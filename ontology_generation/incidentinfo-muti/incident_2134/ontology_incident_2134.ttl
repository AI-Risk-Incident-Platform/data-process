ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasPurpose ex:PublicSafetyMonitoring ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasCapability ex:FaceDetectionAndRecognition .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:PublicSafetyMonitoring rdf:type airo:Purpose ;
    rdfs:label "Public Safety Monitoring"@en ;
    rdfs:label "公共安全监控"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:FaceDetectionAndRecognition rdf:type airo:AICapability ;
    rdfs:label "Face Detection and Recognition Capability"@en ;
    rdfs:label "人脸检测与识别能力"@zh .