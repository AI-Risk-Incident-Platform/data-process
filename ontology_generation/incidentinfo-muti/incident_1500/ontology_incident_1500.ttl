@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIChatbots rdf:type airo:AISystem ;
    rdfs:label "AI Chatbots"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:InaccurateElectionInformationRisk ;
    airo:hasPurpose ex:InformationProvision ;
    airo:hasCapability ex:NaturalLanguageGeneration ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:InaccurateElectionInformationRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Election Information Risk"@en ;
    rdfs:label "不准确选举信息风险"@zh ;
    airo:hasConsequence ex:ElectionMisinformation ;
    airo:isRiskSourceFor ex:InadequateTrainingData .

ex:ElectionMisinformation rdf:type airo:Consequence ;
    rdfs:label "Election Misinformation Consequence"@en ;
    rdfs:label "选举错误信息后果"@zh ;
    airo:hasImpact ex:PublicConfusion .

ex:PublicConfusion rdf:type airo:Impact ;
    rdfs:label "Public Confusion Impact"@en ;
    rdfs:label "公众困惑影响"@zh .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:InformationProvision rdf:type airo:Purpose ;
    rdfs:label "Information Provision Purpose"@en ;
    rdfs:label "信息提供目的"@zh .

ex:InadequateTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Training Data Risk Source"@en ;
    rdfs:label "训练数据不足风险源"@zh .

ex:NaturalLanguageGeneration rdf:type airo:AICapability ;
    rdfs:label "Natural Language Generation Capability"@en ;
    rdfs:label "自然语言生成能力"@zh .