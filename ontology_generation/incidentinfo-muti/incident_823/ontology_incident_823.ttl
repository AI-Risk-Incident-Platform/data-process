@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CharacterAIChatbot rdf:type airo:AISystem ;
    rdfs:label "Character.AI Chatbot"@en ;
    rdfs:label "Character.AI聊天机器人"@zh ;
    airo:hasRisk ex:TeenSuicideRiskFromChatbotInteraction ;
    airo:hasAISubject ex:VulnerableTeenager ;
    airo:hasStakeholder ex:GrievingMother .

ex:TeenSuicideRiskFromChatbotInteraction rdf:type airo:Risk ;
    rdfs:label "Teen Suicide Risk from Chatbot Interaction"@en ;
    rdfs:label "青少年与聊天机器人互动后的自杀风险"@zh ;
    airo:hasConsequence ex:TeenSuicide ;
    airo:isRiskSourceFor ex:UnmonitoredChatbotConversations .

ex:TeenSuicide rdf:type airo:Consequence ;
    rdfs:label "Teen Suicide Consequence"@en ;
    rdfs:label "青少年自杀后果"@zh ;
    airo:hasImpact ex:FamilyTrauma .

ex:FamilyTrauma rdf:type airo:Impact ;
    rdfs:label "Family Trauma Impact"@en ;
    rdfs:label "家庭创伤影响"@zh ;
    airo:hasImpactOnStakeholder ex:GrievingMother .

ex:GrievingMother rdf:type airo:Stakeholder ;
    rdfs:label "Grieving Mother"@en ;
    rdfs:label "悲痛母亲"@zh .

ex:VulnerableTeenager rdf:type airo:AISubject ;
    rdfs:label "Vulnerable Teenager"@en ;
    rdfs:label "脆弱青少年"@zh .

ex:UnmonitoredChatbotConversations rdf:type airo:RiskSource ;
    rdfs:label "Unmonitored Chatbot Conversations"@en ;
    rdfs:label "未受监控的聊天机器人对话"@zh ;
    airo:exploitsVulnerability ex:MentalHealthVulnerability .

ex:MentalHealthVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "Mental Health Vulnerability"@en ;
    rdfs:label "心理健康脆弱性"@zh .