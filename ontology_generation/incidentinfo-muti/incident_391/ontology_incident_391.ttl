@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TikTokSearchAlgorithm rdf:type airo:AISystem ;
    rdfs:label "TikTok搜索算法"@zh ;
    rdfs:label "TikTok Search Algorithm"@en ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:hasCapability ex:ContentRecommendationAlgorithm ;
    airo:hasStakeholder ex:TikTokUsers ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatform .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "错误信息传播风险"@zh ;
    rdfs:label "Misinformation Spread Risk"@en ;
    airo:hasConsequence ex:PublicMisinformation .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "公众错误信息后果"@zh ;
    rdfs:label "Public Misinformation Consequence"@en .

ex:ContentRecommendationAlgorithm rdf:type airo:AICapability ;
    rdfs:label "内容推荐算法能力"@zh ;
    rdfs:label "Content Recommendation Algorithm Capability"@en .

ex:TikTokUsers rdf:type airo:Stakeholder ;
    rdfs:label "TikTok用户"@zh ;
    rdfs:label "TikTok Users"@en .

ex:SocialMediaPlatform rdf:type airo:Domain ;
    rdfs:label "社交媒体平台领域"@zh ;
    rdfs:label "Social Media Platform Domain"@en .