@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedArtworkSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Artwork System"@en ;
    rdfs:label "AI生成艺术作品系统"@zh ;
    airo:hasRisk ex:EthicalConcernsInAIArtUsage ;
    airo:hasStakeholder ex:BloomsburyPublishing ;
    airo:hasAISubject ex:SarahJMaasNovelReaders ;
    airo:isAppliedWithinDomain ex:PublishingIndustry ;
    airo:hasPurpose ex:BookCoverArtCreation .

ex:EthicalConcernsInAIArtUsage rdf:type airo:Risk ;
    rdfs:label "Ethical Concerns in AI Art Usage Risk"@en ;
    rdfs:label "AI艺术使用中的伦理问题风险"@zh ;
    airo:hasImpact ex:PotentialDevaluationOfHumanArtists .

ex:BloomsburyPublishing rdf:type airo:Stakeholder ;
    rdfs:label "Bloomsbury Publishing"@en ;
    rdfs:label "布鲁姆斯伯里出版社"@zh .

ex:SarahJMaasNovelReaders rdf:type airo:AISubject ;
    rdfs:label "Sarah J Maas Novel Readers"@en ;
    rdfs:label "莎拉·J·马斯小说读者"@zh .

ex:PublishingIndustry rdf:type airo:Domain ;
    rdfs:label "Publishing Industry Domain"@en ;
    rdfs:label "出版业领域"@zh .

ex:PotentialDevaluationOfHumanArtists rdf:type airo:Impact ;
    rdfs:label "Potential Devaluation of Human Artists Impact"@en ;
    rdfs:label "人类艺术家价值贬低的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BloomsburyPublishing , ex:SarahJMaasNovelReaders .

ex:BookCoverArtCreation rdf:type airo:Purpose ;
    rdfs:label "Book Cover Art Creation Purpose"@en ;
    rdfs:label "书籍封面艺术创作的目的"@zh .