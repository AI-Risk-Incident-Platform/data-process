@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LargeLanguageModels rdf:type airo:AISystem ;
    rdfs:label "Large Language Models"@en ;
    rdfs:label "大型语言模型"@zh ;
    airo:hasRisk ex:IncorrectAnswerGenerationRisk ;
    airo:hasCapability ex:TextGenerationCapability .

ex:IncorrectAnswerGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Incorrect Answer Generation Risk"@en ;
    rdfs:label "错误答案生成风险"@zh ;
    airo:hasConsequence ex:MisinformationPropagation ;
    airo:isRiskSourceFor ex:ModelSizeIncrease ;
    airo:isRiskSourceFor ex:DeceptiveConfidence .

ex:MisinformationPropagation rdf:type airo:Consequence ;
    rdfs:label "Misinformation Propagation Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:UserTrustErosion .

ex:TextGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Text Generation Capability"@en ;
    rdfs:label "文本生成能力"@zh .

ex:ModelSizeIncrease rdf:type airo:RiskSource ;
    rdfs:label "Model Size Increase Risk Source"@en ;
    rdfs:label "模型规模增大风险源"@zh ;
    airo:exploitsVulnerability ex:HumanDetectionLimitation .

ex:UserTrustErosion rdf:type airo:Impact ;
    rdfs:label "User Trust Erosion Impact"@en ;
    rdfs:label "用户信任度降低影响"@zh .

ex:DeceptiveConfidence rdf:type airo:Threat ;
    rdfs:label "Deceptive Confidence Threat"@en ;
    rdfs:label "欺骗性自信威胁"@zh ;
    airo:exploitsVulnerability ex:HumanDetectionLimitation .

ex:HumanDetectionLimitation rdf:type airo:Vulnerability ;
    rdfs:label "Human Detection Limitation Vulnerability"@en ;
    rdfs:label "人类检测局限性脆弱性"@zh .