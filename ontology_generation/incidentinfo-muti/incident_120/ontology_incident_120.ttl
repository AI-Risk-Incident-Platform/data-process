@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonFaceRecognition rdf:type airo:AISystem ;
    rdfs:label "Amazon Face Recognition System"@en ;
    rdfs:label "亚马逊人脸识别系统"@zh ;
    airo:hasRisk ex:FalsePositiveMatchingRisk ;
    airo:hasStakeholder ex:ACLU ;
    airo:hasAISubject ex:MembersOfCongress ;
    airo:hasPurpose ex:FacialIdentification ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:DeepLearningBasedFacialRecognition .

ex:FalsePositiveMatchingRisk rdf:type airo:Risk ;
    rdfs:label "False Positive Matching Risk"@en ;
    rdfs:label "误匹配风险"@zh ;
    airo:hasConsequence ex:IncorrectIdentification ;
    airo:isRiskSourceFor ex:AlgorithmBias .

ex:IncorrectIdentification rdf:type airo:Consequence ;
    rdfs:label "Incorrect Identification Consequence"@en ;
    rdfs:label "错误识别后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ACLU rdf:type airo:Stakeholder ;
    rdfs:label "American Civil Liberties Union"@en ;
    rdfs:label "美国公民自由联盟"@zh .

ex:MembersOfCongress rdf:type airo:AISubject ;
    rdfs:label "Members of Congress"@en ;
    rdfs:label "国会议员"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:ACLU , ex:MembersOfCongress .

ex:FacialIdentification rdf:type airo:Purpose ;
    rdfs:label "Facial Identification Purpose"@en ;
    rdfs:label "面部识别的目的"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:DeepLearningBasedFacialRecognition rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Facial Recognition Technique"@en ;
    rdfs:label "基于深度学习的人脸识别技术"@zh .

ex:AlgorithmBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithm Bias Risk Source"@en ;
    rdfs:label "算法偏差风险源"@zh .