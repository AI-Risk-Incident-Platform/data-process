@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GrokAI rdf:type airo:AISystem ;
    rdfs:label "Grok AI"@en ;
    rdfs:label "Grok AI"@zh ;
    airo:hasRisk ex:DisseminationOfUnverifiedInformation ;
    airo:hasStakeholder ex:ElonMusk .

ex:DisseminationOfUnverifiedInformation rdf:type airo:Risk ;
    rdfs:label "Dissemination of Unverified Information Risk"@en ;
    rdfs:label "传播未经核实信息的风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation .

ex:ElonMusk rdf:type airo:Stakeholder ;
    rdfs:label "Elon Musk"@en ;
    rdfs:label "埃隆·马斯克"@zh .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:ElonMusk .