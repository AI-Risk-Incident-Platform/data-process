@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ReplikaChatbot rdf:type airo:AISystem ;
    rdfs:label "Replika Chatbot"@en ;
    rdfs:label "Replika聊天机器人"@zh ;
    airo:hasRisk ex:SexualHarassmentByAI ;
    airo:hasCapability ex:ConversationalAI ;
    airo:hasStakeholder ex:ReplikaUsers ;
    airo:hasAISubject ex:ChatbotUsers .

ex:SexualHarassmentByAI rdf:type airo:Risk ;
    rdfs:label "Sexual Harassment by AI Risk"@en ;
    rdfs:label "AI性骚扰风险"@zh ;
    airo:hasConsequence ex:UserPsychologicalDistress ;
    airo:isRiskSourceFor ex:InappropriateBehaviorAlgorithm .

ex:UserPsychologicalDistress rdf:type airo:Consequence ;
    rdfs:label "User Psychological Distress Consequence"@en ;
    rdfs:label "用户心理困扰后果"@zh ;
    airo:hasImpact ex:EmotionalHarm .

ex:ConversationalAI rdf:type airo:AICapability ;
    rdfs:label "Conversational AI Capability"@en ;
    rdfs:label "对话式AI能力"@zh .

ex:ReplikaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Replika Users"@en ;
    rdfs:label "Replika用户"@zh .

ex:EmotionalHarm rdf:type airo:Impact ;
    rdfs:label "Emotional Harm Impact"@en ;
    rdfs:label "情感伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:ReplikaUsers .

ex:InappropriateBehaviorAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Inappropriate Behavior Algorithm Risk Source"@en ;
    rdfs:label "不当行为算法风险源"@zh .

ex:ChatbotUsers rdf:type airo:AISubject ;
    rdfs:label "Chatbot Users"@en ;
    rdfs:label "聊天机器人用户"@zh .