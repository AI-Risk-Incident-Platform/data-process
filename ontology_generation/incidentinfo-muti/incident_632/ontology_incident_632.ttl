@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Image Generation System"@en ;
    rdfs:label "深度伪造图像生成系统"@zh ;
    airo:hasRisk ex:SpreadOfExplicitFakeImagesRisk ;
    airo:hasAISubject ex:CelebrityPublicFigure ;
    airo:usesTechnique ex:DeepLearningBasedImageSynthesis ;
    airo:hasStakeholder ex:SocialMediaPlatformX .

ex:SpreadOfExplicitFakeImagesRisk rdf:type airo:Risk ;
    rdfs:label "Spread of Explicit Fake Images Risk"@en ;
    rdfs:label "传播虚假露骨图像风险"@zh ;
    airo:hasConsequence ex:PlatformContentModerationFailure ;
    airo:isRiskSourceFor ex:MaliciousContentCreators .

ex:PlatformContentModerationFailure rdf:type airo:Consequence ;
    rdfs:label "Platform Content Moderation Failure Consequence"@en ;
    rdfs:label "平台内容审核失效后果"@zh ;
    airo:hasImpact ex:ReputationDamageAndPrivacyViolation .

ex:SocialMediaPlatformX rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Platform X"@en ;
    rdfs:label "社交媒体平台X"@zh .

ex:CelebrityPublicFigure rdf:type airo:AISubject ;
    rdfs:label "Celebrity Public Figure"@en ;
    rdfs:label "名人公众人物"@zh .

ex:ReputationDamageAndPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Reputation Damage and Privacy Violation Impact"@en ;
    rdfs:label "声誉损害与隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaPlatformX , ex:CelebrityPublicFigure .

ex:DeepLearningBasedImageSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Image Synthesis Technique"@en ;
    rdfs:label "基于深度学习的图像合成技术"@zh .

ex:MaliciousContentCreators rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creators Risk Source"@en ;
    rdfs:label "恶意内容创作者风险源"@zh .