@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VirtualMeetingAI rdf:type airo:AISystem ;
    rdfs:label "Virtual Meeting AI"@en ;
    rdfs:label "虚拟会议AI"@zh ;
    airo:hasRisk ex:RacistViewsRisk ;
    airo:hasStakeholder ex:BeavertonCityCouncil .

ex:RacistViewsRisk rdf:type airo:Risk ;
    rdfs:label "Racist Views Risk"@en ;
    rdfs:label "种族主义观点风险"@zh ;
    airo:isRiskSourceFor ex:PotentialAIAbuse ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Consequence of Racist Views"@en ;
        rdfs:label "种族主义观点后果"@zh ;
        airo:hasImpact ex:PublicDiscourseDisruption
    ] .

ex:BeavertonCityCouncil rdf:type airo:Stakeholder ;
    rdfs:label "Beaverton City Council"@en ;
    rdfs:label "比弗顿市议会"@zh .

ex:PotentialAIAbuse rdf:type airo:RiskSource ;
    rdfs:label "Potential AI Abuse"@en ;
    rdfs:label "潜在AI滥用"@zh .

ex:PublicDiscourseDisruption rdf:type airo:Impact ;
    rdfs:label "Public Discourse Disruption"@en ;
    rdfs:label "公共话语干扰"@zh ;
    airo:hasImpactOnStakeholder ex:BeavertonCityCouncil .