@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIConferencePhotoEditingSystem rdf:type airo:AISystem ;
    rdfs:label "AI Conference Photo Editing System"@en ;
    rdfs:label "AI会议照片编辑系统"@zh ;
    airo:hasRisk ex:NonConsensualImageManipulationRisk ;
    airo:hasStakeholder ex:FormerGoogleAndFacebookEmployee .

ex:NonConsensualImageManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Non-Consensual Image Manipulation Risk"@en ;
    rdfs:label "未经同意的图像操纵风险"@zh ;
    airo:hasConsequence ex:InappropriateImageModification ;
    airo:hasImpact ex:PersonalPrivacyViolation .

ex:FormerGoogleAndFacebookEmployee rdf:type airo:Stakeholder ;
    rdfs:label "Former Google and Facebook Employee"@en ;
    rdfs:label "前谷歌和Facebook员工"@zh .

ex:PersonalPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Violation Impact"@en ;
    rdfs:label "个人隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:FormerGoogleAndFacebookEmployee .

ex:InappropriateImageModification rdf:type airo:Misuse ;
    rdfs:label "Inappropriate Image Modification Misuse"@en ;
    rdfs:label "不当图像修改滥用"@zh .