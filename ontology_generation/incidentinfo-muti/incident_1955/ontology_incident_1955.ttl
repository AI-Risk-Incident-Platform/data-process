ex:LevisAIModelsSystem rdf:type airo:AISystem ;
    rdfs:label "Levi's AI Models System"@en ;
    rdfs:label "Levi's AI模特系统"@zh ;
    airo:hasRisk ex:ArtificialDiversityRisk ;
    airo:hasPurpose ex:IncreaseDiversityAndSustainability ;
    airo:hasStakeholder ex:LevisCompany ;
    airo:isAppliedWithinDomain ex:FashionIndustry .

ex:ArtificialDiversityRisk rdf:type airo:Risk ;
    rdfs:label "Artificial Diversity Risk"@en ;
    rdfs:label "人工多样性风险"@zh .

ex:IncreaseDiversityAndSustainability rdf:type airo:Purpose ;
    rdfs:label "Increase Diversity and Sustainability"@en ;
    rdfs:label "增加多样性和可持续性"@zh .

ex:LevisCompany rdf:type airo:Stakeholder ;
    rdfs:label "Levi's Company"@en ;
    rdfs:label "Levi's公司"@zh .

ex:FashionIndustry rdf:type airo:Domain ;
    rdfs:label "Fashion Industry"@en ;
    rdfs:label "时尚产业"@zh .