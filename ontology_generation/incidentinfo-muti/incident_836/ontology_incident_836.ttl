@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIPoweredPhishingCallSystem rdf:type airo:AISystem ;
    rdfs:label "AI驱动的钓鱼电话系统"@zh ;
    rdfs:label "AI-Powered Phishing Call System"@en ;
    airo:hasRisk ex:PhishingScamRisk ;
    airo:hasCapability ex:VoiceSynthesisForPhishing ;
    airo:usesTechnique ex:DeepfakeVoiceTechnology ;
    airo:hasStakeholder ex:GmailUsers .

ex:PhishingScamRisk rdf:type airo:Risk ;
    rdfs:label "钓鱼诈骗风险"@zh ;
    rdfs:label "Phishing Scam Risk"@en ;
    airo:hasConsequence ex:AccountTakeover ;
    airo:isRiskSourceFor ex:AIEnabledSocialEngineering .

ex:VoiceSynthesisForPhishing rdf:type airo:AICapability ;
    rdfs:label "用于钓鱼的语音合成能力"@zh ;
    rdfs:label "Voice Synthesis for Phishing Capability"@en .

ex:GmailUsers rdf:type airo:Stakeholder ;
    rdfs:label "Gmail用户"@zh ;
    rdfs:label "Gmail Users"@en .

ex:AccountTakeover rdf:type airo:Consequence ;
    rdfs:label "账户接管后果"@zh ;
    rdfs:label "Account Takeover Consequence"@en ;
    airo:hasImpact ex:FinancialLossAndPrivacyViolation .

ex:FinancialLossAndPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "财务损失和隐私侵犯影响"@zh ;
    rdfs:label "Financial Loss and Privacy Violation Impact"@en ;
    airo:hasImpactOnStakeholder ex:GmailUsers .

ex:DeepfakeVoiceTechnology rdf:type airo:AITechnique ;
    rdfs:label "深度伪造语音技术"@zh ;
    rdfs:label "Deepfake Voice Technology"@en .

ex:AIEnabledSocialEngineering rdf:type airo:RiskSource ;
    rdfs:label "AI增强的社会工程风险源"@zh ;
    rdfs:label "AI-Enabled Social Engineering Risk Source"@en .