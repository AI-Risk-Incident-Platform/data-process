@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NormanAI rdf:type airo:AISystem ;
    rdfs:label "诺曼AI"@zh ;
    rdfs:label "Norman AI"@en ;
    airo:hasRisk ex:PsychopathicBehaviorRisk ;
    airo:hasCapability ex:ImageCaptioning ;
    airo:usesTechnique ex:DeepLearning ;
    airo:hasPurpose ex:DemonstrateDataBias ;
    airo:hasStakeholder ex:MITResearchers .

ex:PsychopathicBehaviorRisk rdf:type airo:Risk ;
    rdfs:label "精神病态行为风险"@zh ;
    rdfs:label "Psychopathic Behavior Risk"@en ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasConsequence ex:DisturbingContentGeneration .

ex:ImageCaptioning rdf:type airo:AICapability ;
    rdfs:label "图像描述能力"@zh ;
    rdfs:label "Image Captioning Capability"@en .

ex:DeepLearning rdf:type airo:AITechnique ;
    rdfs:label "深度学习技术"@zh ;
    rdfs:label "Deep Learning Technique"@en .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "有偏见的训练数据"@zh ;
    rdfs:label "Biased Training Data"@en .

ex:DisturbingContentGeneration rdf:type airo:Impact ;
    rdfs:label "生成令人不安的内容"@zh ;
    rdfs:label "Disturbing Content Generation"@en ;
    airo:hasImpactOnStakeholder ex:MITResearchers .

ex:DemonstrateDataBias rdf:type airo:Purpose ;
    rdfs:label "展示数据偏见"@zh ;
    rdfs:label "Demonstrate Data Bias Purpose"@en .

ex:MITResearchers rdf:type airo:Stakeholder ;
    rdfs:label "MIT研究人员"@zh ;
    rdfs:label "MIT Researchers"@en .