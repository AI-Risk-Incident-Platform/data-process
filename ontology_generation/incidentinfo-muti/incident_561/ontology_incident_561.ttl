@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GannettAIExperiment rdf:type airo:AISystem ;
    rdfs:label "Gannett AI Experiment"@en ;
    rdfs:label "Gannett AI实验"@zh ;
    airo:hasRisk ex:LowQualityContentGenerationRisk ;
    airo:hasStakeholder ex:GannettSportsWriter ;
    airo:isDeployedBy ex:Gannett ;
    airo:isAppliedWithinDomain ex:SportsJournalism .

ex:LowQualityContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Low Quality Content Generation Risk"@en ;
    rdfs:label "低质量内容生成风险"@zh ;
    airo:hasConsequence ex:PublicationOfInaccurateArticles .

ex:PublicationOfInaccurateArticles rdf:type airo:Consequence ;
    rdfs:label "Publication of Inaccurate Articles Consequence"@en ;
    rdfs:label "发布不准确文章的后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:GannettSportsWriter rdf:type airo:Stakeholder ;
    rdfs:label "Gannett Sports Writer"@en ;
    rdfs:label "Gannett体育记者"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:GannettSportsWriter .

ex:Gannett rdf:type airo:AIOperator ;
    rdfs:label "Gannett"@en ;
    rdfs:label "Gannett公司"@zh .

ex:SportsJournalism rdf:type airo:Domain ;
    rdfs:label "Sports Journalism Domain"@en ;
    rdfs:label "体育新闻领域"@zh .