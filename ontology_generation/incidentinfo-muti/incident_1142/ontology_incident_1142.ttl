@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BodegaAIVendingMachine rdf:type airo:AISystem ;
    rdfs:label "Bodega AI Vending Machine"@en ;
    rdfs:label "Bodega AI自动售货机"@zh ;
    airo:hasRisk ex:CulturalAppropriationRisk ;
    airo:isDevelopedBy ex:BodegaStartup ;
    airo:hasStakeholder ex:SiliconValleyStartupCommunity .

ex:CulturalAppropriationRisk rdf:type airo:Risk ;
    rdfs:label "Cultural Appropriation Risk"@en ;
    rdfs:label "文化挪用风险"@zh ;
    airo:hasImpact ex:CommunityBacklashImpact .

ex:SiliconValleyStartupCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Silicon Valley Startup Community"@en ;
    rdfs:label "硅谷创业社区"@zh .

ex:BodegaStartup rdf:type airo:AIDeveloper ;
    rdfs:label "Bodega Startup"@en ;
    rdfs:label "Bodega创业公司"@zh .

ex:CommunityBacklashImpact rdf:type airo:Impact ;
    rdfs:label "Community Backlash Impact"@en ;
    rdfs:label "社区强烈反对影响"@zh ;
    airo:hasImpactOnStakeholder ex:SiliconValleyStartupCommunity .