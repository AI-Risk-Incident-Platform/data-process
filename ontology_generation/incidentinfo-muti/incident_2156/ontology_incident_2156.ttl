ex:IBMRobotDebater rdf:type airo:AISystem ;
    rdfs:label "IBM Robot Debater"@en ;
    rdfs:label "IBM机器人辩论者"@zh ;
    airo:hasRisk ex:SocialMediaDisruptionRisk ;
    airo:hasPurpose ex:HumanDebateSimulation ;
    airo:hasCapability ex:ArgumentGeneration ;
    airo:hasStakeholder ex:SocialMediaUsers .

ex:SocialMediaDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Social Media Disruption Risk"@en ;
    rdfs:label "社交媒体破坏风险"@zh .

ex:HumanDebateSimulation rdf:type airo:Purpose ;
    rdfs:label "Human Debate Simulation"@en ;
    rdfs:label "人类辩论模拟"@zh .

ex:ArgumentGeneration rdf:type airo:AICapability ;
    rdfs:label "Argument Generation Capability"@en ;
    rdfs:label "论点生成能力"@zh .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .