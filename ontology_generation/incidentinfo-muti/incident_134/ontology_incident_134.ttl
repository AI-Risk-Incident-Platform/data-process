@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EthicalAI rdf:type airo:AISystem ;
    rdfs:label "Ethical AI"@en ;
    rdfs:label "伦理AI"@zh ;
    airo:hasRisk ex:UnethicalAIUse ;
    airo:hasPurpose ex:EthicalGuidelines ;
    airo:hasStakeholder ex:TechFirms ;
    airo:hasRiskControl ex:EthicalGuardRails .

ex:UnethicalAIUse rdf:type airo:Risk ;
    rdfs:label "Unethical AI Use Risk"@en ;
    rdfs:label "不道德AI使用风险"@zh .

ex:TechFirms rdf:type airo:Stakeholder ;
    rdfs:label "Tech Firms"@en ;
    rdfs:label "科技公司"@zh .

ex:EthicalGuidelines rdf:type airo:Purpose ;
    rdfs:label "Ethical Guidelines"@en ;
    rdfs:label "伦理指南"@zh .

ex:EthicalGuardRails rdf:type airo:RiskControl ;
    rdfs:label "Ethical Guard Rails"@en ;
    rdfs:label "伦理护栏"@zh ;
    airo:mitigatesRiskConcept ex:UnethicalAIUse .