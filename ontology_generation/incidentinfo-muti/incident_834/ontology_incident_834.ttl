@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FabricatedCNNVideoSystem rdf:type airo:AISystem ;
    rdfs:label "Fabricated CNN Video System"@en ;
    rdfs:label "伪造CNN视频系统"@zh ;
    airo:hasRisk ex:FalseCheatingClaimsRisk ;
    airo:hasStakeholder ex:GeneralPublic ;
    airo:isAppliedWithinDomain ex:MediaAndElections .

ex:FalseCheatingClaimsRisk rdf:type airo:Risk ;
    rdfs:label "False Cheating Claims Risk"@en ;
    rdfs:label "虚假作弊指控风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:AIgeneratedFakeContent .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:ElectionIntegrityThreat .

ex:ElectionIntegrityThreat rdf:type airo:Impact ;
    rdfs:label "Election Integrity Threat Impact"@en ;
    rdfs:label "选举诚信威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:AIgeneratedFakeContent rdf:type airo:RiskSource ;
    rdfs:label "AI-generated Fake Content Risk Source"@en ;
    rdfs:label "AI生成虚假内容风险源"@zh .

ex:MediaAndElections rdf:type airo:Domain ;
    rdfs:label "Media and Elections Domain"@en ;
    rdfs:label "媒体与选举领域"@zh .