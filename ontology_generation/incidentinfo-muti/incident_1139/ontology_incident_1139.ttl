ex:PricingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Pricing Algorithm"@en ;
    rdfs:label "定价算法"@zh ;
    airo:hasRisk ex:AlgorithmicPricingRisk ;
    airo:isAppliedWithinDomain ex:ECommerce ;
    airo:hasPurpose ex:DynamicPricing ;
    airo:usesTechnique ex:MachineLearningForPricing .

ex:AlgorithmicPricingRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Pricing Risk"@en ;
    rdfs:label "算法定价风险"@zh .

ex:ECommerce rdf:type airo:Domain ;
    rdfs:label "E-Commerce"@en ;
    rdfs:label "电子商务"@zh .

ex:DynamicPricing rdf:type airo:Purpose ;
    rdfs:label "Dynamic Pricing"@en ;
    rdfs:label "动态定价"@zh .

ex:MachineLearningForPricing rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning for Pricing"@en ;
    rdfs:label "用于定价的机器学习"@zh .