@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:JournalistsAndWriters .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalActionConsequence ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfCopyrightedMaterial .

ex:JournalistsAndWriters rdf:type airo:Stakeholder ;
    rdfs:label "Journalists and Writers"@en ;
    rdfs:label "记者和作家"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Action Consequence"@en ;
    rdfs:label "法律诉讼后果"@zh ;
    airo:hasImpact ex:FinancialAndReputationalDamage .

ex:UnauthorizedUseOfCopyrightedMaterial rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Use of Copyrighted Material"@en ;
    rdfs:label "未经授权使用受版权保护的材料"@zh .

ex:FinancialAndReputationalDamage rdf:type airo:Impact ;
    rdfs:label "Financial and Reputational Damage"@en ;
    rdfs:label "财务和声誉损害"@zh ;
    airo:hasImpactOnStakeholder ex:JournalistsAndWriters .