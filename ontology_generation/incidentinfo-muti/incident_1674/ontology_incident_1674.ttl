@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaChatbot rdf:type airo:AISystem ;
    rdfs:label "Meta Chatbot"@en ;
    rdfs:label "Meta聊天机器人"@zh ;
    airo:hasRisk ex:FalseAccusationRisk ;
    airo:hasAISubject ex:CollegeProfessor ;
    airo:isDevelopedBy ex:Meta .

ex:FalseAccusationRisk rdf:type airo:Risk ;
    rdfs:label "False Accusation Risk"@en ;
    rdfs:label "虚假指控风险"@zh ;
    airo:hasConsequence ex:ReputationDamage ;
    airo:isRiskSourceFor ex:AlgorithmicError .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "声誉损害后果"@zh ;
    airo:hasImpact ex:PersonalAndProfessionalHarm .

ex:CollegeProfessor rdf:type airo:AISubject ;
    rdfs:label "College Professor"@en ;
    rdfs:label "大学教授"@zh .

ex:PersonalAndProfessionalHarm rdf:type airo:Impact ;
    rdfs:label "Personal and Professional Harm Impact"@en ;
    rdfs:label "个人和职业伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:CollegeProfessor .

ex:AlgorithmicError rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Error Risk Source"@en ;
    rdfs:label "算法错误风险源"@zh .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@en ;
    rdfs:label "Meta公司"@zh .