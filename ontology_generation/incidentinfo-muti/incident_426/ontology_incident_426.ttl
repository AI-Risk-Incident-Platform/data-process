@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    rdfs:label "Tesla Full Self-Driving System"@en ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasStakeholder ex:TeslaDriver .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    airo:hasConsequence ex:MultiVehicleCollision ;
    airo:isRiskSourceFor ex:SoftwareMalfunction .

ex:MultiVehicleCollision rdf:type airo:Consequence ;
    rdfs:label "多车相撞后果"@zh ;
    rdfs:label "Multi-Vehicle Collision Consequence"@en ;
    airo:hasImpact ex:TrafficSafetyThreat .

ex:TeslaDriver rdf:type airo:AIOperator ;
    rdfs:label "特斯拉驾驶员"@zh ;
    rdfs:label "Tesla Driver"@en .

ex:SoftwareMalfunction rdf:type airo:RiskSource ;
    rdfs:label "软件故障风险源"@zh ;
    rdfs:label "Software Malfunction Risk Source"@en .

ex:TrafficSafetyThreat rdf:type airo:Impact ;
    rdfs:label "交通安全威胁影响"@zh ;
    rdfs:label "Traffic Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaDriver .