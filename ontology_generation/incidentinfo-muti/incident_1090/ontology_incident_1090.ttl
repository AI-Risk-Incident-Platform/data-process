@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilot rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot"@en ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionAndMachineLearning .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆致命风险"@zh ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    airo:hasConsequence ex:HumanFatality .

ex:HumanFatality rdf:type airo:Consequence ;
    rdfs:label "人员死亡后果"@zh ;
    rdfs:label "Human Fatality Consequence"@en ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "公共安全威胁影响"@zh ;
    rdfs:label "Public Safety Threat Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaInc .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVisionAndMachineLearning rdf:type airo:AITechnique ;
    rdfs:label "计算机视觉与机器学习技术"@zh ;
    rdfs:label "Computer Vision and Machine Learning Technique"@en .