ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en ;
    airo:hasRisk ex:MisinformationAndScamRisk ;
    airo:hasStakeholder ex:FacebookAndMicrosoft .

ex:MisinformationAndScamRisk rdf:type airo:Risk ;
    rdfs:label "虚假信息和诈骗风险"@zh ;
    rdfs:label "Misinformation and Scam Risk"@en ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:RussianScamNetwork .

ex:FacebookAndMicrosoft rdf:type airo:Stakeholder ;
    rdfs:label "Facebook和微软"@zh ;
    rdfs:label "Facebook and Microsoft"@en .

ex:RussianScamNetwork rdf:type airo:RiskSource ;
    rdfs:label "俄罗斯诈骗网络"@zh ;
    rdfs:label "Russian Scam Network"@en .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "公众误导"@zh ;
    rdfs:label "Public Misinformation"@en .