@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:InstagramAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Instagram Algorithm"@en ;
    rdfs:label "Instagram算法"@zh ;
    airo:hasRisk ex:ContentPrioritizationBiasRisk ;
    airo:hasCapability ex:ContentRecommendationAlgorithm ;
    airo:hasStakeholder ex:InstagramUsers .

ex:ContentPrioritizationBiasRisk rdf:type airo:Risk ;
    rdfs:label "Content Prioritization Bias Risk"@en ;
    rdfs:label "内容优先排序偏见风险"@zh ;
    airo:hasConsequence ex:ExposureToSensitiveContent ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInContentSelection .

ex:ContentRecommendationAlgorithm rdf:type airo:AICapability ;
    rdfs:label "Content Recommendation Algorithm Capability"@en ;
    rdfs:label "内容推荐算法能力"@zh .

ex:InstagramUsers rdf:type airo:Stakeholder ;
    rdfs:label "Instagram Users"@en ;
    rdfs:label "Instagram用户"@zh .

ex:ExposureToSensitiveContent rdf:type airo:Consequence ;
    rdfs:label "Exposure to Sensitive Content Consequence"@en ;
    rdfs:label "接触敏感内容后果"@zh .

ex:AlgorithmicBiasInContentSelection rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias in Content Selection Risk Source"@en ;
    rdfs:label "内容选择中的算法偏见风险源"@zh .