@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIDrivenCrimeSystem rdf:type airo:AISystem ;
    rdfs:label "AI-Driven Crime System"@en ;
    rdfs:label "AI驱动的犯罪系统"@zh ;
    airo:hasRisk ex:OrganizedCrimeThreat ;
    airo:hasStakeholder ex:Europol ;
    airo:isAppliedWithinDomain ex:LawEnforcement .

ex:OrganizedCrimeThreat rdf:type airo:Risk ;
    rdfs:label "Organized Crime Threat"@en ;
    rdfs:label "有组织犯罪威胁"@zh ;
    airo:hasImpact ex:HybridWarfare .

ex:Europol rdf:type airo:Stakeholder ;
    rdfs:label "Europol"@en ;
    rdfs:label "欧洲刑警组织"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement"@en ;
    rdfs:label "执法领域"@zh .

ex:HybridWarfare rdf:type airo:Impact ;
    rdfs:label "Hybrid Warfare Impact"@en ;
    rdfs:label "混合战争影响"@zh ;
    airo:hasImpactOnStakeholder ex:Europol .