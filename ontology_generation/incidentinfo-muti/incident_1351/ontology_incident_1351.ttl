@base <https://w3id.org/airo#> .

ex:MicrosoftAI rdf:type airo:AISystem ;
    rdfs:label "Microsoft AI"@en ;
    rdfs:label "微软AI"@zh ;
    airo:hasRisk ex:UnauthorizedUseOfAcademicWorkRisk ;
    airo:isDeployedBy ex:TaylorAndFrancis ;
    airo:hasStakeholder ex:AcademicAuthors .

ex:UnauthorizedUseOfAcademicWorkRisk rdf:type airo:Risk ;
    rdfs:label "Unauthorized Use of Academic Work Risk"@en ;
    rdfs:label "学术作品未经授权使用风险"@zh ;
    airo:hasConsequence ex:AcademicIntegrityUndermined .

ex:AcademicAuthors rdf:type airo:Stakeholder ;
    rdfs:label "Academic Authors"@en ;
    rdfs:label "学术作者"@zh .

ex:TaylorAndFrancis rdf:type airo:AIOperator ;
    rdfs:label "Taylor & Francis"@en ;
    rdfs:label "泰勒与弗朗西斯"@zh .

ex:AcademicIntegrityUndermined rdf:type airo:Impact ;
    rdfs:label "Academic Integrity Undermined"@en ;
    rdfs:label "学术诚信受损"@zh ;
    airo:hasImpactOnStakeholder ex:AcademicAuthors .