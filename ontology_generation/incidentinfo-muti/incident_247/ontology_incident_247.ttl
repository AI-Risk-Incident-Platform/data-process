@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LicensePlateReaderSystem rdf:type airo:AISystem ;
    rdfs:label "License Plate Reader System"@en ;
    rdfs:label "车牌识别系统"@zh ;
    airo:hasRisk ex:RacialBiasInVehicleIdentification ;
    airo:hasStakeholder ex:LawEnforcementAgencies ;
    airo:hasAISubject ex:BlackFamily .

ex:RacialBiasInVehicleIdentification rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Vehicle Identification Risk"@en ;
    rdfs:label "车辆识别中的种族偏见风险"@zh ;
    airo:isRiskSourceFor ex:FaultyAlgorithmicIdentification ;
    airo:hasConsequence ex:UnjustPoliceHarassment .

ex:LawEnforcementAgencies rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement Agencies"@en ;
    rdfs:label "执法机构"@zh .

ex:BlackFamily rdf:type airo:AISubject ;
    rdfs:label "Black Family"@en ;
    rdfs:label "黑人家庭"@zh .

ex:UnjustPoliceHarassment rdf:type airo:Consequence ;
    rdfs:label "Unjust Police Harassment Consequence"@en ;
    rdfs:label "不公正的警察骚扰后果"@zh ;
    airo:hasImpact ex:PsychologicalTrauma .

ex:PsychologicalTrauma rdf:type airo:Impact ;
    rdfs:label "Psychological Trauma Impact"@en ;
    rdfs:label "心理创伤影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackFamily .

ex:FaultyAlgorithmicIdentification rdf:type airo:RiskSource ;
    rdfs:label "Faulty Algorithmic Identification Risk Source"@en ;
    rdfs:label "错误的算法识别风险源"@zh ;
    airo:exploitsVulnerability ex:OverrelianceOnAutomatedSystems .

ex:OverrelianceOnAutomatedSystems rdf:type airo:Vulnerability ;
    rdfs:label "Overreliance on Automated Systems Vulnerability"@en ;
    rdfs:label "过度依赖自动化系统的脆弱性"@zh .