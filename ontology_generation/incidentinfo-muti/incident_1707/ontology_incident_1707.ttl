@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造生成系统"@zh ;
    rdfs:label "Deepfake Generation System"@en ;
    airo:hasRisk ex:MisinformationAndManipulationRisk ;
    airo:hasCapability ex:RealisticMediaGeneration ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:isAppliedWithinDomain ex:PoliticalCampaigning ;
    airo:hasStakeholder ex:PoliticalParties .

ex:MisinformationAndManipulationRisk rdf:type airo:Risk ;
    rdfs:label "虚假信息和操纵风险"@zh ;
    rdfs:label "Misinformation and Manipulation Risk"@en ;
    airo:hasConsequence ex:PublicMisinformation .

ex:PoliticalParties rdf:type airo:Stakeholder ;
    rdfs:label "政党"@zh ;
    rdfs:label "Political Parties"@en .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "公众误导后果"@zh ;
    rdfs:label "Public Misinformation Consequence"@en ;
    airo:hasImpact ex:PoliticalInstability .

ex:PoliticalInstability rdf:type airo:Impact ;
    rdfs:label "政治不稳定影响"@zh ;
    rdfs:label "Political Instability Impact"@en ;
    airo:hasImpactOnStakeholder ex:PoliticalParties .

ex:RealisticMediaGeneration rdf:type airo:AICapability ;
    rdfs:label "逼真媒体生成能力"@zh ;
    rdfs:label "Realistic Media Generation Capability"@en .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "生成对抗网络技术"@zh ;
    rdfs:label "Generative Adversarial Networks Technique"@en .

ex:PoliticalCampaigning rdf:type airo:Domain ;
    rdfs:label "政治竞选领域"@zh ;
    rdfs:label "Political Campaigning Domain"@en .