@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:ElectionMisinformationRisk ;
    airo:hasCapability ex:InformationVerification ;
    airo:hasStakeholder ex:GeneralPublic ;
    airo:isAppliedWithinDomain ex:InformationDissemination .

ex:ElectionMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Election Misinformation Risk"@en ;
    rdfs:label "选举错误信息风险"@zh ;
    airo:hasConsequence ex:SpreadOfFalseInformation ;
    airo:isRiskSourceFor ex:InadequateFactChecking .

ex:SpreadOfFalseInformation rdf:type airo:Consequence ;
    rdfs:label "Spread of False Information Consequence"@en ;
    rdfs:label "虚假信息传播后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:InformationVerification rdf:type airo:AICapability ;
    rdfs:label "Information Verification Capability"@en ;
    rdfs:label "信息验证能力"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:InformationDissemination rdf:type airo:Domain ;
    rdfs:label "Information Dissemination Domain"@en ;
    rdfs:label "信息传播领域"@zh .

ex:InadequateFactChecking rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Fact Checking Risk Source"@en ;
    rdfs:label "事实核查不足风险源"@zh .