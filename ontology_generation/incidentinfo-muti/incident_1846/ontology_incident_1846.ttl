@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIChatbots rdf:type airo:AISystem ;
    rdfs:label "AI Chatbots"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:VotingDisruptionRisk ;
    airo:hasAISubject ex:DisabilityCommunity ;
    airo:isAppliedWithinDomain ex:VotingSystem .

ex:VotingDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Voting Disruption Risk"@en ;
    rdfs:label "投票中断风险"@zh ;
    airo:hasImpact ex:VotingAccessDisruption .

ex:DisabilityCommunity rdf:type airo:AISubject ;
    rdfs:label "Disability Community"@en ;
    rdfs:label "残障人士群体"@zh .

ex:VotingSystem rdf:type airo:Domain ;
    rdfs:label "Voting System Domain"@en ;
    rdfs:label "投票系统领域"@zh .

ex:VotingAccessDisruption rdf:type airo:Impact ;
    rdfs:label "Voting Access Disruption Impact"@en ;
    rdfs:label "投票渠道中断的影响"@zh ;
    airo:hasImpactOnStakeholder ex:DisabilityCommunity .