@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAIMachineVisionAI rdf:type airo:AISystem ;
    rdfs:label "OpenAI Machine Vision AI"@en ;
    rdfs:label "OpenAI机器视觉AI"@zh ;
    airo:hasRisk ex:TypographicAttackVulnerabilityRisk ;
    airo:usesTechnique ex:MachineVisionTechnique ;
    airo:hasVulnerability ex:VisualDeceptionVulnerability .

ex:TypographicAttackVulnerabilityRisk rdf:type airo:Risk ;
    rdfs:label "Typographic Attack Vulnerability Risk"@en ;
    rdfs:label "排版攻击漏洞风险"@zh ;
    airo:isRiskSourceFor ex:HandwrittenNotesRiskSource .

ex:MachineVisionTechnique rdf:type airo:AITechnique ;
    rdfs:label "Machine Vision Technique"@en ;
    rdfs:label "机器视觉技术"@zh .

ex:HandwrittenNotesRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Handwritten Notes Risk Source"@en ;
    rdfs:label "手写笔记风险源"@zh ;
    airo:exploitsVulnerability ex:VisualDeceptionVulnerability .

ex:VisualDeceptionVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "Visual Deception Vulnerability"@en ;
    rdfs:label "视觉欺骗漏洞"@zh .