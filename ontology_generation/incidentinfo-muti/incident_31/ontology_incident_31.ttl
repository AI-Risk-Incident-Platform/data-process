@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutomatedEmployeeTerminationSystem rdf:type airo:AISystem ;
    rdfs:label "Automated Employee Termination System"@en ;
    rdfs:label "自动化员工解雇系统"@zh ;
    airo:hasRisk ex:UnfairTerminationRisk ;
    airo:isDeployedBy ex:UnspecifiedCompany ;
    airo:hasPurpose ex:AutomatedHRDecisionMaking ;
    airo:hasStakeholder ex:FiredEmployee ;
    airo:isAppliedWithinDomain ex:HumanResourcesManagement ;
    airo:hasVulnerability ex:LackOfHumanOversight .

ex:UnfairTerminationRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Termination Risk"@en ;
    rdfs:label "不公平解雇风险"@zh ;
    airo:hasConsequence ex:JobLossWithoutHumanReview .

ex:JobLossWithoutHumanReview rdf:type airo:Consequence ;
    rdfs:label "Job Loss Without Human Review Consequence"@en ;
    rdfs:label "未经人工审核的失业后果"@zh ;
    airo:hasImpact ex:PersonalFinancialHardship .

ex:UnspecifiedCompany rdf:type airo:AIOperator ;
    rdfs:label "Unspecified Company"@en ;
    rdfs:label "未指定公司"@zh .

ex:FiredEmployee rdf:type airo:Stakeholder ;
    rdfs:label "Fired Employee"@en ;
    rdfs:label "被解雇员工"@zh .

ex:PersonalFinancialHardship rdf:type airo:Impact ;
    rdfs:label "Personal Financial Hardship Impact"@en ;
    rdfs:label "个人财务困难影响"@zh ;
    airo:hasImpactOnStakeholder ex:FiredEmployee .

ex:AutomatedHRDecisionMaking rdf:type airo:Purpose ;
    rdfs:label "Automated HR Decision Making Purpose"@en ;
    rdfs:label "自动化人力资源决策目的"@zh .

ex:HumanResourcesManagement rdf:type airo:Domain ;
    rdfs:label "Human Resources Management Domain"@en ;
    rdfs:label "人力资源管理领域"@zh .

ex:LackOfHumanOversight rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Human Oversight Vulnerability"@en ;
    rdfs:label "缺乏人工监督的脆弱性"@zh .