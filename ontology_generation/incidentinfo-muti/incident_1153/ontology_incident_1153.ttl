@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleTranslate rdf:type airo:AISystem ;
    rdfs:label "Google Translate"@en ;
    rdfs:label "谷歌翻译"@zh ;
    airo:hasRisk ex:GenderBiasInTranslationRisk ;
    airo:hasCapability ex:GenderNeutralTranslationCapability ;
    airo:isDeployedBy ex:Google .

ex:GenderBiasInTranslationRisk rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Translation Risk"@en ;
    rdfs:label "翻译中的性别偏见风险"@zh ;
    airo:hasImpact ex:ReducedGenderStereotypingImpact .

ex:GenderNeutralTranslationCapability rdf:type airo:AICapability ;
    rdfs:label "Gender Neutral Translation Capability"@en ;
    rdfs:label "性别中立翻译能力"@zh .

ex:Google rdf:type airo:AIOperator ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:ReducedGenderStereotypingImpact rdf:type airo:Impact ;
    rdfs:label "Reduced Gender Stereotyping Impact"@en ;
    rdfs:label "减少性别刻板印象的影响"@zh .