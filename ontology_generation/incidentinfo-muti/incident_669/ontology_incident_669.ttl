ex:DeepfakeElectioneeringSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Electioneering System"@en ;
    rdfs:label "深度伪造选举系统"@zh ;
    airo:hasRisk ex:PoliticalManipulationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:PoliticalCampaigns .

ex:PoliticalManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Political Manipulation Risk"@en ;
    rdfs:label "政治操纵风险"@zh ;
    airo:hasImpact ex:PublicDeceptionImpact .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PoliticalCampaigns rdf:type airo:Stakeholder ;
    rdfs:label "Political Campaigns"@en ;
    rdfs:label "政治竞选活动"@zh .

ex:PublicDeceptionImpact rdf:type airo:Impact ;
    rdfs:label "Public Deception Impact"@en ;
    rdfs:label "公众欺骗影响"@zh .