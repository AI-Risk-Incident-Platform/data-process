@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DomesticViolencePredictionAI rdf:type airo:AISystem ;
    rdfs:label "Domestic Violence Prediction AI"@en ;
    rdfs:label "家庭暴力预测AI"@zh ;
    airo:hasRisk ex:UnreliablePredictionRisk ;
    airo:hasStakeholder ex:QueenslandPolice ;
    airo:isAppliedWithinDomain ex:LawEnforcement ;
    airo:hasPurpose ex:DomesticViolencePrevention .

ex:UnreliablePredictionRisk rdf:type airo:Risk ;
    rdfs:label "Unreliable Prediction Risk"@en ;
    rdfs:label "不可靠预测风险"@zh ;
    airo:isRiskSourceFor ex:AlgorithmicBias ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Unjust Targeting Consequence"@en ;
        rdfs:label "不公正针对后果"@zh ;
        airo:hasImpact ex:UnjustTargeting
    ] .

ex:QueenslandPolice rdf:type airo:Stakeholder ;
    rdfs:label "Queensland Police"@en ;
    rdfs:label "昆士兰警方"@zh .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "Law Enforcement Domain"@en ;
    rdfs:label "执法领域"@zh .

ex:DomesticViolencePrevention rdf:type airo:Purpose ;
    rdfs:label "Domestic Violence Prevention Purpose"@en ;
    rdfs:label "预防家庭暴力的目的"@zh .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .

ex:UnjustTargeting rdf:type airo:Impact ;
    rdfs:label "Unjust Targeting Impact"@en ;
    rdfs:label "不公正针对的影响"@zh ;
    airo:hasImpactOnStakeholder ex:QueenslandPolice .