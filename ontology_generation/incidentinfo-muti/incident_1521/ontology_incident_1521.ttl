@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AppleAIVoiceTrainingSystem rdf:type airo:AISystem ;
    rdfs:label "苹果AI语音训练系统"@zh ;
    rdfs:label "Apple AI Voice Training System"@en ;
    airo:hasRisk ex:UnauthorizedVoiceDataUsageRisk ;
    airo:hasAISubject ex:NarratorsVoices ;
    airo:hasStakeholder ex:AudiobookNarrators .

ex:UnauthorizedVoiceDataUsageRisk rdf:type airo:Risk ;
    rdfs:label "未经授权的语音数据使用风险"@zh ;
    rdfs:label "Unauthorized Voice Data Usage Risk"@en ;
    airo:isRiskSourceFor ex:PotentialUnauthorizedDataCollection ;
    airo:hasImpact ex:ProfessionalVoiceIdentityThreat .

ex:AudiobookNarrators rdf:type airo:Stakeholder ;
    rdfs:label "有声书叙述者"@zh ;
    rdfs:label "Audiobook Narrators"@en .

ex:NarratorsVoices rdf:type airo:AISubject ;
    rdfs:label "叙述者声音"@zh ;
    rdfs:label "Narrators' Voices"@en .

ex:PotentialUnauthorizedDataCollection rdf:type airo:RiskSource ;
    rdfs:label "潜在未经授权的数据收集"@zh ;
    rdfs:label "Potential Unauthorized Data Collection"@en .

ex:ProfessionalVoiceIdentityThreat rdf:type airo:Impact ;
    rdfs:label "专业声音身份威胁"@zh ;
    rdfs:label "Professional Voice Identity Threat"@en ;
    airo:hasImpactOnStakeholder ex:AudiobookNarrators .