@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeNewsSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake News System"@en ;
    rdfs:label "深度伪造新闻系统"@zh ;
    airo:hasRisk ex:BroadcastInterruptionRisk ;
    airo:hasAISubject ex:UAEUKCanadianViewers ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:IranianHackers ;
    airo:isAppliedWithinDomain ex:MediaBroadcasting .

ex:BroadcastInterruptionRisk rdf:type airo:Risk ;
    rdfs:label "Broadcast Interruption Risk"@en ;
    rdfs:label "广播中断风险"@zh ;
    airo:hasConsequence ex:BroadcastServiceDisruption .

ex:IranianHackers rdf:type airo:Stakeholder ;
    rdfs:label "Iranian Hackers"@en ;
    rdfs:label "伊朗黑客"@zh .

ex:UAEUKCanadianViewers rdf:type airo:AISubject ;
    rdfs:label "UAE UK Canadian Viewers"@en ;
    rdfs:label "阿联酋英国加拿大观众"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PublicMisinformation rdf:type airo:Impact ;
    rdfs:label "Public Misinformation Impact"@en ;
    rdfs:label "公众误信假新闻的影响"@zh ;
    airo:hasImpactOnStakeholder ex:UAEUKCanadianViewers .

ex:BroadcastServiceDisruption rdf:type airo:Consequence ;
    rdfs:label "Broadcast Service Disruption Consequence"@en ;
    rdfs:label "广播服务中断的后果"@zh ;
    airo:hasImpact ex:PublicMisinformation .

ex:MediaBroadcasting rdf:type airo:Domain ;
    rdfs:label "Media Broadcasting Domain"@en ;
    rdfs:label "媒体广播领域"@zh .