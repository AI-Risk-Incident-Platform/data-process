ex:OutbackSteakhouseAIVideoMonitoringSystem rdf:type airo:AISystem ;
    rdfs:label "Outback Steakhouse AI Video Monitoring System"@en ;
    rdfs:label "Outback Steakhouse AI视频监控系统"@zh ;
    airo:hasRisk ex:PrivacyConcernsFromVideoMonitoring ;
    airo:isAppliedWithinDomain ex:RestaurantIndustry ;
    airo:hasPurpose ex:EnhancingRestaurantOperations ;
    airo:hasStakeholder ex:RestaurantCustomers .

ex:PrivacyConcernsFromVideoMonitoring rdf:type airo:Risk ;
    rdfs:label "Privacy Concerns from Video Monitoring Risk"@en ;
    rdfs:label "视频监控引发的隐私担忧风险"@zh .

ex:RestaurantIndustry rdf:type airo:Domain ;
    rdfs:label "Restaurant Industry"@en ;
    rdfs:label "餐饮行业"@zh .

ex:EnhancingRestaurantOperations rdf:type airo:Purpose ;
    rdfs:label "Enhancing Restaurant Operations"@en ;
    rdfs:label "提升餐厅运营效率"@zh .

ex:RestaurantCustomers rdf:type airo:Stakeholder ;
    rdfs:label "Restaurant Customers"@en ;
    rdfs:label "餐厅顾客"@zh .