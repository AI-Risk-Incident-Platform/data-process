@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIChildSexualAbuseImageGenerator rdf:type airo:AISystem ;
    rdfs:label "AI Child Sexual Abuse Image Generator"@en ;
    rdfs:label "AI儿童性虐待图像生成器"@zh ;
    airo:hasRisk ex:ChildExploitationRisk ;
    airo:usesTechnique ex:DeepfakeImageGeneration ;
    airo:hasPurpose ex:IllicitContentCreation ;
    airo:hasAISubject ex:Minors ;
    airo:hasStakeholder ex:LawEnforcementAgencies .

ex:ChildExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Risk"@en ;
    rdfs:label "儿童剥削风险"@zh ;
    airo:hasConsequence ex:NormalizationOfChildSexualAbuse ;
    airo:isRiskSourceFor ex:AIEnabledContentGeneration ;
    airo:hasRiskSource ex:AIEnabledContentGeneration ;
    airo:hasMisuse ex:AIForIllegalContentCreation .

ex:NormalizationOfChildSexualAbuse rdf:type airo:Consequence ;
    rdfs:label "Normalization of Child Sexual Abuse Consequence"@en ;
    rdfs:label "儿童性虐待正常化后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToChildren .

ex:DeepfakeImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Image Generation Technique"@en ;
    rdfs:label "深度伪造图像生成技术"@zh .

ex:LawEnforcementAgencies rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement Agencies"@en ;
    rdfs:label "执法机构"@zh .

ex:Minors rdf:type airo:AISubject ;
    rdfs:label "Minors"@en ;
    rdfs:label "未成年人"@zh .

ex:PsychologicalHarmToChildren rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Children Impact"@en ;
    rdfs:label "对儿童的心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:Minors .

ex:IllicitContentCreation rdf:type airo:Purpose ;
    rdfs:label "Illicit Content Creation Purpose"@en ;
    rdfs:label "非法内容创作目的"@zh .

ex:AIEnabledContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "AI-Enabled Content Generation Risk Source"@en ;
    rdfs:label "AI赋能内容生成风险源"@zh .

ex:AIForIllegalContentCreation rdf:type airo:Misuse ;
    rdfs:label "AI for Illegal Content Creation Misuse"@en ;
    rdfs:label "AI用于非法内容创作的滥用"@zh .