@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIFaceSwappingTechnology rdf:type airo:AISystem ;
    rdfs:label "AI Face Swapping Technology"@en ;
    rdfs:label "AI换脸技术"@zh ;
    airo:hasRisk ex:FraudRiskUsingAIFaceSwapping ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:HangzhouPublicSecurityBureau .

ex:FraudRiskUsingAIFaceSwapping rdf:type airo:Risk ;
    rdfs:label "Fraud Risk Using AI Face Swapping"@en ;
    rdfs:label "使用AI换脸进行欺诈的风险"@zh ;
    airo:hasConsequence ex:IdentityFraud .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:IdentityFraud rdf:type airo:Consequence ;
    rdfs:label "Identity Fraud Consequence"@en ;
    rdfs:label "身份欺诈后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任度下降的影响"@zh ;
    airo:hasImpactOnStakeholder ex:HangzhouPublicSecurityBureau .

ex:HangzhouPublicSecurityBureau rdf:type airo:Stakeholder ;
    rdfs:label "Hangzhou Public Security Bureau"@en ;
    rdfs:label "杭州市公安局"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .