@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SportsIllustratedAIWriters rdf:type airo:AISystem ;
    rdfs:label "体育画报AI作者"@zh ;
    rdfs:label "Sports Illustrated AI Writers"@en ;
    airo:hasRisk ex:MisrepresentationRisk ;
    airo:hasStakeholder ex:RossLevinsohn ;
    airo:isDeployedBy ex:SportsIllustratedPublisher .

ex:MisrepresentationRisk rdf:type airo:Risk ;
    rdfs:label "虚假陈述风险"@zh ;
    rdfs:label "Misrepresentation Risk"@en ;
    airo:hasConsequence ex:CEOJobLoss .

ex:RossLevinsohn rdf:type airo:Stakeholder ;
    rdfs:label "罗斯·莱文索恩"@zh ;
    rdfs:label "Ross Levinsohn"@en .

ex:CEOJobLoss rdf:type airo:Consequence ;
    rdfs:label "CEO职位丢失后果"@zh ;
    rdfs:label "CEO Job Loss Consequence"@en ;
    airo:hasImpact ex:ReputationDamage .

ex:SportsIllustratedPublisher rdf:type airo:AIOperator ;
    rdfs:label "体育画报出版商"@zh ;
    rdfs:label "Sports Illustrated Publisher"@en .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "声誉损害影响"@zh ;
    rdfs:label "Reputation Damage Impact"@en ;
    airo:hasImpactOnStakeholder ex:RossLevinsohn , ex:SportsIllustratedPublisher .