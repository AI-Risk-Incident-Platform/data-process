@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ScriptTrainingAI rdf:type airo:AISystem ;
    rdfs:label "Script Training AI"@en ;
    rdfs:label "剧本训练AI"@zh ;
    airo:hasRisk ex:UnauthorizedUseOfCreativeWorks ;
    airo:hasStakeholder ex:TVWriters .

ex:UnauthorizedUseOfCreativeWorks rdf:type airo:Risk ;
    rdfs:label "Unauthorized Use of Creative Works Risk"@en ;
    rdfs:label "未经授权使用创意作品风险"@zh ;
    airo:isRiskSourceFor ex:UnconsentedDataCollection ;
    airo:hasConsequence [ 
        rdf:type airo:Consequence ;
        airo:hasImpact ex:CreativeRightsViolation 
    ] .

ex:TVWriters rdf:type airo:Stakeholder ;
    rdfs:label "TV Writers"@en ;
    rdfs:label "电视编剧"@zh .

ex:UnconsentedDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Unconsented Data Collection Risk Source"@en ;
    rdfs:label "未经同意的数据收集风险源"@zh .

ex:CreativeRightsViolation rdf:type airo:Impact ;
    rdfs:label "Creative Rights Violation Impact"@en ;
    rdfs:label "创作权利侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:TVWriters .