@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SepsisPredictionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Sepsis Prediction Algorithm"@en ;
    rdfs:label "败血症预测算法"@zh ;
    airo:hasRisk ex:InaccurateSepsisPredictionRisk .

ex:InaccurateSepsisPredictionRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Sepsis Prediction Risk"@en ;
    rdfs:label "败血症预测不准确风险"@zh ;
    airo:hasConsequence ex:MissedDiagnosisOfSepsisCases ;
    airo:isRiskSourceFor ex:AlgorithmicInaccuracy .

ex:MissedDiagnosisOfSepsisCases rdf:type airo:Consequence ;
    rdfs:label "Missed Diagnosis of Sepsis Cases Consequence"@en ;
    rdfs:label "败血症病例漏诊后果"@zh ;
    airo:hasImpact ex:PatientSafetyThreat .

ex:PatientSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Patient Safety Threat Impact"@en ;
    rdfs:label "患者安全威胁影响"@zh .

ex:AlgorithmicInaccuracy rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Inaccuracy Risk Source"@en ;
    rdfs:label "算法不准确性风险源"@zh .