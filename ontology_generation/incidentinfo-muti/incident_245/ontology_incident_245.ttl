@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IndustrialRobot rdf:type airo:AISystem ;
    rdfs:label "Industrial Robot"@en ;
    rdfs:label "工业机器人"@zh ;
    airo:hasRisk ex:WorkplaceSafetyRisk ;
    airo:hasAISubject ex:FactoryEmployee ;
    airo:isAppliedWithinDomain ex:ManufacturingIndustryDomain .

ex:WorkplaceSafetyRisk rdf:type airo:Risk ;
    rdfs:label "Workplace Safety Risk"@en ;
    rdfs:label "工作场所安全风险"@zh ;
    airo:hasConsequence ex:EmployeeFatality ;
    airo:isRiskSourceFor ex:RobotMalfunctionOrHumanError .

ex:EmployeeFatality rdf:type airo:Consequence ;
    rdfs:label "Employee Fatality Consequence"@en ;
    rdfs:label "员工死亡后果"@zh ;
    airo:hasImpact ex:LossOfHumanLife .

ex:FactoryEmployee rdf:type airo:AISubject ;
    rdfs:label "Factory Employee"@en ;
    rdfs:label "工厂员工"@zh .

ex:LossOfHumanLife rdf:type airo:Impact ;
    rdfs:label "Loss of Human Life Impact"@en ;
    rdfs:label "人员生命损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:FactoryEmployee .

ex:ManufacturingIndustryDomain rdf:type airo:Domain ;
    rdfs:label "Manufacturing Industry Domain"@en ;
    rdfs:label "制造业领域"@zh .

ex:RobotMalfunctionOrHumanError rdf:type airo:RiskSource ;
    rdfs:label "Robot Malfunction or Human Error Risk Source"@en ;
    rdfs:label "机器人故障或人为错误风险源"@zh .