@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasRisk ex:FalsePositiveIdentificationRisk ;
    airo:isDeployedBy ex:FacialRecognitionSystemOperator ;
    airo:hasStakeholder ex:UKPolice ;
    airo:isAppliedWithinDomain ex:PublicSecurity .

ex:FalsePositiveIdentificationRisk rdf:type airo:Risk ;
    rdfs:label "False Positive Identification Risk"@en ;
    rdfs:label "误识别风险"@zh ;
    airo:hasConsequence ex:InnocentPeopleMisidentified ;
    airo:isRiskSourceFor ex:HighFalsePositiveRate .

ex:InnocentPeopleMisidentified rdf:type airo:Consequence ;
    rdfs:label "Innocent People Misidentified"@en ;
    rdfs:label "无辜人员被误识别"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:UKPolice rdf:type airo:Stakeholder ;
    rdfs:label "UK Police"@en ;
    rdfs:label "英国警方"@zh .

ex:FacialRecognitionSystemOperator rdf:type airo:AIOperator ;
    rdfs:label "Facial Recognition System Operator"@en ;
    rdfs:label "面部识别系统运营商"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任受损"@zh ;
    airo:hasImpactOnStakeholder ex:UKPolice .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security"@en ;
    rdfs:label "公共安全"@zh .

ex:HighFalsePositiveRate rdf:type airo:RiskSource ;
    rdfs:label "High False Positive Rate"@en ;
    rdfs:label "高误报率"@zh .