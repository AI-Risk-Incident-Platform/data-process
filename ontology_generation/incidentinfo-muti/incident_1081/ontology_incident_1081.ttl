@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:EmotionalBondRisk ;
    airo:hasCapability ex:VoiceToVoiceWithVision .

ex:EmotionalBondRisk rdf:type airo:Risk ;
    rdfs:label "Emotional Bond Risk"@en ;
    rdfs:label "情感纽带风险"@zh ;
    airo:hasImpact ex:HumanLikeInteractionImpact .

ex:VoiceToVoiceWithVision rdf:type airo:AICapability ;
    rdfs:label "Voice-to-Voice With Vision Capability"@en ;
    rdfs:label "带视觉的语音到语音能力"@zh .

ex:HumanLikeInteractionImpact rdf:type airo:Impact ;
    rdfs:label "Human-like Interaction Impact"@en ;
    rdfs:label "类人交互影响"@zh .