@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NYPDArmedRobots rdf:type airo:AISystem ;
    rdfs:label "NYPD Armed Robots"@en ;
    rdfs:label "纽约警察局武装机器人"@zh ;
    airo:hasRisk ex:PublicSafetyRiskFromArmedRobots ;
    airo:isDeployedBy ex:NYPD ;
    airo:hasStakeholder ex:NewYorkLawmaker .

ex:PublicSafetyRiskFromArmedRobots rdf:type airo:Risk ;
    rdfs:label "Public Safety Risk from Armed Robots"@en ;
    rdfs:label "武装机器人带来的公共安全风险"@zh ;
    airo:hasConsequence ex:PublicDistrustInLawEnforcement .

ex:NewYorkLawmaker rdf:type airo:Stakeholder ;
    rdfs:label "New York Lawmaker"@en ;
    rdfs:label "纽约立法者"@zh .

ex:NYPD rdf:type airo:AIOperator ;
    rdfs:label "NYPD"@en ;
    rdfs:label "纽约警察局"@zh .

ex:PublicDistrustInLawEnforcement rdf:type airo:Impact ;
    rdfs:label "Public Distrust in Law Enforcement Impact"@en ;
    rdfs:label "公众对执法的不信任影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewYorkLawmaker .