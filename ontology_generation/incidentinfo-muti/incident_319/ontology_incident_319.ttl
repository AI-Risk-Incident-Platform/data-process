@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasPurpose ex:AutonomousDrivingAssistance ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionBasedNavigation ;
    airo:hasStakeholder ex:EmergencyServiceProviders , ex:TeslaVehicleOccupants ;
    airo:hasAISubject ex:TeslaVehicleOccupants ;
    airo:isDevelopedBy ex:TeslaInc ;
    airo:hasVulnerability ex:StationaryObjectDetectionFailure .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    airo:hasConsequence ex:VehicleDamage ;
    airo:isRiskSourceFor ex:SensorLimitations , ex:DriverOverrelianceOnAutopilot .

ex:VehicleDamage rdf:type airo:Consequence ;
    rdfs:label "车辆损坏后果"@zh ;
    rdfs:label "Vehicle Damage Consequence"@en ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:EmergencyServiceProviders rdf:type airo:Stakeholder ;
    rdfs:label "紧急服务提供者"@zh ;
    rdfs:label "Emergency Service Providers"@en .

ex:TeslaVehicleOccupants rdf:type airo:AISubject ;
    rdfs:label "特斯拉车辆乘员"@zh ;
    rdfs:label "Tesla Vehicle Occupants"@en .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "公共安全威胁"@zh ;
    rdfs:label "Public Safety Threat"@en ;
    airo:hasImpactOnStakeholder ex:EmergencyServiceProviders , ex:TeslaVehicleOccupants .

ex:AutonomousDrivingAssistance rdf:type airo:Purpose ;
    rdfs:label "自动驾驶辅助目的"@zh ;
    rdfs:label "Autonomous Driving Assistance Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVisionBasedNavigation rdf:type airo:AITechnique ;
    rdfs:label "基于计算机视觉的导航技术"@zh ;
    rdfs:label "Computer Vision Based Navigation Technique"@en .

ex:StationaryObjectDetectionFailure rdf:type airo:Vulnerability ;
    rdfs:label "静止物体检测失败的脆弱性"@zh ;
    rdfs:label "Stationary Object Detection Failure Vulnerability"@en .

ex:SensorLimitations rdf:type airo:RiskSource ;
    rdfs:label "传感器限制的风险源"@zh ;
    rdfs:label "Sensor Limitations Risk Source"@en .

ex:DriverOverrelianceOnAutopilot rdf:type airo:RiskSource ;
    rdfs:label "驾驶员过度依赖自动驾驶的威胁"@zh ;
    rdfs:label "Driver Overreliance on Autopilot Threat"@en .

ex:AutonomousVehicleSafetyConcerns rdf:type airo:RiskConcept ;
    rdfs:label "自动驾驶车辆安全问题"@zh ;
    rdfs:label "Autonomous Vehicle Safety Concerns Risk Concept"@en .