@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIVoiceScamSystem rdf:type airo:AISystem ;
    rdfs:label "AI Voice Scam System"@en ;
    rdfs:label "AI语音诈骗系统"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:isDevelopedBy ex:ScamOrganizers ;
    airo:hasStakeholder ex:BusinessLeaders .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:BusinessLeaders rdf:type airo:Stakeholder ;
    rdfs:label "Business Leaders"@en ;
    rdfs:label "商业领袖"@zh .

ex:ScamOrganizers rdf:type airo:AIOperator ;
    rdfs:label "Scam Organizers"@en ;
    rdfs:label "诈骗组织者"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpactOnStakeholder ex:BusinessLeaders .