ex:ProChinaPropagandaAI rdf:type airo:AISystem ;
    rdfs:label "Pro-China Propaganda AI System"@en ;
    rdfs:label "亲华宣传AI系统"@zh ;
    airo:hasRisk ex:DisinformationRisk ;
    airo:usesTechnique ex:AIGeneratedImages ;
