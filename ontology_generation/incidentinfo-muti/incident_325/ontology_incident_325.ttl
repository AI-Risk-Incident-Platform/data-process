    rdfs:label "AI-Generated Images Technique"@en ;
    rdfs:label "AI生成图像技术"@zh .

ex:ChineseGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Chinese Government"@en ;
    rdfs:label "中国政府"@zh .

ex:PublicOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "Public Opinion Manipulation Impact"@en ;
    rdfs:label "舆论操纵影响"@zh .