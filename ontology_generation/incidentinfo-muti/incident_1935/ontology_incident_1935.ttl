@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedImage rdf:type airo:AISystem ;
    rdfs:label "AI Generated Image"@en ;
    rdfs:label "AI生成图像"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:hasStakeholder ex:GeneralPublic .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:isRiskSourceFor ex:ImageManipulation ;
    airo:hasConsequence ex:PublicMisinformation .

ex:ImageManipulation rdf:type airo:RiskSource ;
    rdfs:label "Image Manipulation"@en ;
    rdfs:label "图像操纵"@zh .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation"@en ;
    rdfs:label "公众误导"@zh ;
    airo:hasImpact ex:ConflictMisrepresentation .

ex:ConflictMisrepresentation rdf:type airo:Impact ;
    rdfs:label "Conflict Misrepresentation"@en ;
    rdfs:label "冲突误报"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .