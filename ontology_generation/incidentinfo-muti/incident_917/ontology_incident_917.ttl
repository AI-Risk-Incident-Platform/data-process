ex:AIGeneratedTrumpGoldInvestmentScam rdf:type airo:AISystem ;
    rdfs:label "AI Generated Trump Gold Investment Scam"@en ;
    rdfs:label "AI生成的特朗普黄金投资骗局"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:DeceptiveFinancialGain ;
    airo:hasStakeholder ex:ScamVictim ;
    airo:isAppliedWithinDomain ex:FinancialInvestment .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:AIGeneratedContent .

ex:ScamVictim rdf:type airo:Stakeholder ;
    rdfs:label "Scam Victim"@en ;
    rdfs:label "诈骗受害者"@zh .

ex:AIGeneratedContent rdf:type airo:RiskSource ;
    rdfs:label "AI Generated Content"@en ;
    rdfs:label "AI生成内容"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpactOnStakeholder ex:ScamVictim .

ex:FinancialInvestment rdf:type airo:Domain ;
    rdfs:label "Financial Investment Domain"@en ;
    rdfs:label "金融投资领域"@zh .

ex:DeceptiveFinancialGain rdf:type airo:Purpose ;
    rdfs:label "Deceptive Financial Gain Purpose"@en ;
    rdfs:label "欺骗性财务收益目的"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .