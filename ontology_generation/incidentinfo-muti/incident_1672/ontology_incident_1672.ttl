@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ZoomAISystem rdf:type airo:AISystem ;
    rdfs:label "Zoom AI System"@en ;
    rdfs:label "Zoom AI系统"@zh ;
    airo:hasRisk ex:PrivacyConcernsRisk ;
    airo:hasPurpose ex:AITraining ;
    airo:isDevelopedBy ex:ZoomTechnologies ;
    airo:hasStakeholder ex:ZoomUsers .

ex:PrivacyConcernsRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Concerns Risk"@en ;
    rdfs:label "隐私担忧风险"@zh ;
    airo:isRiskSourceFor ex:PrivateDataCollection ;
    airo:hasConsequence [
        rdf:type airo:Consequence ;
        rdfs:label "Privacy Consequence"@en ;
        rdfs:label "隐私后果"@zh ;
        airo:hasImpact ex:ConsumerTrustLoss
    ] .

ex:ZoomUsers rdf:type airo:Stakeholder ;
    rdfs:label "Zoom Users"@en ;
    rdfs:label "Zoom用户"@zh .

ex:PrivateDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Private Data Collection"@en ;
    rdfs:label "私人数据收集"@zh .

ex:ConsumerTrustLoss rdf:type airo:Impact ;
    rdfs:label "Consumer Trust Loss"@en ;
    rdfs:label "消费者信任丧失"@zh ;
    airo:hasImpactOnStakeholder ex:ZoomUsers .

ex:AITraining rdf:type airo:Purpose ;
    rdfs:label "AI Training"@en ;
    rdfs:label "AI训练"@zh .

ex:ZoomTechnologies rdf:type airo:AIOperator ;
    rdfs:label "Zoom Technologies"@en ;
    rdfs:label "Zoom科技公司"@zh .