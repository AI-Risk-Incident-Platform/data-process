@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HuaweiUighurDetectionSystem rdf:type airo:AISystem ;
    rdfs:label "Huawei Uighur Detection System"@en ;
    rdfs:label "华为维吾尔族检测系统"@zh ;
    airo:hasRisk ex:EthnicDiscriminationRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:SurveillanceGroup ;
    airo:hasImpactOnStakeholder ex:SurveillanceGroup .

ex:EthnicDiscriminationRisk rdf:type airo:Risk ;
    rdfs:label "Ethnic Discrimination Risk"@en ;
    rdfs:label "种族歧视风险"@zh ;
    airo:hasImpact ex:HumanRightsViolation .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:SurveillanceGroup rdf:type airo:Stakeholder ;
    rdfs:label "Surveillance Group"@en ;
    rdfs:label "监控组织"@zh .

ex:HumanRightsViolation rdf:type airo:Impact ;
    rdfs:label "Human Rights Violation"@en ;
    rdfs:label "人权侵犯"@zh .