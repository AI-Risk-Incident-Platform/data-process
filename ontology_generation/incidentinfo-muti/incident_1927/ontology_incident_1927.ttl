@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GenerativeAIImageGenerator rdf:type airo:AISystem ;
    rdfs:label "Generative AI Image Generator"@en ;
    rdfs:label "生成式AI图像生成器"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:usesTechnique ex:ImageGenerationModel .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalDispute ;
    airo:isRiskSourceFor ex:TrainingDataExtraction .

ex:LegalDispute rdf:type airo:Consequence ;
    rdfs:label "Legal Dispute Consequence"@en ;
    rdfs:label "法律纠纷后果"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:ImageGenerationModel rdf:type airo:AITechnique ;
    rdfs:label "Image Generation Model Technique"@en ;
    rdfs:label "图像生成模型技术"@zh .

ex:TrainingDataExtraction rdf:type airo:RiskSource ;
    rdfs:label "Training Data Extraction Risk Source"@en ;
    rdfs:label "训练数据提取风险源"@zh .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh .