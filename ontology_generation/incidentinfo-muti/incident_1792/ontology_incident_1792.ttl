ex:FacebookNewsBanSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook News Ban System"@en ;
    rdfs:label "Facebook新闻禁令系统"@zh ;
    airo:hasRisk ex:NegotiationTacticRisk ;
    airo:hasStakeholder ex:FacebookCompany ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatform ;
    airo:isDeployedBy ex:FacebookPlatform .

ex:NegotiationTacticRisk rdf:type airo:Risk ;
    rdfs:label "Negotiation Tactic Risk"@en ;
    rdfs:label "谈判策略风险"@zh .

ex:FacebookCompany rdf:type airo:Stakeholder ;
    rdfs:label "Facebook Company"@en ;
    rdfs:label "Facebook公司"@zh .

ex:FacebookPlatform rdf:type airo:AIOperator ;
    rdfs:label "Facebook Platform"@en ;
    rdfs:label "Facebook平台"@zh .

ex:SocialMediaPlatform rdf:type airo:Domain ;
    rdfs:label "Social Media Platform"@en ;
    rdfs:label "社交媒体平台"@zh .