@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleNewsAI rdf:type airo:AISystem ;
    rdfs:label "Google News AI"@en ;
    rdfs:label "谷歌新闻AI"@zh ;
    airo:hasRisk ex:LowQualityContentRisk ;
    airo:hasAICapability ex:ContentGeneration ;
    airo:hasStakeholder ex:NewsConsumers ;
    airo:isProvidedBy ex:Google ;
    airo:hasPurpose ex:AutomatedNewsAggregation ;
    airo:isAppliedWithinDomain ex:DigitalMedia .

ex:LowQualityContentRisk rdf:type airo:Risk ;
    rdfs:label "Low Quality Content Risk"@en ;
    rdfs:label "低质量内容风险"@zh ;
    airo:hasConsequence ex:ReducedInformationQuality ;
    airo:isRiskSourceFor ex:UnverifiedContentGeneration .

ex:ReducedInformationQuality rdf:type airo:Consequence ;
    rdfs:label "Reduced Information Quality Consequence"@en ;
    rdfs:label "信息质量下降后果"@zh ;
    airo:hasImpact ex:UserTrustErosion .

ex:ContentGeneration rdf:type airo:AICapability ;
    rdfs:label "Content Generation Capability"@en ;
    rdfs:label "内容生成能力"@zh .

ex:NewsConsumers rdf:type airo:Stakeholder ;
    rdfs:label "News Consumers"@en ;
    rdfs:label "新闻消费者"@zh .

ex:Google rdf:type airo:AIProvider ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:UserTrustErosion rdf:type airo:Impact ;
    rdfs:label "User Trust Erosion Impact"@en ;
    rdfs:label "用户信任度降低影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewsConsumers .

ex:AutomatedNewsAggregation rdf:type airo:Purpose ;
    rdfs:label "Automated News Aggregation Purpose"@en ;
    rdfs:label "自动化新闻聚合目的"@zh .

ex:DigitalMedia rdf:type airo:Domain ;
    rdfs:label "Digital Media Domain"@en ;
    rdfs:label "数字媒体领域"@zh .

ex:UnverifiedContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unverified Content Generation Risk Source"@en ;
    rdfs:label "未经验证的内容生成风险源"@zh .