@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LicencePlateRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Licence Plate Recognition System"@en ;
    rdfs:label "车牌识别系统"@zh ;
    airo:hasRisk ex:InaccurateParkingFinesRisk ;
    airo:hasAISubject ex:ParentsDroppingOffChildren .

ex:InaccurateParkingFinesRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Parking Fines Risk"@en ;
    rdfs:label "不准确停车罚款风险"@zh ;
    airo:hasConsequence ex:UnjustFinancialPenalty ;
    airo:isRiskSourceFor ex:CameraInaccuracy .

ex:UnjustFinancialPenalty rdf:type airo:Consequence ;
    rdfs:label "Unjust Financial Penalty Consequence"@en ;
    rdfs:label "不公正经济处罚后果"@zh .

ex:ParentsDroppingOffChildren rdf:type airo:AISubject ;
    rdfs:label "Parents Dropping Off Children"@en ;
    rdfs:label "接送孩子的家长"@zh .

ex:CameraInaccuracy rdf:type airo:RiskSource ;
    rdfs:label "Camera Inaccuracy Risk Source"@en ;
    rdfs:label "摄像头不准确风险源"@zh .