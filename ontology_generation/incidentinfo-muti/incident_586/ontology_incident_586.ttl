@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedNewsWebsites rdf:type airo:AISystem ;
    rdfs:label "AI生成新闻网站"@zh ;
    rdfs:label "AI Generated News Websites"@en ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:usesTechnique ex:NaturalLanguageGeneration ;
    airo:hasStakeholder ex:NewsConsumers .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "虚假信息传播风险"@zh ;
    rdfs:label "Misinformation Spread Risk"@en ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:AutomatedContentGeneration .

ex:AutomatedContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "自动化内容生成"@zh ;
    rdfs:label "Automated Content Generation"@en .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "公众虚假信息"@zh ;
    rdfs:label "Public Misinformation"@en ;
    airo:hasImpact ex:TrustErosionInMedia .

ex:TrustErosionInMedia rdf:type airo:Impact ;
    rdfs:label "媒体信任度下降"@zh ;
    rdfs:label "Trust Erosion in Media"@en ;
    airo:hasImpactOnStakeholder ex:NewsConsumers .

ex:NaturalLanguageGeneration rdf:type airo:AITechnique ;
    rdfs:label "自然语言生成"@zh ;
    rdfs:label "Natural Language Generation"@en .

ex:NewsConsumers rdf:type airo:Stakeholder ;
    rdfs:label "新闻消费者"@zh ;
    rdfs:label "News Consumers"@en .