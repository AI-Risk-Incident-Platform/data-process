@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ShoppingMallRobot rdf:type airo:AISystem ;
    rdfs:label "Shopping Mall Robot"@en ;
    rdfs:label "商场机器人"@zh ;
    airo:hasRisk ex:PhysicalHarmToPassengersRisk ;
    airo:hasAISubject ex:MallVisitors .

ex:PhysicalHarmToPassengersRisk rdf:type airo:Risk ;
    rdfs:label "Physical Harm to Passengers Risk"@en ;
    rdfs:label "对乘客造成身体伤害的风险"@zh ;
    airo:hasConsequence ex:PassengersInjuredConsequence ;
    airo:isRiskSourceFor ex:EscalatorFallRiskSource .

ex:PassengersInjuredConsequence rdf:type airo:Consequence ;
    rdfs:label "Passengers Injured Consequence"@en ;
    rdfs:label "乘客受伤的后果"@zh ;
    airo:hasImpact ex:PhysicalInjuryImpact .

ex:PhysicalInjuryImpact rdf:type airo:Impact ;
    rdfs:label "Physical Injury Impact"@en ;
    rdfs:label "身体伤害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:MallVisitors .

ex:MallVisitors rdf:type airo:AISubject ;
    rdfs:label "Mall Visitors"@en ;
    rdfs:label "商场访客"@zh .

ex:EscalatorFallRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Escalator Fall Risk Source"@en ;
    rdfs:label "从扶梯上跌落的风险源"@zh .