@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleImageSearch rdf:type airo:AISystem ;
    rdfs:label "Google Image Search"@en ;
    rdfs:label "谷歌图片搜索"@zh ;
    airo:hasRisk ex:GenderBiasInSearchResults ;
    airo:hasAICapability ex:ImageRankingAlgorithm ;
    airo:hasStakeholder ex:GoogleInc ;
    airo:hasAISubject ex:WomenInLeadershipPositions ;
    airo:isAppliedWithinDomain ex:OnlineSearchEngine .

ex:GenderBiasInSearchResults rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Search Results Risk"@en ;
    rdfs:label "搜索结果中的性别偏见风险"@zh ;
    airo:hasConsequence ex:ReinforcementOfGenderStereotypes ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:ReinforcementOfGenderStereotypes rdf:type airo:Consequence ;
    rdfs:label "Reinforcement of Gender Stereotypes Consequence"@en ;
    rdfs:label "强化性别刻板印象的后果"@zh ;
    airo:hasImpact ex:PerpetuationOfGenderInequality .

ex:PerpetuationOfGenderInequality rdf:type airo:Impact ;
    rdfs:label "Perpetuation of Gender Inequality Impact"@en ;
    rdfs:label "性别不平等持续的影响"@zh ;
    airo:hasImpactOnStakeholder ex:WomenInLeadershipPositions .

ex:ImageRankingAlgorithm rdf:type airo:AICapability ;
    rdfs:label "Image Ranking Algorithm Capability"@en ;
    rdfs:label "图片排名算法能力"@zh .

ex:GoogleInc rdf:type airo:Stakeholder ;
    rdfs:label "Google Inc."@en ;
    rdfs:label "谷歌公司"@zh .

ex:WomenInLeadershipPositions rdf:type airo:AISubject ;
    rdfs:label "Women in Leadership Positions"@en ;
    rdfs:label "领导职位中的女性"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:OnlineSearchEngine rdf:type airo:Domain ;
    rdfs:label "Online Search Engine Domain"@en ;
    rdfs:label "在线搜索引擎领域"@zh .