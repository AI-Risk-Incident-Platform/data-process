@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:InappropriateContentGenerationRisk ;
    airo:hasCapability ex:TextGeneration ;
    airo:hasStakeholder ex:ContentCreators .

ex:InappropriateContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Generation Risk"@en ;
    rdfs:label "不当内容生成风险"@zh ;
    airo:hasConsequence ex:ExposureToExplicitContent ;
    airo:isRiskSourceFor ex:TrainingDataBias .

ex:ExposureToExplicitContent rdf:type airo:Consequence ;
    rdfs:label "Exposure to Explicit Content"@en ;
    rdfs:label "暴露于露骨内容"@zh ;
    airo:hasImpact ex:EthicalConcerns .

ex:TextGeneration rdf:type airo:AICapability ;
    rdfs:label "Text Generation"@en ;
    rdfs:label "文本生成"@zh .

ex:ContentCreators rdf:type airo:Stakeholder ;
    rdfs:label "Content Creators"@en ;
    rdfs:label "内容创作者"@zh .

ex:TrainingDataBias rdf:type airo:RiskSource ;
    rdfs:label "Training Data Bias"@en ;
    rdfs:label "训练数据偏见"@zh .

ex:EthicalConcerns rdf:type airo:Impact ;
    rdfs:label "Ethical Concerns"@en ;
    rdfs:label "伦理担忧"@zh ;
    airo:hasImpactOnStakeholder ex:ContentCreators .