@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NHSFacialRecognitionApp rdf:type airo:AISystem ;
    rdfs:label "NHS面部识别应用"@zh ;
    rdfs:label "NHS Facial Recognition App"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:PrivacyGroups ;
    airo:isDeployedBy ex:iProov ;
    airo:isAppliedWithinDomain ex:HealthcareServices ;
    airo:hasPurpose ex:PatientIdentityVerification .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:isRiskSourceFor ex:DataSharingWithPrivateCompany ;
    airo:hasConsequence ex:PotentialMisuseOfBiometricData .

ex:PrivacyGroups rdf:type airo:Stakeholder ;
    rdfs:label "隐私保护团体"@zh ;
    rdfs:label "Privacy Groups"@en .

ex:iProov rdf:type airo:AIOperator ;
    rdfs:label "iProov公司"@zh ;
    rdfs:label "iProov"@en .

ex:DataSharingWithPrivateCompany rdf:type airo:RiskSource ;
    rdfs:label "与私营公司共享数据"@zh ;
    rdfs:label "Data Sharing with Private Company"@en .

ex:PotentialMisuseOfBiometricData rdf:type airo:Consequence ;
    rdfs:label "生物特征数据潜在滥用"@zh ;
    rdfs:label "Potential Misuse of Biometric Data"@en .

ex:HealthcareServices rdf:type airo:Domain ;
    rdfs:label "医疗服务领域"@zh ;
    rdfs:label "Healthcare Services"@en .

ex:PatientIdentityVerification rdf:type airo:Purpose ;
    rdfs:label "患者身份验证"@zh ;
    rdfs:label "Patient Identity Verification"@en .