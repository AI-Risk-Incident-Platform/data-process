@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CocaColaAIGeneratedAdSystem rdf:type airo:AISystem ;
    rdfs:label "可口可乐AI生成广告系统"@zh ;
    rdfs:label "Coca-Cola AI-Generated Ad System"@en ;
    airo:hasRisk ex:ConsumerBacklashRisk ;
    airo:hasPurpose ex:HolidayMarketingCampaign ;
    airo:hasStakeholder ex:CocaColaConsumers ;
    airo:isAppliedWithinDomain ex:AdvertisingIndustry .

ex:ConsumerBacklashRisk rdf:type airo:Risk ;
    rdfs:label "消费者反弹风险"@zh ;
    rdfs:label "Consumer Backlash Risk"@en ;
    airo:hasConsequence ex:BrandReputationDamage .

ex:CocaColaConsumers rdf:type airo:Stakeholder ;
    rdfs:label "可口可乐消费者"@zh ;
    rdfs:label "Coca-Cola Consumers"@en .

ex:BrandReputationDamage rdf:type airo:Impact ;
    rdfs:label "品牌声誉损害"@zh ;
    rdfs:label "Brand Reputation Damage"@en ;
    airo:hasImpactOnStakeholder ex:CocaColaConsumers .

ex:HolidayMarketingCampaign rdf:type airo:Purpose ;
    rdfs:label "节日营销活动"@zh ;
    rdfs:label "Holiday Marketing Campaign"@en .

ex:AdvertisingIndustry rdf:type airo:Domain ;
    rdfs:label "广告行业"@zh ;
    rdfs:label "Advertising Industry"@en .