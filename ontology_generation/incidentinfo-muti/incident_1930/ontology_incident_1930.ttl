@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:DeclineInResponseQualityRisk ;
    airo:hasStakeholder ex:ChatGPTUsers .

ex:DeclineInResponseQualityRisk rdf:type airo:Risk ;
    rdfs:label "Decline in Response Quality Risk"@en ;
    rdfs:label "响应质量下降风险"@zh ;
    airo:hasConsequence ex:ReducedUserSatisfactionConsequence .

ex:ReducedUserSatisfactionConsequence rdf:type airo:Consequence ;
    rdfs:label "Reduced User Satisfaction Consequence"@en ;
    rdfs:label "用户满意度降低后果"@zh ;
    airo:hasImpact ex:NegativeUserExperienceImpact .

ex:NegativeUserExperienceImpact rdf:type airo:Impact ;
    rdfs:label "Negative User Experience Impact"@en ;
    rdfs:label "负面用户体验影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChatGPTUsers .

ex:ChatGPTUsers rdf:type airo:Stakeholder ;
    rdfs:label "ChatGPT Users"@en ;
    rdfs:label "ChatGPT用户"@zh .