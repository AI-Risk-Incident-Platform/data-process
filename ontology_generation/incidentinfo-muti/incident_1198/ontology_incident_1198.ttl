ex:AIPoweredBot rdf:type airo:AISystem ;
    rdfs:label "AI Powered Bot"@en ;
    rdfs:label "AI求职机器人"@zh ;
    airo:hasRisk ex:JobMarketDisruptionRisk ;
    airo:isDevelopedBy ex:AIBotDevelopers ;
    airo:hasStakeholder ex:LinkedInPlatform ;
    airo:isAppliedWithinDomain ex:OnlineJobMarket .

ex:JobMarketDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Job Market Disruption Risk"@en ;
    rdfs:label "就业市场扰乱风险"@zh .

ex:LinkedInPlatform rdf:type airo:Stakeholder ;
    rdfs:label "LinkedIn Platform"@en ;
    rdfs:label "领英平台"@zh .

ex:AIBotDevelopers rdf:type airo:AIOperator ;
    rdfs:label "AI Bot Developers"@en ;
    rdfs:label "AI机器人开发者"@zh .

ex:OnlineJobMarket rdf:type airo:Domain ;
    rdfs:label "Online Job Market"@en ;
    rdfs:label "在线就业市场"@zh .