@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MachineLearningTool rdf:type airo:AISystem ;
    rdfs:label "Machine Learning Tool"@en ;
    rdfs:label "机器学习工具"@zh ;
    airo:hasRisk ex:RacialBiasInAI ;
    airo:hasCapability ex:ImageProcessing ;
    airo:hasAISubject ex:BarackObama .

ex:RacialBiasInAI rdf:type airo:Risk ;
    rdfs:label "Racial Bias in AI Risk"@en ;
    rdfs:label "AI中的种族偏见风险"@zh ;
    airo:hasImpact ex:MisrepresentationOfRace ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:ImageProcessing rdf:type airo:AICapability ;
    rdfs:label "Image Processing Capability"@en ;
    rdfs:label "图像处理能力"@zh .

ex:BarackObama rdf:type airo:AISubject ;
    rdfs:label "Barack Obama"@en ;
    rdfs:label "巴拉克·奥巴马"@zh .

ex:MisrepresentationOfRace rdf:type airo:Impact ;
    rdfs:label "Misrepresentation of Race Impact"@en ;
    rdfs:label "种族错误表征的影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .