ex:AIGeneratedPhishingScam rdf:type airo:AISystem ;
    rdfs:label "AI Generated Phishing Scam"@en ;
    rdfs:label "AI生成的钓鱼诈骗"@zh ;
    airo:hasRisk ex:CybersecurityRisk ;
    airo:usesTechnique ex:PhishingContentGeneration .

ex:CybersecurityRisk rdf:type airo:Risk ;
    rdfs:label "Cybersecurity Risk"@en ;
    rdfs:label "网络安全风险"@zh ;
    airo:isRiskSourceFor ex:SophisticatedAITechnology ;
    airo:hasImpact ex:FinancialLoss .

ex:SophisticatedAITechnology rdf:type airo:RiskSource ;
    rdfs:label "Sophisticated AI Technology"@en ;
    rdfs:label "复杂AI技术"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh .

ex:PhishingContentGeneration rdf:type airo:AITechnique ;
    rdfs:label "Phishing Content Generation"@en ;
    rdfs:label "钓鱼内容生成"@zh .