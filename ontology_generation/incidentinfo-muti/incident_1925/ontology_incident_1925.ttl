@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CivitaiAIGenerator rdf:type airo:AISystem ;
    rdfs:label "Civitai AI Generator"@en ;
    rdfs:label "Civitai AI生成器"@zh ;
    airo:hasRisk ex:ChildExploitationContentGenerationRisk ;
    airo:hasCapability ex:ImageGeneration ;
    airo:isAppliedWithinDomain ex:ContentGenerationPlatform .

ex:ChildExploitationContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Content Generation Risk"@en ;
    rdfs:label "儿童剥削内容生成风险"@zh ;
    airo:hasConsequence ex:HarmToChildren ;
    airo:isRiskSourceFor ex:InadequateContentFiltering .

ex:HarmToChildren rdf:type airo:Consequence ;
    rdfs:label "Harm to Children Consequence"@en ;
    rdfs:label "对儿童的伤害后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToMinors .

ex:PsychologicalHarmToMinors rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Minors Impact"@en ;
    rdfs:label "对未成年人心理伤害的影响"@zh .

ex:InadequateContentFiltering rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Content Filtering Risk Source"@en ;
    rdfs:label "内容过滤不足的风险源"@zh .

ex:ImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Image Generation Capability"@en ;
    rdfs:label "图像生成能力"@zh .

ex:ContentGenerationPlatform rdf:type airo:Domain ;
    rdfs:label "Content Generation Platform Domain"@en ;
    rdfs:label "内容生成平台领域"@zh .