@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MSNContentGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "MSN Content Generation System"@en ;
    rdfs:label "MSN内容生成系统"@zh ;
    airo:hasRisk ex:InsensitiveContentGenerationRisk ;
    airo:hasStakeholder ex:NBACommunity .

ex:InsensitiveContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Insensitive Content Generation Risk"@en ;
    rdfs:label "不敏感内容生成风险"@zh ;
    airo:hasConsequence ex:PublicOutrageConsequence ;
    airo:isRiskSourceFor ex:LackOfHumanReview .

ex:PublicOutrageConsequence rdf:type airo:Consequence ;
    rdfs:label "Public Outrage Consequence"@en ;
    rdfs:label "公众愤怒后果"@zh ;
    airo:hasImpact ex:ReputationDamageImpact .

ex:NBACommunity rdf:type airo:Stakeholder ;
    rdfs:label "NBA Community"@en ;
    rdfs:label "NBA社区"@zh .

ex:ReputationDamageImpact rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:NBACommunity .

ex:LackOfHumanReview rdf:type airo:RiskSource ;
    rdfs:label "Lack of Human Review"@en ;
    rdfs:label "缺乏人工审核"@zh .