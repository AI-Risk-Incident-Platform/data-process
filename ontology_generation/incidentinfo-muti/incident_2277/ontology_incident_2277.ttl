@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VoiceDeepfakeAI rdf:type airo:AISystem ;
    rdfs:label "Voice Deepfake AI"@en ;
    rdfs:label "语音深度伪造AI"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:VoiceSynthesisTechnology ;
    airo:hasStakeholder ex:CorporateManagers ;
    airo:hasVulnerability ex:HumanVoiceAuthenticationReliance .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:UnauthorizedFundTransfer ;
    airo:isRiskSourceFor ex:AIEnabledSocialEngineering .

ex:UnauthorizedFundTransfer rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Fund Transfer Consequence"@en ;
    rdfs:label "未经授权的资金转移后果"@zh ;
    airo:hasImpact ex:CorporateFinancialLoss .

ex:VoiceSynthesisTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Synthesis Technology"@en ;
    rdfs:label "语音合成技术"@zh .

ex:CorporateManagers rdf:type airo:Stakeholder ;
    rdfs:label "Corporate Managers"@en ;
    rdfs:label "企业经理"@zh .

ex:CorporateFinancialLoss rdf:type airo:Impact ;
    rdfs:label "Corporate Financial Loss Impact"@en ;
    rdfs:label "企业财务损失的影响"@zh .

ex:AIEnabledSocialEngineering rdf:type airo:RiskSource ;
    rdfs:label "AI-Enabled Social Engineering Risk Source"@en ;
    rdfs:label "AI增强的社会工程学风险源"@zh .

ex:HumanVoiceAuthenticationReliance rdf:type airo:Vulnerability ;
    rdfs:label "Human Voice Authentication Reliance Vulnerability"@en ;
    rdfs:label "人类对语音认证依赖的脆弱性"@zh .