@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:USPSPayCutAlgorithm rdf:type airo:AISystem ;
    rdfs:label "USPS Pay Cut Algorithm"@en ;
    rdfs:label "USPS薪资削减算法"@zh ;
    airo:hasRisk ex:UnfairCompensationRisk ;
    airo:hasStakeholder ex:RuralMailCarriers ;
    airo:isDeployedBy ex:USPS ;
    airo:isAppliedWithinDomain ex:PostalServices .

ex:UnfairCompensationRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Compensation Risk"@en ;
    rdfs:label "不公平薪酬风险"@zh ;
    airo:hasConsequence ex:MassivePayCut ;
    airo:isRiskSourceFor ex:OpaqueAlgorithmicDecisionMaking .

ex:RuralMailCarriers rdf:type airo:Stakeholder ;
    rdfs:label "Rural Mail Carriers"@en ;
    rdfs:label "农村邮递员"@zh .

ex:MassivePayCut rdf:type airo:Consequence ;
    rdfs:label "Massive Pay Cut Consequence"@en ;
    rdfs:label "大规模减薪后果"@zh ;
    airo:hasImpact ex:FinancialHardshipForRuralCarriers .

ex:USPS rdf:type airo:AIOperator ;
    rdfs:label "USPS"@en ;
    rdfs:label "美国邮政服务"@zh .

ex:PostalServices rdf:type airo:Domain ;
    rdfs:label "Postal Services Domain"@en ;
    rdfs:label "邮政服务领域"@zh .

ex:OpaqueAlgorithmicDecisionMaking rdf:type airo:RiskSource ;
    rdfs:label "Opaque Algorithmic Decision Making Risk Source"@en ;
    rdfs:label "不透明的算法决策风险源"@zh .

ex:FinancialHardshipForRuralCarriers rdf:type airo:Impact ;
    rdfs:label "Financial Hardship for Rural Carriers Impact"@en ;
    rdfs:label "农村邮递员经济困难影响"@zh ;
    airo:hasImpactOnStakeholder ex:RuralMailCarriers .