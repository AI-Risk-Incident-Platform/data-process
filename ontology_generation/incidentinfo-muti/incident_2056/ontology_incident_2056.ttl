@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MentorApp rdf:type airo:AISystem ;
    rdfs:label "Mentor应用"@zh ;
    rdfs:label "Mentor App"@en ;
    airo:hasRisk ex:DriverSurveillanceRisk ;
    airo:hasPurpose ex:DriverPerformanceMonitoring ;
    airo:isDeployedBy ex:Amazon ;
    airo:hasStakeholder ex:AmazonDeliveryDrivers ;
    airo:isAppliedWithinDomain ex:LogisticsAndDelivery .

ex:DriverSurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "司机监控风险"@zh ;
    rdfs:label "Driver Surveillance Risk"@en ;
    airo:hasConsequence ex:DriverDiscontent .

ex:AmazonDeliveryDrivers rdf:type airo:Stakeholder ;
    rdfs:label "亚马逊送货司机"@zh ;
    rdfs:label "Amazon Delivery Drivers"@en .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "亚马逊"@zh ;
    rdfs:label "Amazon"@en .

ex:DriverDiscontent rdf:type airo:Impact ;
    rdfs:label "司机不满"@zh ;
    rdfs:label "Driver Discontent"@en ;
    airo:hasImpactOnStakeholder ex:AmazonDeliveryDrivers .

ex:DriverPerformanceMonitoring rdf:type airo:Purpose ;
    rdfs:label "司机绩效监控"@zh ;
    rdfs:label "Driver Performance Monitoring"@en .

ex:LogisticsAndDelivery rdf:type airo:Domain ;
    rdfs:label "物流与配送"@zh ;
    rdfs:label "Logistics and Delivery"@en .