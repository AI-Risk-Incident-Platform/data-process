@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LensaMagicAvatarApp rdf:type airo:AISystem ;
    rdfs:label "Lensa Magic Avatar App"@en ;
    rdfs:label "Lensa魔法头像应用"@zh ;
    airo:hasRisk ex:NonConsensualImageGenerationRisk ;
    airo:hasCapability ex:ImageGenerationFromPhotos ;
    airo:hasStakeholder ex:AppUsers ;
    airo:isDevelopedBy ex:LensaAppDeveloper .

ex:NonConsensualImageGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Non-Consensual Image Generation Risk"@en ;
    rdfs:label "未经同意的图像生成风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation ;
    airo:isRiskSourceFor ex:NonConsensualImageGeneration .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Privacy Violation Consequence"@en ;
    rdfs:label "隐私侵犯后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:AppUsers .

ex:NonConsensualImageGeneration rdf:type airo:Misuse ;
    rdfs:label "Non-Consensual Image Generation Misuse"@en ;
    rdfs:label "未经同意的图像生成滥用"@zh .

ex:ImageGenerationFromPhotos rdf:type airo:AICapability ;
    rdfs:label "Image Generation from Photos Capability"@en ;
    rdfs:label "基于照片的图像生成能力"@zh .

ex:AppUsers rdf:type airo:Stakeholder ;
    rdfs:label "App Users"@en ;
    rdfs:label "应用使用者"@zh .

ex:LensaAppDeveloper rdf:type airo:AIOperator ;
    rdfs:label "Lensa App Developer"@en ;
    rdfs:label "Lensa应用开发者"@zh .