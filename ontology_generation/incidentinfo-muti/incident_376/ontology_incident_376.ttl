@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SocialMediaPlatform rdf:type airo:AISystem ;
    rdfs:label "Social Media Platform"@en ;
    rdfs:label "社交媒体平台"@zh ;
    airo:hasRisk ex:TeenSuicideRisk ;
    airo:hasAISubject ex:Teenagers ;
    airo:hasStakeholder ex:SocialMediaCompany .

ex:TeenSuicideRisk rdf:type airo:Risk ;
    rdfs:label "Teen Suicide Risk"@en ;
    rdfs:label "青少年自杀风险"@zh ;
    airo:hasConsequence ex:MentalHealthImpact .

ex:MentalHealthImpact rdf:type airo:Impact ;
    rdfs:label "Mental Health Impact"@en ;
    rdfs:label "心理健康影响"@zh ;
    airo:hasImpactOnStakeholder ex:Teenagers .

ex:Teenagers rdf:type airo:AISubject ;
    rdfs:label "Teenagers"@en ;
    rdfs:label "青少年"@zh .

ex:SocialMediaCompany rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Company"@en ;
    rdfs:label "社交媒体公司"@zh .