@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:PrivacyComplianceRisk ;
    airo:hasStakeholder ex:EuropeanUnion ;
    airo:isDevelopedBy ex:OpenAI .

ex:PrivacyComplianceRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Compliance Risk"@en ;
    rdfs:label "隐私合规风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation ;
    airo:isRiskSourceFor ex:HallucinationBehavior .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Privacy Violation Consequence"@en ;
    rdfs:label "隐私侵犯后果"@zh ;
    airo:hasImpact ex:RegulatoryScrutiny .

ex:EuropeanUnion rdf:type airo:Stakeholder ;
    rdfs:label "European Union"@en ;
    rdfs:label "欧盟"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:HallucinationBehavior rdf:type airo:RiskSource ;
    rdfs:label "Hallucination Behavior Risk Source"@en ;
    rdfs:label "幻觉行为风险源"@zh .

ex:RegulatoryScrutiny rdf:type airo:Impact ;
    rdfs:label "Regulatory Scrutiny Impact"@en ;
    rdfs:label "监管审查影响"@zh ;
    airo:hasImpactOnStakeholder ex:EuropeanUnion .