@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeScamVideos rdf:type airo:AISystem ;
    rdfs:label "Deepfake Scam Videos"@en ;
    rdfs:label "深度伪造诈骗视频"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:FinancialFraud ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:hasAISubject ex:Celebrities ;
    airo:hasStakeholder ex:SocialMediaUsers .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfCelebrityImages .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:Celebrities rdf:type airo:AISubject ;
    rdfs:label "Celebrities"@en ;
    rdfs:label "名人"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .

ex:FinancialFraud rdf:type airo:Purpose ;
    rdfs:label "Financial Fraud Purpose"@en ;
    rdfs:label "金融欺诈目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:UnauthorizedUseOfCelebrityImages rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Use of Celebrity Images Risk Source"@en ;
    rdfs:label "未经授权使用名人形象的风险源"@zh ;
    airo:exploitsVulnerability ex:PublicTrustInCelebrityEndorsements .

ex:PublicTrustInCelebrityEndorsements rdf:type airo:Vulnerability ;
    rdfs:label "Public Trust in Celebrity Endorsements Vulnerability"@en ;
    rdfs:label "公众对名人代言的信任脆弱性"@zh .