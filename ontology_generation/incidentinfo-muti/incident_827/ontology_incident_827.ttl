ex:AIInternetAidsForBlindUsers rdf:type airo:AISystem ;
    rdfs:label "AI Internet Aids for Blind Users"@en ;
    rdfs:label "盲人互联网辅助AI"@zh ;
    airo:hasRisk ex:ErrorProneAIAssistanceRisk ;
    airo:hasAISubject ex:BlindInternetUsers ;
    airo:hasVulnerability ex:RelianceOnUnreliableAIAssistance .

ex:ErrorProneAIAssistanceRisk rdf:type airo:Risk ;
    rdfs:label "Error-Prone AI Assistance Risk"@en ;
    rdfs:label "易出错AI辅助风险"@zh ;
    airo:hasImpact ex:AccessibilityBarriersForVisuallyImpaired .

ex:BlindInternetUsers rdf:type airo:AISubject ;
    rdfs:label "Blind Internet Users"@en ;
    rdfs:label "盲人互联网用户"@zh .

ex:AccessibilityBarriersForVisuallyImpaired rdf:type airo:Impact ;
    rdfs:label "Accessibility Barriers for Visually Impaired"@en ;
    rdfs:label "视障人士可访问性障碍"@zh .

ex:RelianceOnUnreliableAIAssistance rdf:type airo:Vulnerability ;
    rdfs:label "Reliance on Unreliable AI Assistance"@en ;
    rdfs:label "依赖不可靠AI辅助"@zh .