    rdfs:label "亚马逊员工"@zh ;
    rdfs:label "Amazon Employees"@en .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "亚马逊"@zh ;
    rdfs:label "Amazon"@en .

ex:LawEnforcementAgencies rdf:type airo:AISubject ;
    rdfs:label "执法机构"@zh ;
    rdfs:label "Law Enforcement Agencies"@en .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "执法领域"@zh ;
    rdfs:label "Law Enforcement Domain"@en .