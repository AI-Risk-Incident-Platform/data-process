ex:AmazonFacialRecognitionSoftware rdf:type airo:AISystem ;
    rdfs:label "亚马逊面部识别软件"@zh ;
    rdfs:label "Amazon Facial Recognition Software"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:AmazonEmployees ;
    airo:isDeployedBy ex:Amazon ;
    airo:hasAISubject ex:LawEnforcementAgencies ;
    airo:isAppliedWithinDomain ex:LawEnforcement .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en .

