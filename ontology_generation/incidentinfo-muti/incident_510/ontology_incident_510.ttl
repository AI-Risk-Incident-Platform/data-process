@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPTDetector rdf:type airo:AISystem ;
    rdfs:label "ChatGPT Detector"@en ;
    rdfs:label "ChatGPT检测器"@zh ;
    airo:hasRisk ex:FalsePositiveDetectionRisk ;
    airo:hasAISubject ex:Students ;
    airo:hasPurpose ex:AcademicIntegrityProtection ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:hasStakeholder ex:Teachers .

ex:FalsePositiveDetectionRisk rdf:type airo:Risk ;
    rdfs:label "False Positive Detection Risk"@en ;
    rdfs:label "误报检测风险"@zh ;
    airo:hasConsequence ex:UnfairAccusation ;
    airo:isRiskSourceFor ex:ImperfectDetectionAlgorithm .

ex:UnfairAccusation rdf:type airo:Consequence ;
    rdfs:label "Unfair Accusation Consequence"@en ;
    rdfs:label "不公正指控后果"@zh ;
    airo:hasImpact ex:AcademicReputationDamage .

ex:AcademicReputationDamage rdf:type airo:Impact ;
    rdfs:label "Academic Reputation Damage Impact"@en ;
    rdfs:label "学术声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:Students .

ex:Teachers rdf:type airo:Stakeholder ;
    rdfs:label "Teachers"@en ;
    rdfs:label "教师"@zh .

ex:Students rdf:type airo:AISubject ;
    rdfs:label "Students"@en ;
    rdfs:label "学生"@zh .

ex:ImperfectDetectionAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "Imperfect Detection Algorithm Risk Source"@en ;
    rdfs:label "不完善的检测算法风险源"@zh .

ex:AcademicIntegrityProtection rdf:type airo:Purpose ;
    rdfs:label "Academic Integrity Protection Purpose"@en ;
    rdfs:label "学术诚信保护目的"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .