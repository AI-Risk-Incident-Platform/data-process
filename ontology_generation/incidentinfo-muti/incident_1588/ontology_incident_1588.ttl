@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TextToImageAIModels rdf:type airo:AISystem ;
    rdfs:label "Text-to-Image AI Models"@en ;
    rdfs:label "文本到图像AI模型"@zh ;
    airo:hasRisk ex:GenerationOfDisturbingImagesRisk ;
    airo:hasCapability ex:ImageGenerationFromTextPrompts ;
    airo:hasVulnerability ex:PromptInjectionVulnerability .

ex:GenerationOfDisturbingImagesRisk rdf:type airo:Risk ;
    rdfs:label "Generation of Disturbing Images Risk"@en ;
    rdfs:label "生成令人不安图像的风险"@zh ;
    airo:hasConsequence ex:CreationOfNSFWContent ;
    airo:isRiskSourceFor ex:AdversarialInputs .

ex:CreationOfNSFWContent rdf:type airo:Consequence ;
    rdfs:label "Creation of NSFW Content Consequence"@en ;
    rdfs:label "创建不适合工作场所内容的后果"@zh ;
    airo:hasImpact ex:PotentialHarmToUsers .

ex:ImageGenerationFromTextPrompts rdf:type airo:AICapability ;
    rdfs:label "Image Generation from Text Prompts Capability"@en ;
    rdfs:label "从文本提示生成图像的能力"@zh .

ex:PromptInjectionVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "Prompt Injection Vulnerability"@en ;
    rdfs:label "提示注入漏洞"@zh .

ex:MaliciousPromptEngineering rdf:type airo:Threat ;
    rdfs:label "Malicious Prompt Engineering Threat"@en ;
    rdfs:label "恶意提示工程威胁"@zh ;
    airo:isRiskSourceFor ex:GenerationOfDisturbingImagesRisk .

ex:PotentialHarmToUsers rdf:type airo:Impact ;
    rdfs:label "Potential Harm to Users Impact"@en ;
    rdfs:label "对用户的潜在伤害影响"@zh .

ex:AdversarialInputs rdf:type airo:RiskSource ;
    rdfs:label "Adversarial Inputs Risk Source"@en ;
    rdfs:label "对抗性输入风险源"@zh ;
    airo:exploitsVulnerability ex:PromptInjectionVulnerability .