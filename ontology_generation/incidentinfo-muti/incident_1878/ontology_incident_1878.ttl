@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LAIONMultimodalDataset rdf:type airo:AISystem ;
    rdfs:label "LAION Multimodal Dataset"@en ;
    rdfs:label "LAION多模态数据集"@zh ;
    airo:hasRisk ex:HateContentPropagationRisk ;
    airo:hasAICapability ex:LargeScaleDataProcessing ;
    airo:hasStakeholder ex:AIResearchCommunity .

ex:HateContentPropagationRisk rdf:type airo:Risk ;
    rdfs:label "Hate Content Propagation Risk"@en ;
    rdfs:label "仇恨内容传播风险"@zh ;
    airo:isRiskSourceFor ex:UnfilteredWebDataCollection ;
    airo:hasConsequence ex:HarmfulContentGeneration .

ex:UnfilteredWebDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered Web Data Collection"@en ;
    rdfs:label "未过滤的网络数据收集"@zh .

ex:HarmfulContentGeneration rdf:type airo:Consequence ;
    rdfs:label "Harmful Content Generation Consequence"@en ;
    rdfs:label "有害内容生成后果"@zh ;
    airo:hasImpact ex:SocialDiscrimination .

ex:LargeScaleDataProcessing rdf:type airo:AICapability ;
    rdfs:label "Large-Scale Data Processing Capability"@en ;
    rdfs:label "大规模数据处理能力"@zh .

ex:AIResearchCommunity rdf:type airo:Stakeholder ;
    rdfs:label "AI Research Community"@en ;
    rdfs:label "AI研究社区"@zh .

ex:SocialDiscrimination rdf:type airo:Impact ;
    rdfs:label "Social Discrimination Impact"@en ;
    rdfs:label "社会歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:AIResearchCommunity .