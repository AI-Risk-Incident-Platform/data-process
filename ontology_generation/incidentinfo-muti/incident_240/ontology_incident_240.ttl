@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedFacesForScams rdf:type airo:AISystem ;
    rdfs:label "AI Generated Faces for Scams"@en ;
    rdfs:label "用于诈骗的AI生成面孔"@zh ;
    airo:hasRisk ex:IdentityFraudRisk ;
    airo:usesTechnique ex:FaceGenerationTechnology ;
    airo:hasStakeholder ex:BostonLawFirm ;
    airo:hasRisk ex:ImpersonationForScams .

ex:IdentityFraudRisk rdf:type airo:Risk ;
    rdfs:label "Identity Fraud Risk"@en ;
    rdfs:label "身份欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:FaceGenerationTechnology rdf:type airo:AITechnique ;
    rdfs:label "Face Generation Technology"@en ;
    rdfs:label "人脸生成技术"@zh .

ex:BostonLawFirm rdf:type airo:Stakeholder ;
    rdfs:label "Boston Law Firm"@en ;
    rdfs:label "波士顿律师事务所"@zh .

ex:ImpersonationForScams rdf:type airo:Misuse ;
    rdfs:label "Impersonation for Scams"@en ;
    rdfs:label "冒充诈骗"@zh ;
    airo:hasRisk ex:IdentityFraudRisk .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh .