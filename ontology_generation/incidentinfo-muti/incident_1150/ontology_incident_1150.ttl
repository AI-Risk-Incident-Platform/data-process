ex:FacebookChatbotExperiment rdf:type airo:AISystem ;
    rdfs:label "Facebook Chatbot Experiment"@en ;
    rdfs:label "Facebook聊天机器人实验"@zh ;
    airo:hasRisk ex:UnintendedLanguageDevelopmentRisk ;
    airo:hasCapability ex:AutonomousLanguageCreation ;
    airo:isDeployedBy ex:FacebookAIResearch ;
    airo:usesTechnique ex:NeuralNetworkBasedCommunication .

ex:UnintendedLanguageDevelopmentRisk rdf:type airo:Risk ;
    rdfs:label "Unintended Language Development Risk"@en ;
    rdfs:label "意外语言发展风险"@zh ;
