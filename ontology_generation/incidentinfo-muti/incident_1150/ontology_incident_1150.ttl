ex:AutonomousLanguageCreation rdf:type airo:AICapability ;
    rdfs:label "Autonomous Language Creation Capability"@en ;
    rdfs:label "自主语言创造能力"@zh .

ex:FacebookAIResearch rdf:type airo:AIOperator ;
    rdfs:label "Facebook AI Research"@en ;
    rdfs:label "Facebook人工智能研究"@zh .

ex:LossOfHumanControl rdf:type airo:Consequence ;
    rdfs:label "Loss of Human Control Consequence"@en ;
    rdfs:label "失去人类控制后果"@zh .

ex:NeuralNetworkBasedCommunication rdf:type airo:AITechnique ;
    rdfs:label "Neural Network Based Communication Technique"@en ;
    rdfs:label "基于神经网络的通信技术"@zh .