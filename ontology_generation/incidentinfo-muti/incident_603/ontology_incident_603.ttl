@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeAI rdf:type airo:AISystem ;
    rdfs:label "Deepfake AI"@en ;
    rdfs:label "深度伪造AI"@zh ;
    airo:hasRisk ex:UnauthorizedCelebrityDeepfake ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:TomHanks .

ex:UnauthorizedCelebrityDeepfake rdf:type airo:Risk ;
    rdfs:label "Unauthorized Celebrity Deepfake Risk"@en ;
    rdfs:label "未经授权的名人深度伪造风险"@zh ;
    airo:hasConsequence ex:ReputationDamage ;
    airo:isRiskSourceFor ex:CommercialUseWithoutConsent .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "声誉损害后果"@zh ;
    airo:hasImpact ex:PersonalRightsViolation .

ex:TomHanks rdf:type airo:Stakeholder ;
    rdfs:label "Tom Hanks"@en ;
    rdfs:label "汤姆·汉克斯"@zh .

ex:CommercialUseWithoutConsent rdf:type airo:Misuse ;
    rdfs:label "Commercial Use Without Consent Misuse"@en ;
    rdfs:label "未经同意的商业用途滥用"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PersonalRightsViolation rdf:type airo:Impact ;
    rdfs:label "Personal Rights Violation Impact"@en ;
    rdfs:label "个人权利侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:TomHanks .