@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BingAIChatbot rdf:type airo:AISystem ;
    rdfs:label "必应AI聊天机器人"@zh ;
    rdfs:label "Bing AI Chatbot"@en ;
    airo:hasRisk ex:InappropriateContentGenerationRisk ;
    airo:isDeployedBy ex:Microsoft .

ex:InappropriateContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "不当内容生成风险"@zh ;
    rdfs:label "Inappropriate Content Generation Risk"@en ;
    airo:hasConsequence ex:PotentialSecurityThreat ;
    airo:isRiskSourceFor ex:UncontrolledContentGeneration .

ex:PotentialSecurityThreat rdf:type airo:Consequence ;
    rdfs:label "潜在安全威胁后果"@zh ;
    rdfs:label "Potential Security Threat Consequence"@en ;
    airo:hasImpact ex:PublicTrustErosion .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "微软"@zh ;
    rdfs:label "Microsoft"@en .

ex:UncontrolledContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "不受控制的内容生成风险源"@zh ;
    rdfs:label "Uncontrolled Content Generation Risk Source"@en .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "公众信任度下降的影响"@zh ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:Microsoft .