@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GmailPhishingAI rdf:type airo:AISystem ;
    rdfs:label "Gmail Phishing AI"@en ;
    rdfs:label "Gmail钓鱼AI"@zh ;
    airo:hasRisk ex:DataTheftRisk ;
    airo:usesTechnique ex:AdvancedPhishingTechniques ;
    airo:isAppliedWithinDomain ex:EmailServices ;
    airo:hasStakeholder ex:GmailUsers .

ex:DataTheftRisk rdf:type airo:Risk ;
    rdfs:label "Data Theft Risk"@en ;
    rdfs:label "数据盗窃风险"@zh ;
    airo:hasConsequence ex:PersonalDataCompromise ;
    airo:isRiskSourceFor ex:CyberCriminals .

ex:GmailUsers rdf:type airo:Stakeholder ;
    rdfs:label "Gmail Users"@en ;
    rdfs:label "Gmail用户"@zh .

ex:CyberCriminals rdf:type airo:RiskSource ;
    rdfs:label "Cyber Criminals"@en ;
    rdfs:label "网络犯罪分子"@zh .

ex:PersonalDataCompromise rdf:type airo:Consequence ;
    rdfs:label "Personal Data Compromise Consequence"@en ;
    rdfs:label "个人数据泄露后果"@zh ;
    airo:hasImpact ex:PrivacyViolation .

ex:PrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:GmailUsers .

ex:AdvancedPhishingTechniques rdf:type airo:AITechnique ;
    rdfs:label "Advanced Phishing Techniques"@en ;
    rdfs:label "高级钓鱼技术"@zh .

ex:EmailServices rdf:type airo:Domain ;
    rdfs:label "Email Services Domain"@en ;
    rdfs:label "电子邮件服务领域"@zh .