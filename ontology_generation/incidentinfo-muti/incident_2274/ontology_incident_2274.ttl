@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RobotSermonDeliverySystem rdf:type airo:AISystem ;
    rdfs:label "Robot Sermon Delivery System"@en ;
    rdfs:label "机器人布道系统"@zh ;
    airo:hasRisk ex:EthicalAndReligiousImpactRisk ;
    airo:isAppliedWithinDomain ex:ReligiousServices ;
    airo:hasPurpose ex:AutomatedReligiousServiceDelivery ;
    airo:hasCapability ex:SpeechGenerationAndDelivery .

ex:EthicalAndReligiousImpactRisk rdf:type airo:Risk ;
    rdfs:label "Ethical and Religious Impact Risk"@en ;
    rdfs:label "伦理与宗教影响风险"@zh .

ex:ReligiousServices rdf:type airo:Domain ;
    rdfs:label "Religious Services Domain"@en ;
    rdfs:label "宗教服务领域"@zh .

ex:AutomatedReligiousServiceDelivery rdf:type airo:Purpose ;
    rdfs:label "Automated Religious Service Delivery Purpose"@en ;
    rdfs:label "自动化宗教服务交付目的"@zh .

ex:SpeechGenerationAndDelivery rdf:type airo:AICapability ;
    rdfs:label "Speech Generation and Delivery Capability"@en ;
    rdfs:label "语音生成与交付能力"@zh .