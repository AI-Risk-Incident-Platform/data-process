@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SmartTram rdf:type airo:AISystem ;
    rdfs:label "Smart Tram"@en ;
    rdfs:label "智能电车"@zh ;
    airo:hasRisk ex:PedestrianCollisionRisk ;
    airo:isAppliedWithinDomain ex:PublicTransportation .

ex:PedestrianCollisionRisk rdf:type airo:Risk ;
    rdfs:label "Pedestrian Collision Risk"@en ;
    rdfs:label "行人碰撞风险"@zh ;
    airo:hasConsequence ex:PhysicalInjury .

ex:PhysicalInjury rdf:type airo:Consequence ;
    rdfs:label "Physical Injury"@en ;
    rdfs:label "身体伤害"@zh ;
    airo:hasImpact ex:PublicSafetyThreat .

ex:PublicSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Public Safety Threat"@en ;
    rdfs:label "公共安全威胁"@zh .

ex:PublicTransportation rdf:type airo:Domain ;
    rdfs:label "Public Transportation"@en ;
    rdfs:label "公共交通"@zh .