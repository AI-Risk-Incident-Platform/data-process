@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonDevice rdf:type airo:AISystem ;
    rdfs:label "Amazon Device"@en ;
    rdfs:label "亚马逊设备"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:isDeployedBy ex:Amazon ;
    airo:hasAISubject ex:DeviceUser .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PrivateConversationLeak ;
    airo:isRiskSourceFor ex:UnauthorizedRecording .

ex:PrivateConversationLeak rdf:type airo:Consequence ;
    rdfs:label "Private Conversation Leak Consequence"@en ;
    rdfs:label "私人对话泄露后果"@zh ;
    airo:hasImpact ex:PersonalPrivacyCompromise .

ex:PersonalPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Compromise Impact"@en ;
    rdfs:label "个人隐私受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:DeviceUser .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:DeviceUser rdf:type airo:AISubject ;
    rdfs:label "Device User"@en ;
    rdfs:label "设备使用者"@zh .

ex:UnauthorizedRecording rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Recording Risk Source"@en ;
    rdfs:label "未经授权的录音风险源"@zh ;
    airo:exploitsVulnerability ex:VoiceActivationVulnerability .

ex:VoiceActivationVulnerability rdf:type airo:Vulnerability ;
    rdfs:label "Voice Activation Vulnerability"@en ;
    rdfs:label "语音激活漏洞"@zh .