ex:AmazonVoiceCloningAI rdf:type airo:AISystem ;
    rdfs:label "Amazon Voice Cloning AI"@en ;
    rdfs:label "亚马逊语音克隆AI"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:usesTechnique ex:VoiceCloningTechnology ;
    airo:hasStakeholder ex:Actors .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:isRiskSourceFor ex:UnauthorizedVoiceReplication .

ex:Actors rdf:type airo:Stakeholder ;
    rdfs:label "Actors"@en ;
    rdfs:label "演员"@zh .

ex:VoiceCloningTechnology rdf:type airo:AITechnique ;
    rdfs:label "Voice Cloning Technology"@en ;
    rdfs:label "语音克隆技术"@zh .

ex:UnauthorizedVoiceReplication rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Voice Replication"@en ;
    rdfs:label "未经授权的语音复制"@zh .