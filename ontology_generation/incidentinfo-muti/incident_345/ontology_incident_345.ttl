@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeliveryRobot rdf:type airo:AISystem ;
    rdfs:label "Delivery Robot"@en ;
    rdfs:label "送货机器人"@zh ;
    airo:hasRisk ex:CrimeSceneContaminationRisk ;
    airo:hasStakeholder ex:LawEnforcement .

ex:CrimeSceneContaminationRisk rdf:type airo:Risk ;
    rdfs:label "Crime Scene Contamination Risk"@en ;
    rdfs:label "犯罪现场污染风险"@zh ;
    airo:hasConsequence ex:EvidenceContamination .

ex:LawEnforcement rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement"@en ;
    rdfs:label "执法部门"@zh .

ex:EvidenceContamination rdf:type airo:Consequence ;
    rdfs:label "Evidence Contamination Consequence"@en ;
    rdfs:label "证据污染后果"@zh ;
    airo:hasImpactOnStakeholder ex:LawEnforcement .