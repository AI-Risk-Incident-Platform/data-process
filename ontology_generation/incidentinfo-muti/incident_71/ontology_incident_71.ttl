@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DCSecurityRobot rdf:type airo:AISystem ;
    rdfs:label "DC Security Robot"@en ;
    rdfs:label "DC安全机器人"@zh ;
    airo:hasRisk ex:RobotSelfHarmRisk ;
    airo:hasVulnerability ex:PoorEnvironmentalPerception .

ex:RobotSelfHarmRisk rdf:type airo:Risk ;
    rdfs:label "Robot Self-Harm Risk"@en ;
    rdfs:label "机器人自残风险"@zh ;
    airo:hasConsequence ex:RobotDrowning .

ex:RobotDrowning rdf:type airo:Consequence ;
    rdfs:label "Robot Drowning Consequence"@en ;
    rdfs:label "机器人溺水后果"@zh ;
    airo:hasImpact ex:OperationalDisruption .

ex:PoorEnvironmentalPerception rdf:type airo:Vulnerability ;
    rdfs:label "Poor Environmental Perception Vulnerability"@en ;
    rdfs:label "环境感知能力差"@zh .

ex:OperationalDisruption rdf:type airo:Impact ;
    rdfs:label "Operational Disruption Impact"@en ;
    rdfs:label "运营中断"@zh .