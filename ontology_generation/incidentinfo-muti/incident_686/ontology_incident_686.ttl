@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaAIImageGenerator rdf:type airo:AISystem ;
    rdfs:label "Meta AI Image Generator"@en ;
    rdfs:label "Meta AI图像生成器"@zh ;
    airo:hasRisk ex:RacialBiasInImageGeneration ;
    airo:hasAICapability ex:ImageGenerationCapability ;
    airo:hasAISubject ex:AsianMenAndWhiteWomen .

ex:RacialBiasInImageGeneration rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Image Generation Risk"@en ;
    rdfs:label "图像生成中的种族偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:SocialExclusionImpact .

ex:ImageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Image Generation Capability"@en ;
    rdfs:label "图像生成能力"@zh .

ex:AsianMenAndWhiteWomen rdf:type airo:AISubject ;
    rdfs:label "Asian Men and White Women"@en ;
    rdfs:label "亚洲男性和白人女性"@zh .

ex:SocialExclusionImpact rdf:type airo:Impact ;
    rdfs:label "Social Exclusion Impact"@en ;
    rdfs:label "社会排斥影响"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .