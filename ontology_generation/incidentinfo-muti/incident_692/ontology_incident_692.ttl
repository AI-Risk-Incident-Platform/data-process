@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleSearchAI rdf:type airo:AISystem ;
    rdfs:label "Google Search AI"@en ;
    rdfs:label "谷歌搜索AI"@zh ;
    airo:hasRisk ex:InaccurateInformationRisk ;
    airo:hasCapability ex:NaturalLanguageProcessing ;
    airo:isDeployedBy ex:GoogleLLC ;
    airo:isAppliedWithinDomain ex:WebSearch ;
    airo:hasPurpose ex:EnhancedSearchExperience ;
    airo:hasStakeholder ex:SearchEngineUsers .

ex:InaccurateInformationRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Information Risk"@en ;
    rdfs:label "信息不准确风险"@zh ;
    airo:hasConsequence ex:MisleadingInformation ;
    airo:isRiskSourceFor ex:AlgorithmicOverconfidence .

ex:MisleadingInformation rdf:type airo:Consequence ;
    rdfs:label "Misleading Information Consequence"@en ;
    rdfs:label "误导性信息后果"@zh ;
    airo:hasImpact ex:UserTrustErosion .

ex:UserTrustErosion rdf:type airo:Impact ;
    rdfs:label "User Trust Erosion Impact"@en ;
    rdfs:label "用户信任度下降影响"@zh ;
    airo:hasImpactOnStakeholder ex:SearchEngineUsers .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌公司"@zh .

ex:NaturalLanguageProcessing rdf:type airo:AICapability ;
    rdfs:label "Natural Language Processing Capability"@en ;
    rdfs:label "自然语言处理能力"@zh .

ex:AlgorithmicOverconfidence rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Overconfidence Risk Source"@en ;
    rdfs:label "算法过度自信风险源"@zh .

ex:WebSearch rdf:type airo:Domain ;
    rdfs:label "Web Search Domain"@en ;
    rdfs:label "网络搜索领域"@zh .

ex:EnhancedSearchExperience rdf:type airo:Purpose ;
    rdfs:label "Enhanced Search Experience Purpose"@en ;
    rdfs:label "增强搜索体验目的"@zh .

ex:SearchEngineUsers rdf:type airo:Stakeholder ;
    rdfs:label "Search Engine Users"@en ;
    rdfs:label "搜索引擎用户"@zh .