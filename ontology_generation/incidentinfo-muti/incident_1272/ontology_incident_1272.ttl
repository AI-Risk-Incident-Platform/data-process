@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedLegalCitations rdf:type airo:AISystem ;
    rdfs:label "AI Generated Legal Citations"@en ;
    rdfs:label "AI生成法律引用"@zh ;
    airo:hasRisk ex:LegalMisinformationRisk ;
    airo:isDeployedBy ex:MelbourneLawyer ;
    airo:hasStakeholder ex:JudicialSystem ;
    airo:isAppliedWithinDomain ex:LegalServices .

ex:LegalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Misinformation Risk"@en ;
    rdfs:label "法律错误信息风险"@zh ;
    airo:hasConsequence ex:JudicialSystemDisruption ;
    airo:isRiskSourceFor ex:AIHallucination .

ex:JudicialSystemDisruption rdf:type airo:Consequence ;
    rdfs:label "Judicial System Disruption Consequence"@en ;
    rdfs:label "司法系统干扰后果"@zh ;
    airo:hasImpact ex:LegalProceedingsDelay .

ex:MelbourneLawyer rdf:type airo:AIOperator ;
    rdfs:label "Melbourne Lawyer"@en ;
    rdfs:label "墨尔本律师"@zh .

ex:JudicialSystem rdf:type airo:Stakeholder ;
    rdfs:label "Judicial System"@en ;
    rdfs:label "司法系统"@zh ;
    airo:hasImpactOnStakeholder ex:LegalProceedingsDelay .

ex:LegalServices rdf:type airo:Domain ;
    rdfs:label "Legal Services Domain"@en ;
    rdfs:label "法律服务领域"@zh .

ex:AIHallucination rdf:type airo:RiskSource ;
    rdfs:label "AI Hallucination Risk Source"@en ;
    rdfs:label "AI幻觉风险源"@zh .

ex:LegalProceedingsDelay rdf:type airo:Impact ;
    rdfs:label "Legal Proceedings Delay Impact"@en ;
    rdfs:label "法律程序延误影响"@zh .