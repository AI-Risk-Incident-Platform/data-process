@base <https://w3id.org/airo#> .

ex:ChinaAIGeneratedContentSystem rdf:type airo:AISystem ;
    rdfs:label "China AI-Generated Content System"@en ;
    rdfs:label "中国AI生成内容系统"@zh ;
    airo:hasRisk ex:SocialDivisionRisk ;
    airo:isAppliedWithinDomain ex:InternationalRelations ;
    airo:hasPurpose ex:InfluencePublicOpinion ;
    airo:usesTechnique ex:ContentGenerationAI ;
    airo:hasStakeholder ex:Microsoft .

ex:SocialDivisionRisk rdf:type airo:Risk ;
    rdfs:label "Social Division Risk"@en ;
    rdfs:label "社会分裂风险"@zh ;
    airo:hasImpact ex:PoliticalInstability .

ex:Microsoft rdf:type airo:Stakeholder ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:InternationalRelations rdf:type airo:Domain ;
    rdfs:label "International Relations Domain"@en ;
    rdfs:label "国际关系领域"@zh .

ex:PoliticalInstability rdf:type airo:Impact ;
    rdfs:label "Political Instability Impact"@en ;
    rdfs:label "政治不稳定影响"@zh ;
    airo:hasImpactOnStakeholder ex:Microsoft .

ex:InfluencePublicOpinion rdf:type airo:Purpose ;
    rdfs:label "Influence Public Opinion Purpose"@en ;
    rdfs:label "影响公众舆论目的"@zh .

ex:ContentGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Content Generation AI Technique"@en ;
    rdfs:label "内容生成AI技术"@zh .