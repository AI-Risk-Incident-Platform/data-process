@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:PersonalReputationDamageRisk ;
    airo:hasStakeholder ex:QTCinderella ;
    airo:hasRisk ex:DeepfakeMisuseForPublicAttention .

ex:PersonalReputationDamageRisk rdf:type airo:Risk ;
    rdfs:label "Personal Reputation Damage Risk"@en ;
    rdfs:label "个人声誉损害风险"@zh ;
    airo:hasConsequence ex:PublicImageDamage ;
    airo:isRiskSourceFor ex:DeepfakeMisuseForPublicAttention .

ex:QTCinderella rdf:type airo:Stakeholder ;
    rdfs:label "QTCinderella"@en ;
    rdfs:label "QTCinderella"@zh .

ex:PublicImageDamage rdf:type airo:Impact ;
    rdfs:label "Public Image Damage"@en ;
    rdfs:label "公众形象损害"@zh ;
    airo:hasImpactOnStakeholder ex:QTCinderella .

ex:DeepfakeMisuseForPublicAttention rdf:type airo:Misuse ;
    rdfs:label "Deepfake Misuse for Public Attention"@en ;
    rdfs:label "深度伪造技术滥用获取公众关注"@zh ;
    airo:isRiskSourceFor ex:PersonalReputationDamageRisk .