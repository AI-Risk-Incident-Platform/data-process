@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HRResumeScreeningSystem rdf:type airo:AISystem ;
    rdfs:label "HR Resume Screening System"@en ;
    rdfs:label "HR简历筛选系统"@zh ;
    airo:hasRisk ex:UnfairRejectionRisk ;
    airo:hasPurpose ex:AutomatedResumeScreening ;
    airo:isAppliedWithinDomain ex:HumanResources ;
    airo:hasStakeholder ex:HRTeam ;
    airo:isDeployedBy ex:CompanyManagement .

ex:UnfairRejectionRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Rejection Risk"@en ;
    rdfs:label "不公平拒绝风险"@zh ;
    airo:isRiskSourceFor ex:FaultyAlgorithmDesign ;
    airo:hasConsequence ex:HRTeamTermination .

ex:HRTeamTermination rdf:type airo:Consequence ;
    rdfs:label "HR Team Termination Consequence"@en ;
    rdfs:label "人力资源团队解雇后果"@zh ;
    airo:hasImpact ex:LossOfHumanJudgmentInHiring .

ex:HRTeam rdf:type airo:Stakeholder ;
    rdfs:label "HR Team"@en ;
    rdfs:label "人力资源团队"@zh .

ex:CompanyManagement rdf:type airo:AIOperator ;
    rdfs:label "Company Management"@en ;
    rdfs:label "公司管理层"@zh .

ex:LossOfHumanJudgmentInHiring rdf:type airo:Impact ;
    rdfs:label "Loss of Human Judgment in Hiring Impact"@en ;
    rdfs:label "招聘中失去人类判断的影响"@zh ;
    airo:hasImpactOnStakeholder ex:HRTeam .

ex:AutomatedResumeScreening rdf:type airo:Purpose ;
    rdfs:label "Automated Resume Screening Purpose"@en ;
    rdfs:label "自动化简历筛选目的"@zh .

ex:HumanResources rdf:type airo:Domain ;
    rdfs:label "Human Resources Domain"@en ;
    rdfs:label "人力资源领域"@zh .

ex:FaultyAlgorithmDesign rdf:type airo:RiskSource ;
    rdfs:label "Faulty Algorithm Design Risk Source"@en ;
    rdfs:label "有缺陷的算法设计风险源"@zh .