@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:PrivacyViolationAndReputationDamage ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasPurpose ex:ImageManipulation ;
    airo:hasAISubject ex:Lawmaker ;
    airo:isAppliedWithinDomain ex:PoliticalAndPublicFigures .

ex:PrivacyViolationAndReputationDamage rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Reputation Damage Risk"@en ;
    rdfs:label "隐私侵犯与名誉损害风险"@zh ;
    airo:hasConsequence ex:PersonalPrivacyViolation ;
    airo:isRiskSourceFor ex:MaliciousContentCreation .

ex:PersonalPrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Personal Privacy Violation Consequence"@en ;
    rdfs:label "个人隐私侵犯后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "名誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:Lawmaker .

ex:Lawmaker rdf:type airo:AISubject ;
    rdfs:label "Lawmaker"@en ;
    rdfs:label "立法者"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:MaliciousContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creation Risk Source"@en ;
    rdfs:label "恶意内容创建风险源"@zh .

ex:ImageManipulation rdf:type airo:Purpose ;
    rdfs:label "Image Manipulation Purpose"@en ;
    rdfs:label "图像操纵目的"@zh .

ex:PoliticalAndPublicFigures rdf:type airo:Domain ;
    rdfs:label "Political and Public Figures Domain"@en ;
    rdfs:label "政治与公众人物领域"@zh .