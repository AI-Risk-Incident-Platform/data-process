@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClearviewAI rdf:type airo:AISystem ;
    rdfs:label "Clearview AI"@en ;
    rdfs:label "Clearview AI"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasPurpose ex:LawEnforcementSurveillance ;
    airo:hasAISubject ex:AmericanCitizens ;
    airo:hasStakeholder ex:USPostalService ;
    airo:isAppliedWithinDomain ex:GovernmentSurveillance .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:UnauthorizedDataCollection ;
    airo:hasConsequence ex:LossOfPrivacy .

ex:USPostalService rdf:type airo:Stakeholder ;
    rdfs:label "US Postal Service"@en ;
    rdfs:label "美国邮政服务"@zh .

ex:AmericanCitizens rdf:type airo:AISubject ;
    rdfs:label "American Citizens"@en ;
    rdfs:label "美国公民"@zh .

ex:MassSurveillanceImpact rdf:type airo:Impact ;
    rdfs:label "Mass Surveillance Impact"@en ;
    rdfs:label "大规模监控影响"@zh ;
    airo:hasImpactOnStakeholder ex:AmericanCitizens .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:LawEnforcementSurveillance rdf:type airo:Purpose ;
    rdfs:label "Law Enforcement Surveillance Purpose"@en ;
    rdfs:label "执法监控目的"@zh .

ex:GovernmentSurveillance rdf:type airo:Domain ;
    rdfs:label "Government Surveillance Domain"@en ;
    rdfs:label "政府监控领域"@zh .

ex:UnauthorizedDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Data Collection Risk Source"@en ;
    rdfs:label "未经授权的数据收集风险源"@zh .

ex:LossOfPrivacy rdf:type airo:Consequence ;
    rdfs:label "Loss of Privacy Consequence"@en ;
    rdfs:label "隐私丧失后果"@zh ;
    airo:hasImpact ex:MassSurveillanceImpact .