@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh ;
    airo:hasRisk ex:FalseArrestRisk ;
    airo:hasStakeholder ex:DetroitPoliceDepartment ;
    airo:isDeployedBy ex:FacialRecognitionVendor ;
    airo:hasAISubject ex:PregnantWoman .

ex:FalseArrestRisk rdf:type airo:Risk ;
    rdfs:label "False Arrest Risk"@en ;
    rdfs:label "错误逮捕风险"@zh ;
    airo:hasConsequence ex:WrongfulArrest ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:WrongfulArrest rdf:type airo:Consequence ;
    rdfs:label "Wrongful Arrest Consequence"@en ;
    rdfs:label "错误逮捕后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:PregnantWoman .

ex:DetroitPoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "Detroit Police Department"@en ;
    rdfs:label "底特律警察局"@zh .

ex:FacialRecognitionVendor rdf:type airo:AIOperator ;
    rdfs:label "Facial Recognition Vendor"@en ;
    rdfs:label "面部识别技术供应商"@zh .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .

ex:PregnantWoman rdf:type airo:AISubject ;
    rdfs:label "Pregnant Woman"@en ;
    rdfs:label "孕妇"@zh .