@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "面部识别技术"@zh ;
    rdfs:label "Facial Recognition Technology"@en ;
    airo:hasRisk ex:MisidentificationRisk ;
    airo:hasAISubject ex:InnocentIndividuals ;
    airo:hasStakeholder ex:PoliceDepartment .

ex:MisidentificationRisk rdf:type airo:Risk ;
    rdfs:label "误识别风险"@zh ;
    rdfs:label "Misidentification Risk"@en ;
    airo:hasConsequence ex:FalseAccusation ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:FalseAccusation rdf:type airo:Consequence ;
    rdfs:label "错误指控后果"@zh ;
    rdfs:label "False Accusation Consequence"@en ;
    airo:hasImpact ex:ReputationDamage .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "名誉损害影响"@zh ;
    rdfs:label "Reputation Damage Impact"@en ;
    airo:hasImpactOnStakeholder ex:InnocentIndividuals ;
    airo:hasImpactOnStakeholder ex:PoliceDepartment .

ex:InnocentIndividuals rdf:type airo:AISubject ;
    rdfs:label "无辜个体"@zh ;
    rdfs:label "Innocent Individuals"@en .

ex:PoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "警察部门"@zh ;
    rdfs:label "Police Department"@en .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "算法偏见风险源"@zh ;
    rdfs:label "Algorithmic Bias Risk Source"@en .