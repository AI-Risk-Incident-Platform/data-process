ex:BoldGlamourFilter rdf:type airo:AISystem ;
    rdfs:label "Bold Glamour Filter"@en ;
    rdfs:label "Bold Glamour滤镜"@zh ;
    airo:hasRisk ex:LackOfTransparencyRisk ;
    airo:hasStakeholder ex:TikTokPlatform ;
    airo:hasCapability ex:FacialModificationCapability ;
    airo:hasPurpose ex:BeautyEnhancementPurpose .

ex:LackOfTransparencyRisk rdf:type airo:Risk ;
    rdfs:label "Lack of Transparency Risk"@en ;
    rdfs:label "透明度缺失风险"@zh .

ex:TikTokPlatform rdf:type airo:Stakeholder ;
    rdfs:label "TikTok Platform"@en ;
    rdfs:label "TikTok平台"@zh .

ex:FacialModificationCapability rdf:type airo:AICapability ;
    rdfs:label "Facial Modification Capability"@en ;
    rdfs:label "面部修饰能力"@zh .

ex:BeautyEnhancementPurpose rdf:type airo:Purpose ;
    rdfs:label "Beauty Enhancement Purpose"@en ;
    rdfs:label "美颜增强目的"@zh .