@base <https://w3id.org/airo#> .

ex:SamBankmanFriedDeepfake rdf:type airo:AISystem ;
    rdfs:label "Sam Bankman-Fried Deepfake"@en ;
    rdfs:label "Sam Bankman-Fried深度伪造"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:FTXCollapseVictims .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:FTXCollapseVictims rdf:type airo:Stakeholder ;
    rdfs:label "FTX Collapse Victims"@en ;
    rdfs:label "FTX崩溃受害者"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "财务损失"@zh ;
    airo:hasImpact ex:VictimFinancialHarm .

ex:VictimFinancialHarm rdf:type airo:Impact ;
    rdfs:label "Victim Financial Harm"@en ;
    rdfs:label "受害者财务损害"@zh ;
    airo:hasImpactOnStakeholder ex:FTXCollapseVictims .