@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAIOverview rdf:type airo:AISystem ;
    rdfs:label "Google AI Overview"@en ;
    rdfs:label "谷歌AI概览"@zh ;
    airo:hasRisk ex:UnreliableAIResultsRisk ;
    airo:hasStakeholder ex:Google .

ex:UnreliableAIResultsRisk rdf:type airo:Risk ;
    rdfs:label "Unreliable AI Results Risk"@en ;
    rdfs:label "不可靠AI结果风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:InaccurateInformationGeneration .

ex:Google rdf:type airo:Stakeholder ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:InaccurateInformationGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate Information Generation"@en ;
    rdfs:label "不准确信息生成"@zh .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread"@en ;
    rdfs:label "错误信息传播"@zh .