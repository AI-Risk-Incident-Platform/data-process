@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EmotionReadingAIForChildren rdf:type airo:AISystem ;
    rdfs:label "Emotion Reading AI for Children"@en ;
    rdfs:label "儿童情感识别AI"@zh ;
    airo:hasRisk ex:PrivacyRiskInEmotionRecognition ;
    airo:hasAISubject ex:ChildrenAsAISubjects ;
    airo:hasPurpose ex:EducationalEmotionMonitoring ;
    airo:isAppliedWithinDomain ex:EducationalTechnology ;
    airo:hasCapability ex:RealTimeEmotionAnalysis ;
    airo:hasStakeholder ex:EducationalInstitutions .

ex:PrivacyRiskInEmotionRecognition rdf:type airo:Risk ;
    rdfs:label "Privacy Risk in Emotion Recognition"@en ;
    rdfs:label "情感识别中的隐私风险"@zh ;
    airo:hasConsequence ex:PsychologicalProfiling ;
    airo:isRiskSourceFor ex:UnregulatedEmotionAI .

ex:ChildrenAsAISubjects rdf:type airo:AISubject ;
    rdfs:label "Children as AI Subjects"@en ;
    rdfs:label "作为AI主题的儿童"@zh .

ex:EducationalEmotionMonitoring rdf:type airo:Purpose ;
    rdfs:label "Educational Emotion Monitoring"@en ;
    rdfs:label "教育情感监控"@zh .

ex:EducationalTechnology rdf:type airo:Domain ;
    rdfs:label "Educational Technology"@en ;
    rdfs:label "教育技术"@zh .

ex:RealTimeEmotionAnalysis rdf:type airo:AICapability ;
    rdfs:label "Real-time Emotion Analysis Capability"@en ;
    rdfs:label "实时情感分析能力"@zh .

ex:EducationalInstitutions rdf:type airo:Stakeholder ;
    rdfs:label "Educational Institutions"@en ;
    rdfs:label "教育机构"@zh .

ex:UnregulatedEmotionAI rdf:type airo:RiskSource ;
    rdfs:label "Unregulated Emotion AI"@en ;
    rdfs:label "未受监管的情感AI"@zh .

ex:PsychologicalProfiling rdf:type airo:Consequence ;
    rdfs:label "Psychological Profiling Consequence"@en ;
    rdfs:label "心理画像后果"@zh ;
    airo:hasImpact ex:ChildrenPrivacyViolation .

ex:ChildrenPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Children Privacy Violation Impact"@en ;
    rdfs:label "儿童隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChildrenAsAISubjects .