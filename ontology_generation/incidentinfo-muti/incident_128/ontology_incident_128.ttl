@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookFacialRecognitionTech rdf:type airo:AISystem ;
    rdfs:label "Facebook Facial Recognition Tech"@en ;
    rdfs:label "Facebook面部识别技术"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:Facebook .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LegalSettlement .

ex:LegalSettlement rdf:type airo:Consequence ;
    rdfs:label "Legal Settlement Consequence"@en ;
    rdfs:label "法律和解后果"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:Facebook rdf:type airo:Stakeholder ;
    rdfs:label "Facebook"@en ;
    rdfs:label "Facebook"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss Impact"@en ;
    rdfs:label "财务损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Facebook .