@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedImageSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Image System"@en ;
    rdfs:label "AI生成图像系统"@zh ;
    airo:hasRisk ex:GenderBiasInImageGeneration ;
    airo:usesTechnique ex:ImageAutocompletionAlgorithm ;
    airo:hasAISubject ex:FemalePublicFigures .

ex:GenderBiasInImageGeneration rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Image Generation Risk"@en ;
    rdfs:label "图像生成中的性别偏见风险"@zh ;
    airo:hasConsequence ex:StereotypicalGenderRepresentation ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:StereotypicalGenderRepresentation rdf:type airo:Consequence ;
    rdfs:label "Stereotypical Gender Representation Consequence"@en ;
    rdfs:label "刻板性别形象呈现的后果"@zh ;
    airo:hasImpact ex:ReinforcementOfGenderStereotypes .

ex:FemalePublicFigures rdf:type airo:AISubject ;
    rdfs:label "Female Public Figures"@en ;
    rdfs:label "女性公众人物"@zh .

ex:ReinforcementOfGenderStereotypes rdf:type airo:Impact ;
    rdfs:label "Reinforcement of Gender Stereotypes Impact"@en ;
    rdfs:label "强化性别刻板印象的影响"@zh ;
    airo:hasImpactOnStakeholder ex:FemalePublicFigures .

ex:ImageAutocompletionAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "Image Autocompletion Algorithm"@en ;
    rdfs:label "图像自动补全算法"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .