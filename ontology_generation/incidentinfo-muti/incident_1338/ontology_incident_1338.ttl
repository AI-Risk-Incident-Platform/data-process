@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:MisinformationSpreadRisk ;
    airo:hasAISubject ex:PoliticalFigure ;
    airo:hasStakeholder ex:TikTokPlatform .

ex:MisinformationSpreadRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Spread Risk"@en ;
    rdfs:label "虚假信息传播风险"@zh ;
    airo:hasImpact ex:PublicMisinformation .

ex:TikTokPlatform rdf:type airo:Stakeholder ;
    rdfs:label "TikTok Platform"@en ;
    rdfs:label "TikTok平台"@zh .

ex:PoliticalFigure rdf:type airo:AISubject ;
    rdfs:label "Political Figure"@en ;
    rdfs:label "政治人物"@zh .

ex:PublicMisinformation rdf:type airo:Impact ;
    rdfs:label "Public Misinformation Impact"@en ;
    rdfs:label "公众误导影响"@zh ;
    airo:hasImpactOnStakeholder ex:TikTokPlatform .