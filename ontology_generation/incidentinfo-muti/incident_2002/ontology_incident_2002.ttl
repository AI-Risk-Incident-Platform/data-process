ex:MaskedFaceNetDataset rdf:type airo:AISystem ;
    rdfs:label "MaskedFace-Net Dataset"@en ;
    rdfs:label "MaskedFace-Net数据集"@zh ;
    airo:hasRisk ex:PrivacyRiskFromFacialData ;
    airo:usesTechnique ex:DeepLearningForFaceMaskDetection ;
    airo:hasPurpose ex:COVID19PandemicResponse ;
    airo:isAppliedWithinDomain ex:PublicHealthMonitoring .

ex:PrivacyRiskFromFacialData rdf:type airo:Risk ;
    rdfs:label "Privacy Risk from Facial Data"@en ;
    rdfs:label "面部数据隐私风险"@zh .

ex:DeepLearningForFaceMaskDetection rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning for Face Mask Detection Technique"@en ;
    rdfs:label "用于口罩检测的深度学习技术"@zh .

ex:COVID19PandemicResponse rdf:type airo:Purpose ;
    rdfs:label "COVID-19 Pandemic Response Purpose"@en ;
    rdfs:label "COVID-19疫情应对目的"@zh .

ex:PublicHealthMonitoring rdf:type airo:Domain ;
    rdfs:label "Public Health Monitoring Domain"@en ;
    rdfs:label "公共卫生监测领域"@zh .