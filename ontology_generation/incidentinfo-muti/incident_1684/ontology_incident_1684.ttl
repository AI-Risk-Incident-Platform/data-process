@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:FinancialScamRisk ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasAISubject ex:PublicFigures ;
    airo:hasStakeholder ex:SocialMediaUsers ;
    airo:hasPurpose ex:DeceptiveFinancialGain .

ex:FinancialScamRisk rdf:type airo:Risk ;
    rdfs:label "Financial Scam Risk"@en ;
    rdfs:label "金融诈骗风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:MaliciousContentCreators .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:SocialMediaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Social Media Users"@en ;
    rdfs:label "社交媒体用户"@zh .

ex:PublicFigures rdf:type airo:AISubject ;
    rdfs:label "Public Figures"@en ;
    rdfs:label "公众人物"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:SocialMediaUsers .

ex:DeceptiveFinancialGain rdf:type airo:Purpose ;
    rdfs:label "Deceptive Financial Gain Purpose"@en ;
    rdfs:label "欺骗性财务收益目的"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:MaliciousContentCreators rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creators Risk Source"@en ;
    rdfs:label "恶意内容创作者风险源"@zh .