ex:SuicidePreventionAI rdf:type airo:AISystem ;
    rdfs:label "Suicide Prevention AI System"@en ;
    rdfs:label "自杀预防AI系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:MachineLearningModel ;
    airo:hasPurpose ex:SuicidePrevention ;
    airo:isAppliedWithinDomain ex:PublicSafety .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:MachineLearningModel rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning Model"@en ;
    rdfs:label "机器学习模型"@zh .

ex:SuicidePrevention rdf:type airo:Purpose ;
    rdfs:label "Suicide Prevention"@en ;
    rdfs:label "自杀预防"@zh .

ex:PublicSafety rdf:type airo:Domain ;
    rdfs:label "Public Safety"@en ;
    rdfs:label "公共安全"@zh .