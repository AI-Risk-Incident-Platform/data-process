@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AppleFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Apple Facial Recognition System"@en ;
    rdfs:label "苹果面部识别系统"@zh ;
    airo:hasRisk ex:FalseArrestRisk ;
    airo:hasAISubject ex:TeenagerFalselyIdentified ;
    airo:hasStakeholder ex:AppleInc ;
    airo:isDeployedBy ex:AppleStoreSecurity .

ex:FalseArrestRisk rdf:type airo:Risk ;
    rdfs:label "False Arrest Risk"@en ;
    rdfs:label "错误逮捕风险"@zh ;
    airo:hasConsequence ex:LegalActionAgainstApple ;
    airo:isRiskSourceFor ex:FacialRecognitionError .

ex:LegalActionAgainstApple rdf:type airo:Consequence ;
    rdfs:label "Legal Action Against Apple Consequence"@en ;
    rdfs:label "对苹果的法律诉讼后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:TeenagerFalselyIdentified rdf:type airo:AISubject ;
    rdfs:label "Teenager Falsely Identified"@en ;
    rdfs:label "被错误识别的青少年"@zh .

ex:AppleInc rdf:type airo:Stakeholder ;
    rdfs:label "Apple Inc."@en ;
    rdfs:label "苹果公司"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:AppleInc .

ex:FacialRecognitionError rdf:type airo:RiskSource ;
    rdfs:label "Facial Recognition Error Risk Source"@en ;
    rdfs:label "面部识别错误风险源"@zh .

ex:AppleStoreSecurity rdf:type airo:AIOperator ;
    rdfs:label "Apple Store Security"@en ;
    rdfs:label "苹果商店安保"@zh .