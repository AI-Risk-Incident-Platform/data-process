@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeAI rdf:type airo:AISystem ;
    rdfs:label "深度伪造AI"@zh ;
    rdfs:label "Deepfake AI"@en ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:hasStakeholder ex:SlovakianVoters .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "选举干预风险"@zh ;
    rdfs:label "Election Interference Risk"@en ;
    airo:hasConsequence ex:SpreadOfDisinformation ;
    airo:isRiskSourceFor ex:MaliciousActors .

ex:SpreadOfDisinformation rdf:type airo:Consequence ;
    rdfs:label "虚假信息传播后果"@zh ;
    rdfs:label "Spread of Disinformation Consequence"@en ;
    airo:hasImpact ex:DemocraticProcessUndermining .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "政治选举领域"@zh ;
    rdfs:label "Political Elections Domain"@en .

ex:SlovakianVoters rdf:type airo:Stakeholder ;
    rdfs:label "斯洛伐克选民"@zh ;
    rdfs:label "Slovakian Voters"@en .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "深度伪造生成技术"@zh ;
    rdfs:label "Deepfake Generation Technique"@en .

ex:DemocraticProcessUndermining rdf:type airo:Impact ;
    rdfs:label "民主进程破坏影响"@zh ;
    rdfs:label "Democratic Process Undermining Impact"@en ;
    airo:hasImpactOnStakeholder ex:SlovakianVoters .

ex:MaliciousActors rdf:type airo:RiskSource ;
    rdfs:label "恶意行为者风险源"@zh ;
    rdfs:label "Malicious Actors Risk Source"@en .