@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:FakeContentGenerationRisk ;
    airo:hasStakeholder ex:GuardianPublication ;
    airo:isAppliedWithinDomain ex:MediaPublishing .

ex:FakeContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Fake Content Generation Risk"@en ;
    rdfs:label "虚假内容生成风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:hasRiskControl ex:AIWorkingGroupFormation .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpactOnStakeholder ex:GuardianPublication .

ex:GuardianPublication rdf:type airo:Stakeholder ;
    rdfs:label "Guardian Publication"@en ;
    rdfs:label "卫报出版方"@zh .

ex:AIWorkingGroupFormation rdf:type airo:RiskControl ;
    rdfs:label "AI Working Group Formation RiskControl"@en ;
    rdfs:label "AI工作组组建管控"@zh ;
    airo:mitigatesRiskConcept ex:FakeContentGenerationRisk .

ex:MediaPublishing rdf:type airo:Domain ;
    rdfs:label "Media Publishing Domain"@en ;
    rdfs:label "媒体出版领域"@zh .