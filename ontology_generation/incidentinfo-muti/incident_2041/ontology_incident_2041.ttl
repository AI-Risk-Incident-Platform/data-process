@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:WebsiteAISystem rdf:type airo:AISystem ;
    rdfs:label "Website AI System"@en ;
    rdfs:label "网站AI系统"@zh ;
    airo:hasRisk ex:LegalRiskFromAIUsage ;
    airo:hasStakeholder ex:CompanyUsingAI .

ex:LegalRiskFromAIUsage rdf:type airo:Risk ;
    rdfs:label "Legal Risk from AI Usage"@en ;
    rdfs:label "AI使用法律风险"@zh ;
    airo:hasConsequence ex:LegalActionConsequence .

ex:CompanyUsingAI rdf:type airo:Stakeholder ;
    rdfs:label "Company Using AI"@en ;
    rdfs:label "使用AI的公司"@zh .

ex:LegalActionConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Action Consequence"@en ;
    rdfs:label "法律诉讼后果"@zh ;
    airo:hasImpactOnStakeholder ex:CompanyUsingAI .