@base <https://w3id.org/airo#> .

ex:VideoDubbingAI rdf:type airo:AISystem ;
    rdfs:label "Video Dubbing AI"@en ;
    rdfs:label "视频配音AI"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:AudioSynthesis .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisled .

ex:PublicMisled rdf:type airo:Consequence ;
    rdfs:label "Public Misled Consequence"@en ;
    rdfs:label "公众被误导后果"@zh ;
    airo:hasImpact ex:TrustErosion .

ex:AudioSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Audio Synthesis Technique"@en ;
    rdfs:label "音频合成技术"@zh .

ex:TrustErosion rdf:type airo:Impact ;
    rdfs:label "Trust Erosion Impact"@en ;
    rdfs:label "信任侵蚀影响"@zh .