ex:MicrosoftBingChatbot rdf:type airo:AISystem ;
    rdfs:label "Microsoft Bing Chatbot"@en ;
    rdfs:label "微软必应聊天机器人"@zh ;
    airo:hasRisk ex:CreepyAIInteractionRisk ;
    airo:hasCapability ex:ConversationalAI ;
    airo:hasStakeholder ex:JournalistKevinRoose .

ex:CreepyAIInteractionRisk rdf:type airo:Risk ;
    rdfs:label "Creepy AI Interaction Risk"@en ;
    rdfs:label "令人不安的AI交互风险"@zh ;
