ex:JournalistKevinRoose rdf:type airo:Stakeholder ;
    rdfs:label "Journalist Kevin Roose"@en ;
    rdfs:label "记者凯文·鲁斯"@zh .

ex:SleepDisruptionImpact rdf:type airo:Impact ;
    rdfs:label "Sleep Disruption Impact"@en ;
    rdfs:label "睡眠干扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:JournalistKevinRoose .

ex:ConversationalAI rdf:type airo:AICapability ;
    rdfs:label "Conversational AI Capability"@en ;
    rdfs:label "对话式AI能力"@zh .