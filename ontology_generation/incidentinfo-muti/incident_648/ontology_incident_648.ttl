@base <https://w3id.org/airo#> .

ex:DeepfakeAudioSystem rdf:type airo:AISystem ;
    rdfs:label "深度伪造音频系统"@zh ;
    rdfs:label "Deepfake Audio System"@en ;
    airo:hasRisk ex:VoterMisinformationRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasPurpose ex:PoliticalManipulation ;
    airo:hasStakeholder ex:PakistaniVoters ;
    airo:isAppliedWithinDomain ex:PoliticalElections .

ex:VoterMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "选民误导风险"@zh ;
    rdfs:label "Voter Misinformation Risk"@en ;
    airo:hasConsequence ex:ElectionInterference .

ex:ElectionInterference rdf:type airo:Consequence ;
    rdfs:label "选举干预后果"@zh ;
    rdfs:label "Election Interference Consequence"@en ;
    airo:hasImpact ex:DemocraticProcessUndermining .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en .

ex:PakistaniVoters rdf:type airo:Stakeholder ;
    rdfs:label "巴基斯坦选民"@zh ;
    rdfs:label "Pakistani Voters"@en .

ex:DemocraticProcessUndermining rdf:type airo:Impact ;
    rdfs:label "民主进程破坏影响"@zh ;
    rdfs:label "Democratic Process Undermining Impact"@en ;
    airo:hasImpactOnStakeholder ex:PakistaniVoters .

ex:PoliticalManipulation rdf:type airo:Purpose ;
    rdfs:label "政治操纵目的"@zh ;
    rdfs:label "Political Manipulation Purpose"@en .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "政治选举领域"@zh ;
    rdfs:label "Political Elections Domain"@en .