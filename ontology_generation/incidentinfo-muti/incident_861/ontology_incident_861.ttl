@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:XPlatform rdf:type airo:AISystem ;
    rdfs:label "X Platform"@en ;
    rdfs:label "X平台"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:hasAISubject ex:GhanaElectionProcess ;
    airo:hasStakeholder ex:GhanaGovernment .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:isRiskSourceFor ex:FakeAccounts .

ex:GhanaGovernment rdf:type airo:Stakeholder ;
    rdfs:label "Ghana Government"@en ;
    rdfs:label "加纳政府"@zh .

ex:GhanaElectionProcess rdf:type airo:AISubject ;
    rdfs:label "Ghana Election Process"@en ;
    rdfs:label "加纳选举过程"@zh .

ex:FakeAccounts rdf:type airo:RiskSource ;
    rdfs:label "Fake Accounts"@en ;
    rdfs:label "虚假账户"@zh .