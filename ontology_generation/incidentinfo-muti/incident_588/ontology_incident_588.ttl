@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CignaClaimsProcessingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Cigna Claims Processing Algorithm"@en ;
    rdfs:label "Cigna理赔处理算法"@zh ;
    airo:hasRisk ex:UnfairClaimDenialRisk ;
    airo:isDeployedBy ex:CignaCorporation ;
    airo:hasAISubject ex:MedicalInsuranceClaimants ;
    airo:isAppliedWithinDomain ex:HealthcareInsurance ;
    airo:hasPurpose ex:StreamliningInsuranceClaimProcessing .

ex:UnfairClaimDenialRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Claim Denial Risk"@en ;
    rdfs:label "不公平理赔拒绝风险"@zh ;
    airo:hasConsequence ex:PatientsDeniedNecessaryMedicalCoverage ;
    airo:isRiskSourceFor ex:AutomatedClaimReviewWithoutHumanOversight .

ex:CignaCorporation rdf:type airo:AIOperator ;
    rdfs:label "Cigna Corporation"@en ;
    rdfs:label "Cigna公司"@zh .

ex:MedicalInsuranceClaimants rdf:type airo:AISubject ;
    rdfs:label "Medical Insurance Claimants"@en ;
    rdfs:label "医疗保险申请人"@zh .

ex:HealthcareInsurance rdf:type airo:Domain ;
    rdfs:label "Healthcare Insurance Domain"@en ;
    rdfs:label "医疗保险领域"@zh .

ex:PatientsDeniedNecessaryMedicalCoverage rdf:type airo:Consequence ;
    rdfs:label "Patients Denied Necessary Medical Coverage Consequence"@en ;
    rdfs:label "患者被拒绝必要医疗保险的后果"@zh ;
    airo:hasImpact ex:FinancialBurdenOnPatients .

ex:FinancialBurdenOnPatients rdf:type airo:Impact ;
    rdfs:label "Financial Burden on Patients Impact"@en ;
    rdfs:label "患者经济负担加重的影响"@zh ;
    airo:hasImpactOnStakeholder ex:MedicalInsuranceClaimants .

ex:AutomatedClaimReviewWithoutHumanOversight rdf:type airo:RiskSource ;
    rdfs:label "Automated Claim Review Without Human Oversight Risk Source"@en ;
    rdfs:label "无人监督的自动理赔审核风险源"@zh .

ex:StreamliningInsuranceClaimProcessing rdf:type airo:Purpose ;
    rdfs:label "Streamlining Insurance Claim Processing Purpose"@en ;
    rdfs:label "简化保险理赔处理流程的目的"@zh .