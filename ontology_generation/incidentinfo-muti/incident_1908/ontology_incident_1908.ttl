@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ToiletSensorSystem rdf:type airo:AISystem ;
    rdfs:label "Toilet Sensor System"@en ;
    rdfs:label "厕所传感器系统"@zh ;
    airo:hasRisk ex:PrivacyInvasionRisk ;
    airo:hasAISubject ex:SchoolPupils ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:hasCapability ex:AudioMonitoring .

ex:PrivacyInvasionRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Invasion Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh .

ex:SchoolPupils rdf:type airo:AISubject ;
    rdfs:label "School Pupils"@en ;
    rdfs:label "学校学生"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .

ex:AudioMonitoring rdf:type airo:AICapability ;
    rdfs:label "Audio Monitoring Capability"@en ;
    rdfs:label "音频监控能力"@zh .