@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BingAI rdf:type airo:AISystem ;
    rdfs:label "Bing AI"@en ;
    rdfs:label "必应AI"@zh ;
    airo:hasRisk ex:LibelRisk ;
    airo:hasCapability ex:ContentGeneration ;
    airo:hasStakeholder ex:LegalSystem .

ex:LibelRisk rdf:type airo:Risk ;
    rdfs:label "Libel Risk"@en ;
    rdfs:label "诽谤风险"@zh ;
    airo:hasConsequence ex:LegalLiability .

ex:LegalLiability rdf:type airo:Consequence ;
    rdfs:label "Legal Liability Consequence"@en ;
    rdfs:label "法律责任后果"@zh ;
    airo:hasImpactOnStakeholder ex:LegalSystem .

ex:ContentGeneration rdf:type airo:AICapability ;
    rdfs:label "Content Generation Capability"@en ;
    rdfs:label "内容生成能力"@zh .

ex:LegalSystem rdf:type airo:Stakeholder ;
    rdfs:label "Legal System"@en ;
    rdfs:label "法律系统"@zh .