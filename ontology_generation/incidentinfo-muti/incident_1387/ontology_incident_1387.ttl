@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSoftware rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Software"@en ;
    rdfs:label "面部识别软件"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:TransgenderYouTubers ;
    airo:hasAISubject ex:TransPeople .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:ExposedDataset ;
    airo:hasImpact ex:PrivacyViolationImpact .

ex:TransgenderYouTubers rdf:type airo:Stakeholder ;
    rdfs:label "Transgender YouTubers"@en ;
    rdfs:label "跨性别YouTuber"@zh .

ex:TransPeople rdf:type airo:AISubject ;
    rdfs:label "Trans People"@en ;
    rdfs:label "跨性别者"@zh .

ex:ExposedDataset rdf:type airo:RiskSource ;
    rdfs:label "Exposed Dataset"@en ;
    rdfs:label "暴露的数据集"@zh .

ex:PrivacyViolationImpact rdf:type airo:Impact ;
    rdfs:label "Privacy Violation Impact"@en ;
    rdfs:label "隐私侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:TransgenderYouTubers .