ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:InappropriateUseOfAIInLegalDocuments ;
    airo:hasAIOperator ex:VictorianChildProtectionWorker ;
    airo:hasStakeholder ex:VictorianCourtSystem ;
    airo:isAppliedWithinDomain ex:ChildProtectionServices ;
    airo:hasPurpose ex:DraftingChildProtectionReports .

ex:InappropriateUseOfAIInLegalDocuments rdf:type airo:Risk ;
    rdfs:label "Inappropriate Use of AI in Legal Documents Risk"@en ;
    rdfs:label "在法律文件中不当使用AI的风险"@zh ;
    airo:hasConsequence ex:PotentialLegalInaccuracies ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfAIInLegalContext .

ex:PotentialLegalInaccuracies rdf:type airo:Consequence ;
    rdfs:label "Potential Legal Inaccuracies Consequence"@en ;
    rdfs:label "潜在法律不准确的后果"@zh ;
    airo:hasImpact ex:CompromisedLegalProceedings .

ex:VictorianChildProtectionWorker rdf:type airo:AIOperator ;
    rdfs:label "Victorian Child Protection Worker"@en ;
    rdfs:label "维多利亚州儿童保护工作者"@zh .

ex:VictorianCourtSystem rdf:type airo:Stakeholder ;
    rdfs:label "Victorian Court System"@en ;
    rdfs:label "维多利亚州法院系统"@zh ;
    airo:hasImpactOnStakeholder ex:CompromisedLegalProceedings .

ex:ChildProtectionServices rdf:type airo:Domain ;
    rdfs:label "Child Protection Services Domain"@en ;
    rdfs:label "儿童保护服务领域"@zh .

ex:DraftingChildProtectionReports rdf:type airo:Purpose ;
    rdfs:label "Drafting Child Protection Reports Purpose"@en ;
