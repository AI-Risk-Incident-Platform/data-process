    rdfs:label "Spam and Porn Websites"@en ;
    rdfs:label "垃圾邮件和色情网站"@zh .

ex:ModelBiasAndInaccuracy rdf:type airo:Consequence ;
    rdfs:label "Model Bias and Inaccuracy"@en ;
    rdfs:label "模型偏见和不准确性"@zh .

ex:TokenTraining rdf:type airo:AITechnique ;
