ex:CreditScoringAI rdf:type airo:AISystem ;
    rdfs:label "Credit Scoring AI System"@en ;
    rdfs:label "信用评分AI系统"@zh ;
    airo:hasRisk ex:BiasInCreditScoring ;
    airo:isAppliedWithinDomain ex:FinancialServices ;
    airo:hasStakeholder ex:LoanApplicants .

ex:BiasInCreditScoring rdf:type airo:Risk ;
    rdfs:label "Bias in Credit Scoring Risk"@en ;
    rdfs:label "信用评分中的偏见风险"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .

ex:LoanApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Loan Applicants"@en ;
    rdfs:label "贷款申请人"@zh .