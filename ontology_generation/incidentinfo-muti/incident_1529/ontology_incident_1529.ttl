@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonProductListingAI rdf:type airo:AISystem ;
    rdfs:label "亚马逊产品列表AI"@zh ;
    rdfs:label "Amazon Product Listing AI"@en ;
    airo:hasRisk ex:InappropriateProductNamingRisk ;
    airo:usesTechnique ex:AutomatedProductNamingAlgorithm ;
    airo:hasStakeholder ex:AmazonSellers .

ex:InappropriateProductNamingRisk rdf:type airo:Risk ;
    rdfs:label "不当产品命名风险"@zh ;
    rdfs:label "Inappropriate Product Naming Risk"@en ;
    airo:hasConsequence ex:ConfusingProductListings ;
    airo:isRiskSourceFor ex:OverrelianceOnAIForContentGeneration .

ex:ConfusingProductListings rdf:type airo:Consequence ;
    rdfs:label "混乱的产品列表后果"@zh ;
    rdfs:label "Confusing Product Listings Consequence"@en .

ex:AutomatedProductNamingAlgorithm rdf:type airo:AITechnique ;
    rdfs:label "自动产品命名算法技术"@zh ;
    rdfs:label "Automated Product Naming Algorithm Technique"@en .

ex:AmazonSellers rdf:type airo:Stakeholder ;
    rdfs:label "亚马逊卖家"@zh ;
    rdfs:label "Amazon Sellers"@en .

ex:OverrelianceOnAIForContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "过度依赖AI生成内容的风险源"@zh ;
    rdfs:label "Overreliance on AI for Content Generation Risk Source"@en .