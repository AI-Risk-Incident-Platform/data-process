@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIModelsForPoliticalInformation rdf:type airo:AISystem ;
    rdfs:label "AI Models for Political Information"@en ;
    rdfs:label "政治信息AI模型"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:hasCapability ex:TextGeneration ;
    airo:isAppliedWithinDomain ex:PoliticalInformation ;
    airo:hasStakeholder ex:Voters .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicMisinformation ;
    airo:isRiskSourceFor ex:UnverifiedDataSources .

ex:PublicMisinformation rdf:type airo:Consequence ;
    rdfs:label "Public Misinformation Consequence"@en ;
    rdfs:label "公众误导后果"@zh ;
    airo:hasImpact ex:ElectionInterference .

ex:ElectionInterference rdf:type airo:Impact ;
    rdfs:label "Election Interference Impact"@en ;
    rdfs:label "选举干预影响"@zh ;
    airo:hasImpactOnStakeholder ex:Voters .

ex:PoliticalInformation rdf:type airo:Domain ;
    rdfs:label "Political Information Domain"@en ;
    rdfs:label "政治信息领域"@zh .

ex:TextGeneration rdf:type airo:AICapability ;
    rdfs:label "Text Generation Capability"@en ;
    rdfs:label "文本生成能力"@zh .

ex:UnverifiedDataSources rdf:type airo:RiskSource ;
    rdfs:label "Unverified Data Sources Risk Source"@en ;
    rdfs:label "未经验证数据源"@zh .

ex:Voters rdf:type airo:Stakeholder ;
    rdfs:label "Voters Stakeholder"@en ;
    rdfs:label "选民"@zh .