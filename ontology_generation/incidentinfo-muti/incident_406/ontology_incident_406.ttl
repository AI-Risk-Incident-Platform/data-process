@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClearviewAI rdf:type airo:AISystem ;
    rdfs:label "Clearview AI"@en ;
    rdfs:label "Clearview AI"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAICapability ex:FacialRecognition ;
    airo:hasStakeholder ex:PoliceDepartments ;
    airo:isDeployedBy ex:FinnishPolice .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:LackOfRegulation ;
    airo:hasImpact ex:PersonalDataExposure .

ex:PoliceDepartments rdf:type airo:Stakeholder ;
    rdfs:label "Police Departments"@en ;
    rdfs:label "警察部门"@zh .

ex:FinnishPolice rdf:type airo:AIOperator ;
    rdfs:label "Finnish Police"@en ;
    rdfs:label "芬兰警察"@zh .

ex:FacialRecognition rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition"@en ;
    rdfs:label "面部识别"@zh .

ex:PersonalDataExposure rdf:type airo:Impact ;
    rdfs:label "Personal Data Exposure"@en ;
    rdfs:label "个人数据暴露"@zh .

ex:LackOfRegulation rdf:type airo:RiskSource ;
    rdfs:label "Lack of Regulation"@en ;
    rdfs:label "缺乏监管"@zh .