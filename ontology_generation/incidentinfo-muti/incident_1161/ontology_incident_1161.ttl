@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PassportVerificationAI rdf:type airo:AISystem ;
    rdfs:label "Passport Verification AI"@en ;
    rdfs:label "护照验证AI"@zh ;
    airo:hasRisk ex:RacialBiasInPassportVerification ;
    airo:hasAISubject ex:PassportApplicant ;
    airo:isAppliedWithinDomain ex:ImmigrationAndBorderControl .

ex:RacialBiasInPassportVerification rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Passport Verification Risk"@en ;
    rdfs:label "护照验证中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryOutcomes ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:PassportApplicant rdf:type airo:AISubject ;
    rdfs:label "Passport Applicant"@en ;
    rdfs:label "护照申请人"@zh .

ex:UnfairDenialOfPassportApplication rdf:type airo:Impact ;
    rdfs:label "Unfair Denial of Passport Application Impact"@en ;
    rdfs:label "护照申请被不公平拒绝的影响"@zh ;
    airo:hasImpactOnStakeholder ex:PassportApplicant .

ex:ImmigrationAndBorderControl rdf:type airo:Domain ;
    rdfs:label "Immigration and Border Control Domain"@en ;
    rdfs:label "移民和边境控制领域"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:DiscriminatoryOutcomes rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Outcomes Consequence"@en ;
    rdfs:label "歧视性结果后果"@zh ;
    airo:hasImpact ex:UnfairDenialOfPassportApplication .