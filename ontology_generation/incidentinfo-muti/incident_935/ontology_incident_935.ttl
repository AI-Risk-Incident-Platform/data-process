@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RomanceScamAI rdf:type airo:AISystem ;
    rdfs:label "Romance Scam AI"@en ;
    rdfs:label "浪漫诈骗AI"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:ImpersonationTechnique ;
    airo:hasStakeholder ex:ScamVictim .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:OnlineDatingPlatform .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:EmotionalDistress .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Emotional Distress Impact"@en ;
    rdfs:label "情感困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:ScamVictim .

ex:ScamVictim rdf:type airo:Stakeholder ;
    rdfs:label "Scam Victim"@en ;
    rdfs:label "诈骗受害者"@zh .

ex:ImpersonationTechnique rdf:type airo:AITechnique ;
    rdfs:label "Impersonation Technique"@en ;
    rdfs:label "冒充技术"@zh .

ex:OnlineDatingPlatform rdf:type airo:RiskSource ;
    rdfs:label "Online Dating Platform Risk Source"@en ;
    rdfs:label "在线交友平台风险源"@zh .