@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasAISubject ex:ScamVictims ;
    airo:hasStakeholder ex:IndonesianPublic ;
    airo:usesTechnique ex:FacialManipulation ;
    airo:hasPurpose ex:DeceptiveScams .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:MisuseOfDeepfakeTechnology .

ex:ScamVictims rdf:type airo:AISubject ;
    rdfs:label "Scam Victims"@en ;
    rdfs:label "诈骗受害者"@zh .

ex:IndonesianPublic rdf:type airo:Stakeholder ;
    rdfs:label "Indonesian Public"@en ;
    rdfs:label "印尼公众"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:FacialManipulation rdf:type airo:AITechnique ;
    rdfs:label "Facial Manipulation Technique"@en ;
    rdfs:label "面部操纵技术"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:IndonesianPublic .

ex:DeceptiveScams rdf:type airo:Purpose ;
    rdfs:label "Deceptive Scams"@en ;
    rdfs:label "欺骗性诈骗"@zh .

ex:MisuseOfDeepfakeTechnology rdf:type airo:RiskSource ;
    rdfs:label "Misuse of Deepfake Technology"@en ;
    rdfs:label "深度伪造技术滥用"@zh .