@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CruiseRobotaxi rdf:type airo:AISystem ;
    rdfs:label "Cruise Robotaxi"@en ;
    rdfs:label "Cruise自动驾驶出租车"@zh ;
    airo:hasRisk ex:PedestrianSafetyRisk ;
    airo:isDeployedBy ex:GeneralMotors ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleTransportation ;
    airo:hasStakeholder ex:CaliforniaRegulators .

ex:PedestrianSafetyRisk rdf:type airo:Risk ;
    rdfs:label "Pedestrian Safety Risk"@en ;
    rdfs:label "行人安全风险"@zh ;
    airo:hasConsequence ex:PhysicalHarmToPedestrians ;
    airo:isRiskSourceFor ex:SystemFailureInEmergencySituations .

ex:PhysicalHarmToPedestrians rdf:type airo:Consequence ;
    rdfs:label "Physical Harm to Pedestrians"@en ;
    rdfs:label "对行人造成身体伤害"@zh ;
    airo:hasImpact ex:PublicSafetyConcerns .

ex:GeneralMotors rdf:type airo:AIOperator ;
    rdfs:label "General Motors"@en ;
    rdfs:label "通用汽车"@zh .

ex:CaliforniaRegulators rdf:type airo:Stakeholder ;
    rdfs:label "California Regulators"@en ;
    rdfs:label "加州监管机构"@zh .

ex:PublicSafetyConcerns rdf:type airo:Impact ;
    rdfs:label "Public Safety Concerns"@en ;
    rdfs:label "公共安全问题"@zh ;
    airo:hasImpactOnStakeholder ex:CaliforniaRegulators .

ex:AutonomousVehicleTransportation rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Transportation Domain"@en ;
    rdfs:label "自动驾驶交通领域"@zh .

ex:SystemFailureInEmergencySituations rdf:type airo:RiskSource ;
    rdfs:label "System Failure in Emergency Situations"@en ;
    rdfs:label "紧急情况下的系统故障"@zh .