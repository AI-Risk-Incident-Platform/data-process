@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SelfDrivingCarSystem rdf:type airo:AISystem ;
    rdfs:label "Self-Driving Car System"@en ;
    rdfs:label "自动驾驶汽车系统"@zh ;
    airo:hasRisk ex:PedestrianDetectionBiasRisk ;
    airo:hasCapability ex:PedestrianDetectionCapability ;
    airo:hasAISubject ex:DarkSkinnedPedestrians ;
    airo:hasVulnerability ex:AlgorithmicBiasInComputerVision .

ex:PedestrianDetectionBiasRisk rdf:type airo:Risk ;
    rdfs:label "Pedestrian Detection Bias Risk"@en ;
    rdfs:label "行人检测偏见风险"@zh ;
    airo:hasConsequence ex:IncreasedAccidentRiskForDarkSkinnedPedestrians ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:IncreasedAccidentRiskForDarkSkinnedPedestrians rdf:type airo:Consequence ;
    rdfs:label "Increased Accident Risk for Dark-Skinned Pedestrians"@en ;
    rdfs:label "深色皮肤行人事故风险增加"@zh ;
    airo:hasImpact ex:RacialDiscriminationInTransportationSafety .

ex:PedestrianDetectionCapability rdf:type airo:AICapability ;
    rdfs:label "Pedestrian Detection Capability"@en ;
    rdfs:label "行人检测能力"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "偏见训练数据"@zh .

ex:RacialDiscriminationInTransportationSafety rdf:type airo:Impact ;
    rdfs:label "Racial Discrimination in Transportation Safety Impact"@en ;
    rdfs:label "交通安全中的种族歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:DarkSkinnedPedestrians .

ex:DarkSkinnedPedestrians rdf:type airo:AISubject ;
    rdfs:label "Dark-Skinned Pedestrians"@en ;
    rdfs:label "深色皮肤行人"@zh .

ex:AlgorithmicBiasInComputerVision rdf:type airo:Vulnerability ;
    rdfs:label "Algorithmic Bias in Computer Vision Vulnerability"@en ;
    rdfs:label "计算机视觉中的算法偏见脆弱性"@zh .