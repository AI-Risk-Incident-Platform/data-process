@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AutomaticTranscriptionSystem rdf:type airo:AISystem ;
    rdfs:label "Automatic Transcription System"@en ;
    rdfs:label "自动转录系统"@zh ;
    airo:hasRisk ex:TranscriptionErrorRisk ;
    airo:isAppliedWithinDomain ex:JudicialSystem .

ex:TranscriptionErrorRisk rdf:type airo:Risk ;
    rdfs:label "Transcription Error Risk"@en ;
    rdfs:label "转录错误风险"@zh ;
    airo:hasConsequence ex:LegalDecisionErrors .

ex:JudicialSystem rdf:type airo:Domain ;
    rdfs:label "Judicial System"@en ;
    rdfs:label "司法系统"@zh .

ex:LegalDecisionErrors rdf:type airo:Consequence ;
    rdfs:label "Legal Decision Errors"@en ;
    rdfs:label "法律决策错误"@zh ;
    airo:hasImpact ex:JusticeSystemCompromise .

ex:JusticeSystemCompromise rdf:type airo:Impact ;
    rdfs:label "Justice System Compromise"@en ;
    rdfs:label "司法系统受损"@zh .