@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "AI Image Generation System"@en ;
    rdfs:label "AI图像生成系统"@zh ;
    airo:hasRisk ex:InappropriateContentGenerationRisk ;
    airo:isAppliedWithinDomain ex:EducationalInstitution ;
    airo:hasStakeholder ex:SchoolAdministration .

ex:InappropriateContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Generation Risk"@en ;
    rdfs:label "不当内容生成风险"@zh ;
    airo:hasConsequence ex:InvestigationTriggered .

ex:InvestigationTriggered rdf:type airo:Consequence ;
    rdfs:label "Investigation Triggered Consequence"@en ;
    rdfs:label "触发调查的后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:EducationalInstitution rdf:type airo:Domain ;
    rdfs:label "Educational Institution Domain"@en ;
    rdfs:label "教育机构领域"@zh .

ex:SchoolAdministration rdf:type airo:Stakeholder ;
    rdfs:label "School Administration Stakeholder"@en ;
    rdfs:label "学校管理部门"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害的影响"@zh ;
    airo:hasImpactOnStakeholder ex:SchoolAdministration .