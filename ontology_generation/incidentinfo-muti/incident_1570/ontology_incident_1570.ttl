@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isProvidedBy ex:OpenAI ;
    airo:usesTechnique ex:TextDataTraining ;
    airo:hasPurpose ex:LanguageModelTraining ;
    airo:isAppliedWithinDomain ex:MediaAndPublishing ;
    airo:hasStakeholder ex:NewsPublishers .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:ContentBlocking .

ex:NewsPublishers rdf:type airo:Stakeholder ;
    rdfs:label "News Publishers"@en ;
    rdfs:label "新闻出版商"@zh .

ex:OpenAI rdf:type airo:AIProvider ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:ContentBlocking rdf:type airo:Consequence ;
    rdfs:label "Content Blocking"@en ;
    rdfs:label "内容屏蔽"@zh ;
    airo:hasImpactOnStakeholder ex:NewsPublishers .

ex:TextDataTraining rdf:type airo:AITechnique ;
    rdfs:label "Text Data Training"@en ;
    rdfs:label "文本数据训练"@zh .

ex:LanguageModelTraining rdf:type airo:Purpose ;
    rdfs:label "Language Model Training"@en ;
    rdfs:label "语言模型训练"@zh .

ex:MediaAndPublishing rdf:type airo:Domain ;
    rdfs:label "Media and Publishing"@en ;
    rdfs:label "媒体与出版"@zh .