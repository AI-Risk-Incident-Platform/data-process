@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ProChinaYouTubeNetwork rdf:type airo:AISystem ;
    rdfs:label "Pro-China YouTube Network"@en ;
    rdfs:label "亲华YouTube网络"@zh ;
    airo:hasRisk ex:InfluenceOperationRisk ;
    airo:hasCapability ex:ContentGenerationAndDissemination ;
    airo:hasStakeholder ex:USGovernment .

ex:InfluenceOperationRisk rdf:type airo:Risk ;
    rdfs:label "Influence Operation Risk"@en ;
    rdfs:label "影响力操作风险"@zh ;
    airo:hasConsequence ex:PublicOpinionManipulation .

ex:USGovernment rdf:type airo:Stakeholder ;
    rdfs:label "US Government"@en ;
    rdfs:label "美国政府"@zh .

ex:ContentGenerationAndDissemination rdf:type airo:AICapability ;
    rdfs:label "Content Generation and Dissemination Capability"@en ;
    rdfs:label "内容生成与传播能力"@zh .

ex:PublicOpinionManipulation rdf:type airo:Impact ;
    rdfs:label "Public Opinion Manipulation Impact"@en ;
    rdfs:label "舆论操纵影响"@zh ;
    airo:hasImpactOnStakeholder ex:USGovernment .