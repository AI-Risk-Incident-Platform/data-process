@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RunwayVideoAI rdf:type airo:AISystem ;
    rdfs:label "Runway Video AI"@en ;
    rdfs:label "Runway视频AI"@zh ;
    airo:hasRisk ex:UnauthorizedDataScrapingRisk ;
    airo:usesTechnique ex:VideoGenerationAI ;
    airo:isAppliedWithinDomain ex:VideoContentCreation ;
    airo:hasStakeholder ex:MKBHD .

ex:UnauthorizedDataScrapingRisk rdf:type airo:Risk ;
    rdfs:label "Unauthorized Data Scraping Risk"@en ;
    rdfs:label "未经授权的数据抓取风险"@zh ;
    airo:hasConsequence ex:CopyrightViolation ;
    airo:isRiskSourceFor ex:DataScrapingWithoutConsent .

ex:MKBHD rdf:type airo:Stakeholder ;
    rdfs:label "MKBHD"@en ;
    rdfs:label "MKBHD"@zh .

ex:DataScrapingWithoutConsent rdf:type airo:RiskSource ;
    rdfs:label "Data Scraping Without Consent"@en ;
    rdfs:label "未经同意的数据抓取"@zh .

ex:CopyrightViolation rdf:type airo:Consequence ;
    rdfs:label "Copyright Violation"@en ;
    rdfs:label "版权侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:MKBHD .

ex:VideoGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Video Generation AI Technique"@en ;
    rdfs:label "视频生成AI技术"@zh .

ex:VideoContentCreation rdf:type airo:Domain ;
    rdfs:label "Video Content Creation Domain"@en ;
    rdfs:label "视频内容创作领域"@zh .