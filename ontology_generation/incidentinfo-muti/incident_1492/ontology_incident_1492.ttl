@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OpenAIGPT rdf:type airo:AISystem ;
    rdfs:label "OpenAI GPT"@en ;
    rdfs:label "OpenAI GPT"@zh ;
    airo:hasRisk ex:RacialBiasInHiring ;
    airo:hasCapability ex:AutomatedResumeScreening ;
    airo:hasPurpose ex:StreamliningRecruitmentProcess ;
    airo:hasStakeholder ex:JobApplicants ;
    airo:isAppliedWithinDomain ex:HumanResources .

ex:RacialBiasInHiring rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Hiring Risk"@en ;
    rdfs:label "招聘中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryHiringPractices ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:DiscriminatoryHiringPractices rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Hiring Practices Consequence"@en ;
    rdfs:label "歧视性招聘行为后果"@zh ;
    airo:hasImpact ex:UnfairEmploymentOpportunities .

ex:AutomatedResumeScreening rdf:type airo:AICapability ;
    rdfs:label "Automated Resume Screening Capability"@en ;
    rdfs:label "自动化简历筛选能力"@zh .

ex:JobApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Job Applicants"@en ;
    rdfs:label "求职者"@zh .

ex:UnfairEmploymentOpportunities rdf:type airo:Impact ;
    rdfs:label "Unfair Employment Opportunities Impact"@en ;
    rdfs:label "不公平就业机会影响"@zh ;
    airo:hasImpactOnStakeholder ex:JobApplicants .

ex:StreamliningRecruitmentProcess rdf:type airo:Purpose ;
    rdfs:label "Streamlining Recruitment Process Purpose"@en ;
    rdfs:label "简化招聘流程目的"@zh .

ex:HumanResources rdf:type airo:Domain ;
    rdfs:label "Human Resources Domain"@en ;
    rdfs:label "人力资源领域"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "偏见训练数据风险源"@zh .