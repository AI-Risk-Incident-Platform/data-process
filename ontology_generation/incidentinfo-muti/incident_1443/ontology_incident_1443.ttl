@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeAdCreationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Ad Creation System"@en ;
    rdfs:label "深度伪造广告创建系统"@zh ;
    airo:hasRisk ex:IdentityTheftAndMisuseRisk ;
    airo:usesTechnique ex:DeepfakeGenerationTechnology ;
    airo:hasPurpose ex:CommercialAdvertising ;
    airo:hasStakeholder ex:VictimOfDeepfake ;
    airo:hasAISubject ex:IndividualsWithDigitalPresence ;
    airo:isAppliedWithinDomain ex:DigitalAdvertising .

ex:IdentityTheftAndMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Identity Theft and Misuse Risk"@en ;
    rdfs:label "身份盗用和滥用风险"@zh ;
    airo:hasConsequence ex:UnauthorizedUseOfPersonalLikeness ;
    airo:isRiskSourceFor ex:UnethicalAIUsageByCompanies .

ex:UnauthorizedUseOfPersonalLikeness rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Use of Personal Likeness Consequence"@en ;
    rdfs:label "未经授权使用个人形象后果"@zh ;
    airo:hasImpact ex:ReputationDamageAndEmotionalDistress .

ex:VictimOfDeepfake rdf:type airo:Stakeholder ;
    rdfs:label "Victim of Deepfake"@en ;
    rdfs:label "深度伪造受害者"@zh .

ex:ReputationDamageAndEmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Reputation Damage and Emotional Distress Impact"@en ;
    rdfs:label "声誉损害和情感困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:VictimOfDeepfake .

ex:DeepfakeGenerationTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technology"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:UnethicalAIUsageByCompanies rdf:type airo:RiskSource ;
    rdfs:label "Unethical AI Usage by Companies Risk Source"@en ;
    rdfs:label "公司不道德使用AI的风险源"@zh .

ex:CommercialAdvertising rdf:type airo:Purpose ;
    rdfs:label "Commercial Advertising Purpose"@en ;
    rdfs:label "商业广告目的"@zh .

ex:DigitalAdvertising rdf:type airo:Domain ;
    rdfs:label "Digital Advertising Domain"@en ;
    rdfs:label "数字广告领域"@zh .

ex:IndividualsWithDigitalPresence rdf:type airo:AISubject ;
    rdfs:label "Individuals with Digital Presence"@en ;
    rdfs:label "有数字存在的个人"@zh .