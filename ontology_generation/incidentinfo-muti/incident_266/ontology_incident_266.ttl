@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UberEatsFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Uber Eats面部识别系统"@zh ;
    rdfs:label "Uber Eats Facial Recognition System"@en ;
    airo:hasRisk ex:RacialDiscriminationInFacialRecognition ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:UberEatsCouriers ;
    airo:isDevelopedBy ex:UberTechnologiesInc .

ex:RacialDiscriminationInFacialRecognition rdf:type airo:Risk ;
    rdfs:label "面部识别中的种族歧视风险"@zh ;
    rdfs:label "Racial Discrimination in Facial Recognition Risk"@en ;
    airo:hasConsequence ex:UnfairDismissalOfCouriers ;
    airo:isRiskSourceFor ex:BiasedAlgorithmDesign .

ex:UberEatsCouriers rdf:type airo:Stakeholder ;
    rdfs:label "Uber Eats快递员"@zh ;
    rdfs:label "Uber Eats Couriers"@en .

ex:UberTechnologiesInc rdf:type airo:AIOperator ;
    rdfs:label "优步科技公司"@zh ;
    rdfs:label "Uber Technologies Inc."@en .

ex:UnfairDismissalOfCouriers rdf:type airo:Consequence ;
    rdfs:label "快递员被不公平解雇的后果"@zh ;
    rdfs:label "Unfair Dismissal of Couriers Consequence"@en ;
    airo:hasImpact ex:DiscriminationAgainstMinorityGroups .

ex:DiscriminationAgainstMinorityGroups rdf:type airo:Impact ;
    rdfs:label "对少数族裔群体歧视的影响"@zh ;
    rdfs:label "Discrimination Against Minority Groups Impact"@en ;
    airo:hasImpactOnStakeholder ex:UberEatsCouriers .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "面部识别技术"@zh ;
    rdfs:label "Facial Recognition Technology"@en .

ex:BiasedAlgorithmDesign rdf:type airo:RiskSource ;
    rdfs:label "有偏见的算法设计风险源"@zh ;
    rdfs:label "Biased Algorithm Design Risk Source"@en .