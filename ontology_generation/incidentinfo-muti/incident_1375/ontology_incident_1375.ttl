@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MegaFaceFacialRecognitionDataset rdf:type airo:AISystem ;
    rdfs:label "MegaFace Facial Recognition Dataset"@en ;
    rdfs:label "MegaFace面部识别数据集"@zh ;
    airo:hasRisk ex:PrivacyAndLiabilityConcerns ;
    airo:hasAISubject ex:ChildrenInFlickrPhotos .

ex:PrivacyAndLiabilityConcerns rdf:type airo:Risk ;
    rdfs:label "Privacy and Liability Concerns Risk"@en ;
    rdfs:label "隐私与责任风险"@zh ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfPersonalImages ;
    airo:hasImpact ex:PotentialPrivacyViolation .

ex:ChildrenInFlickrPhotos rdf:type airo:AISubject ;
    rdfs:label "Children in Flickr Photos"@en ;
    rdfs:label "Flickr照片中的儿童"@zh .

ex:UnauthorizedUseOfPersonalImages rdf:type airo:RiskSource ;
    rdfs:label "Unauthorized Use of Personal Images Risk Source"@en ;
    rdfs:label "未经授权使用个人图像"@zh .

ex:PotentialPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Potential Privacy Violation Impact"@en ;
    rdfs:label "潜在隐私侵犯"@zh .