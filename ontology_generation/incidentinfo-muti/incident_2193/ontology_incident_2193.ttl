@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:JurySelectionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "陪审团选择算法"@zh ;
    rdfs:label "Jury Selection Algorithm"@en ;
    airo:hasRisk ex:RacialBiasInJurySelection ;
    airo:hasStakeholder ex:Jurors .

ex:RacialBiasInJurySelection rdf:type airo:Risk ;
    rdfs:label "陪审团选择中的种族偏见风险"@zh ;
    rdfs:label "Racial Bias in Jury Selection Risk"@en ;
    airo:hasConsequence ex:UnfairTrialOutcomes ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:Jurors rdf:type airo:Stakeholder ;
    rdfs:label "陪审员"@zh ;
    rdfs:label "Jurors"@en .

ex:UnfairTrialOutcomes rdf:type airo:Consequence ;
    rdfs:label "不公平审判结果后果"@zh ;
    rdfs:label "Unfair Trial Outcomes Consequence"@en ;
    airo:hasImpact ex:JusticeSystemDistrust .

ex:JusticeSystemDistrust rdf:type airo:Impact ;
    rdfs:label "司法系统不信任影响"@zh ;
    rdfs:label "Justice System Distrust Impact"@en ;
    airo:hasImpactOnStakeholder ex:Jurors .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "偏见训练数据风险源"@zh ;
    rdfs:label "Biased Training Data Risk Source"@en .