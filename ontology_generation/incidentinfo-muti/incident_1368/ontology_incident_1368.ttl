@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IBMAIWriterForWimbledon rdf:type airo:AISystem ;
    rdfs:label "IBM AI Writer for Wimbledon"@en ;
    rdfs:label "IBM温网AI写作系统"@zh ;
    airo:hasRisk ex:FactualErrorsInAIReporting ;
    airo:isDevelopedBy ex:IBM ;
    airo:hasStakeholder ex:WimbledonTournament ;
    airo:hasPurpose ex:AutomatedSportsReporting ;
    airo:isAppliedWithinDomain ex:SportsJournalism ;
    airo:usesTechnique ex:NaturalLanguageGeneration .

ex:FactualErrorsInAIReporting rdf:type airo:Risk ;
    rdfs:label "Factual Errors in AI Reporting Risk"@en ;
    rdfs:label "AI报道中的事实错误风险"@zh ;
    airo:hasConsequence ex:MisinformationInSportsReporting ;
    airo:isRiskSourceFor ex:InadequateFactChecking .

ex:MisinformationInSportsReporting rdf:type airo:Consequence ;
    rdfs:label "Misinformation in Sports Reporting Consequence"@en ;
    rdfs:label "体育报道中的错误信息后果"@zh ;
    airo:hasImpact ex:DiminishedCredibilityOfAutomatedReporting .

ex:IBM rdf:type airo:AIOperator ;
    rdfs:label "IBM"@en ;
    rdfs:label "IBM公司"@zh .

ex:WimbledonTournament rdf:type airo:Stakeholder ;
    rdfs:label "Wimbledon Tournament"@en ;
    rdfs:label "温布尔登网球锦标赛"@zh .

ex:AutomatedSportsReporting rdf:type airo:Purpose ;
    rdfs:label "Automated Sports Reporting Purpose"@en ;
    rdfs:label "自动化体育报道的目的"@zh .

ex:SportsJournalism rdf:type airo:Domain ;
    rdfs:label "Sports Journalism Domain"@en ;
    rdfs:label "体育新闻领域"@zh .

ex:NaturalLanguageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Natural Language Generation Technique"@en ;
    rdfs:label "自然语言生成技术"@zh .

ex:InadequateFactChecking rdf:type airo:RiskSource ;
    rdfs:label "Inadequate Fact Checking Risk Source"@en ;
    rdfs:label "事实核查不足的风险源"@zh .

ex:DiminishedCredibilityOfAutomatedReporting rdf:type airo:Impact ;
    rdfs:label "Diminished Credibility of Automated Reporting Impact"@en ;
    rdfs:label "自动化报道可信度降低的影响"@zh ;
    airo:hasImpactOnStakeholder ex:WimbledonTournament .