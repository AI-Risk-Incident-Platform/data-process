@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeAI rdf:type airo:AISystem ;
    rdfs:label "深度伪造AI"@zh ;
    rdfs:label "Deepfake AI"@en ;
    airo:hasRisk ex:ReputationDamageRisk ;
    airo:hasAISubject ex:ArtGalleryOwner ;
    airo:usesTechnique ex:DeepLearningForImageSynthesis ;
    airo:hasStakeholder ex:CelebritySubjects .

ex:ReputationDamageRisk rdf:type airo:Risk ;
    rdfs:label "声誉损害风险"@zh ;
    rdfs:label "Reputation Damage Risk"@en ;
    airo:hasConsequence ex:PersonalReputationDamage ;
    airo:isRiskSourceFor ex:MaliciousContentCreation .

ex:PersonalReputationDamage rdf:type airo:Consequence ;
    rdfs:label "个人声誉损害后果"@zh ;
    rdfs:label "Personal Reputation Damage Consequence"@en ;
    airo:hasImpact ex:ProfessionalAndSocialImpact .

ex:ProfessionalAndSocialImpact rdf:type airo:Impact ;
    rdfs:label "职业和社会影响"@zh ;
    rdfs:label "Professional and Social Impact"@en ;
    airo:hasImpactOnStakeholder ex:ArtGalleryOwner .

ex:ArtGalleryOwner rdf:type airo:AISubject ;
    rdfs:label "艺术画廊所有者"@zh ;
    rdfs:label "Art Gallery Owner"@en .

ex:DeepLearningForImageSynthesis rdf:type airo:AITechnique ;
    rdfs:label "用于图像合成的深度学习技术"@zh ;
    rdfs:label "Deep Learning for Image Synthesis Technique"@en .

ex:CelebritySubjects rdf:type airo:Stakeholder ;
    rdfs:label "名人主体"@zh ;
    rdfs:label "Celebrity Subjects"@en .

ex:MaliciousContentCreation rdf:type airo:RiskSource ;
    rdfs:label "恶意内容创作风险源"@zh ;
    rdfs:label "Malicious Content Creation Risk Source"@en .