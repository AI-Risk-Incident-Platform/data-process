@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedChildPornographySystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Child Pornography System"@en ;
    rdfs:label "AI生成儿童色情系统"@zh ;
    airo:hasRisk ex:ChildExploitationRisk ;
    airo:isDeployedBy ex:QuebecMan ;
    airo:hasPurpose ex:CreationOfIllegalContent ;
    airo:isAppliedWithinDomain ex:IllegalContentProduction ;
    airo:usesTechnique ex:SyntheticImageGeneration .

ex:ChildExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Risk"@en ;
    rdfs:label "儿童剥削风险"@zh ;
    airo:hasConsequence ex:LegalPenalties ;
    airo:isRiskSourceFor ex:MisuseOfAITechnology .

ex:LegalPenalties rdf:type airo:Consequence ;
    rdfs:label "Legal Penalties Consequence"@en ;
    rdfs:label "法律处罚后果"@zh ;
    airo:hasImpact ex:HarmToChildren .

ex:QuebecMan rdf:type airo:AIOperator ;
    rdfs:label "Quebec Man"@en ;
    rdfs:label "魁北克男子"@zh .

ex:HarmToChildren rdf:type airo:Impact ;
    rdfs:label "Harm to Children Impact"@en ;
    rdfs:label "对儿童的伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:QuebecMan .

ex:CreationOfIllegalContent rdf:type airo:Purpose ;
    rdfs:label "Creation of Illegal Content Purpose"@en ;
    rdfs:label "创建非法内容的目的"@zh .

ex:IllegalContentProduction rdf:type airo:Domain ;
    rdfs:label "Illegal Content Production Domain"@en ;
    rdfs:label "非法内容生产领域"@zh .

ex:SyntheticImageGeneration rdf:type airo:AITechnique ;
    rdfs:label "Synthetic Image Generation Technique"@en ;
    rdfs:label "合成图像生成技术"@zh .

ex:MisuseOfAITechnology rdf:type airo:RiskSource ;
    rdfs:label "Misuse of AI Technology Risk Source"@en ;
    rdfs:label "AI技术滥用的风险源"@zh .

ex:EthicalViolationInAIContentCreation rdf:type airo:RiskConcept ;
    rdfs:label "Ethical Violation in AI Content Creation Risk Concept"@en ;
    rdfs:label "AI内容创作中的伦理违规"@zh .