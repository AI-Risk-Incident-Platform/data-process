@base <https://w3id.org/airo#> .

ex:MartyTheRobot rdf:type airo:AISystem ;
    rdfs:label "Marty the Robot"@en ;
    rdfs:label "Marty机器人"@zh ;
    airo:hasRisk ex:JobDisplacementRisk ;
    airo:hasAICapability ex:RetailAutomation ;
    airo:isAppliedWithinDomain ex:RetailIndustry ;
    airo:hasStakeholder ex:RetailWorkers .

ex:JobDisplacementRisk rdf:type airo:Risk ;
    rdfs:label "Job Displacement Risk"@en ;
    rdfs:label "工作岗位替代风险"@zh .

ex:RetailIndustry rdf:type airo:Domain ;
    rdfs:label "Retail Industry"@en ;
    rdfs:label "零售业"@zh .

ex:RetailAutomation rdf:type airo:AICapability ;
    rdfs:label "Retail Automation Capability"@en ;
    rdfs:label "零售自动化能力"@zh .

ex:RetailWorkers rdf:type airo:Stakeholder ;
    rdfs:label "Retail Workers"@en ;
    rdfs:label "零售业员工"@zh .