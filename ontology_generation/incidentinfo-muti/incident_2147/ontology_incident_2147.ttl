@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:StateFarmLoanApprovalSystem rdf:type airo:AISystem ;
    rdfs:label "State Farm Loan Approval System"@en ;
    rdfs:label "State Farm贷款审批系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasInLoanApproval ;
    airo:hasStakeholder ex:BlackHomeowners ;
    airo:isDeployedBy ex:StateFarm ;
    airo:isAppliedWithinDomain ex:FinancialServices .

ex:AlgorithmicBiasInLoanApproval rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias in Loan Approval Risk"@en ;
    rdfs:label "贷款审批中的算法偏见风险"@zh .

ex:BlackHomeowners rdf:type airo:Stakeholder ;
    rdfs:label "Black Homeowners"@en ;
    rdfs:label "黑人房主"@zh .

ex:StateFarm rdf:type airo:AIOperator ;
    rdfs:label "State Farm"@en ;
    rdfs:label "State Farm"@zh .

ex:FinancialServices rdf:type airo:Domain ;
    rdfs:label "Financial Services Domain"@en ;
    rdfs:label "金融服务领域"@zh .