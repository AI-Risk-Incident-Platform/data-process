ex:BuzzfeedAIGeneratedBarbies rdf:type airo:AISystem ;
    rdfs:label "Buzzfeed AI生成芭比娃娃"@zh ;
    rdfs:label "Buzzfeed AI-Generated Barbies"@en ;
    airo:hasRisk ex:CulturalInaccuraciesRisk ;
    airo:usesTechnique ex:ImageGenerationAI ;
    airo:hasStakeholder ex:MenaRegionCommunity .

ex:CulturalInaccuraciesRisk rdf:type airo:Risk ;
    rdfs:label "文化不准确风险"@zh ;
    rdfs:label "Cultural Inaccuracies Risk"@en ;
    airo:hasConsequence ex:CulturalMisrepresentation .

ex:CulturalMisrepresentation rdf:type airo:Consequence ;
    rdfs:label "文化误传后果"@zh ;
    rdfs:label "Cultural Misrepresentation Consequence"@en ;
    airo:hasImpact ex:PublicBacklash .

ex:PublicBacklash rdf:type airo:Impact ;
    rdfs:label "公众反弹影响"@zh ;
    rdfs:label "Public Backlash Impact"@en ;
    airo:hasImpactOnStakeholder ex:MenaRegionCommunity .

ex:MenaRegionCommunity rdf:type airo:Stakeholder ;
    rdfs:label "中东地区社区"@zh ;
    rdfs:label "Mena Region Community"@en .

ex:ImageGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "图像生成AI技术"@zh ;
    rdfs:label "Image Generation AI Technique"@en .