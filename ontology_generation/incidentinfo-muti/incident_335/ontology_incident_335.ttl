@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:AcademicCheatingRisk ;
    airo:hasAISubject ex:AcademicAssignments ;
    airo:hasPurpose ex:AcademicAssignmentGeneration ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:usesTechnique ex:TextGeneration ;
    airo:hasStakeholder ex:Students .

ex:AcademicCheatingRisk rdf:type airo:Risk ;
    rdfs:label "Academic Cheating Risk"@en ;
    rdfs:label "学术作弊风险"@zh ;
    airo:isRiskSourceFor ex:EasyAccessToAITools ;
    airo:hasConsequence ex:UnderminingAcademicIntegrity .

ex:UnderminingAcademicIntegrity rdf:type airo:Consequence ;
    rdfs:label "Undermining Academic Integrity Consequence"@en ;
    rdfs:label "破坏学术诚信后果"@zh ;
    airo:hasImpact ex:DevaluationOfEducation .

ex:Students rdf:type airo:Stakeholder ;
    rdfs:label "Students"@en ;
    rdfs:label "学生"@zh ;
    airo:hasImpactOnStakeholder ex:DevaluationOfEducation .

ex:AcademicAssignments rdf:type airo:AISubject ;
    rdfs:label "Academic Assignments"@en ;
    rdfs:label "学术作业"@zh .

ex:DevaluationOfEducation rdf:type airo:Impact ;
    rdfs:label "Devaluation of Education Impact"@en ;
    rdfs:label "教育贬值影响"@zh .

ex:AcademicAssignmentGeneration rdf:type airo:Purpose ;
    rdfs:label "Academic Assignment Generation Purpose"@en ;
    rdfs:label "学术作业生成目的"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .

ex:TextGeneration rdf:type airo:AITechnique ;
    rdfs:label "Text Generation Technique"@en ;
    rdfs:label "文本生成技术"@zh .

ex:EasyAccessToAITools rdf:type airo:RiskSource ;
    rdfs:label "Easy Access to AI Tools Risk Source"@en ;
    rdfs:label "AI工具易获取风险源"@zh .