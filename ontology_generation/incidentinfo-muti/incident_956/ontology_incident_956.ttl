@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedChildSexualAbuseMaterial rdf:type airo:AISystem ;
    rdfs:label "AI-Generated Child Sexual Abuse Material"@en ;
    rdfs:label "AI生成的儿童性虐待材料"@zh ;
    airo:hasRisk ex:ChildExploitationRisk ;
    airo:hasStakeholder ex:Europol .

ex:ChildExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Child Exploitation Risk"@en ;
    rdfs:label "儿童剥削风险"@zh ;
    airo:hasConsequence ex:GlobalLawEnforcementOperation ;
    airo:isRiskSourceFor ex:MisuseOfAIGenerationTechnology .

ex:GlobalLawEnforcementOperation rdf:type airo:Consequence ;
    rdfs:label "Global Law Enforcement Operation Consequence"@en ;
    rdfs:label "全球执法行动后果"@zh ;
    airo:hasImpact ex:ChildSafetyThreat .

ex:Europol rdf:type airo:Stakeholder ;
    rdfs:label "Europol"@en ;
    rdfs:label "欧洲刑警组织"@zh .

ex:MisuseOfAIGenerationTechnology rdf:type airo:RiskSource ;
    rdfs:label "Misuse of AI Generation Technology Risk Source"@en ;
    rdfs:label "AI生成技术滥用风险源"@zh .

ex:ChildSafetyThreat rdf:type airo:Impact ;
    rdfs:label "Child Safety Threat Impact"@en ;
    rdfs:label "儿童安全威胁影响"@zh ;
    airo:hasImpactOnStakeholder ex:Europol .