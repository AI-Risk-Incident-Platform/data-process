@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HPFacialTrackingSoftware rdf:type airo:AISystem ;
    rdfs:label "HP Facial Tracking Software"@en ;
    rdfs:label "HP面部追踪软件"@zh ;
    airo:hasRisk ex:RacialBiasInFacialRecognition ;
    airo:hasCapability ex:FacialDetectionAndTracking ;
    airo:hasStakeholder ex:BlackUsers ;
    airo:isDeployedBy ex:HPInc .

ex:RacialBiasInFacialRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Facial Recognition Risk"@en ;
    rdfs:label "面部识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:FailureToDetectBlackFaces ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:FailureToDetectBlackFaces rdf:type airo:Consequence ;
    rdfs:label "Failure to Detect Black Faces Consequence"@en ;
    rdfs:label "无法检测黑人面孔的后果"@zh ;
    airo:hasImpact ex:DiscriminationInTechnologyAccess .

ex:FacialDetectionAndTracking rdf:type airo:AICapability ;
    rdfs:label "Facial Detection and Tracking Capability"@en ;
    rdfs:label "面部检测与追踪能力"@zh .

ex:HPInc rdf:type airo:AIOperator ;
    rdfs:label "HP Inc."@en ;
    rdfs:label "惠普公司"@zh .

ex:BlackUsers rdf:type airo:Stakeholder ;
    rdfs:label "Black Users"@en ;
    rdfs:label "黑人用户"@zh .

ex:DiscriminationInTechnologyAccess rdf:type airo:Impact ;
    rdfs:label "Discrimination in Technology Access Impact"@en ;
    rdfs:label "技术获取中的歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:BlackUsers .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .