@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RomanceScamAI rdf:type airo:AISystem ;
    rdfs:label "Romance Scam AI System"@en ;
    rdfs:label "浪漫诈骗AI系统"@zh ;
    airo:hasRisk ex:FinancialLossRisk ;
    airo:hasStakeholder ex:FBI ;
    airo:isAppliedWithinDomain ex:OnlineDating .

ex:FinancialLossRisk rdf:type airo:Risk ;
    rdfs:label "Financial Loss Risk"@en ;
    rdfs:label "财务损失风险"@zh ;
    airo:hasImpact ex:EconomicImpact .

ex:FBI rdf:type airo:Stakeholder ;
    rdfs:label "Federal Bureau of Investigation"@en ;
    rdfs:label "联邦调查局"@zh .

ex:EconomicImpact rdf:type airo:Impact ;
    rdfs:label "Economic Impact"@en ;
    rdfs:label "经济影响"@zh ;
    airo:hasImpactOnStakeholder ex:FBI .

ex:OnlineDating rdf:type airo:Domain ;
    rdfs:label "Online Dating Domain"@en ;
    rdfs:label "在线约会领域"@zh .