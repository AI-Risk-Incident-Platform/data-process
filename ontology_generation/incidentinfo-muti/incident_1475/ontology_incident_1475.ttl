ex:RussianTVFakeVideoAI rdf:type airo:AISystem ;
    rdfs:label "Russian TV Fake Video AI"@en ;
    rdfs:label "俄罗斯电视台虚假视频AI"@zh ;
    airo:hasRisk ex:DisinformationRisk ;
    airo:hasStakeholder ex:RussianTV ;
    airo:hasAISubject ex:UkrainianGovernment ;
    airo:hasPurpose ex:Propaganda ;
    airo:isAppliedWithinDomain ex:MediaAndPropaganda ;
    airo:usesTechnique ex:DeepfakeTechnology .

ex:DisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Disinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:isRiskSourceFor ex:MisuseOfAITechnology ;
    airo:hasImpact ex:PoliticalTensions .

ex:RussianTV rdf:type airo:Stakeholder ;
    rdfs:label "Russian TV"@en ;
    rdfs:label "俄罗斯电视台"@zh .

ex:UkrainianGovernment rdf:type airo:AISubject ;
    rdfs:label "Ukrainian Government"@en ;
    rdfs:label "乌克兰政府"@zh .

ex:PoliticalTensions rdf:type airo:Impact ;
    rdfs:label "Political Tensions"@en ;
    rdfs:label "政治紧张局势"@zh ;
