ex:Propaganda rdf:type airo:Purpose ;
    rdfs:label "Propaganda"@en ;
    rdfs:label "宣传"@zh .

ex:MediaAndPropaganda rdf:type airo:Domain ;
    rdfs:label "Media and Propaganda"@en ;
    rdfs:label "媒体与宣传"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
