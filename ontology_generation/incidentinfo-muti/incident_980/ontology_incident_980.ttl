@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoCallSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Call System"@en ;
    rdfs:label "深度伪造视频通话系统"@zh ;
    airo:hasRisk ex:CorporateImpersonationRisk ;
    airo:hasStakeholder ex:SingaporeAuthorities ;
    airo:isAppliedWithinDomain ex:CorporateCommunication .

ex:CorporateImpersonationRisk rdf:type airo:Risk ;
    rdfs:label "Corporate Impersonation Risk"@en ;
    rdfs:label "企业冒充风险"@zh ;
    airo:isRiskSourceFor ex:DeepfakeTechnology .

ex:SingaporeAuthorities rdf:type airo:Stakeholder ;
    rdfs:label "Singapore Authorities"@en ;
    rdfs:label "新加坡当局"@zh .

ex:CorporateCommunication rdf:type airo:Domain ;
    rdfs:label "Corporate Communication Domain"@en ;
    rdfs:label "企业通信领域"@zh .

ex:DeepfakeTechnology rdf:type airo:RiskSource ;
    rdfs:label "Deepfake Technology Risk Source"@en ;
    rdfs:label "深度伪造技术风险源"@zh .