@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClassroomSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Classroom Surveillance System"@en ;
    rdfs:label "教室监控系统"@zh ;
    airo:hasRisk ex:StudentPrivacyViolationRisk ;
    airo:hasAISubject ex:StudentsUnderSurveillance ;
    airo:hasStakeholder ex:SchoolAdministration ;
    airo:isAppliedWithinDomain ex:EducationSystem ;
    airo:hasPurpose ex:ClassroomBehaviorMonitoring .

ex:StudentPrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Student Privacy Violation Risk"@en ;
    rdfs:label "学生隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LossOfStudentPrivacy ;
    airo:isRiskSourceFor ex:ContinuousVideoRecording ;
    airo:hasVulnerability ex:LackOfConsentFromMinors .

ex:StudentsUnderSurveillance rdf:type airo:AISubject ;
    rdfs:label "Students Under Surveillance"@en ;
    rdfs:label "被监控学生"@zh .

ex:SchoolAdministration rdf:type airo:Stakeholder ;
    rdfs:label "School Administration"@en ;
    rdfs:label "学校管理部门"@zh .

ex:EducationSystem rdf:type airo:Domain ;
    rdfs:label "Education System Domain"@en ;
    rdfs:label "教育系统领域"@zh .

ex:ClassroomBehaviorMonitoring rdf:type airo:Purpose ;
    rdfs:label "Classroom Behavior Monitoring Purpose"@en ;
    rdfs:label "课堂行为监控目的"@zh .

ex:PsychologicalStressOnStudents rdf:type airo:Impact ;
    rdfs:label "Psychological Stress on Students Impact"@en ;
    rdfs:label "学生心理压力影响"@zh ;
    airo:hasImpactOnStakeholder ex:StudentsUnderSurveillance .

ex:LossOfStudentPrivacy rdf:type airo:Consequence ;
    rdfs:label "Loss of Student Privacy Consequence"@en ;
    rdfs:label "学生隐私丧失后果"@zh ;
    airo:hasImpact ex:PsychologicalStressOnStudents .

ex:ContinuousVideoRecording rdf:type airo:RiskSource ;
    rdfs:label "Continuous Video Recording Risk Source"@en ;
    rdfs:label "持续视频记录风险源"@zh .

ex:LackOfConsentFromMinors rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Consent from Minors Vulnerability"@en ;
    rdfs:label "未成年人缺乏同意的脆弱性"@zh .