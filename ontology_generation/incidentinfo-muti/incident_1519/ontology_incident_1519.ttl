@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:LegalMisinformationRisk ;
    airo:isDevelopedBy ex:OpenAI ;
    airo:hasStakeholder ex:LegalProfessionals ;
    airo:isAppliedWithinDomain ex:LegalSector .

ex:LegalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Legal Misinformation Risk"@en ;
    rdfs:label "法律错误信息风险"@zh ;
    airo:hasConsequence ex:ProfessionalSanctions ;
    airo:isRiskSourceFor ex:LegalCitationWithoutVerification .

ex:ProfessionalSanctions rdf:type airo:Consequence ;
    rdfs:label "Professional Sanctions Consequence"@en ;
    rdfs:label "职业处罚后果"@zh ;
    airo:hasImpact ex:LegalSystemIntegrityThreat .

ex:LegalProfessionals rdf:type airo:Stakeholder ;
    rdfs:label "Legal Professionals"@en ;
    rdfs:label "法律专业人士"@zh .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:LegalSector rdf:type airo:Domain ;
    rdfs:label "Legal Sector"@en ;
    rdfs:label "法律领域"@zh .

ex:LegalCitationWithoutVerification rdf:type airo:Misuse ;
    rdfs:label "Legal Citation Without Verification"@en ;
    rdfs:label "未经核实的法律引用"@zh .

ex:LegalSystemIntegrityThreat rdf:type airo:Impact ;
    rdfs:label "Legal System Integrity Threat"@en ;
    rdfs:label "法律系统完整性威胁"@zh ;
    airo:hasImpactOnStakeholder ex:LegalProfessionals .