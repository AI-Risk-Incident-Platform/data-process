@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RealTimeDeepfakeSystem rdf:type airo:AISystem ;
    rdfs:label "Real-Time Deepfake System"@en ;
    rdfs:label "实时深度伪造系统"@zh ;
    airo:hasRisk ex:MisuseOfDeepfakeTechnology ;
    airo:usesTechnique ex:NeuralNetworkForFaceSwapping ;
    airo:hasStakeholder ex:Livestreamers .

ex:MisuseOfDeepfakeTechnology rdf:type airo:Risk ;
    rdfs:label "Misuse of Deepfake Technology Risk"@en ;
    rdfs:label "深度伪造技术滥用风险"@zh ;
    airo:hasConsequence ex:IdentityFraud ;
    airo:isRiskSourceFor ex:EasyAccessToDeepfakeTools .

ex:NeuralNetworkForFaceSwapping rdf:type airo:AITechnique ;
    rdfs:label "Neural Network for Face Swapping Technique"@en ;
    rdfs:label "面部替换神经网络技术"@zh .

ex:Livestreamers rdf:type airo:Stakeholder ;
    rdfs:label "Livestreamers"@en ;
    rdfs:label "直播主"@zh .

ex:IdentityFraud rdf:type airo:Consequence ;
    rdfs:label "Identity Fraud Consequence"@en ;
    rdfs:label "身份欺诈后果"@zh ;
    airo:hasImpact ex:TrustErosionInDigitalCommunication .

ex:TrustErosionInDigitalCommunication rdf:type airo:Impact ;
    rdfs:label "Trust Erosion in Digital Communication Impact"@en ;
    rdfs:label "数字通信信任度下降的影响"@zh ;
    airo:hasImpactOnStakeholder ex:Livestreamers .

ex:EasyAccessToDeepfakeTools rdf:type airo:RiskSource ;
    rdfs:label "Easy Access to Deepfake Tools Risk Source"@en ;
    rdfs:label "深度伪造工具易获取的风险源"@zh .