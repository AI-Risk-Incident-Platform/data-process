@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition System"@en ;
    rdfs:label "面部识别系统"@zh ;
    airo:hasAISubject ex:IndividualsInPublicSpaces ;
    airo:hasPurpose ex:IdentityVerification ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:ImageRecognition ;
    airo:hasRisk ex:PrivacyInvasionRisk .

ex:PrivacyInvasionRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Invasion Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:SurveillanceTechnology ;
    airo:hasImpact ex:LossOfAnonymity .

ex:IndividualsInPublicSpaces rdf:type airo:AISubject ;
    rdfs:label "Individuals in Public Spaces"@en ;
    rdfs:label "公共场所中的个人"@zh .

ex:LossOfAnonymity rdf:type airo:Impact ;
    rdfs:label "Loss of Anonymity"@en ;
    rdfs:label "匿名性丧失"@zh .

ex:IdentityVerification rdf:type airo:Purpose ;
    rdfs:label "Identity Verification"@en ;
    rdfs:label "身份验证"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security"@en ;
    rdfs:label "公共安全"@zh .

ex:ImageRecognition rdf:type airo:AITechnique ;
    rdfs:label "Image Recognition"@en ;
    rdfs:label "图像识别"@zh .

ex:SurveillanceTechnology rdf:type airo:RiskSource ;
    rdfs:label "Surveillance Technology"@en ;
    rdfs:label "监控技术"@zh .