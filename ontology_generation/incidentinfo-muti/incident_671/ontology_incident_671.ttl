@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasStakeholder ex:PakistaniVoters .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasConsequence [ rdf:type airo:Consequence ;
                          rdfs:label "Election Consequence"@en ;
                          rdfs:label "选举后果"@zh ;
                          airo:hasImpact ex:PublicTrustErosion
                        ] .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections"@en ;
    rdfs:label "政治选举"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:PakistaniVoters .

ex:PakistaniVoters rdf:type airo:Stakeholder ;
    rdfs:label "Pakistani Voters"@en ;
    rdfs:label "巴基斯坦选民"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks"@en ;
    rdfs:label "生成对抗网络"@zh .