@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AirbnbSmartPricingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Airbnb Smart Pricing Algorithm"@en ;
    rdfs:label "Airbnb智能定价算法"@zh ;
    airo:hasRisk ex:RacialDisparitiesInPricing ;
    airo:hasCapability ex:DynamicPricingBasedOnMarketData ;
    airo:hasStakeholder ex:AirbnbHosts ;
    airo:hasAISubject ex:RacialMinorityHosts ;
    airo:hasPurpose ex:OptimizingHostRevenue ;
    airo:isAppliedWithinDomain ex:ShortTermRentalMarket ;
    airo:usesTechnique ex:MachineLearningForPricePrediction .

ex:RacialDisparitiesInPricing rdf:type airo:Risk ;
    rdfs:label "Racial Disparities in Pricing Risk"@en ;
    rdfs:label "定价中的种族差异风险"@zh ;
    airo:hasConsequence ex:WorseningRacialDisparities ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:WorseningRacialDisparities rdf:type airo:Consequence ;
    rdfs:label "Worsening Racial Disparities Consequence"@en ;
    rdfs:label "种族差异加剧的后果"@zh ;
    airo:hasImpact ex:EconomicDisadvantageForMinorities .

ex:DynamicPricingBasedOnMarketData rdf:type airo:AICapability ;
    rdfs:label "Dynamic Pricing Based on Market Data Capability"@en ;
    rdfs:label "基于市场数据的动态定价能力"@zh .

ex:AirbnbHosts rdf:type airo:Stakeholder ;
    rdfs:label "Airbnb Hosts"@en ;
    rdfs:label "Airbnb房东"@zh .

ex:RacialMinorityHosts rdf:type airo:AISubject ;
    rdfs:label "Racial Minority Hosts"@en ;
    rdfs:label "少数族裔房东"@zh .

ex:EconomicDisadvantageForMinorities rdf:type airo:Impact ;
    rdfs:label "Economic Disadvantage for Minorities Impact"@en ;
    rdfs:label "少数族裔经济劣势的影响"@zh ;
    airo:hasImpactOnStakeholder ex:RacialMinorityHosts .

ex:OptimizingHostRevenue rdf:type airo:Purpose ;
    rdfs:label "Optimizing Host Revenue Purpose"@en ;
    rdfs:label "优化房东收入的目的"@zh .

ex:ShortTermRentalMarket rdf:type airo:Domain ;
    rdfs:label "Short-Term Rental Market Domain"@en ;
    rdfs:label "短期租赁市场领域"@zh .

ex:MachineLearningForPricePrediction rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning for Price Prediction Technique"@en ;
    rdfs:label "用于价格预测的机器学习技术"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据的风险源"@zh .