ex:DeepfakeVideoOfMLKJr rdf:type airo:AISystem ;
    rdfs:label "马丁·路德·金深度伪造视频"@zh ;
    rdfs:label "Deepfake Video of MLK Jr."@en ;
    airo:hasRisk ex:MisinformationAndReputationDamageRisk ;
    airo:hasAISubject ex:HistoricalFigureMLKJr ;
    airo:hasPurpose ex:PoliticalEndorsementFabrication ;
    airo:usesTechnique ex:DeepfakeSynthesisTechnology ;
    airo:isAppliedWithinDomain ex:PoliticalCommunication ;
    airo:hasStakeholder ex:MLKJrFamily .

ex:MisinformationAndReputationDamageRisk rdf:type airo:Risk ;
