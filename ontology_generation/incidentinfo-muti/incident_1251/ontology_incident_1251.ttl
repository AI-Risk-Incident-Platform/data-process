ex:VoiceCloningAI rdf:type airo:AISystem ;
    rdfs:label "Voice Cloning AI"@en ;
    rdfs:label "语音克隆AI"@zh ;
    airo:hasRisk ex:UnauthorizedVoiceCloningRisk ;
    airo:hasStakeholder ex:MarquesBrownlee .

ex:UnauthorizedVoiceCloningRisk rdf:type airo:Risk ;
    rdfs:label "Unauthorized Voice Cloning Risk"@en ;
    rdfs:label "未经授权的语音克隆风险"@zh ;
    airo:hasConsequence ex:NonConsensualVoiceReplication ;
    airo:hasImpact ex:PersonalPrivacyViolation .

ex:MarquesBrownlee rdf:type airo:Stakeholder ;
    rdfs:label "Marques Brownlee"@en ;
    rdfs:label "Marques Brownlee"@zh .

ex:PersonalPrivacyViolation rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Violation"@en ;
    rdfs:label "个人隐私侵犯"@zh ;
    airo:hasImpactOnStakeholder ex:MarquesBrownlee .

ex:NonConsensualVoiceReplication rdf:type airo:Misuse ;
    rdfs:label "Non-Consensual Voice Replication"@en ;
    rdfs:label "未经同意的语音复制"@zh .