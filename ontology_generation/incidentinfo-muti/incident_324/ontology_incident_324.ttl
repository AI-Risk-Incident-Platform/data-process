@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookNostalgiaFeature rdf:type airo:AISystem ;
    rdfs:label "Facebook Nostalgia Feature"@en ;
    rdfs:label "Facebook怀旧功能"@zh ;
    airo:hasRisk ex:EmotionalDistressRisk ;
    airo:hasAISubject ex:FacebookUsers ;
    airo:hasPurpose ex:MemoryRecollection .

ex:EmotionalDistressRisk rdf:type airo:Risk ;
    rdfs:label "Emotional Distress Risk"@en ;
    rdfs:label "情感困扰风险"@zh ;
    airo:hasConsequence ex:PsychologicalImpact .

ex:FacebookUsers rdf:type airo:AISubject ;
    rdfs:label "Facebook Users"@en ;
    rdfs:label "Facebook用户"@zh .

ex:PsychologicalImpact rdf:type airo:Impact ;
    rdfs:label "Psychological Impact"@en ;
    rdfs:label "心理影响"@zh ;
    airo:hasImpactOnStakeholder ex:FacebookUsers .

ex:MemoryRecollection rdf:type airo:Purpose ;
    rdfs:label "Memory Recollection"@en ;
    rdfs:label "记忆回顾"@zh .