@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeImageGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Image Generation System"@en ;
    rdfs:label "深度伪造图像生成系统"@zh ;
    airo:hasRisk ex:PrivacyViolationAndReputationDamageRisk ;
    airo:hasAISubject ex:PublicFigure ;
    airo:usesTechnique ex:GenerativeAdversarialNetwork ;
    airo:hasPurpose ex:FraudulentActivity ;
    airo:hasStakeholder ex:NagaMunchetty .

ex:PrivacyViolationAndReputationDamageRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Reputation Damage Risk"@en ;
    rdfs:label "隐私侵犯和声誉损害风险"@zh ;
    airo:hasConsequence ex:PersonalReputationDamage ;
    airo:isRiskSourceFor ex:MaliciousActor .

ex:PersonalReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Personal Reputation Damage Consequence"@en ;
    rdfs:label "个人声誉损害后果"@zh ;
    airo:hasImpact ex:EmotionalDistress .

ex:NagaMunchetty rdf:type airo:Stakeholder ;
    rdfs:label "Naga Munchetty"@en ;
    rdfs:label "娜迦·蒙切蒂"@zh .

ex:PublicFigure rdf:type airo:AISubject ;
    rdfs:label "Public Figure"@en ;
    rdfs:label "公众人物"@zh .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Emotional Distress Impact"@en ;
    rdfs:label "情绪困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:NagaMunchetty .

ex:FraudulentActivity rdf:type airo:Purpose ;
    rdfs:label "Fraudulent Activity Purpose"@en ;
    rdfs:label "欺诈活动目的"@zh .

ex:GenerativeAdversarialNetwork rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Network Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:MaliciousActor rdf:type airo:RiskSource ;
    rdfs:label "Malicious Actor Risk Source"@en ;
    rdfs:label "恶意行为者风险源"@zh .