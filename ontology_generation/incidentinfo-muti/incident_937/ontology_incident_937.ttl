@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSoftware rdf:type airo:AISystem ;
    rdfs:label "特斯拉全自动驾驶软件"@zh ;
    rdfs:label "Tesla Full Self-Driving Software"@en ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:hasAICapability ex:AutonomousDrivingCapability ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasPurpose ex:AutonomousVehicleTransportation ;
    airo:hasStakeholder ex:TeslaVehicleOwners ;
    airo:hasAISubject ex:CybertruckOccupants ;
    airo:usesTechnique ex:ComputerVisionBasedAutonomousDriving ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    airo:hasConsequence ex:VehicleCollisionWithPole ;
    airo:isRiskSourceFor ex:SoftwareAlgorithmLimitations ;
    airo:isRiskSourceFor ex:AutonomousSystemMalfunctionHazard ;
    airo:isRiskSourceFor ex:RoadInfrastructureCollisionThreat .

ex:VehicleCollisionWithPole rdf:type airo:Consequence ;
    rdfs:label "车辆与电线杆碰撞后果"@zh ;
    rdfs:label "Vehicle Collision with Pole Consequence"@en ;
    airo:hasImpact ex:VehicleDamageAndPotentialInjury .

ex:AutonomousDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:TeslaVehicleOwners rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉车主"@zh ;
    rdfs:label "Tesla Vehicle Owners"@en .

ex:CybertruckOccupants rdf:type airo:AISubject ;
    rdfs:label "Cybertruck乘员"@zh ;
    rdfs:label "Cybertruck Occupants"@en .

ex:VehicleDamageAndPotentialInjury rdf:type airo:Impact ;
    rdfs:label "车辆损坏和潜在伤害影响"@zh ;
    rdfs:label "Vehicle Damage and Potential Injury Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaVehicleOwners ;
    airo:hasImpactOnStakeholder ex:CybertruckOccupants .

ex:AutonomousVehicleTransportation rdf:type airo:Purpose ;
    rdfs:label "自动驾驶车辆运输目的"@zh ;
    rdfs:label "Autonomous Vehicle Transportation Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVisionBasedAutonomousDriving rdf:type airo:AITechnique ;
    rdfs:label "基于计算机视觉的自动驾驶技术"@zh ;
    rdfs:label "Computer Vision Based Autonomous Driving Technique"@en .

ex:SoftwareAlgorithmLimitations rdf:type airo:RiskSource ;
    rdfs:label "软件算法限制的风险源"@zh ;
    rdfs:label "Software Algorithm Limitations Risk Source"@en ;
    airo:exploitsVulnerability ex:ObjectRecognitionFailure .

ex:AutonomousSystemMalfunctionHazard rdf:type airo:Hazard ;
    rdfs:label "自动驾驶系统故障危害"@zh ;
    rdfs:label "Autonomous System Malfunction Hazard"@en ;
    airo:exploitsVulnerability ex:ObjectRecognitionFailure .

ex:RoadInfrastructureCollisionThreat rdf:type airo:Threat ;
    rdfs:label "道路基础设施碰撞威胁"@zh ;
    rdfs:label "Road Infrastructure Collision Threat"@en ;
    airo:exploitsVulnerability ex:ObjectRecognitionFailure .

ex:ObjectRecognitionFailure rdf:type airo:Vulnerability ;
    rdfs:label "物体识别失败的脆弱性"@zh ;
    rdfs:label "Object Recognition Failure Vulnerability"@en .

ex:AutonomousVehicleSafetyConcerns rdf:type airo:RiskConcept ;
    rdfs:label "自动驾驶车辆安全问题"@zh ;
    rdfs:label "Autonomous Vehicle Safety Concerns Risk Concept"@en .