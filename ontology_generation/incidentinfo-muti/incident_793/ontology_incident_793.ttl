@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:hasAICapability ex:RealisticVideoGeneration ;
    airo:hasAISubject ex:CelebrityFigures ;
    airo:hasPurpose ex:ContentManipulation ;
    airo:isAppliedWithinDomain ex:OnlineMedia ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasStakeholder ex:GeneralPublic ;
    airo:hasVulnerability ex:PublicCredulity .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:EconomicLoss ;
    airo:isRiskSourceFor ex:MaliciousContentCreators , ex:IdentityImpersonation , ex:UnregulatedAIApplications .

ex:EconomicLoss rdf:type airo:Consequence ;
    rdfs:label "Economic Loss"@en ;
    rdfs:label "经济损失"@zh ;
    airo:hasImpact ex:TrustErosion .

ex:RealisticVideoGeneration rdf:type airo:AICapability ;
    rdfs:label "Realistic Video Generation Capability"@en ;
    rdfs:label "逼真视频生成能力"@zh .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:CelebrityFigures rdf:type airo:AISubject ;
    rdfs:label "Celebrity Figures"@en ;
    rdfs:label "名人形象"@zh .

ex:TrustErosion rdf:type airo:Impact ;
    rdfs:label "Trust Erosion"@en ;
    rdfs:label "信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:ContentManipulation rdf:type airo:Purpose ;
    rdfs:label "Content Manipulation"@en ;
    rdfs:label "内容操纵"@zh .

ex:OnlineMedia rdf:type airo:Domain ;
    rdfs:label "Online Media"@en ;
    rdfs:label "在线媒体"@zh .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks"@en ;
    rdfs:label "生成对抗网络"@zh .

ex:PublicCredulity rdf:type airo:Vulnerability ;
    rdfs:label "Public Credulity"@en ;
    rdfs:label "公众轻信"@zh .

ex:IdentityImpersonation rdf:type airo:Threat ;
    rdfs:label "Identity Impersonation"@en ;
    rdfs:label "身份冒充"@zh .

ex:UnregulatedAIApplications rdf:type airo:Hazard ;
    rdfs:label "Unregulated AI Applications"@en ;
    rdfs:label "未受监管的AI应用"@zh .

ex:MaliciousContentCreators rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creators"@en ;
    rdfs:label "恶意内容创作者"@zh .