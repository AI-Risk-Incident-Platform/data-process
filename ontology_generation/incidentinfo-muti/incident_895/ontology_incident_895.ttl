@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CharacterAIPlatform rdf:type airo:AISystem ;
    rdfs:label "Character.AI Platform"@en ;
    rdfs:label "Character.AI平台"@zh ;
    airo:hasRisk ex:ChildGroomingRisk ;
    airo:hasStakeholder ex:UnderageUsers ;
    airo:hasPurpose ex:KidFriendlyPlatformPurpose ;
    airo:isAppliedWithinDomain ex:OnlineSocialPlatformsDomain .

ex:ChildGroomingRisk rdf:type airo:Risk ;
    rdfs:label "Child Grooming Risk"@en ;
    rdfs:label "儿童诱骗风险"@zh ;
    airo:isRiskSourceFor ex:PedophileBots ;
    airo:hasConsequence ex:ChildExploitationConsequence .

ex:UnderageUsers rdf:type airo:Stakeholder ;
    rdfs:label "Underage Users"@en ;
    rdfs:label "未成年用户"@zh .

ex:PedophileBots rdf:type airo:RiskSource ;
    rdfs:label "Pedophile Bots"@en ;
    rdfs:label "恋童癖机器人"@zh .

ex:ChildExploitationConsequence rdf:type airo:Consequence ;
    rdfs:label "Child Exploitation Consequence"@en ;
    rdfs:label "儿童剥削后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToChildrenImpact .

ex:PsychologicalHarmToChildrenImpact rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Children Impact"@en ;
    rdfs:label "对儿童的心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:UnderageUsers .

ex:KidFriendlyPlatformPurpose rdf:type airo:Purpose ;
    rdfs:label "Kid-Friendly Platform Purpose"@en ;
    rdfs:label "儿童友好平台目的"@zh .

ex:OnlineSocialPlatformsDomain rdf:type airo:Domain ;
    rdfs:label "Online Social Platforms Domain"@en ;
    rdfs:label "在线社交平台领域"@zh .