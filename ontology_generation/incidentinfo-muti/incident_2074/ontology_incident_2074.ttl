@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SerbianSurveillanceCameras rdf:type airo:AISystem ;
    rdfs:label "Serbian Surveillance Cameras"@en ;
    rdfs:label "塞尔维亚监控摄像头"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:GeneralPublic ;
    airo:hasStakeholder ex:PrivacyAdvocates ;
    airo:hasCapability ex:FacialRecognition ;
    airo:hasPurpose ex:PublicSurveillance ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:FacialRecognitionTechnology .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:LossOfPrivacy ;
    airo:isRiskSourceFor ex:MassSurveillance .

ex:FacialRecognition rdf:type airo:AICapability ;
    rdfs:label "Facial Recognition Capability"@en ;
    rdfs:label "面部识别能力"@zh .

ex:PrivacyAdvocates rdf:type airo:Stakeholder ;
    rdfs:label "Privacy Advocates"@en ;
    rdfs:label "隐私倡导者"@zh .

ex:GeneralPublic rdf:type airo:AISubject ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:LossOfPrivacy rdf:type airo:Impact ;
    rdfs:label "Loss of Privacy Impact"@en ;
    rdfs:label "隐私丧失影响"@zh ;
    airo:hasImpactOnStakeholder ex:GeneralPublic .

ex:PublicSurveillance rdf:type airo:Purpose ;
    rdfs:label "Public Surveillance Purpose"@en ;
    rdfs:label "公共监控目的"@zh .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:MassSurveillance rdf:type airo:RiskSource ;
    rdfs:label "Mass Surveillance Risk Source"@en ;
    rdfs:label "大规模监控风险源"@zh .