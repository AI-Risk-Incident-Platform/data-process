ex:AIJudgmentSystem rdf:type airo:AISystem ;
    rdfs:label "AI Judgment System"@en ;
    rdfs:label "AI判断系统"@zh ;
    airo:hasRisk ex:LackOfSoundJudgmentRisk ;
    airo:hasPurpose ex:DecisionMakingSupport ;
    airo:hasStakeholder ex:AIDevelopers .

ex:LackOfSoundJudgmentRisk rdf:type airo:Risk ;
    rdfs:label "Lack of Sound Judgment Risk"@en ;
    rdfs:label "缺乏合理判断风险"@zh .

ex:AIDevelopers rdf:type airo:Stakeholder ;
    rdfs:label "AI Developers"@en ;
    rdfs:label "AI开发者"@zh .

ex:DecisionMakingSupport rdf:type airo:Purpose ;
    rdfs:label "Decision Making Support"@en ;
    rdfs:label "决策支持"@zh .