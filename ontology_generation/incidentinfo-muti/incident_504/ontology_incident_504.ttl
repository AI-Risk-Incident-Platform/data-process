@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:FactFabricationRisk ;
    airo:hasStakeholder ex:LawProfessor .

ex:FactFabricationRisk rdf:type airo:Risk ;
    rdfs:label "Fact Fabrication Risk"@en ;
    rdfs:label "事实捏造风险"@zh ;
    airo:hasConsequence ex:LegalImplications ;
    airo:isRiskSourceFor ex:InaccurateInformationGeneration .

ex:LegalImplications rdf:type airo:Consequence ;
    rdfs:label "Legal Implications Consequence"@en ;
    rdfs:label "法律影响后果"@zh ;
    airo:hasImpactOnStakeholder ex:LawProfessor .

ex:LawProfessor rdf:type airo:Stakeholder ;
    rdfs:label "Law Professor"@en ;
    rdfs:label "法律教授"@zh .

ex:InaccurateInformationGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate Information Generation Risk Source"@en ;
    rdfs:label "不准确信息生成风险源"@zh .