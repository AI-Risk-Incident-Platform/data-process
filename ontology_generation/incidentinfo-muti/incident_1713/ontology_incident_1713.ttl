@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmericanBitcoinAcademy rdf:type airo:AISystem ;
    rdfs:label "美国比特币学院"@zh ;
    rdfs:label "American Bitcoin Academy"@en ;
    airo:hasRisk ex:CryptocurrencyInvestmentScamRisk ;
    airo:isDeployedBy ex:AmericanBitcoinAcademyFounder ;
    airo:hasStakeholder ex:SEC .

ex:CryptocurrencyInvestmentScamRisk rdf:type airo:Risk ;
    rdfs:label "加密货币投资诈骗风险"@zh ;
    rdfs:label "Cryptocurrency Investment Scam Risk"@en ;
    airo:hasConsequence ex:FinancialLossToStudents ;
    airo:isRiskSourceFor ex:FalseInvestmentPromises .

ex:SEC rdf:type airo:Stakeholder ;
    rdfs:label "美国证券交易委员会"@zh ;
    rdfs:label "Securities and Exchange Commission"@en .

ex:AmericanBitcoinAcademyFounder rdf:type airo:AIOperator ;
    rdfs:label "美国比特币学院创始人"@zh ;
    rdfs:label "American Bitcoin Academy Founder"@en .

ex:FinancialLossToStudents rdf:type airo:Consequence ;
    rdfs:label "学生经济损失后果"@zh ;
    rdfs:label "Financial Loss to Students Consequence"@en ;
    airo:hasImpact ex:InvestorTrustErosion .

ex:InvestorTrustErosion rdf:type airo:Impact ;
    rdfs:label "投资者信任侵蚀影响"@zh ;
    rdfs:label "Investor Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:SEC .

ex:FalseInvestmentPromises rdf:type airo:RiskSource ;
    rdfs:label "虚假投资承诺风险源"@zh ;
    rdfs:label "False Investment Promises Risk Source"@en .