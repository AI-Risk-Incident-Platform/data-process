@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIHeadshotEnhancementSystem rdf:type airo:AISystem ;
    rdfs:label "AI Headshot Enhancement System"@en ;
    rdfs:label "AI头像增强系统"@zh ;
    airo:hasRisk ex:RacialBiasInAIHeadshotEnhancement ;
    airo:hasAISubject ex:MITStudent .

ex:RacialBiasInAIHeadshotEnhancement rdf:type airo:Risk ;
    rdfs:label "Racial Bias in AI Headshot Enhancement Risk"@en ;
    rdfs:label "AI头像增强中的种族偏见风险"@zh ;
    airo:hasConsequence ex:UnrealisticBeautyStandardsPromotion ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:UnrealisticBeautyStandardsPromotion rdf:type airo:Consequence ;
    rdfs:label "Unrealistic Beauty Standards Promotion Consequence"@en ;
    rdfs:label "推广不切实际的美容标准后果"@zh ;
    airo:hasImpact ex:NegativeSelfImageAndDiscrimination .

ex:NegativeSelfImageAndDiscrimination rdf:type airo:Impact ;
    rdfs:label "Negative Self-Image and Discrimination Impact"@en ;
    rdfs:label "负面自我形象和歧视影响"@zh ;
    airo:hasImpactOnStakeholder ex:MITStudent .

ex:MITStudent rdf:type airo:AISubject ;
    rdfs:label "MIT Student"@en ;
    rdfs:label "MIT学生"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .