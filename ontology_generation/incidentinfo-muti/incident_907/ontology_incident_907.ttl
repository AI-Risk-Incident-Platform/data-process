@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RealTimeDeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "Real-Time Deepfake Technology"@en ;
    rdfs:label "实时深度伪造技术"@zh ;
    airo:hasRisk ex:RomanceScamRisk ;
    airo:usesTechnique ex:DeepfakeGeneration ;
    airo:hasStakeholder ex:ScamVictims .

ex:RomanceScamRisk rdf:type airo:Risk ;
    rdfs:label "Romance Scam Risk"@en ;
    rdfs:label "浪漫骗局风险"@zh ;
    airo:hasConsequence ex:FinancialLoss .

ex:DeepfakeGeneration rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Generation Technique"@en ;
    rdfs:label "深度伪造生成技术"@zh .

ex:ScamVictims rdf:type airo:Stakeholder ;
    rdfs:label "Scam Victims"@en ;
    rdfs:label "诈骗受害者"@zh .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:EmotionalDistress .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Emotional Distress Impact"@en ;
    rdfs:label "情感困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:ScamVictims .