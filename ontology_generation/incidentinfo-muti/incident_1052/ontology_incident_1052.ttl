@base <https://w3id.org/airo#> .

ex:DeepFakeGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deep Fake Generation System"@en ;
    rdfs:label "深度伪造生成系统"@zh ;
    airo:hasRisk ex:MisinformationAndIdentityFraudRisk ;
    airo:hasCapability ex:CelebrityImageSynthesis ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasStakeholder ex:CelebrityFans .

ex:MisinformationAndIdentityFraudRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation and Identity Fraud Risk"@en ;
    rdfs:label "虚假信息和身份欺诈风险"@zh ;
    airo:hasConsequence ex:PublicConfusionAndMisidentification .

ex:CelebrityImageSynthesis rdf:type airo:AICapability ;
    rdfs:label "Celebrity Image Synthesis Capability"@en ;
    rdfs:label "名人图像合成能力"@zh .

ex:CelebrityFans rdf:type airo:Stakeholder ;
    rdfs:label "Celebrity Fans"@en ;
    rdfs:label "名人粉丝"@zh .

ex:PublicConfusionAndMisidentification rdf:type airo:Impact ;
    rdfs:label "Public Confusion and Misidentification Impact"@en ;
    rdfs:label "公众混淆和错误识别影响"@zh ;
    airo:hasImpactOnStakeholder ex:CelebrityFans .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .