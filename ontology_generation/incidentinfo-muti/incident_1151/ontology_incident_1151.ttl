@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookAdTargetingSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook广告定向系统"@zh ;
    rdfs:label "Facebook Ad Targeting System"@en ;
    airo:hasRisk ex:HateSpeechPropagationRisk ;
    airo:isDeployedBy ex:MetaPlatformsInc ;
    airo:hasStakeholder ex:JewishCommunity .

ex:HateSpeechPropagationRisk rdf:type airo:Risk ;
    rdfs:label "仇恨言论传播风险"@zh ;
    rdfs:label "Hate Speech Propagation Risk"@en ;
    airo:isRiskSourceFor ex:AlgorithmicBiasInAdTargeting ;
    airo:hasConsequence ex:HateGroupAmplification .

ex:JewishCommunity rdf:type airo:Stakeholder ;
    rdfs:label "犹太社区"@zh ;
    rdfs:label "Jewish Community"@en .

ex:SocialDiscriminationImpact rdf:type airo:Impact ;
    rdfs:label "社会歧视影响"@zh ;
    rdfs:label "Social Discrimination Impact"@en ;
    airo:hasImpactOnStakeholder ex:JewishCommunity .

ex:MetaPlatformsInc rdf:type airo:AIOperator ;
    rdfs:label "Meta平台公司"@zh ;
    rdfs:label "Meta Platforms Inc."@en .

ex:AlgorithmicBiasInAdTargeting rdf:type airo:RiskSource ;
    rdfs:label "广告定向中的算法偏见风险源"@zh ;
    rdfs:label "Algorithmic Bias in Ad Targeting Risk Source"@en .

ex:HateGroupAmplification rdf:type airo:Consequence ;
    rdfs:label "仇恨团体放大后果"@zh ;
    rdfs:label "Hate Group Amplification Consequence"@en ;
    airo:hasImpact ex:SocialDiscriminationImpact .