@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IntelliVisionFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "IntelliVision Facial Recognition System"@en ;
    rdfs:label "IntelliVision面部识别系统"@zh ;
    airo:hasRisk ex:DeceptiveAccuracyAndBiasClaimsRisk ;
    airo:hasStakeholder ex:FederalTradeCommission .

ex:DeceptiveAccuracyAndBiasClaimsRisk rdf:type airo:Risk ;
    rdfs:label "Deceptive Accuracy and Bias Claims Risk"@en ;
    rdfs:label "欺骗性准确性和偏见声明风险"@zh ;
    airo:isRiskSourceFor ex:MisleadingMarketingClaims ;
    airo:hasImpact ex:ConsumerDeception .

ex:FederalTradeCommission rdf:type airo:Stakeholder ;
    rdfs:label "Federal Trade Commission"@en ;
    rdfs:label "联邦贸易委员会"@zh .

ex:MisleadingMarketingClaims rdf:type airo:RiskSource ;
    rdfs:label "Misleading Marketing Claims"@en ;
    rdfs:label "误导性营销声明"@zh .

ex:ConsumerDeception rdf:type airo:Impact ;
    rdfs:label "Consumer Deception"@en ;
    rdfs:label "消费者欺骗"@zh ;
    airo:hasImpactOnStakeholder ex:FederalTradeCommission .