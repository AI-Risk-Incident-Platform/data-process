@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AppleAI rdf:type airo:AISystem ;
    rdfs:label "Apple AI"@en ;
    rdfs:label "苹果AI"@zh ;
    airo:hasRisk ex:InappropriateContentGenerationRisk ;
    airo:hasAISubject ex:ElderlyUser ;
    airo:isDevelopedBy ex:AppleInc .

ex:InappropriateContentGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Inappropriate Content Generation Risk"@en ;
    rdfs:label "不当内容生成风险"@zh ;
    airo:hasConsequence ex:OffensiveMessageToElderlyUser ;
    airo:isRiskSourceFor ex:UnfilteredContentGeneration .

ex:OffensiveMessageToElderlyUser rdf:type airo:Consequence ;
    rdfs:label "Offensive Message to Elderly User Consequence"@en ;
    rdfs:label "对老年用户的冒犯性信息后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:ElderlyUser rdf:type airo:AISubject ;
    rdfs:label "Elderly User"@en ;
    rdfs:label "老年用户"@zh .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:ElderlyUser .

ex:UnfilteredContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Unfiltered Content Generation Risk Source"@en ;
    rdfs:label "未过滤内容生成风险源"@zh .

ex:AppleInc rdf:type airo:AIOperator ;
    rdfs:label "Apple Inc."@en ;
    rdfs:label "苹果公司"@zh .