@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "Tesla Autopilot System"@en ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleFatalityRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasStakeholder ex:CrashVictimFamily ;
    airo:isAppliedWithinDomain ex:AutonomousVehicleIndustry .

ex:AutonomousVehicleFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Fatality Risk"@en ;
    rdfs:label "自动驾驶车辆致命风险"@zh ;
    airo:hasConsequence ex:FatalCrashConsequence ;
    airo:isRiskSourceFor ex:SystemOverrelianceRiskSource .

ex:FatalCrashConsequence rdf:type airo:Consequence ;
    rdfs:label "Fatal Crash Consequence"@en ;
    rdfs:label "致命碰撞后果"@zh ;
    airo:hasImpact ex:LossOfLifeImpact .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:CrashVictimFamily rdf:type airo:Stakeholder ;
    rdfs:label "Crash Victim Family"@en ;
    rdfs:label "事故受害者家属"@zh .

ex:LossOfLifeImpact rdf:type airo:Impact ;
    rdfs:label "Loss of Life Impact"@en ;
    rdfs:label "生命损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:CrashVictimFamily .

ex:AutonomousVehicleIndustry rdf:type airo:Domain ;
    rdfs:label "Autonomous Vehicle Industry"@en ;
    rdfs:label "自动驾驶汽车行业"@zh .

ex:SystemOverrelianceRiskSource rdf:type airo:RiskSource ;
    rdfs:label "System Overreliance Risk Source"@en ;
    rdfs:label "系统过度依赖风险源"@zh .