@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GitHubCopilot rdf:type airo:AISystem ;
    rdfs:label "GitHub Copilot"@en ;
    rdfs:label "GitHub Copilot"@zh ;
    airo:hasRisk ex:LegalRiskOfCodeCopyrightInfringement ;
    airo:isProvidedBy ex:GitHubAndOpenAI ;
    airo:usesTechnique ex:CodeGenerationAI ;
    airo:isAppliedWithinDomain ex:SoftwareDevelopment .

ex:LegalRiskOfCodeCopyrightInfringement rdf:type airo:Risk ;
    rdfs:label "Legal Risk of Code Copyright Infringement"@en ;
    rdfs:label "代码版权侵权的法律风险"@zh ;
    airo:isRiskSourceFor ex:UseOfOpenSourceCodeWithoutProperLicensing ;
    airo:hasConsequence ex:PotentialLegalActionFromCodeAuthors .

ex:GitHubAndOpenAI rdf:type airo:AIProvider ;
    rdfs:label "GitHub and OpenAI"@en ;
    rdfs:label "GitHub和OpenAI"@zh .

ex:CodeGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Code Generation AI Technique"@en ;
    rdfs:label "代码生成AI技术"@zh .

ex:UseOfOpenSourceCodeWithoutProperLicensing rdf:type airo:RiskSource ;
    rdfs:label "Use of Open Source Code Without Proper Licensing Risk Source"@en ;
    rdfs:label "未经适当许可使用开源代码的风险源"@zh .

ex:PotentialLegalActionFromCodeAuthors rdf:type airo:Consequence ;
    rdfs:label "Potential Legal Action From Code Authors Consequence"@en ;
    rdfs:label "代码作者可能采取法律行动的后果"@zh .

ex:SoftwareDevelopment rdf:type airo:Domain ;
    rdfs:label "Software Development Domain"@en ;
    rdfs:label "软件开发领域"@zh .