ex:DeepFakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deep Fake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:usesTechnique ex:DeepLearningBasedVideoSynthesis .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Political Misinformation Risk"@en ;
    rdfs:label "政治虚假信息风险"@zh ;
    airo:hasConsequence ex:PoliticalCampaignDisruption .

ex:DeepLearningBasedVideoSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Video Synthesis Technique"@en ;
    rdfs:label "基于深度学习的视频合成技术"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh .

ex:PoliticalCampaignDisruption rdf:type airo:Consequence ;
    rdfs:label "Political Campaign Disruption Consequence"@en ;
    rdfs:label "政治竞选干扰后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .