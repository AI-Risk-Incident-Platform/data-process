@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LensaAIApp rdf:type airo:AISystem ;
    rdfs:label "Lensa AI App"@en ;
    rdfs:label "Lensa AI应用"@zh ;
    airo:hasRisk ex:UnauthorizedUseOfArtworkRisk ;
    airo:hasAISubject ex:AppUsers ;
    airo:hasStakeholder ex:Artists ;
    airo:hasMisuse ex:NonConsensualImageModification .

ex:UnauthorizedUseOfArtworkRisk rdf:type airo:Risk ;
    rdfs:label "Unauthorized Use of Artwork Risk"@en ;
    rdfs:label "未经授权使用艺术作品风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation ;
    airo:isRiskSourceFor ex:InappropriateContentGeneration .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Privacy Violation Consequence"@en ;
    rdfs:label "隐私侵犯后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:Artists rdf:type airo:Stakeholder ;
    rdfs:label "Artists"@en ;
    rdfs:label "艺术家"@zh .

ex:AppUsers rdf:type airo:AISubject ;
    rdfs:label "App Users"@en ;
    rdfs:label "应用使用者"@zh .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:Artists , ex:AppUsers .

ex:InappropriateContentGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inappropriate Content Generation Risk Source"@en ;
    rdfs:label "不当内容生成风险源"@zh .

ex:NonConsensualImageModification rdf:type airo:Misuse ;
    rdfs:label "Non-Consensual Image Modification Misuse"@en ;
    rdfs:label "未经同意的图像修改滥用"@zh .