@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GPT-3 rdf:type airo:AISystem ;
    rdfs:label "GPT-3"@en ;
    rdfs:label "GPT-3"@zh ;
    airo:hasRisk ex:ToxicLanguageGenerationRisk ;
    airo:hasCapability ex:NaturalLanguageGenerationCapability ;
    airo:isProvidedBy ex:OpenAI .

ex:ToxicLanguageGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Toxic Language Generation Risk"@en ;
    rdfs:label "生成有毒语言风险"@zh ;
    airo:isRiskSourceFor ex:TrainingDataBiasRiskSource .

ex:NaturalLanguageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Natural Language Generation Capability"@en ;
    rdfs:label "自然语言生成能力"@zh .

ex:OpenAI rdf:type airo:AIProvider ;
    rdfs:label "OpenAI"@en ;
    rdfs:label "OpenAI"@zh .

ex:TrainingDataBiasRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Training Data Bias Risk Source"@en ;
    rdfs:label "训练数据偏差风险源"@zh .