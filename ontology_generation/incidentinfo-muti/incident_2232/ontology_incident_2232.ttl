@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AMSAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Austrian Employment Service Algorithm"@en ;
    rdfs:label "奥地利就业服务中心算法"@zh ;
    airo:hasRisk ex:DiscriminatoryAlgorithmRisk ;
    airo:isDeployedBy ex:AustrianEmploymentAgency ;
    airo:hasStakeholder ex:DataProtectionAuthority .

ex:DiscriminatoryAlgorithmRisk rdf:type airo:Risk ;
    rdfs:label "Discriminatory Algorithm Risk"@en ;
    rdfs:label "歧视性算法风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryEmploymentDecisions ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:DataProtectionAuthority rdf:type airo:Stakeholder ;
    rdfs:label "Data Protection Authority"@en ;
    rdfs:label "数据保护机构"@zh .

ex:AustrianEmploymentAgency rdf:type airo:AIOperator ;
    rdfs:label "Austrian Employment Agency"@en ;
    rdfs:label "奥地利就业服务中心"@zh .

ex:DiscriminatoryEmploymentDecisions rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Employment Decisions Consequence"@en ;
    rdfs:label "歧视性就业决策后果"@zh ;
    airo:hasImpact ex:SocialInequality .

ex:SocialInequality rdf:type airo:Impact ;
    rdfs:label "Social Inequality Impact"@en ;
    rdfs:label "社会不平等影响"@zh ;
    airo:hasImpactOnStakeholder ex:DataProtectionAuthority .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .

ex:AlgorithmSuspension rdf:type airo:RiskControl ;
    rdfs:label "Algorithm Suspension Risk Control"@en ;
    rdfs:label "算法暂停管控"@zh ;
    airo:mitigatesRiskConcept ex:DiscriminatoryAlgorithmRisk .