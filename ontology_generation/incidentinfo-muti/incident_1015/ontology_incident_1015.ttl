@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RemoteJobApplicationAI rdf:type airo:AISystem ;
    rdfs:label "Remote Job Application AI System"@en ;
    rdfs:label "远程职位申请AI系统"@zh ;
    airo:hasRisk ex:FakeJobApplicationRisk ;
    airo:hasStakeholder ex:TechCEOs ;
    airo:isAppliedWithinDomain ex:RemoteWorkIndustry .

ex:FakeJobApplicationRisk rdf:type airo:Risk ;
    rdfs:label "Fake Job Application Risk"@en ;
    rdfs:label "虚假职位申请风险"@zh ;
    airo:isRiskSourceFor ex:AutomatedJobApplicationBots .

ex:TechCEOs rdf:type airo:Stakeholder ;
    rdfs:label "Tech CEOs"@en ;
    rdfs:label "科技公司CEO"@zh .

ex:RemoteWorkIndustry rdf:type airo:Domain ;
    rdfs:label "Remote Work Industry"@en ;
    rdfs:label "远程工作行业"@zh .

ex:AutomatedJobApplicationBots rdf:type airo:RiskSource ;
    rdfs:label "Automated Job Application Bots"@en ;
    rdfs:label "自动化职位申请机器人"@zh .