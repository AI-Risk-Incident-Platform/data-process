@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:OregonChildWelfareAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Oregon Child Welfare Algorithm"@en ;
    rdfs:label "俄勒冈州儿童福利算法"@zh ;
    airo:hasRisk ex:RacialBiasRisk ;
    airo:isAppliedWithinDomain ex:ChildWelfareServices ;
    airo:hasStakeholder ex:MinorityFamilies .

ex:RacialBiasRisk rdf:type airo:Risk ;
    rdfs:label "Racial Bias Risk"@en ;
    rdfs:label "种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryOutcomesImpact .

ex:ChildWelfareServices rdf:type airo:Domain ;
    rdfs:label "Child Welfare Services"@en ;
    rdfs:label "儿童福利服务"@zh .

ex:MinorityFamilies rdf:type airo:Stakeholder ;
    rdfs:label "Minority Families"@en ;
    rdfs:label "少数族裔家庭"@zh .

ex:DiscriminatoryOutcomesImpact rdf:type airo:Impact ;
    rdfs:label "Discriminatory Outcomes Impact"@en ;
    rdfs:label "歧视性结果影响"@zh ;
    airo:hasImpactOnStakeholder ex:MinorityFamilies .