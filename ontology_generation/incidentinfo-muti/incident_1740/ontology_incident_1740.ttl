@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Generation System"@en ;
    rdfs:label "深度伪造生成系统"@zh ;
    airo:hasRisk ex:DeepfakeMisuseRisk ;
    airo:hasStakeholder ex:GeneralPublic .

ex:DeepfakeMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Deepfake Misuse Risk"@en ;
    rdfs:label "深度伪造滥用风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:LackOfContentControl .

ex:GeneralPublic rdf:type airo:Stakeholder ;
    rdfs:label "General Public"@en ;
    rdfs:label "公众"@zh .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "虚假信息传播后果"@zh .

ex:LackOfContentControl rdf:type airo:RiskSource ;
    rdfs:label "Lack of Content Control Risk Source"@en ;
    rdfs:label "内容控制缺失风险源"@zh .