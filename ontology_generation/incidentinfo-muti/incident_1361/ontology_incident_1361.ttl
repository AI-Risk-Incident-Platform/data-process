@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DWPHousingBenefitFraudDetectionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "DWP Housing Benefit Fraud Detection Algorithm"@en ;
    rdfs:label "DWP住房福利欺诈检测算法"@zh ;
    airo:hasRisk ex:FalseFraudAccusationRisk ;
    airo:hasAISubject ex:HousingBenefitClaimants ;
    airo:hasStakeholder ex:DepartmentForWorkAndPensions ;
    airo:isAppliedWithinDomain ex:SocialWelfareAdministration .

ex:FalseFraudAccusationRisk rdf:type airo:Risk ;
    rdfs:label "False Fraud Accusation Risk"@en ;
    rdfs:label "虚假欺诈指控风险"@zh ;
    airo:hasConsequence ex:UnjustifiedBenefitSuspension ;
    airo:isRiskSourceFor ex:AlgorithmicErrorInFraudDetection .

ex:UnjustifiedBenefitSuspension rdf:type airo:Consequence ;
    rdfs:label "Unjustified Benefit Suspension Consequence"@en ;
    rdfs:label "不公正福利暂停后果"@zh ;
    airo:hasImpact ex:FinancialHardshipForInnocentClaimants .

ex:FinancialHardshipForInnocentClaimants rdf:type airo:Impact ;
    rdfs:label "Financial Hardship for Innocent Claimants Impact"@en ;
    rdfs:label "无辜申请人财务困难影响"@zh ;
    airo:hasImpactOnStakeholder ex:HousingBenefitClaimants .

ex:HousingBenefitClaimants rdf:type airo:AISubject ;
    rdfs:label "Housing Benefit Claimants"@en ;
    rdfs:label "住房福利申请人"@zh .

ex:DepartmentForWorkAndPensions rdf:type airo:Stakeholder ;
    rdfs:label "Department for Work and Pensions"@en ;
    rdfs:label "工作和养老金部"@zh .

ex:AlgorithmicErrorInFraudDetection rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Error in Fraud Detection Risk Source"@en ;
    rdfs:label "欺诈检测中的算法错误风险源"@zh .

ex:SocialWelfareAdministration rdf:type airo:Domain ;
    rdfs:label "Social Welfare Administration Domain"@en ;
    rdfs:label "社会福利管理领域"@zh .