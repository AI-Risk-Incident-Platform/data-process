@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIRealEstateFraudSystem rdf:type airo:AISystem ;
    rdfs:label "AI Real Estate Fraud System"@en ;
    rdfs:label "AI房地产欺诈系统"@zh ;
    airo:hasRisk ex:RealEstateFraudRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:PropertyOwners .

ex:RealEstateFraudRisk rdf:type airo:Risk ;
    rdfs:label "Real Estate Fraud Risk"@en ;
    rdfs:label "房地产欺诈风险"@zh ;
    airo:hasConsequence ex:PropertyTheft ;
    airo:isRiskSourceFor ex:AIEnabledFraud .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:PropertyTheft rdf:type airo:Consequence ;
    rdfs:label "Property Theft Consequence"@en ;
    rdfs:label "财产盗窃后果"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss Impact"@en ;
    rdfs:label "经济损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:PropertyOwners .

ex:PropertyOwners rdf:type airo:Stakeholder ;
    rdfs:label "Property Owners"@en ;
    rdfs:label "房产所有者"@zh .

ex:AIEnabledFraud rdf:type airo:RiskSource ;
    rdfs:label "AI-Enabled Fraud Risk Source"@en ;
    rdfs:label "AI助长欺诈风险源"@zh .