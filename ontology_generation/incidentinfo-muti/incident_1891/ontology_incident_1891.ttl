@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GPT-4TrainingSystem rdf:type airo:AISystem ;
    rdfs:label "GPT-4训练系统"@zh ;
    rdfs:label "GPT-4 Training System"@en ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:isDeployedBy ex:OpenAI ;
    airo:usesTechnique ex:VideoTranscriptionForTraining ;
    airo:isAppliedWithinDomain ex:AIModelTraining ;
    airo:hasPurpose ex:LargeLanguageModelTraining ;
    airo:hasStakeholder ex:YouTubeContentCreators .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "版权侵权风险"@zh ;
    rdfs:label "Copyright Infringement Risk"@en ;
    airo:isRiskSourceFor ex:UnauthorizedUseOfYouTubeContent .

ex:OpenAI rdf:type airo:AIOperator ;
    rdfs:label "OpenAI"@zh ;
    rdfs:label "OpenAI"@en .

ex:VideoTranscriptionForTraining rdf:type airo:AITechnique ;
    rdfs:label "视频转录训练技术"@zh ;
    rdfs:label "Video Transcription for Training Technique"@en .

ex:AIModelTraining rdf:type airo:Domain ;
    rdfs:label "AI模型训练领域"@zh ;
    rdfs:label "AI Model Training Domain"@en .

ex:UnauthorizedUseOfYouTubeContent rdf:type airo:RiskSource ;
    rdfs:label "未经授权使用YouTube内容"@zh ;
    rdfs:label "Unauthorized Use of YouTube Content Risk Source"@en .

ex:YouTubeContentCreators rdf:type airo:Stakeholder ;
    rdfs:label "YouTube内容创作者"@zh ;
    rdfs:label "YouTube Content Creators"@en .

ex:LargeLanguageModelTraining rdf:type airo:Purpose ;
    rdfs:label "大语言模型训练目的"@zh ;
    rdfs:label "Large Language Model Training Purpose"@en .