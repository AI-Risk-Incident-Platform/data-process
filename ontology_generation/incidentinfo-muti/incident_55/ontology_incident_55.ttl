@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonAlexa rdf:type airo:AISystem ;
    rdfs:label "亚马逊Alexa"@zh ;
    rdfs:label "Amazon Alexa"@en ;
    airo:hasRisk ex:InappropriateContentExposureRisk ;
    airo:hasAISubject ex:YoungChildren ;
    airo:isDevelopedBy ex:Amazon ;
    airo:hasPurpose ex:VoiceAssistantForHomeUse ;
    airo:isAppliedWithinDomain ex:SmartHome .

ex:InappropriateContentExposureRisk rdf:type airo:Risk ;
    rdfs:label "不当内容暴露风险"@zh ;
    rdfs:label "Inappropriate Content Exposure Risk"@en ;
    airo:hasConsequence ex:ChildrenExposureToPornography ;
    airo:isRiskSourceFor ex:ContentFilteringFailure .

ex:ChildrenExposureToPornography rdf:type airo:Consequence ;
    rdfs:label "儿童接触色情内容后果"@zh ;
    rdfs:label "Children Exposure to Pornography Consequence"@en ;
    airo:hasImpact ex:PsychologicalHarmToChildren .

ex:YoungChildren rdf:type airo:AISubject ;
    rdfs:label "幼儿"@zh ;
    rdfs:label "Young Children"@en .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "亚马逊"@zh ;
    rdfs:label "Amazon"@en .

ex:ContentFilteringFailure rdf:type airo:RiskSource ;
    rdfs:label "内容过滤失败风险源"@zh ;
    rdfs:label "Content Filtering Failure Risk Source"@en .

ex:PsychologicalHarmToChildren rdf:type airo:Impact ;
    rdfs:label "对儿童心理伤害的影响"@zh ;
    rdfs:label "Psychological Harm to Children Impact"@en ;
    airo:hasImpactOnStakeholder ex:YoungChildren .

ex:VoiceAssistantForHomeUse rdf:type airo:Purpose ;
    rdfs:label "家庭语音助手目的"@zh ;
    rdfs:label "Voice Assistant for Home Use Purpose"@en .

ex:SmartHome rdf:type airo:Domain ;
    rdfs:label "智能家居领域"@zh ;
    rdfs:label "Smart Home Domain"@en .