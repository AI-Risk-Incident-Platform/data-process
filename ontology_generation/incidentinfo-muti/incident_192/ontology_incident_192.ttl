@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    rdfs:label "Tesla Full Self Driving System"@en ;
    airo:hasRisk ex:EmployeeTerminationForTechReviewDisclosure ;
    airo:hasStakeholder ex:TeslaEmployee ;
    airo:isDeployedBy ex:TeslaInc .

ex:EmployeeTerminationForTechReviewDisclosure rdf:type airo:Risk ;
    rdfs:label "员工因技术评测披露被解雇风险"@zh ;
    rdfs:label "Employee Termination for Tech Review Disclosure Risk"@en ;
    airo:isRiskSourceFor ex:UnauthorizedTechReviewPublication ;
    airo:hasConsequence ex:NegativePublicityForTesla .

ex:TeslaEmployee rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉员工"@zh ;
    rdfs:label "Tesla Employee"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:NegativePublicityForTesla rdf:type airo:Consequence ;
    rdfs:label "特斯拉负面宣传后果"@zh ;
    rdfs:label "Negative Publicity for Tesla Consequence"@en ;
    airo:hasImpact ex:DamageToCompanyReputation .

ex:DamageToCompanyReputation rdf:type airo:Impact ;
    rdfs:label "公司声誉受损影响"@zh ;
    rdfs:label "Damage to Company Reputation Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaInc .

ex:UnauthorizedTechReviewPublication rdf:type airo:RiskSource ;
    rdfs:label "未经授权的技术评测发布风险源"@zh ;
    rdfs:label "Unauthorized Tech Review Publication Risk Source"@en ;
    airo:isRiskSourceFor ex:EmployeeTerminationForTechReviewDisclosure .