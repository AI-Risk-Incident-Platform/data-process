@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:YouTubeRecommendationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "YouTube Recommendation Algorithm"@en ;
    rdfs:label "YouTube推荐算法"@zh ;
    airo:hasRisk ex:RadicalizationRisk ;
    airo:hasCapability ex:ContentRecommendation ;
    airo:hasStakeholder ex:YouTubeUsers ;
    airo:hasAISubject ex:VulnerableIndividuals ;
    airo:hasPurpose ex:UserEngagement ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms ;
    airo:usesTechnique ex:DeepLearningRecommendation .

ex:RadicalizationRisk rdf:type airo:Risk ;
    rdfs:label "Radicalization Risk"@en ;
    rdfs:label "激进主义风险"@zh ;
    airo:hasConsequence ex:ViolentExtremism ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:ViolentExtremism rdf:type airo:Consequence ;
    rdfs:label "Violent Extremism Consequence"@en ;
    rdfs:label "暴力极端主义后果"@zh ;
    airo:hasImpact ex:SocialHarm .

ex:ContentRecommendation rdf:type airo:AICapability ;
    rdfs:label "Content Recommendation Capability"@en ;
    rdfs:label "内容推荐能力"@zh .

ex:YouTubeUsers rdf:type airo:Stakeholder ;
    rdfs:label "YouTube Users"@en ;
    rdfs:label "YouTube用户"@zh .

ex:VulnerableIndividuals rdf:type airo:AISubject ;
    rdfs:label "Vulnerable Individuals"@en ;
    rdfs:label "易受影响个体"@zh .

ex:SocialHarm rdf:type airo:Impact ;
    rdfs:label "Social Harm Impact"@en ;
    rdfs:label "社会危害影响"@zh ;
    airo:hasImpactOnStakeholder ex:YouTubeUsers .

ex:UserEngagement rdf:type airo:Purpose ;
    rdfs:label "User Engagement Enhancement Purpose"@en ;
    rdfs:label "用户参与度提升目的"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .

ex:DeepLearningRecommendation rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Recommendation Technique"@en ;
    rdfs:label "深度学习推荐技术"@zh .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh ;
    airo:exploitsVulnerability ex:HumanCognitiveBias .

ex:HumanCognitiveBias rdf:type airo:Vulnerability ;
    rdfs:label "Human Cognitive Bias Vulnerability"@en ;
    rdfs:label "人类认知偏差脆弱性"@zh .