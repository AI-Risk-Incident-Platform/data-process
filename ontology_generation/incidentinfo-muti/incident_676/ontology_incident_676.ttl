@base <https://w3id.org/airo#> .

ex:DeepfakeTechnology rdf:type airo:AISystem ;
    rdfs:label "深度伪造技术"@zh ;
    rdfs:label "Deepfake Technology"@en ;
    airo:hasRisk ex:PoliticalMisinformationRisk ;
    airo:usesTechnique ex:SyntheticMediaGeneration ;
    airo:hasAISubject ex:MarcosAdministration ;
    airo:hasStakeholder ex:PhilippineGovernment .

ex:PoliticalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "政治虚假信息风险"@zh ;
    rdfs:label "Political Misinformation Risk"@en ;
    airo:isRiskSourceFor ex:ForeignActor ;
    airo:hasConsequence ex:InternationalTensions .

ex:InternationalTensions rdf:type airo:Consequence ;
    rdfs:label "国际紧张局势后果"@zh ;
    rdfs:label "International Tensions Consequence"@en ;
    airo:hasImpact ex:DiplomaticRelationsDamage .

ex:PhilippineGovernment rdf:type airo:Stakeholder ;
    rdfs:label "菲律宾政府"@zh ;
    rdfs:label "Philippine Government"@en ;
    airo:hasImpactOnStakeholder ex:DiplomaticRelationsDamage .

ex:MarcosAdministration rdf:type airo:AISubject ;
    rdfs:label "马科斯政府"@zh ;
    rdfs:label "Marcos Administration"@en .

ex:DiplomaticRelationsDamage rdf:type airo:Impact ;
    rdfs:label "外交关系损害影响"@zh ;
    rdfs:label "Diplomatic Relations Damage Impact"@en .

ex:ForeignActor rdf:type airo:RiskSource ;
    rdfs:label "外国行为者风险源"@zh ;
    rdfs:label "Foreign Actor Risk Source"@en .

ex:SyntheticMediaGeneration rdf:type airo:AITechnique ;
    rdfs:label "合成媒体生成技术"@zh ;
    rdfs:label "Synthetic Media Generation Technique"@en .