@base <https://w3id.org/airo#> .

ex:GrokChatbot rdf:type airo:AISystem ;
    rdfs:label "Grok Chatbot"@en ;
    rdfs:label "Grok聊天机器人"@zh ;
    airo:hasRisk ex:PoliticalBiasInAIChatbots ;
    airo:isDeployedBy ex:ElonMusk ;
    airo:hasStakeholder ex:PoliticalFigures .

ex:PoliticalBiasInAIChatbots rdf:type airo:Risk ;
    rdfs:label "Political Bias in AI Chatbots Risk"@en ;
    rdfs:label "AI聊天机器人中的政治偏见风险"@zh ;
    airo:hasConsequence ex:PublicControversy .

ex:PoliticalFigures rdf:type airo:Stakeholder ;
    rdfs:label "Political Figures"@en ;
    rdfs:label "政治人物"@zh .

ex:ElonMusk rdf:type airo:AIOperator ;
    rdfs:label "Elon Musk"@en ;
    rdfs:label "埃隆·马斯克"@zh .

ex:PublicControversy rdf:type airo:Consequence ;
    rdfs:label "Public Controversy"@en ;
    rdfs:label "公众争议"@zh ;
    airo:hasImpactOnStakeholder ex:PoliticalFigures .