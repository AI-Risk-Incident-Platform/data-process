@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:InstagramAutoTranslateTool rdf:type airo:AISystem ;
    rdfs:label "Instagram Auto Translate Tool"@en ;
    rdfs:label "Instagram自动翻译工具"@zh ;
    airo:hasRisk ex:InaccurateTranslationRisk ;
    airo:isDevelopedBy ex:Meta ;
    airo:hasStakeholder ex:PalestineSupporters .

ex:InaccurateTranslationRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Translation Risk"@en ;
    rdfs:label "翻译不准确风险"@zh ;
    airo:hasConsequence ex:MisrepresentationOfUsers ;
    airo:isRiskSourceFor ex:TranslationAlgorithmError .

ex:MisrepresentationOfUsers rdf:type airo:Consequence ;
    rdfs:label "Misrepresentation of Users Consequence"@en ;
    rdfs:label "用户形象误传后果"@zh ;
    airo:hasImpact ex:HarmToUserReputation .

ex:HarmToUserReputation rdf:type airo:Impact ;
    rdfs:label "Harm to User Reputation Impact"@en ;
    rdfs:label "损害用户声誉的影响"@zh ;
    airo:hasImpactOnStakeholder ex:PalestineSupporters .

ex:PalestineSupporters rdf:type airo:Stakeholder ;
    rdfs:label "Palestine Supporters"@en ;
    rdfs:label "巴勒斯坦支持者"@zh .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@en ;
    rdfs:label "Meta公司"@zh .

ex:TranslationAlgorithmError rdf:type airo:RiskSource ;
    rdfs:label "Translation Algorithm Error Risk Source"@en ;
    rdfs:label "翻译算法错误风险源"@zh .