@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:SiriVoiceAssistant rdf:type airo:AISystem ;
    rdfs:label "Siri语音助手"@zh ;
    rdfs:label "Siri Voice Assistant"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:AppleInc ;
    airo:isDeployedBy ex:AppleAsAIOperator ;
    airo:hasPurpose ex:VoiceCommandProcessing .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:hasConsequence ex:FinancialPenalty ;
    airo:isRiskSourceFor ex:UnauthorizedVoiceRecording .

ex:FinancialPenalty rdf:type airo:Consequence ;
    rdfs:label "经济处罚后果"@zh ;
    rdfs:label "Financial Penalty Consequence"@en ;
    airo:hasImpact ex:UserTrustErosion .

ex:AppleInc rdf:type airo:Stakeholder ;
    rdfs:label "苹果公司"@zh ;
    rdfs:label "Apple Inc."@en .

ex:AppleAsAIOperator rdf:type airo:AIOperator ;
    rdfs:label "作为AI运营商的苹果"@zh ;
    rdfs:label "Apple as AI Operator"@en .

ex:UserTrustErosion rdf:type airo:Impact ;
    rdfs:label "用户信任度下降的影响"@zh ;
    rdfs:label "User Trust Erosion Impact"@en ;
    airo:hasImpactOnStakeholder ex:AppleInc .

ex:UnauthorizedVoiceRecording rdf:type airo:RiskSource ;
    rdfs:label "未经授权的语音记录风险源"@zh ;
    rdfs:label "Unauthorized Voice Recording Risk Source"@en .

ex:VoiceCommandProcessing rdf:type airo:Purpose ;
    rdfs:label "语音命令处理的目的"@zh ;
    rdfs:label "Voice Command Processing Purpose"@en .