@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Facial Recognition System"@en ;
    rdfs:label "Facebook面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatform ;
    airo:isDevelopedBy ex:FacebookInc ;
    airo:hasStakeholder ex:SouthKoreaDataWatchdog .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:UnauthorizedBiometricDataCollection ;
    airo:isRiskSourceFor ex:LackOfUserConsentMechanism .

ex:SouthKoreaDataWatchdog rdf:type airo:Stakeholder ;
    rdfs:label "South Korea Data Watchdog"@en ;
    rdfs:label "韩国数据监管机构"@zh .

ex:FacebookInc rdf:type airo:AIOperator ;
    rdfs:label "Facebook Inc."@en ;
    rdfs:label "Facebook公司"@zh .

ex:UnauthorizedBiometricDataCollection rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Biometric Data Collection Consequence"@en ;
    rdfs:label "未经授权的生物特征数据收集后果"@zh ;
    airo:hasImpact ex:PersonalPrivacyRightsInfringement .

ex:PersonalPrivacyRightsInfringement rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Rights Infringement Impact"@en ;
    rdfs:label "个人隐私权侵犯影响"@zh ;
    airo:hasImpactOnStakeholder ex:SouthKoreaDataWatchdog .

ex:LackOfUserConsentMechanism rdf:type airo:RiskSource ;
    rdfs:label "Lack of User Consent Mechanism Risk Source"@en ;
    rdfs:label "缺乏用户同意机制的风险源"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:SocialMediaPlatform rdf:type airo:Domain ;
    rdfs:label "Social Media Platform Domain"@en ;
    rdfs:label "社交媒体平台领域"@zh .