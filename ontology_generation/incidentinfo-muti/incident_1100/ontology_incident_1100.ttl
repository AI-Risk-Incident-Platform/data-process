@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionSunglasses rdf:type airo:AISystem ;
    rdfs:label "面部识别太阳镜"@zh ;
    rdfs:label "Facial Recognition Sunglasses"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:ChinesePolice ;
    airo:hasAISubject ex:TrackedCitizens ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:RealTimeFacialRecognition .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:hasConsequence ex:LossOfPersonalPrivacy .

ex:ChinesePolice rdf:type airo:Stakeholder ;
    rdfs:label "中国警方"@zh ;
    rdfs:label "Chinese Police"@en .

ex:TrackedCitizens rdf:type airo:AISubject ;
    rdfs:label "被追踪公民"@zh ;
    rdfs:label "Tracked Citizens"@en .

ex:LossOfPersonalPrivacy rdf:type airo:Impact ;
    rdfs:label "个人隐私丧失"@zh ;
    rdfs:label "Loss of Personal Privacy"@en ;
    airo:hasImpactOnStakeholder ex:TrackedCitizens .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "公共安全"@zh ;
    rdfs:label "Public Security"@en .

ex:RealTimeFacialRecognition rdf:type airo:AITechnique ;
    rdfs:label "实时面部识别"@zh ;
    rdfs:label "Real-Time Facial Recognition"@en .