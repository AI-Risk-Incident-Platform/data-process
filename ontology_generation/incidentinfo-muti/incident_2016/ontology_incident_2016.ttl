@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MicrosoftPregnancyPredictionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Microsoft Pregnancy Prediction Algorithm"@en ;
    rdfs:label "微软怀孕预测算法"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:PreteenGirls ;
    airo:hasPurpose ex:PredictPregnancies ;
    airo:isDevelopedBy ex:Microsoft ;
    airo:hasStakeholder ex:Microsoft .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PrivacyConcerns .

ex:PreteenGirls rdf:type airo:AISubject ;
    rdfs:label "Preteen Girls"@en ;
    rdfs:label "青春期前女孩"@zh .

ex:Microsoft rdf:type airo:Stakeholder ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:PredictPregnancies rdf:type airo:Purpose ;
    rdfs:label "Predict Pregnancies"@en ;
    rdfs:label "预测怀孕"@zh .

ex:PrivacyConcerns rdf:type airo:Consequence ;
    rdfs:label "Privacy Concerns"@en ;
    rdfs:label "隐私担忧"@zh ;
    airo:hasImpact ex:EthicalConcerns .

ex:EthicalConcerns rdf:type airo:Impact ;
    rdfs:label "Ethical Concerns"@en ;
    rdfs:label "伦理担忧"@zh ;
    airo:hasImpactOnStakeholder ex:PreteenGirls .