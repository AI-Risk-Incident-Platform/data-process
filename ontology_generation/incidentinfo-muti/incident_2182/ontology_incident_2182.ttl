ex:AIGradingSystem rdf:type airo:AISystem ;
    rdfs:label "AI Grading System"@en ;
    rdfs:label "AI评分系统"@zh ;
    airo:hasRisk ex:CheatingRiskInAIGrading ;
    airo:hasStakeholder ex:Students .

ex:CheatingRiskInAIGrading rdf:type airo:Risk ;
    rdfs:label "Cheating Risk in AI Grading"@en ;
    rdfs:label "AI评分中的作弊风险"@zh ;
    airo:isRiskSourceFor ex:ExploitingAIGradingVulnerabilities .

ex:Students rdf:type airo:Stakeholder ;
    rdfs:label "Students"@en ;
    rdfs:label "学生"@zh .

ex:ExploitingAIGradingVulnerabilities rdf:type airo:Misuse ;
    rdfs:label "Exploiting AI Grading Vulnerabilities"@en ;
    rdfs:label "利用AI评分漏洞"@zh .