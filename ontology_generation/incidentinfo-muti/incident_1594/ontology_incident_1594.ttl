@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:IncorrectProgrammingAnswersRisk ;
    airo:isAppliedWithinDomain ex:SoftwareDevelopment ;
    airo:hasCapability ex:ProgrammingQuestionAnswering .

ex:IncorrectProgrammingAnswersRisk rdf:type airo:Risk ;
    rdfs:label "Incorrect Programming Answers Risk"@en ;
    rdfs:label "编程问题回答错误风险"@zh ;
    airo:hasConsequence ex:MisleadingProgrammingGuidance .

ex:MisleadingProgrammingGuidance rdf:type airo:Consequence ;
    rdfs:label "Misleading Programming Guidance Consequence"@en ;
    rdfs:label "误导性编程指导后果"@zh ;
    airo:hasImpact ex:ReducedDeveloperProductivity .

ex:SoftwareDevelopment rdf:type airo:Domain ;
    rdfs:label "Software Development Domain"@en ;
    rdfs:label "软件开发领域"@zh .

ex:ProgrammingQuestionAnswering rdf:type airo:AICapability ;
    rdfs:label "Programming Question Answering Capability"@en ;
    rdfs:label "编程问题回答能力"@zh .

ex:ReducedDeveloperProductivity rdf:type airo:Impact ;
    rdfs:label "Reduced Developer Productivity Impact"@en ;
    rdfs:label "开发者生产力下降影响"@zh .