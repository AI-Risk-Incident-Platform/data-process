@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:EpicSepsisAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Epic Sepsis Algorithm"@en ;
    rdfs:label "Epic脓毒症算法"@zh ;
    airo:hasRisk ex:MissedSepsisCasesRisk ;
    airo:isDeployedBy ex:EpicSystems ;
    airo:isAppliedWithinDomain ex:Healthcare ;
    airo:hasPurpose ex:SepsisPrediction .

ex:MissedSepsisCasesRisk rdf:type airo:Risk ;
    rdfs:label "Missed Sepsis Cases Risk"@en ;
    rdfs:label "漏诊脓毒症病例风险"@zh ;
    airo:hasConsequence ex:FaultyMedicalAlarms .

ex:FaultyMedicalAlarms rdf:type airo:Consequence ;
    rdfs:label "Faulty Medical Alarms Consequence"@en ;
    rdfs:label "错误医疗警报后果"@zh .

ex:EpicSystems rdf:type airo:AIOperator ;
    rdfs:label "Epic Systems"@en ;
    rdfs:label "Epic系统公司"@zh .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "Healthcare Domain"@en ;
    rdfs:label "医疗保健领域"@zh .

ex:SepsisPrediction rdf:type airo:Purpose ;
    rdfs:label "Sepsis Prediction Purpose"@en ;
    rdfs:label "脓毒症预测目的"@zh .