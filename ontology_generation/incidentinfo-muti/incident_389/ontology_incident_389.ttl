@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonDeliveryDriverSurveillanceSystem rdf:type airo:AISystem ;
    rdfs:label "Amazon Delivery Driver Surveillance System"@en ;
    rdfs:label "亚马逊送货司机监控系统"@zh ;
    airo:hasRisk ex:PrivacyViolationAndWorkerExploitationRisk ;
    airo:hasStakeholder ex:AmazonDeliveryDrivers ;
    airo:isDeployedBy ex:Amazon ;
    airo:hasPurpose ex:DriverPerformanceMonitoring .

ex:PrivacyViolationAndWorkerExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Worker Exploitation Risk"@en ;
    rdfs:label "隐私侵犯和工人剥削风险"@zh ;
    airo:hasConsequence ex:WorkerStressAndJobDissatisfaction ;
    airo:isRiskSourceFor ex:BiometricDataCollection .

ex:AmazonDeliveryDrivers rdf:type airo:Stakeholder ;
    rdfs:label "Amazon Delivery Drivers"@en ;
    rdfs:label "亚马逊送货司机"@zh .

ex:Amazon rdf:type airo:AIOperator ;
    rdfs:label "Amazon"@en ;
    rdfs:label "亚马逊"@zh .

ex:WorkerStressAndJobDissatisfaction rdf:type airo:Consequence ;
    rdfs:label "Worker Stress and Job Dissatisfaction Consequence"@en ;
    rdfs:label "工人压力和工作不满后果"@zh ;
    airo:hasImpact ex:NegativeImpactOnWorkerWellbeing .

ex:NegativeImpactOnWorkerWellbeing rdf:type airo:Impact ;
    rdfs:label "Negative Impact on Worker Wellbeing Impact"@en ;
    rdfs:label "对工人福祉的负面影响"@zh ;
    airo:hasImpactOnStakeholder ex:AmazonDeliveryDrivers .

ex:BiometricDataCollection rdf:type airo:RiskSource ;
    rdfs:label "Biometric Data Collection Risk Source"@en ;
    rdfs:label "生物特征数据收集风险源"@zh .

ex:DriverPerformanceMonitoring rdf:type airo:Purpose ;
    rdfs:label "Driver Performance Monitoring Purpose"@en ;
    rdfs:label "司机绩效监控目的"@zh .