@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:UKPassportPhotoChecker rdf:type airo:AISystem ;
    rdfs:label "UK Passport Photo Checker"@en ;
    rdfs:label "英国护照照片检查系统"@zh ;
    airo:hasRisk ex:RacialBiasInPhotoRecognition ;
    airo:hasAISubject ex:DarkSkinnedWomenApplyingForPassports ;
    airo:hasStakeholder ex:UKHomeOffice ;
    airo:isAppliedWithinDomain ex:GovernmentIdentityVerification .

ex:RacialBiasInPhotoRecognition rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Photo Recognition Risk"@en ;
    rdfs:label "照片识别中的种族偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminationAgainstDarkSkinnedWomen .

ex:DiscriminationAgainstDarkSkinnedWomen rdf:type airo:Consequence ;
    rdfs:label "Discrimination Against Dark-Skinned Women Consequence"@en ;
    rdfs:label "对深肤色女性的歧视后果"@zh ;
    airo:hasImpact ex:UnfairTreatmentInPassportApplications .

ex:DarkSkinnedWomenApplyingForPassports rdf:type airo:AISubject ;
    rdfs:label "Dark-Skinned Women Applying for Passports"@en ;
    rdfs:label "申请护照的深肤色女性"@zh .

ex:UKHomeOffice rdf:type airo:Stakeholder ;
    rdfs:label "UK Home Office"@en ;
    rdfs:label "英国内政部"@zh .

ex:UnfairTreatmentInPassportApplications rdf:type airo:Impact ;
    rdfs:label "Unfair Treatment in Passport Applications Impact"@en ;
    rdfs:label "护照申请中的不公平待遇影响"@zh ;
    airo:hasImpactOnStakeholder ex:DarkSkinnedWomenApplyingForPassports .

ex:GovernmentIdentityVerification rdf:type airo:Domain ;
    rdfs:label "Government Identity Verification Domain"@en ;
    rdfs:label "政府身份验证领域"@zh .