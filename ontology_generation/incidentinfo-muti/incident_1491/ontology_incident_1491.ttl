@base <https://w3id.org/airo#> .

ex:AIChatbots rdf:type airo:AISystem ;
    rdfs:label "AI Chatbots"@en ;
    rdfs:label "AI聊天机器人"@zh ;
    airo:hasRisk ex:RacialStereotypesRisk .

ex:RacialStereotypesRisk rdf:type airo:Risk ;
    rdfs:label "Racial Stereotypes Risk"@en ;
    rdfs:label "种族刻板印象风险"@zh ;
    airo:hasConsequence ex:CovertRacismPerpetuation ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:CovertRacismPerpetuation rdf:type airo:Consequence ;
    rdfs:label "Covert Racism Perpetuation Consequence"@en ;
    rdfs:label "隐性种族主义延续后果"@zh ;
    airo:hasImpact ex:SocialDiscriminationImpact .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "偏见训练数据风险源"@zh .

ex:SocialDiscriminationImpact rdf:type airo:Impact ;
    rdfs:label "Social Discrimination Impact"@en ;
    rdfs:label "社会歧视影响"@zh .