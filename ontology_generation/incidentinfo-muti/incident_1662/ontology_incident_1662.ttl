@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleSearchAutocompleteSystem rdf:type airo:AISystem ;
    rdfs:label "Google Search Autocomplete System"@en ;
    rdfs:label "谷歌搜索自动补全系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasCapability ex:AutocompletePrediction ;
    airo:isDevelopedBy ex:GoogleLLC ;
    airo:hasAISubject ex:CrimeVictims ;
    airo:hasStakeholder ex:RapeVictims ;
    airo:hasVulnerability ex:LackOfContentFiltering .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:VictimIdentification ;
    airo:isRiskSourceFor ex:SensitiveDataExposure .

ex:VictimIdentification rdf:type airo:Consequence ;
    rdfs:label "Victim Identification Consequence"@en ;
    rdfs:label "受害者身份暴露后果"@zh ;
    airo:hasImpact ex:PsychologicalHarmToVictims .

ex:PsychologicalHarmToVictims rdf:type airo:Impact ;
    rdfs:label "Psychological Harm to Victims Impact"@en ;
    rdfs:label "对受害者的心理伤害影响"@zh ;
    airo:hasImpactOnStakeholder ex:RapeVictims .

ex:AutocompletePrediction rdf:type airo:AICapability ;
    rdfs:label "Autocomplete Prediction Capability"@en ;
    rdfs:label "自动补全预测能力"@zh .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌公司"@zh .

ex:RapeVictims rdf:type airo:Stakeholder ;
    rdfs:label "Rape Victims"@en ;
    rdfs:label "强奸受害者"@zh .

ex:CrimeVictims rdf:type airo:AISubject ;
    rdfs:label "Crime Victims"@en ;
    rdfs:label "犯罪受害者"@zh .

ex:SensitiveDataExposure rdf:type airo:RiskSource ;
    rdfs:label "Sensitive Data Exposure Risk Source"@en ;
    rdfs:label "敏感数据暴露风险源"@zh .

ex:LackOfContentFiltering rdf:type airo:Vulnerability ;
    rdfs:label "Lack of Content Filtering Vulnerability"@en ;
    rdfs:label "缺乏内容过滤的脆弱性"@zh .