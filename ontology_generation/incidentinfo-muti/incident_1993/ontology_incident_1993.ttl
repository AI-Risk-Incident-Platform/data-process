@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TinderFacialDataset rdf:type airo:AISystem ;
    rdfs:label "Tinder Facial Dataset"@en ;
    rdfs:label "Tinder面部数据集"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:TinderUsers .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalDataExposure ;
    airo:isRiskSourceFor ex:DataScrapingWithoutConsent .

ex:TinderUsers rdf:type airo:Stakeholder ;
    rdfs:label "Tinder Users"@en ;
    rdfs:label "Tinder用户"@zh .

ex:DataScrapingWithoutConsent rdf:type airo:RiskSource ;
    rdfs:label "Data Scraping Without Consent"@en ;
    rdfs:label "未经同意的数据抓取"@zh .

ex:PersonalDataExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Data Exposure"@en ;
    rdfs:label "个人数据暴露"@zh ;
    airo:hasImpactOnStakeholder ex:TinderUsers .