@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RussianFakeNewsRing rdf:type airo:AISystem ;
    rdfs:label "Russian Fake News Ring"@en ;
    rdfs:label "俄罗斯虚假新闻网络"@zh ;
    airo:hasRisk ex:DisinformationRisk ;
    airo:hasStakeholder ex:USAID ;
    airo:isRiskSourceFor ex:RussianPropaganda .

ex:DisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Disinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicTrustErosion ;
    airo:isRiskSourceFor ex:RussianPropaganda .

ex:USAID rdf:type airo:Stakeholder ;
    rdfs:label "USAID"@en ;
    rdfs:label "美国国际开发署"@zh .

ex:RussianPropaganda rdf:type airo:RiskSource ;
    rdfs:label "Russian Propaganda"@en ;
    rdfs:label "俄罗斯宣传"@zh ;
    airo:isRiskSourceFor ex:DisinformationRisk .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion"@en ;
    rdfs:label "公众信任侵蚀"@zh ;
    airo:hasImpactOnStakeholder ex:USAID .