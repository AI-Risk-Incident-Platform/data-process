@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleBooks rdf:type airo:AISystem ;
    rdfs:label "Google Books"@en ;
    rdfs:label "谷歌图书"@zh ;
    airo:hasRisk ex:LowQualityContentIndexingRisk ;
    airo:hasAICapability ex:ContentIndexing ;
    airo:hasStakeholder ex:BookReaders .

ex:LowQualityContentIndexingRisk rdf:type airo:Risk ;
    rdfs:label "Low Quality Content Indexing Risk"@en ;
    rdfs:label "低质量内容索引风险"@zh ;
    airo:hasConsequence ex:InformationPollution ;
    airo:isRiskSourceFor ex:AIWrittenWorks .

ex:InformationPollution rdf:type airo:Consequence ;
    rdfs:label "Information Pollution Consequence"@en ;
    rdfs:label "信息污染后果"@zh ;
    airo:hasImpact ex:ReducedContentReliability .

ex:ContentIndexing rdf:type airo:AICapability ;
    rdfs:label "Content Indexing Capability"@en ;
    rdfs:label "内容索引能力"@zh .

ex:BookReaders rdf:type airo:Stakeholder ;
    rdfs:label "Book Readers"@en ;
    rdfs:label "图书读者"@zh .

ex:AIWrittenWorks rdf:type airo:RiskSource ;
    rdfs:label "AI Written Works"@en ;
    rdfs:label "AI写作作品"@zh .

ex:ReducedContentReliability rdf:type airo:Impact ;
    rdfs:label "Reduced Content Reliability Impact"@en ;
    rdfs:label "内容可靠性降低的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BookReaders .