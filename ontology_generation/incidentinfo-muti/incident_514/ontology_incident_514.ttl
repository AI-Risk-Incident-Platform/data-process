@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "面部识别技术"@zh ;
    rdfs:label "Facial Recognition Technology"@en ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:IndividualsUnderSurveillance ;
    airo:hasStakeholder ex:LawEnforcementAgencies ;
    airo:hasPurpose ex:CriminalIdentification ;
    airo:isAppliedWithinDomain ex:PublicSafety ;
    airo:usesTechnique ex:BiometricIdentification .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "隐私侵犯风险"@zh ;
    rdfs:label "Privacy Violation Risk"@en ;
    airo:hasConsequence ex:UnjustArrests ;
    airo:isRiskSourceFor ex:MassSurveillance .

ex:LawEnforcementAgencies rdf:type airo:Stakeholder ;
    rdfs:label "执法机构"@zh ;
    rdfs:label "Law Enforcement Agencies"@en .

ex:IndividualsUnderSurveillance rdf:type airo:AISubject ;
    rdfs:label "被监控的个人"@zh ;
    rdfs:label "Individuals Under Surveillance"@en .

ex:LossOfPrivacy rdf:type airo:Impact ;
    rdfs:label "隐私丧失"@zh ;
    rdfs:label "Loss of Privacy"@en ;
    airo:hasImpactOnStakeholder ex:IndividualsUnderSurveillance .

ex:CriminalIdentification rdf:type airo:Purpose ;
    rdfs:label "犯罪识别"@zh ;
    rdfs:label "Criminal Identification"@en .

ex:PublicSafety rdf:type airo:Domain ;
    rdfs:label "公共安全"@zh ;
    rdfs:label "Public Safety"@en .

ex:BiometricIdentification rdf:type airo:AITechnique ;
    rdfs:label "生物特征识别"@zh ;
    rdfs:label "Biometric Identification"@en .

ex:MassSurveillance rdf:type airo:RiskSource ;
    rdfs:label "大规模监控"@zh ;
    rdfs:label "Mass Surveillance"@en .

ex:UnjustArrests rdf:type airo:Consequence ;
    rdfs:label "不公正逮捕"@zh ;
    rdfs:label "Unjust Arrests"@en ;
    airo:hasImpact ex:LossOfPrivacy .