@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookNewsSpamFilteringSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook News Spam Filtering System"@en ;
    rdfs:label "Facebook新闻垃圾信息过滤系统"@zh ;
    airo:hasRisk ex:LegitimateContentMisclassificationRisk ;
    airo:hasStakeholder ex:NewsPublishers .

ex:LegitimateContentMisclassificationRisk rdf:type airo:Risk ;
    rdfs:label "Legitimate Content Misclassification Risk"@en ;
    rdfs:label "合法内容误分类风险"@zh ;
    airo:hasConsequence ex:ReducedContentVisibility .

ex:ReducedContentVisibility rdf:type airo:Consequence ;
    rdfs:label "Reduced Content Visibility Consequence"@en ;
    rdfs:label "内容可见性降低后果"@zh ;
    airo:hasImpact ex:PublisherRevenueLoss .

ex:PublisherRevenueLoss rdf:type airo:Impact ;
    rdfs:label "Publisher Revenue Loss Impact"@en ;
    rdfs:label "出版商收入损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewsPublishers .

ex:NewsPublishers rdf:type airo:Stakeholder ;
    rdfs:label "News Publishers"@en ;
    rdfs:label "新闻出版商"@zh .