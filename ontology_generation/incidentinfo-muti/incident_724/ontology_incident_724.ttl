@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RussianDisinformationAI rdf:type airo:AISystem ;
    rdfs:label "Russian Disinformation AI"@en ;
    rdfs:label "俄罗斯虚假信息AI"@zh ;
    airo:hasRisk ex:ElectionInterferenceRisk ;
    airo:isAppliedWithinDomain ex:PoliticalElections ;
    airo:hasPurpose ex:InfluenceElectionOutcome ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:USVoters .

ex:ElectionInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Election Interference Risk"@en ;
    rdfs:label "选举干预风险"@zh ;
    airo:hasConsequence ex:PublicTrustErosion .

ex:PoliticalElections rdf:type airo:Domain ;
    rdfs:label "Political Elections Domain"@en ;
    rdfs:label "政治选举领域"@zh .

ex:InfluenceElectionOutcome rdf:type airo:Purpose ;
    rdfs:label "Influence Election Outcome Purpose"@en ;
    rdfs:label "影响选举结果的目的"@zh .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀的影响"@zh ;
    airo:hasImpactOnStakeholder ex:USVoters .

ex:USVoters rdf:type airo:Stakeholder ;
    rdfs:label "US Voters"@en ;
    rdfs:label "美国选民"@zh .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .