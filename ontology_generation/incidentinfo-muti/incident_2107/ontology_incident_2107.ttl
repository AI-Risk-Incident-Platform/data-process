@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:InstagramAndTwitterContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "Instagram和Twitter内容审核系统"@zh ;
    rdfs:label "Instagram and Twitter Content Moderation System"@en ;
    airo:hasRisk ex:ContentModerationBiasRisk ;
    airo:hasStakeholder ex:PalestinianContentCreators .

ex:ContentModerationBiasRisk rdf:type airo:Risk ;
    rdfs:label "内容审核偏见风险"@zh ;
    rdfs:label "Content Moderation Bias Risk"@en ;
    airo:hasConsequence ex:SuppressionOfPalestinianVoices ;
    airo:isRiskSourceFor ex:AlgorithmicGlitches .

ex:PalestinianContentCreators rdf:type airo:Stakeholder ;
    rdfs:label "巴勒斯坦内容创作者"@zh ;
    rdfs:label "Palestinian Content Creators"@en .

ex:SuppressionOfPalestinianVoices rdf:type airo:Consequence ;
    rdfs:label "巴勒斯坦声音被压制的后果"@zh ;
    rdfs:label "Suppression of Palestinian Voices Consequence"@en ;
    airo:hasImpact ex:ReducedFreedomOfExpression .

ex:ReducedFreedomOfExpression rdf:type airo:Impact ;
    rdfs:label "言论自由受限的影响"@zh ;
    rdfs:label "Reduced Freedom of Expression Impact"@en ;
    airo:hasImpactOnStakeholder ex:PalestinianContentCreators .

ex:AlgorithmicGlitches rdf:type airo:RiskSource ;
    rdfs:label "算法故障风险源"@zh ;
    rdfs:label "Algorithmic Glitches Risk Source"@en .