@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NewsAppAIWritingSystem rdf:type airo:AISystem ;
    rdfs:label "News App AI Writing System"@en ;
    rdfs:label "新闻应用AI写作系统"@zh ;
    airo:hasRisk ex:FakeNewsPublicationRisk ;
    airo:hasStakeholder ex:NewsAppUsers .

ex:FakeNewsPublicationRisk rdf:type airo:Risk ;
    rdfs:label "Fake News Publication Risk"@en ;
    rdfs:label "虚假新闻发布风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread .

ex:NewsAppUsers rdf:type airo:Stakeholder ;
    rdfs:label "News App Users"@en ;
    rdfs:label "新闻应用用户"@zh .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:PublicTrustErosion .

ex:PublicTrustErosion rdf:type airo:Impact ;
    rdfs:label "Public Trust Erosion Impact"@en ;
    rdfs:label "公众信任侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:NewsAppUsers .