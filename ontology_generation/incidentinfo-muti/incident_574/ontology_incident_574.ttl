@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DALLE rdf:type airo:AISystem ;
    rdfs:label "DALL-E"@en ;
    rdfs:label "DALL-E"@zh ;
    airo:hasRisk ex:BiasInAIGeneratedContent ;
    airo:hasAISubject ex:TransgenderIndividuals ;
    airo:hasCapability ex:ImageGenerationFromTextPrompts .

ex:BiasInAIGeneratedContent rdf:type airo:Risk ;
    rdfs:label "Bias in AI-Generated Content Risk"@en ;
    rdfs:label "AI生成内容中的偏见风险"@zh ;
    airo:isRiskSourceFor ex:BiasedTrainingData ;
    airo:hasImpact ex:ReinforcementOfHarmfulStereotypes .

ex:ImageGenerationFromTextPrompts rdf:type airo:AICapability ;
    rdfs:label "Image Generation from Text Prompts Capability"@en ;
    rdfs:label "基于文本提示的图像生成能力"@zh .

ex:TransgenderIndividuals rdf:type airo:AISubject ;
    rdfs:label "Transgender Individuals"@en ;
    rdfs:label "跨性别者"@zh .

ex:ReinforcementOfHarmfulStereotypes rdf:type airo:Impact ;
    rdfs:label "Reinforcement of Harmful Stereotypes Impact"@en ;
    rdfs:label "强化有害刻板印象的影响"@zh ;
    airo:hasImpactOnStakeholder ex:TransgenderIndividuals .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data"@en ;
    rdfs:label "有偏见的训练数据"@zh .