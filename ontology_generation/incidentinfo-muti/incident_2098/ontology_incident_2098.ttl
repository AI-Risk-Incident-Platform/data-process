ex:KpopAvatarAI rdf:type airo:AISystem ;
    rdfs:label "K-pop Avatar AI System"@en ;
    rdfs:label "K-pop虚拟偶像AI系统"@zh ;
    airo:hasRisk ex:EthicalConcernsInVirtualIdols ;
    airo:isAppliedWithinDomain ex:EntertainmentIndustry ;
    airo:hasPurpose ex:CreatingVirtualKpopStars ;
    airo:hasCapability ex:VirtualCelebrityCreation .

ex:EthicalConcernsInVirtualIdols rdf:type airo:Risk ;
    rdfs:label "Ethical Concerns in Virtual Idols Risk"@en ;
    rdfs:label "虚拟偶像伦理问题风险"@zh .

ex:EntertainmentIndustry rdf:type airo:Domain ;
    rdfs:label "Entertainment Industry Domain"@en ;
    rdfs:label "娱乐产业领域"@zh .

ex:CreatingVirtualKpopStars rdf:type airo:Purpose ;
    rdfs:label "Creating Virtual K-pop Stars Purpose"@en ;
    rdfs:label "创造虚拟K-pop明星的目的"@zh .

ex:VirtualCelebrityCreation rdf:type airo:AICapability ;
    rdfs:label "Virtual Celebrity Creation Capability"@en ;
    rdfs:label "虚拟名人创造能力"@zh .