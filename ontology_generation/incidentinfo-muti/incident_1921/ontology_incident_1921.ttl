ex:TelevisionAILeaderRoles rdf:type airo:AISystem ;
    rdfs:label "电视AI领导者角色"@zh ;
    rdfs:label "Television AI Leader Roles"@en ;
    airo:hasRisk ex:UnreliableDecisionMakingRisk ;
    airo:isAppliedWithinDomain ex:TelevisionIndustry ;
    airo:hasPurpose ex:LeadershipDecisionMaking ;
    airo:hasCapability ex:HoroscopeSportsWeatherAnalysis .

ex:UnreliableDecisionMakingRisk rdf:type airo:Risk ;
    rdfs:label "不可靠决策风险"@zh ;
    rdfs:label "Unreliable Decision Making Risk"@en .

ex:TelevisionIndustry rdf:type airo:Domain ;
    rdfs:label "电视行业"@zh ;
    rdfs:label "Television Industry"@en .

ex:LeadershipDecisionMaking rdf:type airo:Purpose ;
    rdfs:label "领导决策"@zh ;
    rdfs:label "Leadership Decision Making"@en .

ex:HoroscopeSportsWeatherAnalysis rdf:type airo:AICapability ;
    rdfs:label "星座体育天气分析能力"@zh ;
    rdfs:label "Horoscope Sports Weather Analysis Capability"@en .