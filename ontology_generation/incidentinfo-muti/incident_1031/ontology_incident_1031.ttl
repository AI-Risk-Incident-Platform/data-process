@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIPoweredFraudSystem rdf:type airo:AISystem ;
    rdfs:label "AI-Powered Fraud System"@en ;
    rdfs:label "AI驱动的欺诈系统"@zh ;
    airo:hasRisk ex:EmergingFraudThreats ;
    airo:hasStakeholder ex:Microsoft .

ex:EmergingFraudThreats rdf:type airo:Risk ;
    rdfs:label "Emerging Fraud Threats Risk"@en ;
    rdfs:label "新兴欺诈威胁风险"@zh ;
    airo:hasConsequence ex:OnlineShoppingScams ;
    airo:isRiskSourceFor ex:AIBots .

ex:Microsoft rdf:type airo:Stakeholder ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:OnlineShoppingScams rdf:type airo:Consequence ;
    rdfs:label "Online Shopping Scams Consequence"@en ;
    rdfs:label "网络购物诈骗后果"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:AIBots rdf:type airo:RiskSource ;
    rdfs:label "AI Bots Risk Source"@en ;
    rdfs:label "AI机器人风险源"@zh .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss Impact"@en ;
    rdfs:label "经济损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:Microsoft .