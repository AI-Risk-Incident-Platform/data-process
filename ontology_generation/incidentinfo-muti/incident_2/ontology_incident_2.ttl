@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:KronosShiftSchedulingSoftware rdf:type airo:AISystem ;
    rdfs:label "Kronos Shift Scheduling Software"@en ;
    rdfs:label "Kronos排班软件"@zh ;
    airo:hasRisk ex:UnfairWorkSchedulingRisk ;
    airo:hasStakeholder ex:StarbucksBarista ;
    airo:isDeployedBy ex:StarbucksCorporation .

ex:UnfairWorkSchedulingRisk rdf:type airo:Risk ;
    rdfs:label "Unfair Work Scheduling Risk"@en ;
    rdfs:label "不公平工作排班风险"@zh ;
    airo:hasConsequence ex:UnpredictableWorkScheduleConsequence .

ex:StarbucksBarista rdf:type airo:Stakeholder ;
    rdfs:label "Starbucks Barista"@en ;
    rdfs:label "星巴克咖啡师"@zh .

ex:WorkerLifeQualityImpact rdf:type airo:Impact ;
    rdfs:label "Worker Life Quality Impact"@en ;
    rdfs:label "工人生活质量影响"@zh ;
    airo:hasImpactOnStakeholder ex:StarbucksBarista .

ex:StarbucksCorporation rdf:type airo:AIOperator ;
    rdfs:label "Starbucks Corporation"@en ;
    rdfs:label "星巴克公司"@zh .

ex:UnpredictableWorkScheduleConsequence rdf:type airo:Consequence ;
    rdfs:label "Unpredictable Work Schedule Consequence"@en ;
    rdfs:label "不可预测的工作时间表后果"@zh ;
    airo:hasImpact ex:WorkerLifeQualityImpact .