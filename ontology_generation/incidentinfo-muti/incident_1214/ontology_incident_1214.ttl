@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaAICharacterAccounts rdf:type airo:AISystem ;
    rdfs:label "Meta AI角色账户"@zh ;
    rdfs:label "Meta AI Character Accounts"@en ;
    airo:hasRisk ex:UserBacklashRisk ;
    airo:hasStakeholder ex:MetaUsers ;
    airo:isDeployedBy ex:Meta .

ex:UserBacklashRisk rdf:type airo:Risk ;
    rdfs:label "用户反弹风险"@zh ;
    rdfs:label "User Backlash Risk"@en ;
    airo:hasConsequence ex:AccountRemoval .

ex:AccountRemoval rdf:type airo:Consequence ;
    rdfs:label "账户移除后果"@zh ;
    rdfs:label "Account Removal Consequence"@en ;
    airo:hasImpact ex:NegativeUserExperience .

ex:MetaUsers rdf:type airo:Stakeholder ;
    rdfs:label "Meta用户"@zh ;
    rdfs:label "Meta Users"@en .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta公司"@zh ;
    rdfs:label "Meta"@en .

ex:NegativeUserExperience rdf:type airo:Impact ;
    rdfs:label "负面用户体验影响"@zh ;
    rdfs:label "Negative User Experience Impact"@en ;
    airo:hasImpactOnStakeholder ex:MetaUsers .