@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RedditAIArchives rdf:type airo:AISystem ;
    rdfs:label "Reddit AI Archives"@en ;
    rdfs:label "Reddit AI档案"@zh ;
    airo:hasRisk ex:DataExploitationRisk ;
    airo:hasAISubject ex:HumanArchives ;
    airo:hasStakeholder ex:RedditCompany .

ex:DataExploitationRisk rdf:type airo:Risk ;
    rdfs:label "Data Exploitation Risk"@en ;
    rdfs:label "数据利用风险"@zh ;
    airo:isRiskSourceFor ex:CommercialUseOfHumanData .

ex:RedditCompany rdf:type airo:Stakeholder ;
    rdfs:label "Reddit Company"@en ;
    rdfs:label "Reddit公司"@zh .

ex:HumanArchives rdf:type airo:AISubject ;
    rdfs:label "Human Archives"@en ;
    rdfs:label "人类档案"@zh .

ex:CommercialUseOfHumanData rdf:type airo:RiskSource ;
    rdfs:label "Commercial Use of Human Data"@en ;
    rdfs:label "人类数据的商业使用"@zh .