@base <https://w3id.org/airo#> .

ex:Jammable rdf:type airo:AISystem ;
    rdfs:label "Jammable"@en ;
    rdfs:label "Jammable"@zh ;
    airo:hasRisk ex:LegalActionRisk ;
    airo:usesTechnique ex:DeepfakeModel ;
    airo:hasPurpose ex:VocalCloning ;
    airo:isAppliedWithinDomain ex:MusicIndustry ;
    airo:hasStakeholder ex:BPI ;
    airo:hasAIOperator ex:Voicify .

ex:LegalActionRisk rdf:type airo:Risk ;
    rdfs:label "Legal Action Risk"@en ;
    rdfs:label "法律诉讼风险"@zh ;
    airo:hasConsequence ex:LegalConsequences .

ex:BPI rdf:type airo:Stakeholder ;
    rdfs:label "BPI"@en ;
    rdfs:label "英国唱片业协会"@zh .

ex:Voicify rdf:type airo:AIOperator ;
    rdfs:label "Voicify"@en ;
    rdfs:label "Voicify"@zh .

ex:VocalCloning rdf:type airo:Purpose ;
    rdfs:label "Vocal Cloning"@en ;
    rdfs:label "声音克隆"@zh .

ex:MusicIndustry rdf:type airo:Domain ;
    rdfs:label "Music Industry"@en ;
    rdfs:label "音乐产业"@zh .

ex:DeepfakeModel rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Model"@en ;
    rdfs:label "深度伪造模型"@zh .

ex:LegalConsequences rdf:type airo:Consequence ;
    rdfs:label "Legal Consequences"@en ;
    rdfs:label "法律后果"@zh ;
    airo:hasImpact ex:MusicIndustryConflict .

ex:MusicIndustryConflict rdf:type airo:Impact ;
    rdfs:label "Music Industry Conflict"@en ;
    rdfs:label "音乐产业冲突"@zh .