@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VirtualResurrectionAI rdf:type airo:AISystem ;
    rdfs:label "Virtual Resurrection AI System"@en ;
    rdfs:label "虚拟复活AI系统"@zh ;
    airo:hasRisk ex:EthicalControversyRisk ;
    airo:hasPurpose ex:EmotionalClosure ;
    airo:hasAISubject ex:BereavedFamilyMembers ;
    airo:hasStakeholder ex:ChinesePublic .

ex:EthicalControversyRisk rdf:type airo:Risk ;
    rdfs:label "Ethical Controversy Risk"@en ;
    rdfs:label "伦理争议风险"@zh ;
    airo:hasConsequence ex:SocialControversy .

ex:EmotionalClosure rdf:type airo:Purpose ;
    rdfs:label "Emotional Closure Purpose"@en ;
    rdfs:label "情感慰藉目的"@zh .

ex:BereavedFamilyMembers rdf:type airo:AISubject ;
    rdfs:label "Bereaved Family Members"@en ;
    rdfs:label "丧亲家庭成员"@zh .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:ChinesePublic .

ex:ChinesePublic rdf:type airo:Stakeholder ;
    rdfs:label "Chinese Public"@en ;
    rdfs:label "中国公众"@zh .

ex:SocialControversy rdf:type airo:Consequence ;
    rdfs:label "Social Controversy Consequence"@en ;
    rdfs:label "社会争议后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .