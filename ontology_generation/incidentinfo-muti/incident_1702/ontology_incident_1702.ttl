@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:LiveFaceRecognitionCameraSystem rdf:type airo:AISystem ;
    rdfs:label "Live Face Recognition Camera System"@en ;
    rdfs:label "实时人脸识别摄像头系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasStakeholder ex:FrasersGroup ;
    airo:isDeployedBy ex:SportsDirectOwner ;
    airo:isAppliedWithinDomain ex:RetailSurveillance .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PublicConcernOverPrivacy .

ex:FrasersGroup rdf:type airo:Stakeholder ;
    rdfs:label "Frasers Group"@en ;
    rdfs:label "弗雷泽集团"@zh .

ex:SportsDirectOwner rdf:type airo:AIOperator ;
    rdfs:label "Sports Direct Owner"@en ;
    rdfs:label "Sports Direct所有者"@zh .

ex:RetailSurveillance rdf:type airo:Domain ;
    rdfs:label "Retail Surveillance Domain"@en ;
    rdfs:label "零售监控领域"@zh .

ex:PublicConcernOverPrivacy rdf:type airo:Impact ;
    rdfs:label "Public Concern Over Privacy Impact"@en ;
    rdfs:label "公众对隐私的担忧影响"@zh ;
    airo:hasImpactOnStakeholder ex:FrasersGroup .