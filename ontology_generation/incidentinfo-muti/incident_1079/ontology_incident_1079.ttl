@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AlexaVoiceAssistant rdf:type airo:AISystem ;
    rdfs:label "Alexa Voice Assistant"@en ;
    rdfs:label "Alexa语音助手"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:AlexaUsers ;
    airo:hasStakeholder ex:AmazonEmployees .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalDataExposure ;
    airo:isRiskSourceFor ex:HumanReviewProcess .

ex:AmazonEmployees rdf:type airo:Stakeholder ;
    rdfs:label "Amazon Employees"@en ;
    rdfs:label "亚马逊员工"@zh .

ex:AlexaUsers rdf:type airo:AISubject ;
    rdfs:label "Alexa Users"@en ;
    rdfs:label "Alexa用户"@zh .

ex:PersonalDataExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Data Exposure Consequence"@en ;
    rdfs:label "个人数据暴露后果"@zh ;
    airo:hasImpactOnStakeholder ex:AlexaUsers .

ex:HumanReviewProcess rdf:type airo:RiskSource ;
    rdfs:label "Human Review Process Risk Source"@en ;
    rdfs:label "人工审核流程风险源"@zh .