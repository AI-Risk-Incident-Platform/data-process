@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:COVID19DiagnosisFromCoughsAI rdf:type airo:AISystem ;
    rdfs:label "COVID-19 Diagnosis from Coughs AI System"@en ;
    rdfs:label "通过咳嗽诊断COVID-19的AI系统"@zh ;
    airo:hasRisk ex:InaccurateMedicalDiagnosisRisk ;
    airo:hasCapability ex:AudioBasedDiseaseDetection ;
    airo:isAppliedWithinDomain ex:MedicalDiagnostics .

ex:InaccurateMedicalDiagnosisRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Medical Diagnosis Risk"@en ;
    rdfs:label "不准确医疗诊断风险"@zh ;
    airo:hasConsequence ex:MisdiagnosisOfCOVID19 .

ex:MisdiagnosisOfCOVID19 rdf:type airo:Consequence ;
    rdfs:label "Misdiagnosis of COVID-19 Consequence"@en ;
    rdfs:label "COVID-19误诊后果"@zh .

ex:MedicalDiagnostics rdf:type airo:Domain ;
    rdfs:label "Medical Diagnostics Domain"@en ;
    rdfs:label "医疗诊断领域"@zh .

ex:AudioBasedDiseaseDetection rdf:type airo:AICapability ;
    rdfs:label "Audio-based Disease Detection Capability"@en ;
    rdfs:label "基于音频的疾病检测能力"@zh .