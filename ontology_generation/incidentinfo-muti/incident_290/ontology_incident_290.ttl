@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TorontoBeachWaterQualityAI rdf:type airo:AISystem ;
    rdfs:label "Toronto Beach Water Quality AI System"@en ;
    rdfs:label "多伦多海滩水质AI系统"@zh ;
    airo:hasRisk ex:MisleadingWaterQualityPrediction ;
    airo:hasStakeholder ex:WaterQualityAdvocates .

ex:MisleadingWaterQualityPrediction rdf:type airo:Risk ;
    rdfs:label "Misleading Water Quality Prediction Risk"@en ;
    rdfs:label "误导性水质预测风险"@zh ;
    airo:hasConsequence ex:PublicHealthRisk .

ex:WaterQualityAdvocates rdf:type airo:Stakeholder ;
    rdfs:label "Water Quality Advocates"@en ;
    rdfs:label "水质倡导者"@zh .

ex:PublicHealthRisk rdf:type airo:Consequence ;
    rdfs:label "Public Health Risk Consequence"@en ;
    rdfs:label "公共健康风险后果"@zh ;
    airo:hasImpact ex:SwimmerSafetyCompromise .

ex:SwimmerSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Swimmer Safety Compromise Impact"@en ;
    rdfs:label "游泳者安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:WaterQualityAdvocates .