@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Content Moderation System"@en ;
    rdfs:label "Facebook内容审核系统"@zh ;
    airo:hasRisk ex:InaccurateContentModerationRisk ;
    airo:usesTechnique ex:AutomatedTextAnalysis ;
    airo:hasStakeholder ex:FacebookInc .

ex:InaccurateContentModerationRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Content Moderation Risk"@en ;
    rdfs:label "内容审核不准确风险"@zh ;
    airo:hasConsequence ex:HistoricalDocumentMisclassification ;
    airo:isRiskSourceFor ex:OverlySensitiveLanguageDetection .

ex:HistoricalDocumentMisclassification rdf:type airo:Consequence ;
    rdfs:label "Historical Document Misclassification Consequence"@en ;
    rdfs:label "历史文件误分类后果"@zh .

ex:FacebookInc rdf:type airo:AIOperator ;
    rdfs:label "Facebook Inc."@en ;
    rdfs:label "Facebook公司"@zh .

ex:AutomatedTextAnalysis rdf:type airo:AITechnique ;
    rdfs:label "Automated Text Analysis Technique"@en ;
    rdfs:label "自动化文本分析技术"@zh .

ex:OverlySensitiveLanguageDetection rdf:type airo:RiskSource ;
    rdfs:label "Overly Sensitive Language Detection Risk Source"@en ;
    rdfs:label "过度敏感的语言检测风险源"@zh .