@base <https://w3id.org/airo#> .

ex:FacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh ;
    airo:hasRisk ex:LackOfTransparencyInLawEnforcementUse ;
    airo:hasStakeholder ex:LawEnforcementAgencies ;
    airo:isAppliedWithinDomain ex:PublicSafetyAndLawEnforcement ;
    airo:hasPurpose ex:IdentityVerificationAndSurveillance .

ex:LackOfTransparencyInLawEnforcementUse rdf:type airo:Risk ;
    rdfs:label "Lack of Transparency in Law Enforcement Use Risk"@en ;
    rdfs:label "执法使用中缺乏透明度风险"@zh .

ex:LawEnforcementAgencies rdf:type airo:Stakeholder ;
    rdfs:label "Law Enforcement Agencies"@en ;
    rdfs:label "执法机构"@zh .

ex:PublicSafetyAndLawEnforcement rdf:type airo:Domain ;
    rdfs:label "Public Safety and Law Enforcement Domain"@en ;
    rdfs:label "公共安全与执法领域"@zh .

ex:IdentityVerificationAndSurveillance rdf:type airo:Purpose ;
    rdfs:label "Identity Verification and Surveillance Purpose"@en ;
    rdfs:label "身份验证与监控目的"@zh .