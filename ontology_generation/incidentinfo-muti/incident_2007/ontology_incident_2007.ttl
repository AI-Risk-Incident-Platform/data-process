@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:BuenosAiresFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Buenos Aires Facial Recognition System"@en ;
    rdfs:label "布宜诺斯艾利斯面部识别系统"@zh ;
    airo:hasRisk ex:UnconstitutionalSurveillanceRisk ;
    airo:usesTechnique ex:BiometricIdentification ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:hasStakeholder ex:ArgentinianJudicialSystem .

ex:UnconstitutionalSurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Unconstitutional Surveillance Risk"@en ;
    rdfs:label "违宪监控风险"@zh ;
    airo:hasConsequence ex:CivilLibertiesViolation .

ex:CivilLibertiesViolation rdf:type airo:Consequence ;
    rdfs:label "Civil Liberties Violation Consequence"@en ;
    rdfs:label "公民自由侵犯后果"@zh ;
    airo:hasImpact ex:PrivacyRightsErosion .

ex:ArgentinianJudicialSystem rdf:type airo:Stakeholder ;
    rdfs:label "Argentinian Judicial System"@en ;
    rdfs:label "阿根廷司法系统"@zh .

ex:PrivacyRightsErosion rdf:type airo:Impact ;
    rdfs:label "Privacy Rights Erosion Impact"@en ;
    rdfs:label "隐私权侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:ArgentinianJudicialSystem .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:BiometricIdentification rdf:type airo:AITechnique ;
    rdfs:label "Biometric Identification Technique"@en ;
    rdfs:label "生物特征识别技术"@zh .