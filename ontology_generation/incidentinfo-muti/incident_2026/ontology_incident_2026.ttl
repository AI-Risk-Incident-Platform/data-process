@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:RussianOppressionBots rdf:type airo:AISystem ;
    rdfs:label "Russian Oppression Bots"@en ;
    rdfs:label "俄罗斯压制反对派机器人"@zh ;
    airo:hasRisk ex:PoliticalManipulationRisk ;
    airo:isDeployedBy ex:RussianGovernment ;
    airo:hasStakeholder ex:RussianOpposition .

ex:PoliticalManipulationRisk rdf:type airo:Risk ;
    rdfs:label "Political Manipulation Risk"@en ;
    rdfs:label "政治操纵风险"@zh ;
    airo:hasImpact ex:SuppressionOfPoliticalDissent .

ex:RussianOpposition rdf:type airo:Stakeholder ;
    rdfs:label "Russian Opposition"@en ;
    rdfs:label "俄罗斯反对派"@zh .

ex:RussianGovernment rdf:type airo:AIOperator ;
    rdfs:label "Russian Government"@en ;
    rdfs:label "俄罗斯政府"@zh .

ex:SuppressionOfPoliticalDissent rdf:type airo:Impact ;
    rdfs:label "Suppression of Political Dissent"@en ;
    rdfs:label "压制政治异议"@zh ;
    airo:hasImpactOnStakeholder ex:RussianOpposition .