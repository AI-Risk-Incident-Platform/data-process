@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IndustrialRobot rdf:type airo:AISystem ;
    rdfs:label "Industrial Robot"@en ;
    rdfs:label "工业机器人"@zh ;
    airo:hasRisk ex:HumanFatalityRisk ;
    airo:hasCapability ex:ObjectRecognition ;
    airo:isAppliedWithinDomain ex:WarehouseAutomation ;
    airo:hasVulnerability ex:LimitedPerceptionInDynamicEnvironments .

ex:HumanFatalityRisk rdf:type airo:Risk ;
    rdfs:label "Human Fatality Risk"@en ;
    rdfs:label "人员死亡风险"@zh ;
    airo:hasConsequence ex:FatalInjury ;
    airo:isRiskSourceFor ex:ObjectMisidentification .

ex:FatalInjury rdf:type airo:Consequence ;
    rdfs:label "Fatal Injury Consequence"@en ;
    rdfs:label "致命伤害后果"@zh ;
    airo:hasImpact ex:LossOfHumanLife .

ex:LossOfHumanLife rdf:type airo:Impact ;
    rdfs:label "Loss of Human Life Impact"@en ;
    rdfs:label "人员死亡影响"@zh .

ex:ObjectMisidentification rdf:type airo:RiskSource ;
    rdfs:label "Object Misidentification Risk Source"@en ;
    rdfs:label "物体识别错误风险源"@zh .

ex:WarehouseAutomation rdf:type airo:Domain ;
    rdfs:label "Warehouse Automation Domain"@en ;
    rdfs:label "仓库自动化领域"@zh .

ex:ObjectRecognition rdf:type airo:AICapability ;
    rdfs:label "Object Recognition Capability"@en ;
    rdfs:label "物体识别能力"@zh .

ex:LimitedPerceptionInDynamicEnvironments rdf:type airo:Vulnerability ;
    rdfs:label "Limited Perception in Dynamic Environments Vulnerability"@en ;
    rdfs:label "动态环境中感知受限的脆弱性"@zh .