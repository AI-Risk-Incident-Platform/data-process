@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedImageSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Image System"@en ;
    rdfs:label "AI生成图像系统"@zh ;
    airo:hasRisk ex:FinancialMisinformationRisk ;
    airo:hasStakeholder ex:NigerianCentralBank .

ex:FinancialMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Financial Misinformation Risk"@en ;
    rdfs:label "金融虚假信息风险"@zh ;
    airo:hasConsequence ex:PublicConfusionAboutCurrency .

ex:NigerianCentralBank rdf:type airo:Stakeholder ;
    rdfs:label "Nigerian Central Bank"@en ;
    rdfs:label "尼日利亚中央银行"@zh .

ex:PublicConfusionAboutCurrency rdf:type airo:Consequence ;
    rdfs:label "Public Confusion About Currency Consequence"@en ;
    rdfs:label "公众对货币的困惑后果"@zh ;
    airo:hasImpact ex:PotentialEconomicInstability .

ex:PotentialEconomicInstability rdf:type airo:Impact ;
    rdfs:label "Potential Economic Instability Impact"@en ;
    rdfs:label "潜在经济不稳定影响"@zh ;
    airo:hasImpactOnStakeholder ex:NigerianCentralBank .