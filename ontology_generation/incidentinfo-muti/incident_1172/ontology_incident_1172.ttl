@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HowOldDoYouLookApp rdf:type airo:AISystem ;
    rdfs:label "How Old Do You Look App"@en ;
    rdfs:label "How Old Do You Look应用"@zh ;
    airo:hasRisk ex:InaccurateAgePredictionRisk ;
    airo:hasCapability ex:FaceRecognitionForAgePrediction ;
    airo:isDevelopedBy ex:Microsoft ;
    airo:hasStakeholder ex:Microsoft .

ex:InaccurateAgePredictionRisk rdf:type airo:Risk ;
    rdfs:label "Inaccurate Age Prediction Risk"@en ;
    rdfs:label "年龄预测不准确风险"@zh ;
    airo:hasConsequence ex:PublicMemeCreation .

ex:Microsoft rdf:type airo:AIOperator ;
    rdfs:label "Microsoft"@en ;
    rdfs:label "微软"@zh .

ex:FaceRecognitionForAgePrediction rdf:type airo:AICapability ;
    rdfs:label "Face Recognition for Age Prediction Capability"@en ;
    rdfs:label "面部识别用于年龄预测的能力"@zh .

ex:PublicMemeCreation rdf:type airo:Consequence ;
    rdfs:label "Public Meme Creation Consequence"@en ;
    rdfs:label "公众制作表情包的后果"@zh ;
    airo:hasImpact ex:BrandReputationImpact .

ex:BrandReputationImpact rdf:type airo:Impact ;
    rdfs:label "Brand Reputation Impact"@en ;
    rdfs:label "品牌声誉影响"@zh ;
    airo:hasImpactOnStakeholder ex:Microsoft .