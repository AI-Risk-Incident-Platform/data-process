@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:PoliceFacialRecognitionTechnology rdf:type airo:AISystem ;
    rdfs:label "Police Facial Recognition Technology"@en ;
    rdfs:label "警方人脸识别技术"@zh ;
    airo:hasRisk ex:UnlawfulSurveillanceRisk ;
    airo:hasStakeholder ex:UKPolice ;
    airo:isDeployedBy ex:LawEnforcementAgencies ;
    airo:isAppliedWithinDomain ex:PublicSecurity ;
    airo:usesTechnique ex:BiometricIdentification .

ex:UnlawfulSurveillanceRisk rdf:type airo:Risk ;
    rdfs:label "Unlawful Surveillance Risk"@en ;
    rdfs:label "非法监控风险"@zh ;
    airo:hasConsequence ex:PrivacyViolation .

ex:UKPolice rdf:type airo:Stakeholder ;
    rdfs:label "UK Police"@en ;
    rdfs:label "英国警方"@zh .

ex:LawEnforcementAgencies rdf:type airo:AIOperator ;
    rdfs:label "Law Enforcement Agencies"@en ;
    rdfs:label "执法机构"@zh .

ex:PrivacyViolation rdf:type airo:Consequence ;
    rdfs:label "Privacy Violation Consequence"@en ;
    rdfs:label "隐私侵犯后果"@zh ;
    airo:hasImpact ex:CivilLibertiesErosion .

ex:CivilLibertiesErosion rdf:type airo:Impact ;
    rdfs:label "Civil Liberties Erosion Impact"@en ;
    rdfs:label "公民自由侵蚀影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKPolice .

ex:PublicSecurity rdf:type airo:Domain ;
    rdfs:label "Public Security Domain"@en ;
    rdfs:label "公共安全领域"@zh .

ex:BiometricIdentification rdf:type airo:AITechnique ;
    rdfs:label "Biometric Identification Technique"@en ;
    rdfs:label "生物特征识别技术"@zh .