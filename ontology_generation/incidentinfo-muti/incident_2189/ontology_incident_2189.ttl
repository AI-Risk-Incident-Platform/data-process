@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleAutomatedContentSystem rdf:type airo:AISystem ;
    rdfs:label "Google Automated Content System"@en ;
    rdfs:label "谷歌自动化内容系统"@zh ;
    airo:hasRisk ex:AutomatedDefamationRisk ;
    airo:isDevelopedBy ex:GoogleLLC ;
    airo:hasStakeholder ex:GoogleLLC .

ex:AutomatedDefamationRisk rdf:type airo:Risk ;
    rdfs:label "Automated Defamation Risk"@en ;
    rdfs:label "自动化诽谤风险"@zh ;
    airo:hasConsequence ex:LegalLiabilityForDefamation .

ex:LegalLiabilityForDefamation rdf:type airo:Consequence ;
    rdfs:label "Legal Liability for Defamation Consequence"@en ;
    rdfs:label "诽谤法律责任后果"@zh ;
    airo:hasImpact ex:ReputationalHarmToIndividuals .

ex:GoogleLLC rdf:type airo:AIOperator ;
    rdfs:label "Google LLC"@en ;
    rdfs:label "谷歌有限责任公司"@zh .

ex:ReputationalHarmToIndividuals rdf:type airo:Impact ;
    rdfs:label "Reputational Harm to Individuals Impact"@en ;
    rdfs:label "个人名誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:GoogleLLC .