@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AITrafficCameras rdf:type airo:AISystem ;
    rdfs:label "AI Traffic Cameras"@en ;
    rdfs:label "AI交通摄像头"@zh ;
    airo:hasRisk ex:PrivacyInvasionRisk ;
    airo:hasCapability ex:SeatbeltMonitoring ;
    airo:hasStakeholder ex:UKDrivers .

ex:PrivacyInvasionRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Invasion Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasImpact ex:PersonalPrivacyCompromise .

ex:SeatbeltMonitoring rdf:type airo:AICapability ;
    rdfs:label "Seatbelt Monitoring Capability"@en ;
    rdfs:label "安全带监控能力"@zh .

ex:UKDrivers rdf:type airo:Stakeholder ;
    rdfs:label "UK Drivers"@en ;
    rdfs:label "英国司机"@zh .

ex:PersonalPrivacyCompromise rdf:type airo:Impact ;
    rdfs:label "Personal Privacy Compromise Impact"@en ;
    rdfs:label "个人隐私受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:UKDrivers .