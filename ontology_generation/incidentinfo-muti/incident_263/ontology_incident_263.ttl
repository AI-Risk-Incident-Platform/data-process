@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DALL·EMiniImageGenerator rdf:type airo:AISystem ;
    rdfs:label "DALL·E Mini Image Generator"@en ;
    rdfs:label "DALL·E Mini图像生成器"@zh ;
    airo:hasRisk ex:RacialBiasInImageGenerationRisk ;
    airo:hasCapability ex:TextToImageGenerationCapability .

ex:RacialBiasInImageGenerationRisk rdf:type airo:Risk ;
    rdfs:label "Racial Bias in Image Generation Risk"@en ;
    rdfs:label "图像生成中的种族偏见风险"@zh ;
    airo:hasConsequence ex:HarmfulStereotypePropagationConsequence ;
    airo:isRiskSourceFor ex:BiasedTrainingDataRiskSource .

ex:HarmfulStereotypePropagationConsequence rdf:type airo:Consequence ;
    rdfs:label "Harmful Stereotype Propagation Consequence"@en ;
    rdfs:label "有害刻板印象传播后果"@zh ;
    airo:hasImpact ex:CulturalStereotypeReinforcementImpact .

ex:TextToImageGenerationCapability rdf:type airo:AICapability ;
    rdfs:label "Text-to-Image Generation Capability"@en ;
    rdfs:label "文本到图像生成能力"@zh .

ex:BiasedTrainingDataRiskSource rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "偏见训练数据风险源"@zh .

ex:CulturalStereotypeReinforcementImpact rdf:type airo:Impact ;
    rdfs:label "Cultural Stereotype Reinforcement Impact"@en ;
    rdfs:label "文化刻板印象强化影响"@zh .