ex:DeepLiveCam rdf:type airo:AISystem ;
    rdfs:label "Deep-Live-Cam"@en ;
    rdfs:label "Deep-Live-Cam"@zh ;
    airo:hasRisk ex:DigitalIdentityMisuseRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:hasStakeholder ex:GeneralPublic .

ex:DigitalIdentityMisuseRisk rdf:type airo:Risk ;
    rdfs:label "Digital Identity Misuse Risk"@en ;
    rdfs:label "数字身份滥用风险"@zh ;
