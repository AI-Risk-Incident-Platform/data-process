@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HealthcareDecisionAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Healthcare Decision Algorithm"@en ;
    rdfs:label "医疗决策算法"@zh ;
    airo:hasRisk ex:IrrationalHealthcareDecisionRisk ;
    airo:isAppliedWithinDomain ex:Healthcare ;
    rdfs:comment "来源：What happened when a 'wildly irrational' algorithm made crucial healthcare decisions"@en .

ex:IrrationalHealthcareDecisionRisk rdf:type airo:Risk ;
    rdfs:label "Irrational Healthcare Decision Risk"@en ;
    rdfs:label "非理性医疗决策风险"@zh ;
    airo:hasConsequence ex:InappropriateMedicalDecisions ;
    rdfs:comment "来源：a 'wildly irrational' algorithm made crucial healthcare decisions"@en .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "Healthcare Domain"@en ;
    rdfs:label "医疗领域"@zh ;
    rdfs:comment "来源：crucial healthcare decisions"@en .

ex:InappropriateMedicalDecisions rdf:type airo:Consequence ;
    rdfs:label "Inappropriate Medical Decisions Consequence"@en ;
    rdfs:label "不当医疗决策后果"@zh ;
    airo:hasImpact ex:PatientHarm ;
    rdfs:comment "来源：made crucial healthcare decisions"@en .

ex:PatientHarm rdf:type airo:Impact ;
    rdfs:label "Patient Harm Impact"@en ;
    rdfs:label "患者伤害影响"@zh ;
    rdfs:comment "来源：crucial healthcare decisions"@en .