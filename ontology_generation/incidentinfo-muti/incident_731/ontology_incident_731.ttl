@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AISpambots rdf:type airo:AISystem ;
    rdfs:label "AI Spambots"@en ;
    rdfs:label "AI垃圾邮件机器人"@zh ;
    airo:hasRisk ex:ClassroomDisruptionRisk ;
    airo:hasStakeholder ex:CollegeProfessor ;
    airo:isAppliedWithinDomain ex:Education .

ex:ClassroomDisruptionRisk rdf:type airo:Risk ;
    rdfs:label "Classroom Disruption Risk"@en ;
    rdfs:label "课堂干扰风险"@zh ;
    airo:hasConsequence ex:TeachingDisruption .

ex:CollegeProfessor rdf:type airo:Stakeholder ;
    rdfs:label "College Professor"@en ;
    rdfs:label "大学教授"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education Domain"@en ;
    rdfs:label "教育领域"@zh .

ex:TeachingDisruption rdf:type airo:Impact ;
    rdfs:label "Teaching Disruption Impact"@en ;
    rdfs:label "教学中断影响"@zh ;
    airo:hasImpactOnStakeholder ex:CollegeProfessor .