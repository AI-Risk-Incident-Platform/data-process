@base <https://w3id.org/airo#> .

ex:ExamMonitoringSoftware rdf:type airo:AISystem ;
    rdfs:label "Exam Monitoring Software"@en ;
    rdfs:label "考试监控软件"@zh ;
    airo:hasRisk ex:DisadvantagesForBIPOCStudents ;
    airo:hasAISubject ex:BIPOCStudents ;
    airo:hasStakeholder ex:UniversityOfToronto .

ex:DisadvantagesForBIPOCStudents rdf:type airo:Risk ;
    rdfs:label "Disadvantages for BIPOC Students Risk"@en ;
    rdfs:label "BIPOC学生面临劣势的风险"@zh ;
    airo:hasConsequence ex:EducationalDisadvantage .

ex:BIPOCStudents rdf:type airo:AISubject ;
    rdfs:label "BIPOC Students"@en ;
    rdfs:label "BIPOC学生"@zh .

ex:UniversityOfToronto rdf:type airo:Stakeholder ;
    rdfs:label "University of Toronto"@en ;
    rdfs:label "多伦多大学"@zh .

ex:EducationalDisadvantage rdf:type airo:Impact ;
    rdfs:label "Educational Disadvantage Impact"@en ;
    rdfs:label "教育劣势的影响"@zh ;
    airo:hasImpactOnStakeholder ex:BIPOCStudents .