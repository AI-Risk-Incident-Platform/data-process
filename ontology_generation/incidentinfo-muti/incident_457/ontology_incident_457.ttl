@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CruiseRobotaxiSystem rdf:type airo:AISystem ;
    rdfs:label "Cruise Robotaxi System"@en ;
    rdfs:label "Cruise自动驾驶出租车系统"@zh ;
    airo:hasRisk ex:EmergencyResponseInterferenceRisk ;
    airo:isDeployedBy ex:CruiseLLC ;
    airo:hasStakeholder ex:SanFranciscoPoliceDepartment ;
    airo:isAppliedWithinDomain ex:UrbanTransportation .

ex:EmergencyResponseInterferenceRisk rdf:type airo:Risk ;
    rdfs:label "Emergency Response Interference Risk"@en ;
    rdfs:label "干扰应急响应风险"@zh ;
    airo:hasConsequence ex:EmergencyServiceDisruption ;
    airo:isRiskSourceFor ex:AutonomousVehicleBehavior .

ex:SanFranciscoPoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "San Francisco Police Department"@en ;
    rdfs:label "旧金山警察局"@zh .

ex:CruiseLLC rdf:type airo:AIOperator ;
    rdfs:label "Cruise LLC"@en ;
    rdfs:label "Cruise有限责任公司"@zh .

ex:PublicSafetyCompromise rdf:type airo:Impact ;
    rdfs:label "Public Safety Compromise Impact"@en ;
    rdfs:label "公共安全受损影响"@zh ;
    airo:hasImpactOnStakeholder ex:SanFranciscoPoliceDepartment .

ex:UrbanTransportation rdf:type airo:Domain ;
    rdfs:label "Urban Transportation Domain"@en ;
    rdfs:label "城市交通领域"@zh .

ex:EmergencyServiceDisruption rdf:type airo:Consequence ;
    rdfs:label "Emergency Service Disruption Consequence"@en ;
    rdfs:label "紧急服务中断后果"@zh ;
    airo:hasImpact ex:PublicSafetyCompromise .

ex:AutonomousVehicleBehavior rdf:type airo:RiskSource ;
    rdfs:label "Autonomous Vehicle Behavior Risk Source"@en ;
    rdfs:label "自动驾驶车辆行为风险源"@zh .