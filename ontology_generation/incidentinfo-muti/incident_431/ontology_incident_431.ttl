@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DetroitFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "底特律面部识别系统"@zh ;
    rdfs:label "Detroit Facial Recognition System"@en ;
    airo:hasRisk ex:WrongfulArrestRisk ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:hasStakeholder ex:ArrestedIndividual ;
    airo:isAppliedWithinDomain ex:LawEnforcement .

ex:WrongfulArrestRisk rdf:type airo:Risk ;
    rdfs:label "错误逮捕风险"@zh ;
    rdfs:label "Wrongful Arrest Risk"@en ;
    airo:hasConsequence ex:WrongfulArrestConsequence ;
    airo:isRiskSourceFor ex:FaultyRecognitionAlgorithm .

ex:WrongfulArrestConsequence rdf:type airo:Consequence ;
    rdfs:label "错误逮捕后果"@zh ;
    rdfs:label "Wrongful Arrest Consequence"@en ;
    airo:hasImpact ex:RacialBiasImpact .

ex:RacialBiasImpact rdf:type airo:Impact ;
    rdfs:label "种族偏见影响"@zh ;
    rdfs:label "Racial Bias Impact"@en ;
    airo:hasImpactOnStakeholder ex:ArrestedIndividual .

ex:ArrestedIndividual rdf:type airo:Stakeholder ;
    rdfs:label "被逮捕个人"@zh ;
    rdfs:label "Arrested Individual"@en .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "面部识别技术"@zh ;
    rdfs:label "Facial Recognition Technology"@en .

ex:FaultyRecognitionAlgorithm rdf:type airo:RiskSource ;
    rdfs:label "错误识别算法风险源"@zh ;
    rdfs:label "Faulty Recognition Algorithm Risk Source"@en .

ex:LawEnforcement rdf:type airo:Domain ;
    rdfs:label "执法领域"@zh ;
    rdfs:label "Law Enforcement Domain"@en .