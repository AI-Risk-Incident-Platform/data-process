@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IDFFacialRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "IDF Facial Recognition System"@en ;
    rdfs:label "以色列国防军面部识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasAISubject ex:PalestiniansInHebron ;
    airo:hasStakeholder ex:IsraelDefenseForces ;
    airo:usesTechnique ex:FacialRecognitionTechnology ;
    airo:isAppliedWithinDomain ex:MilitarySurveillance .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasImpact ex:SurveillanceImpact .

ex:PalestiniansInHebron rdf:type airo:AISubject ;
    rdfs:label "Palestinians in Hebron"@en ;
    rdfs:label "希伯伦的巴勒斯坦人"@zh .

ex:IsraelDefenseForces rdf:type airo:Stakeholder ;
    rdfs:label "Israel Defense Forces"@en ;
    rdfs:label "以色列国防军"@zh .

ex:FacialRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Facial Recognition Technology"@en ;
    rdfs:label "面部识别技术"@zh .

ex:SurveillanceImpact rdf:type airo:Impact ;
    rdfs:label "Surveillance Impact"@en ;
    rdfs:label "监控影响"@zh ;
    airo:hasImpactOnStakeholder ex:PalestiniansInHebron .

ex:MilitarySurveillance rdf:type airo:Domain ;
    rdfs:label "Military Surveillance Domain"@en ;
    rdfs:label "军事监控领域"@zh .