@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ClassroomFaceRecognitionSystem rdf:type airo:AISystem ;
    rdfs:label "Classroom Face Recognition System"@en ;
    rdfs:label "教室人脸识别系统"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:hasPurpose ex:ClassroomBehaviorMonitoring ;
    airo:isAppliedWithinDomain ex:Education ;
    airo:usesTechnique ex:FaceRecognitionTechnology ;
    airo:hasStakeholder ex:Megvii ;
    airo:hasAISubject ex:Students .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:hasConsequence ex:PersonalPrivacyExposure .

ex:Megvii rdf:type airo:Stakeholder ;
    rdfs:label "Megvii"@en ;
    rdfs:label "旷视科技"@zh .

ex:Students rdf:type airo:AISubject ;
    rdfs:label "Students"@en ;
    rdfs:label "学生"@zh .

ex:ClassroomBehaviorMonitoring rdf:type airo:Purpose ;
    rdfs:label "Classroom Behavior Monitoring"@en ;
    rdfs:label "课堂行为监控"@zh .

ex:Education rdf:type airo:Domain ;
    rdfs:label "Education"@en ;
    rdfs:label "教育"@zh .

ex:FaceRecognitionTechnology rdf:type airo:AITechnique ;
    rdfs:label "Face Recognition Technology"@en ;
    rdfs:label "人脸识别技术"@zh .

ex:PersonalPrivacyExposure rdf:type airo:Consequence ;
    rdfs:label "Personal Privacy Exposure"@en ;
    rdfs:label "个人隐私暴露"@zh ;
    airo:hasImpact ex:PsychologicalPressureOnStudents .

ex:PsychologicalPressureOnStudents rdf:type airo:Impact ;
    rdfs:label "Psychological Pressure on Students"@en ;
    rdfs:label "学生心理压力"@zh ;
    airo:hasImpactOnStakeholder ex:Students .