@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeVideoScam rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Scam"@en ;
    rdfs:label "深度伪造视频诈骗"@zh ;
    airo:hasRisk ex:FinancialFraudRisk ;
    airo:usesTechnique ex:DeepfakeTechnology ;
    airo:isAppliedWithinDomain ex:CryptocurrencyInvestment ;
    airo:hasStakeholder ex:ElderlyVictims .

ex:FinancialFraudRisk rdf:type airo:Risk ;
    rdfs:label "Financial Fraud Risk"@en ;
    rdfs:label "金融欺诈风险"@zh ;
    airo:hasConsequence ex:FinancialLoss ;
    airo:isRiskSourceFor ex:SocialMediaPlatforms .

ex:FinancialLoss rdf:type airo:Consequence ;
    rdfs:label "Financial Loss Consequence"@en ;
    rdfs:label "经济损失后果"@zh ;
    airo:hasImpact ex:PersonalFinancialRuin .

ex:DeepfakeTechnology rdf:type airo:AITechnique ;
    rdfs:label "Deepfake Technology"@en ;
    rdfs:label "深度伪造技术"@zh .

ex:ElderlyVictims rdf:type airo:Stakeholder ;
    rdfs:label "Elderly Victims"@en ;
    rdfs:label "老年受害者"@zh .

ex:PersonalFinancialRuin rdf:type airo:Impact ;
    rdfs:label "Personal Financial Ruin Impact"@en ;
    rdfs:label "个人财务崩溃影响"@zh ;
    airo:hasImpactOnStakeholder ex:ElderlyVictims .

ex:CryptocurrencyInvestment rdf:type airo:Domain ;
    rdfs:label "Cryptocurrency Investment Domain"@en ;
    rdfs:label "加密货币投资领域"@zh .

ex:SocialMediaPlatforms rdf:type airo:RiskSource ;
    rdfs:label "Social Media Platforms Risk Source"@en ;
    rdfs:label "社交媒体平台风险源"@zh .