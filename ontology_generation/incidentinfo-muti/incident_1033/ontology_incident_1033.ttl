@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CustomerSupportAI rdf:type airo:AISystem ;
    rdfs:label "Customer Support AI"@en ;
    rdfs:label "客户支持AI"@zh ;
    airo:hasRisk ex:AIReplacingWorkersRisk ;
    airo:hasStakeholder ex:CompaniesConsideringAutomation .

ex:AIReplacingWorkersRisk rdf:type airo:Risk ;
    rdfs:label "AI Replacing Workers Risk"@en ;
    rdfs:label "AI替代工人风险"@zh ;
    airo:hasConsequence ex:WorkforceDisplacement .

ex:WorkforceDisplacement rdf:type airo:Consequence ;
    rdfs:label "Workforce Displacement Consequence"@en ;
    rdfs:label "劳动力替代后果"@zh ;
    airo:hasImpactOnStakeholder ex:CompaniesConsideringAutomation .

ex:CompaniesConsideringAutomation rdf:type airo:Stakeholder ;
    rdfs:label "Companies Considering Automation"@en ;
    rdfs:label "考虑自动化的公司"@zh .