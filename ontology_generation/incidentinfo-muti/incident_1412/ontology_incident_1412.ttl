@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaFullSelfDrivingSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉全自动驾驶系统"@zh ;
    rdfs:label "Tesla Full Self-Driving System"@en ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasAICapability ex:AutonomousDrivingCapability ;
    airo:isDevelopedBy ex:TeslaInc ;
    airo:hasPurpose ex:AutonomousTransportation ;
    airo:hasStakeholder ex:TeslaVehicleOwners ;
    airo:hasAISubject ex:TrainAndVehicleOccupants ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionBasedNavigation ;
    airo:hasVulnerability ex:ObjectRecognitionFailure .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    airo:hasConsequence ex:PotentialVehicleCollision ;
    airo:isRiskSourceFor ex:SensorLimitationsInAdverseConditions ;
    airo:isRiskSourceFor ex:MisclassificationOfMovingObjects ;
    airo:isRiskSourceFor ex:HighSpeedRailCollisionHazard .

ex:PotentialVehicleCollision rdf:type airo:Consequence ;
    rdfs:label "潜在车辆碰撞后果"@zh ;
    rdfs:label "Potential Vehicle Collision Consequence"@en ;
    airo:hasImpact ex:SafetyHazardForAllRoadUsers .

ex:AutonomousDrivingCapability rdf:type airo:AICapability ;
    rdfs:label "自动驾驶能力"@zh ;
    rdfs:label "Autonomous Driving Capability"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:TeslaVehicleOwners rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉车主"@zh ;
    rdfs:label "Tesla Vehicle Owners"@en .

ex:TrainAndVehicleOccupants rdf:type airo:AISubject ;
    rdfs:label "列车和车辆乘员"@zh ;
    rdfs:label "Train and Vehicle Occupants"@en .

ex:SafetyHazardForAllRoadUsers rdf:type airo:Impact ;
    rdfs:label "所有道路使用者的安全隐患"@zh ;
    rdfs:label "Safety Hazard for All Road Users Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaVehicleOwners ;
    airo:hasImpactOnStakeholder ex:TrainAndVehicleOccupants .

ex:AutonomousTransportation rdf:type airo:Purpose ;
    rdfs:label "自动驾驶运输目的"@zh ;
    rdfs:label "Autonomous Transportation Purpose"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVisionBasedNavigation rdf:type airo:AITechnique ;
    rdfs:label "基于计算机视觉的导航技术"@zh ;
    rdfs:label "Computer Vision Based Navigation Technique"@en .

ex:ObjectRecognitionFailure rdf:type airo:Vulnerability ;
    rdfs:label "物体识别失败的脆弱性"@zh ;
    rdfs:label "Object Recognition Failure Vulnerability"@en .

ex:MisclassificationOfMovingObjects rdf:type airo:Threat ;
    rdfs:label "移动物体误分类的威胁"@zh ;
    rdfs:label "Misclassification of Moving Objects Threat"@en ;
    airo:exploitsVulnerability ex:ObjectRecognitionFailure .

ex:HighSpeedRailCollisionHazard rdf:type airo:Hazard ;
    rdfs:label "高速铁路碰撞危害"@zh ;
    rdfs:label "High-Speed Rail Collision Hazard"@en ;
    airo:exploitsVulnerability ex:ObjectRecognitionFailure .

ex:SensorLimitationsInAdverseConditions rdf:type airo:RiskSource ;
    rdfs:label "恶劣条件下的传感器限制风险源"@zh ;
    rdfs:label "Sensor Limitations in Adverse Conditions Risk Source"@en ;
    airo:exploitsVulnerability ex:ObjectRecognitionFailure .

ex:AutonomousVehicleSafetyConcerns rdf:type airo:RiskConcept ;
    rdfs:label "自动驾驶车辆安全问题"@zh ;
    rdfs:label "Autonomous Vehicle Safety Concerns Risk Concept"@en .