@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:MetaAI rdf:type airo:AISystem ;
    rdfs:label "Meta AI"@en ;
    rdfs:label "Meta人工智能"@zh ;
    airo:hasRisk ex:AIHallucinationRisk ;
    airo:isDevelopedBy ex:MetaCompany .

ex:AIHallucinationRisk rdf:type airo:Risk ;
    rdfs:label "AI Hallucination Risk"@en ;
    rdfs:label "AI幻觉风险"@zh ;
    airo:hasConsequence ex:MisinformationSpread ;
    airo:isRiskSourceFor ex:AIHallucination .

ex:MisinformationSpread rdf:type airo:Consequence ;
    rdfs:label "Misinformation Spread Consequence"@en ;
    rdfs:label "错误信息传播后果"@zh ;
    airo:hasImpact ex:PublicConfusion .

ex:MetaCompany rdf:type airo:AIOperator ;
    rdfs:label "Meta Company"@en ;
    rdfs:label "Meta公司"@zh .

ex:AIHallucination rdf:type airo:RiskSource ;
    rdfs:label "AI Hallucination Risk Source"@en ;
    rdfs:label "AI幻觉风险源"@zh .

ex:PublicConfusion rdf:type airo:Impact ;
    rdfs:label "Public Confusion Impact"@en ;
    rdfs:label "公众困惑影响"@zh ;
    airo:hasImpactOnStakeholder ex:MetaCompany .