@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GeminiAI rdf:type airo:AISystem ;
    rdfs:label "Gemini AI"@en ;
    rdfs:label "Gemini AI"@zh ;
    airo:hasRisk ex:BiasInAIOutput ;
    airo:hasCapability ex:ImageGeneration ;
    airo:hasStakeholder ex:Google .

ex:BiasInAIOutput rdf:type airo:Risk ;
    rdfs:label "Bias in AI Output Risk"@en ;
    rdfs:label "AI输出中的偏见风险"@zh ;
    airo:hasConsequence ex:PublicCriticism ;
    airo:isRiskSourceFor ex:AlgorithmicBias .

ex:PublicCriticism rdf:type airo:Consequence ;
    rdfs:label "Public Criticism Consequence"@en ;
    rdfs:label "公众批评后果"@zh ;
    airo:hasImpact ex:ReputationDamage .

ex:Google rdf:type airo:Stakeholder ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:ReputationDamage rdf:type airo:Impact ;
    rdfs:label "Reputation Damage Impact"@en ;
    rdfs:label "声誉损害影响"@zh ;
    airo:hasImpactOnStakeholder ex:Google .

ex:AlgorithmicBias rdf:type airo:RiskSource ;
    rdfs:label "Algorithmic Bias Risk Source"@en ;
    rdfs:label "算法偏见风险源"@zh .

ex:ImageGeneration rdf:type airo:AICapability ;
    rdfs:label "Image Generation Capability"@en ;
    rdfs:label "图像生成能力"@zh .