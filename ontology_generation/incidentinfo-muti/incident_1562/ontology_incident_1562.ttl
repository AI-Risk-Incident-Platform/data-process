@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:NateShoppingApp rdf:type airo:AISystem ;
    rdfs:label "Nate Shopping App"@en ;
    rdfs:label "Nate购物应用"@zh ;
    airo:hasRisk ex:MisrepresentationOfAI ;
    airo:hasStakeholder ex:CoatueInvestors .

ex:MisrepresentationOfAI rdf:type airo:Risk ;
    rdfs:label "Misrepresentation of AI Risk"@en ;
    rdfs:label "AI虚假宣传风险"@zh ;
    airo:hasConsequence ex:AppFeatureDisabling ;
    airo:isRiskSourceFor ex:HumanLaborDisguisedAsAI .

ex:CoatueInvestors rdf:type airo:Stakeholder ;
    rdfs:label "Coatue Investors"@en ;
    rdfs:label "Coatue投资者"@zh .

ex:HumanLaborDisguisedAsAI rdf:type airo:Misuse ;
    rdfs:label "Human Labor Disguised as AI"@en ;
    rdfs:label "人工劳动伪装成AI"@zh .

ex:AppFeatureDisabling rdf:type airo:Consequence ;
    rdfs:label "App Feature Disabling Consequence"@en ;
    rdfs:label "应用功能停用后果"@zh .