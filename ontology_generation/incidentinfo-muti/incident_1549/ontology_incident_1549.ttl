@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ChatGPT rdf:type airo:AISystem ;
    rdfs:label "ChatGPT"@en ;
    rdfs:label "ChatGPT"@zh ;
    airo:hasRisk ex:MedicalMisinformationRisk ;
    airo:hasCapability ex:MedicalInformationGeneration ;
    airo:hasStakeholder ex:CancerDoctors ;
    airo:hasAISubject ex:CancerPatients ;
    airo:hasPurpose ex:MedicalInformationProvision ;
    airo:isAppliedWithinDomain ex:Healthcare .

ex:MedicalMisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Medical Misinformation Risk"@en ;
    rdfs:label "医疗错误信息风险"@zh ;
    airo:hasConsequence ex:IncorrectMedicalAdvice ;
    airo:isRiskSourceFor ex:InaccurateDataGeneration .

ex:IncorrectMedicalAdvice rdf:type airo:Consequence ;
    rdfs:label "Incorrect Medical Advice Consequence"@en ;
    rdfs:label "错误的医疗建议后果"@zh ;
    airo:hasImpact ex:PatientHealthRisk .

ex:MedicalInformationGeneration rdf:type airo:AICapability ;
    rdfs:label "Medical Information Generation Capability"@en ;
    rdfs:label "医疗信息生成能力"@zh .

ex:CancerDoctors rdf:type airo:Stakeholder ;
    rdfs:label "Cancer Doctors"@en ;
    rdfs:label "癌症医生"@zh .

ex:CancerPatients rdf:type airo:AISubject ;
    rdfs:label "Cancer Patients"@en ;
    rdfs:label "癌症患者"@zh .

ex:PatientHealthRisk rdf:type airo:Impact ;
    rdfs:label "Patient Health Risk Impact"@en ;
    rdfs:label "患者健康风险影响"@zh ;
    airo:hasImpactOnStakeholder ex:CancerPatients .

ex:Healthcare rdf:type airo:Domain ;
    rdfs:label "Healthcare Domain"@en ;
    rdfs:label "医疗保健领域"@zh .

ex:InaccurateDataGeneration rdf:type airo:RiskSource ;
    rdfs:label "Inaccurate Data Generation Risk Source"@en ;
    rdfs:label "不准确数据生成风险源"@zh .

ex:MedicalInformationProvision rdf:type airo:Purpose ;
    rdfs:label "Medical Information Provision Purpose"@en ;
    rdfs:label "医疗信息提供目的"@zh .