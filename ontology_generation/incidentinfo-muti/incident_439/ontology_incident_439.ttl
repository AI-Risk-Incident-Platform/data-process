@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIIncidentDatabase rdf:type airo:AISystem ;
    rdfs:label "AI Incident Database"@en ;
    rdfs:label "AI事件数据库"@zh ;
    airo:hasRisk ex:DatabaseIncidentConversionRisk ;
    airo:hasStakeholder ex:DatabaseUsers .

ex:DatabaseIncidentConversionRisk rdf:type airo:Risk ;
    rdfs:label "Database Incident Conversion Risk"@en ;
    rdfs:label "数据库事件转换风险"@zh ;
    airo:hasConsequence ex:DataLoss .

ex:DatabaseUsers rdf:type airo:Stakeholder ;
    rdfs:label "Database Users"@en ;
    rdfs:label "数据库用户"@zh .

ex:DataLoss rdf:type airo:Consequence ;
    rdfs:label "Data Loss"@en ;
    rdfs:label "数据丢失"@zh ;
    airo:hasImpact ex:ResearchImpact .

ex:ResearchImpact rdf:type airo:Impact ;
    rdfs:label "Research Impact"@en ;
    rdfs:label "研究影响"@zh ;
    airo:hasImpactOnStakeholder ex:DatabaseUsers .