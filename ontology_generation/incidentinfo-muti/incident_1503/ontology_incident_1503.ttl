@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CarInsurancePricingAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Car Insurance Pricing Algorithm"@en ;
    rdfs:label "汽车保险定价算法"@zh ;
    airo:hasRisk ex:PricingBiasAgainstMinorityDrivers ;
    airo:hasAISubject ex:InsuranceApplicants ;
    airo:isAppliedWithinDomain ex:AutomobileInsuranceIndustry ;
    airo:hasPurpose ex:AutomatedInsurancePremiumCalculation ;
    airo:hasStakeholder ex:MinorityDrivers .

ex:PricingBiasAgainstMinorityDrivers rdf:type airo:Risk ;
    rdfs:label "Pricing Bias Against Minority Drivers Risk"@en ;
    rdfs:label "针对少数族裔司机的定价偏见风险"@zh ;
    airo:hasConsequence ex:UnfairFinancialBurden .

ex:MinorityDrivers rdf:type airo:Stakeholder ;
    rdfs:label "Minority Drivers"@en ;
    rdfs:label "少数族裔司机"@zh .

ex:InsuranceApplicants rdf:type airo:AISubject ;
    rdfs:label "Insurance Applicants"@en ;
    rdfs:label "保险申请人"@zh .

ex:AutomobileInsuranceIndustry rdf:type airo:Domain ;
    rdfs:label "Automobile Insurance Industry"@en ;
    rdfs:label "汽车保险行业"@zh .

ex:UnfairFinancialBurden rdf:type airo:Impact ;
    rdfs:label "Unfair Financial Burden Impact"@en ;
    rdfs:label "不公平财务负担的影响"@zh ;
    airo:hasImpactOnStakeholder ex:MinorityDrivers .

ex:AutomatedInsurancePremiumCalculation rdf:type airo:Purpose ;
    rdfs:label "Automated Insurance Premium Calculation Purpose"@en ;
    rdfs:label "自动化保险费计算的目的"@zh .