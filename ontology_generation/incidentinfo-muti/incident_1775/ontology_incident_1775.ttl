@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:ItalianSchoolTeacherTransferAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Italian School Teacher Transfer Algorithm"@en ;
    rdfs:label "意大利学校教师调动算法"@zh ;
    airo:hasRisk ex:AlgorithmicFailureInPublicSector ;
    airo:hasStakeholder ex:ItalianTeachers ;
    airo:isDeployedBy ex:ItalianMinistryOfEducation .

ex:AlgorithmicFailureInPublicSector rdf:type airo:Risk ;
    rdfs:label "Algorithmic Failure in Public Sector Risk"@en ;
    rdfs:label "公共部门算法失效风险"@zh ;
    airo:hasConsequence ex:TeachersTransferredFarFromHome ;
    airo:isRiskSourceFor ex:LackOfAlgorithmicTransparency .

ex:TeachersTransferredFarFromHome rdf:type airo:Consequence ;
    rdfs:label "Teachers Transferred Far From Home Consequence"@en ;
    rdfs:label "教师被调离家乡的后果"@zh ;
    airo:hasImpact ex:DisruptionOfEducationSystem .

ex:ItalianTeachers rdf:type airo:Stakeholder ;
    rdfs:label "Italian Teachers"@en ;
    rdfs:label "意大利教师"@zh .

ex:DisruptionOfEducationSystem rdf:type airo:Impact ;
    rdfs:label "Disruption of Education System Impact"@en ;
    rdfs:label "教育系统中断的影响"@zh ;
    airo:hasImpactOnStakeholder ex:ItalianTeachers .

ex:LackOfAlgorithmicTransparency rdf:type airo:RiskSource ;
    rdfs:label "Lack of Algorithmic Transparency Risk Source"@en ;
    rdfs:label "算法缺乏透明度的风险源"@zh .

ex:ItalianMinistryOfEducation rdf:type airo:AIOperator ;
    rdfs:label "Italian Ministry of Education"@en ;
    rdfs:label "意大利教育部"@zh .