ex:OpenAISora rdf:type airo:AISystem ;
    rdfs:label "OpenAI Sora"@en ;
    rdfs:label "OpenAI Sora"@zh ;
    airo:hasRisk ex:GenerationOfHarmfulContent ;
    airo:hasCapability ex:TextToVideoGeneration ;
    airo:usesTechnique ex:GenerativeAI ;
    airo:hasPurpose ex:ContentCreation .

ex:GenerationOfHarmfulContent rdf:type airo:Risk ;
    rdfs:label "Generation of Harmful Content Risk"@en ;
    rdfs:label "生成有害内容风险"@zh .

ex:TextToVideoGeneration rdf:type airo:AICapability ;
    rdfs:label "Text-to-Video Generation Capability"@en ;
    rdfs:label "文本到视频生成能力"@zh .

ex:GenerativeAI rdf:type airo:AITechnique ;
    rdfs:label "Generative AI Technique"@en ;
    rdfs:label "生成式AI技术"@zh .

ex:ContentCreation rdf:type airo:Purpose ;
    rdfs:label "Content Creation Purpose"@en ;
    rdfs:label "内容创作目的"@zh .