ex:GoogleJigsawToxicSpeechDetectionSystem rdf:type airo:AISystem ;
    rdfs:label "Google Jigsaw有毒言论检测系统"@zh ;
    rdfs:label "Google Jigsaw Toxic Speech Detection System"@en ;
