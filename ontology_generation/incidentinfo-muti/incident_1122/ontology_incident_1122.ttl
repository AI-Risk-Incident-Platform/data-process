@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GooglePhotosAIPanorama rdf:type airo:AISystem ;
    rdfs:label "Google Photos AI Panorama"@en ;
    rdfs:label "谷歌照片AI全景功能"@zh ;
    airo:hasRisk ex:PanoramaProcessingFailureRisk ;
    airo:hasCapability ex:ImageStitchingCapability ;
    airo:isProvidedBy ex:Google ;
    airo:hasPurpose ex:CreateSeamlessPanoramicPhotos .

ex:PanoramaProcessingFailureRisk rdf:type airo:Risk ;
    rdfs:label "Panorama Processing Failure Risk"@en ;
    rdfs:label "全景处理失败风险"@zh .

ex:ImageStitchingCapability rdf:type airo:AICapability ;
    rdfs:label "Image Stitching Capability"@en ;
    rdfs:label "图像拼接能力"@zh .

ex:Google rdf:type airo:AIProvider ;
    rdfs:label "Google"@en ;
    rdfs:label "谷歌"@zh .

ex:CreateSeamlessPanoramicPhotos rdf:type airo:Purpose ;
    rdfs:label "Create Seamless Panoramic Photos"@en ;
    rdfs:label "创建无缝全景照片"@zh .