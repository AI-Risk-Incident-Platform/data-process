@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacebookPlatform rdf:type airo:AISystem ;
    rdfs:label "Facebook Platform"@en ;
    rdfs:label "Facebook平台"@zh ;
    airo:hasRisk ex:ChildSafetyRisk ;
    airo:isDevelopedBy ex:Meta ;
    airo:hasStakeholder ex:OhioAttorneyGeneral .

ex:ChildSafetyRisk rdf:type airo:Risk ;
    rdfs:label "Child Safety Risk"@en ;
    rdfs:label "儿童安全风险"@zh ;
    airo:isRiskSourceFor ex:MisleadingPublicStatements ;
    airo:hasConsequence ex:NegativeEffectOnChildren .

ex:OhioAttorneyGeneral rdf:type airo:Stakeholder ;
    rdfs:label "Ohio Attorney General"@en ;
    rdfs:label "俄亥俄州总检察长"@zh .

ex:Meta rdf:type airo:AIOperator ;
    rdfs:label "Meta"@en ;
    rdfs:label "Meta公司"@zh .

ex:NegativeEffectOnChildren rdf:type airo:Impact ;
    rdfs:label "Negative Effect on Children"@en ;
    rdfs:label "对儿童的负面影响"@zh ;
    airo:hasImpactOnStakeholder ex:OhioAttorneyGeneral .

ex:MisleadingPublicStatements rdf:type airo:RiskSource ;
    rdfs:label "Misleading Public Statements"@en ;
    rdfs:label "误导性公开声明"@zh .