@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AITrainingDataCopyrightSystem rdf:type airo:AISystem ;
    rdfs:label "AI Training Data Copyright System"@en ;
    rdfs:label "AI训练数据版权系统"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:ThomsonReuters .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalPenalties .

ex:ThomsonReuters rdf:type airo:Stakeholder ;
    rdfs:label "Thomson Reuters"@en ;
    rdfs:label "汤森路透"@zh .

ex:LegalPenalties rdf:type airo:Consequence ;
    rdfs:label "Legal Penalties Consequence"@en ;
    rdfs:label "法律处罚后果"@zh ;
    airo:hasImpact ex:FinancialLoss .

ex:FinancialLoss rdf:type airo:Impact ;
    rdfs:label "Financial Loss Impact"@en ;
    rdfs:label "财务损失影响"@zh ;
    airo:hasImpactOnStakeholder ex:ThomsonReuters .