@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:AutonomousVehicleCrashRisk ;
    airo:isDeployedBy ex:TeslaInc ;
    airo:hasStakeholder ex:TexasPoliceDepartment ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry .

ex:AutonomousVehicleCrashRisk rdf:type airo:Risk ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    rdfs:label "Autonomous Vehicle Crash Risk"@en ;
    airo:hasConsequence ex:PoliceOfficerInjury ;
    airo:isRiskSourceFor ex:DrunkDrivingWithAutopilotEngaged .

ex:PoliceOfficerInjury rdf:type airo:Consequence ;
    rdfs:label "警察受伤后果"@zh ;
    rdfs:label "Police Officer Injury Consequence"@en ;
    airo:hasImpact ex:PhysicalHarmToLawEnforcement .

ex:TexasPoliceDepartment rdf:type airo:Stakeholder ;
    rdfs:label "德克萨斯州警察局"@zh ;
    rdfs:label "Texas Police Department"@en .

ex:TeslaInc rdf:type airo:AIOperator ;
    rdfs:label "特斯拉公司"@zh ;
    rdfs:label "Tesla Inc."@en .

ex:PhysicalHarmToLawEnforcement rdf:type airo:Impact ;
    rdfs:label "执法人员身体伤害影响"@zh ;
    rdfs:label "Physical Harm to Law Enforcement Impact"@en ;
    airo:hasImpactOnStakeholder ex:TexasPoliceDepartment .

ex:DrunkDrivingWithAutopilotEngaged rdf:type airo:RiskSource ;
    rdfs:label "醉酒驾驶时启用自动驾驶的风险源"@zh ;
    rdfs:label "Drunk Driving with Autopilot Engaged Risk Source"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .