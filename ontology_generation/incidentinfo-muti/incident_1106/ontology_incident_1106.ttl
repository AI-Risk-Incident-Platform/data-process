@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:IBMWatsonForOncology rdf:type airo:AISystem ;
    rdfs:label "IBM Watson肿瘤治疗系统"@zh ;
    rdfs:label "IBM Watson for Oncology"@en ;
    airo:hasRisk ex:UnsafeCancerTreatmentRecommendationRisk ;
    airo:hasCapability ex:CancerTreatmentRecommendationCapability ;
    airo:isDevelopedBy ex:IBM ;
    airo:hasPurpose ex:AssistingCancerTreatmentDecisions ;
    airo:isAppliedWithinDomain ex:OncologyHealthcare .

ex:UnsafeCancerTreatmentRecommendationRisk rdf:type airo:Risk ;
    rdfs:label "不安全癌症治疗建议风险"@zh ;
    rdfs:label "Unsafe Cancer Treatment Recommendation Risk"@en ;
    airo:hasConsequence ex:PatientHarmFromIncorrectTreatment ;
    airo:isRiskSourceFor ex:InadequateTrainingData .

ex:PatientHarmFromIncorrectTreatment rdf:type airo:Consequence ;
    rdfs:label "错误治疗导致患者伤害后果"@zh ;
    rdfs:label "Patient Harm from Incorrect Treatment Consequence"@en ;
    airo:hasImpact ex:CompromisedPatientSafety .

ex:CancerTreatmentRecommendationCapability rdf:type airo:AICapability ;
    rdfs:label "癌症治疗建议能力"@zh ;
    rdfs:label "Cancer Treatment Recommendation Capability"@en .

ex:IBM rdf:type airo:AIOperator ;
    rdfs:label "IBM公司"@zh ;
    rdfs:label "IBM Corporation"@en .

ex:OncologyHealthcare rdf:type airo:Domain ;
    rdfs:label "肿瘤医疗领域"@zh ;
    rdfs:label "Oncology Healthcare Domain"@en .

ex:AssistingCancerTreatmentDecisions rdf:type airo:Purpose ;
    rdfs:label "辅助癌症治疗决策目的"@zh ;
    rdfs:label "Assisting Cancer Treatment Decisions Purpose"@en .

ex:CompromisedPatientSafety rdf:type airo:Impact ;
    rdfs:label "患者安全受损影响"@zh ;
    rdfs:label "Compromised Patient Safety Impact"@en ;
    airo:hasImpactOnStakeholder ex:Patient .

ex:InadequateTrainingData rdf:type airo:RiskSource ;
    rdfs:label "训练数据不足风险源"@zh ;
    rdfs:label "Inadequate Training Data Risk Source"@en .

ex:Patient rdf:type airo:Stakeholder ;
    rdfs:label "患者"@zh ;
    rdfs:label "Patient"@en .