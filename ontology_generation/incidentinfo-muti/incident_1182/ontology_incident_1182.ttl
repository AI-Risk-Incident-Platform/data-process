@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaCybertruckFSD rdf:type airo:AISystem ;
    rdfs:label "Tesla Cybertruck FSD"@en ;
    rdfs:label "特斯拉Cybertruck全自动驾驶系统"@zh ;
    airo:hasRisk ex:AutonomousVehicleCollisionRisk ;
    airo:hasCapability ex:ObjectDetectionAndCollisionAvoidance ;
    airo:hasStakeholder ex:TeslaInc ;
    airo:hasStakeholder ex:TeslaVehicleOwners ;
    airo:hasPurpose ex:AutonomousDriving ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVisionAndMachineLearning .

ex:AutonomousVehicleCollisionRisk rdf:type airo:Risk ;
    rdfs:label "Autonomous Vehicle Collision Risk"@en ;
    rdfs:label "自动驾驶车辆碰撞风险"@zh ;
    airo:hasConsequence ex:PotentialAccident ;
    airo:isRiskSourceFor ex:SensorLimitations .

ex:PotentialAccident rdf:type airo:Consequence ;
    rdfs:label "Potential Accident Consequence"@en ;
    rdfs:label "潜在事故后果"@zh ;
    airo:hasImpact ex:PublicSafetyConcerns .

ex:ObjectDetectionAndCollisionAvoidance rdf:type airo:AICapability ;
    rdfs:label "Object Detection and Collision Avoidance Capability"@en ;
    rdfs:label "物体检测和碰撞避免能力"@zh .

ex:TeslaInc rdf:type airo:Stakeholder ;
    rdfs:label "Tesla Inc."@en ;
    rdfs:label "特斯拉公司"@zh .

ex:TeslaVehicleOwners rdf:type airo:AIOperator ;
    rdfs:label "Tesla Vehicle Owners"@en ;
    rdfs:label "特斯拉车主"@zh .

ex:PublicSafetyConcerns rdf:type airo:Impact ;
    rdfs:label "Public Safety Concerns Impact"@en ;
    rdfs:label "公共安全问题影响"@zh ;
    airo:hasImpactOnStakeholder ex:TeslaInc ;
    airo:hasImpactOnStakeholder ex:TeslaVehicleOwners .

ex:AutonomousDriving rdf:type airo:Purpose ;
    rdfs:label "Autonomous Driving Purpose"@en ;
    rdfs:label "自动驾驶目的"@zh .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "Automotive Industry Domain"@en ;
    rdfs:label "汽车工业领域"@zh .

ex:ComputerVisionAndMachineLearning rdf:type airo:AITechnique ;
    rdfs:label "Computer Vision and Machine Learning Technique"@en ;
    rdfs:label "计算机视觉和机器学习技术"@zh .

ex:SensorLimitations rdf:type airo:RiskSource ;
    rdfs:label "Sensor Limitations Risk Source"@en ;
    rdfs:label "传感器限制风险源"@zh ;
    airo:exploitsVulnerability ex:EdgeCaseHandling .

ex:EdgeCaseHandling rdf:type airo:Vulnerability ;
    rdfs:label "Edge Case Handling Vulnerability"@en ;
    rdfs:label "边缘情况处理脆弱性"@zh .