@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CommercialArtificialIntelligenceSystems rdf:type airo:AISystem ;
    rdfs:label "Commercial Artificial Intelligence Systems"@en ;
    rdfs:label "商业人工智能系统"@zh ;
    airo:hasRisk ex:AlgorithmicBiasRisk ;
    airo:isAppliedWithinDomain ex:CommercialAIApplications .

ex:AlgorithmicBiasRisk rdf:type airo:Risk ;
    rdfs:label "Algorithmic Bias Risk"@en ;
    rdfs:label "算法偏见风险"@zh ;
    airo:isRiskSourceFor ex:GenderAndSkinTypeBias ;
    airo:hasImpact ex:DiscriminatoryOutcomes .

ex:GenderAndSkinTypeBias rdf:type airo:RiskSource ;
    rdfs:label "Gender and Skin-Type Bias"@en ;
    rdfs:label "性别和肤色偏见"@zh .

ex:DiscriminatoryOutcomes rdf:type airo:Impact ;
    rdfs:label "Discriminatory Outcomes"@en ;
    rdfs:label "歧视性结果"@zh .

ex:CommercialAIApplications rdf:type airo:Domain ;
    rdfs:label "Commercial AI Applications Domain"@en ;
    rdfs:label "商业AI应用领域"@zh .