@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationTool rdf:type airo:AISystem ;
    rdfs:label "Deepfake Generation Tool"@en ;
    rdfs:label "深度伪造生成工具"@zh ;
    airo:hasRisk ex:PrivacyViolationAndPsychologicalHarm ;
    airo:hasAISubject ex:FemaleStudents ;
    airo:hasStakeholder ex:SydneyTeenager ;
    airo:usesTechnique ex:DeepLearningBasedImageSynthesis .

ex:PrivacyViolationAndPsychologicalHarm rdf:type airo:Risk ;
    rdfs:label "Privacy Violation and Psychological Harm Risk"@en ;
    rdfs:label "隐私侵犯与心理伤害风险"@zh ;
    airo:hasConsequence ex:ReputationDamage ;
    airo:isRiskSourceFor ex:MaliciousUseOfAITools .

ex:FemaleStudents rdf:type airo:AISubject ;
    rdfs:label "Female Students"@en ;
    rdfs:label "女学生"@zh .

ex:SydneyTeenager rdf:type airo:Stakeholder ;
    rdfs:label "Sydney Teenager"@en ;
    rdfs:label "悉尼青少年"@zh .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "名誉损害后果"@zh ;
    airo:hasImpact ex:EmotionalDistress .

ex:EmotionalDistress rdf:type airo:Impact ;
    rdfs:label "Emotional Distress Impact"@en ;
    rdfs:label "情绪困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:FemaleStudents .

ex:DeepLearningBasedImageSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Image Synthesis Technique"@en ;
    rdfs:label "基于深度学习的图像合成技术"@zh .

ex:MaliciousUseOfAITools rdf:type airo:RiskSource ;
    rdfs:label "Malicious Use of AI Tools Risk Source"@en ;
    rdfs:label "AI工具的恶意使用风险源"@zh .