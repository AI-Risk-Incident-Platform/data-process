@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:FacialImageResearchDatasets rdf:type airo:AISystem ;
    rdfs:label "Facial Image Research Datasets"@en ;
    rdfs:label "面部图像研究数据集"@zh ;
    airo:hasRisk ex:PrivacyViolationRisk ;
    airo:usesTechnique ex:DataScraping ;
    airo:hasStakeholder ex:ResearchInstitutions ;
    airo:hasAISubject ex:IndividualsWithFacialImagesOnline .

ex:PrivacyViolationRisk rdf:type airo:Risk ;
    rdfs:label "Privacy Violation Risk"@en ;
    rdfs:label "隐私侵犯风险"@zh ;
    airo:isRiskSourceFor ex:InternetScraping ;
    airo:hasConsequence ex:UnauthorizedUseOfPersonalData .

ex:InternetScraping rdf:type airo:RiskSource ;
    rdfs:label "Internet Scraping"@en ;
    rdfs:label "网络爬取"@zh .

ex:UnauthorizedUseOfPersonalData rdf:type airo:Consequence ;
    rdfs:label "Unauthorized Use of Personal Data"@en ;
    rdfs:label "个人数据未经授权使用"@zh ;
    airo:hasImpact ex:LossOfPersonalPrivacy .

ex:LossOfPersonalPrivacy rdf:type airo:Impact ;
    rdfs:label "Loss of Personal Privacy"@en ;
    rdfs:label "个人隐私丧失"@zh ;
    airo:hasImpactOnStakeholder ex:IndividualsWithFacialImagesOnline .

ex:DataScraping rdf:type airo:AITechnique ;
    rdfs:label "Data Scraping Technique"@en ;
    rdfs:label "数据爬取技术"@zh .

ex:ResearchInstitutions rdf:type airo:Stakeholder ;
    rdfs:label "Research Institutions"@en ;
    rdfs:label "研究机构"@zh .

ex:IndividualsWithFacialImagesOnline rdf:type airo:AISubject ;
    rdfs:label "Individuals with Facial Images Online"@en ;
    rdfs:label "面部图像在线的个人"@zh .