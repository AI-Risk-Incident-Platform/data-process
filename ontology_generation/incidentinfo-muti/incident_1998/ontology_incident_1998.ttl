@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CNETAIWrittenStories rdf:type airo:AISystem ;
    rdfs:label "CNET AI Written Stories"@en ;
    rdfs:label "CNET人工智能撰写文章"@zh ;
    airo:hasRisk ex:DisclosureControversyRisk ;
    airo:hasPurpose ex:FinancialNewsCoverage ;
    airo:isAppliedWithinDomain ex:MediaAndJournalism ;
    airo:hasStakeholder ex:CNET .

ex:DisclosureControversyRisk rdf:type airo:Risk ;
    rdfs:label "Disclosure Controversy Risk"@en ;
    rdfs:label "披露争议风险"@zh ;
    airo:hasConsequence ex:PublicationPause ;
    airo:isRiskSourceFor ex:LackOfTransparency .

ex:CNET rdf:type airo:Stakeholder ;
    rdfs:label "CNET"@en ;
    rdfs:label "CNET"@zh .

ex:FinancialNewsCoverage rdf:type airo:Purpose ;
    rdfs:label "Financial News Coverage"@en ;
    rdfs:label "金融新闻报道"@zh .

ex:MediaAndJournalism rdf:type airo:Domain ;
    rdfs:label "Media and Journalism"@en ;
    rdfs:label "媒体与新闻业"@zh .

ex:PublicationPause rdf:type airo:Consequence ;
    rdfs:label "Publication Pause Consequence"@en ;
    rdfs:label "发布暂停后果"@zh .

ex:LackOfTransparency rdf:type airo:RiskSource ;
    rdfs:label "Lack of Transparency Risk Source"@en ;
    rdfs:label "缺乏透明度风险源"@zh .