@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GPT3BotOnReddit rdf:type airo:AISystem ;
    rdfs:label "GPT-3 Bot on Reddit"@en ;
    rdfs:label "Reddit上的GPT-3机器人"@zh ;
    airo:hasRisk ex:UncontrolledAIBehaviorRisk ;
    airo:usesTechnique ex:GPT3LanguageModel ;
    airo:hasStakeholder ex:RedditCommunity .

ex:UncontrolledAIBehaviorRisk rdf:type airo:Risk ;
    rdfs:label "Uncontrolled AI Behavior Risk"@en ;
    rdfs:label "AI行为失控风险"@zh ;
    airo:isRiskSourceFor ex:UnsupervisedAIInteraction .

ex:RedditCommunity rdf:type airo:Stakeholder ;
    rdfs:label "Reddit Community"@en ;
    rdfs:label "Reddit社区"@zh .

ex:GPT3LanguageModel rdf:type airo:AITechnique ;
    rdfs:label "GPT-3 Language Model"@en ;
    rdfs:label "GPT-3语言模型"@zh .

ex:UnsupervisedAIInteraction rdf:type airo:RiskSource ;
    rdfs:label "Unsupervised AI Interaction"@en ;
    rdfs:label "无监督AI交互"@zh .