@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIGeneratedContentSystem rdf:type airo:AISystem ;
    rdfs:label "AI Generated Content System"@en ;
    rdfs:label "AI生成内容系统"@zh ;
    airo:hasRisk ex:ProgrammaticAdRevenueRisk ;
    airo:usesTechnique ex:TextGenerationAI ;
    airo:isAppliedWithinDomain ex:OnlineAdvertising ;
    airo:hasStakeholder ex:Advertisers .

ex:ProgrammaticAdRevenueRisk rdf:type airo:Risk ;
    rdfs:label "Programmatic Ad Revenue Risk"@en ;
    rdfs:label "程序化广告收入风险"@zh ;
    airo:hasConsequence ex:WastedAdSpend ;
    airo:isRiskSourceFor ex:LowQualityAIGeneratedContent .

ex:Advertisers rdf:type airo:Stakeholder ;
    rdfs:label "Advertisers"@en ;
    rdfs:label "广告商"@zh .

ex:OnlineAdvertising rdf:type airo:Domain ;
    rdfs:label "Online Advertising"@en ;
    rdfs:label "在线广告"@zh .

ex:WastedAdSpend rdf:type airo:Consequence ;
    rdfs:label "Wasted Ad Spend"@en ;
    rdfs:label "广告支出浪费"@zh ;
    airo:hasImpact ex:EconomicLossForAdvertisers .

ex:EconomicLossForAdvertisers rdf:type airo:Impact ;
    rdfs:label "Economic Loss for Advertisers"@en ;
    rdfs:label "广告商经济损失"@zh ;
    airo:hasImpactOnStakeholder ex:Advertisers .

ex:LowQualityAIGeneratedContent rdf:type airo:RiskSource ;
    rdfs:label "Low Quality AI Generated Content"@en ;
    rdfs:label "低质量AI生成内容"@zh .

ex:TextGenerationAI rdf:type airo:AITechnique ;
    rdfs:label "Text Generation AI"@en ;
    rdfs:label "文本生成AI"@zh .