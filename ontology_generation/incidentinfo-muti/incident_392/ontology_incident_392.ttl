@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:TeslaAutopilotSystem rdf:type airo:AISystem ;
    rdfs:label "特斯拉自动驾驶系统"@zh ;
    rdfs:label "Tesla Autopilot System"@en ;
    airo:hasRisk ex:ObjectMisclassificationRisk ;
    airo:hasCapability ex:ObjectRecognition ;
    airo:isAppliedWithinDomain ex:AutomotiveIndustry ;
    airo:usesTechnique ex:ComputerVision ;
    airo:hasStakeholder ex:TeslaDrivers .

ex:ObjectMisclassificationRisk rdf:type airo:Risk ;
    rdfs:label "物体误分类风险"@zh ;
    rdfs:label "Object Misclassification Risk"@en ;
    airo:hasConsequence ex:PotentialTrafficAccident .

ex:PotentialTrafficAccident rdf:type airo:Consequence ;
    rdfs:label "潜在交通事故后果"@zh ;
    rdfs:label "Potential Traffic Accident Consequence"@en ;
    airo:hasImpact ex:RoadSafetyConcern .

ex:ObjectRecognition rdf:type airo:AICapability ;
    rdfs:label "物体识别能力"@zh ;
    rdfs:label "Object Recognition Capability"@en .

ex:AutomotiveIndustry rdf:type airo:Domain ;
    rdfs:label "汽车工业领域"@zh ;
    rdfs:label "Automotive Industry Domain"@en .

ex:ComputerVision rdf:type airo:AITechnique ;
    rdfs:label "计算机视觉技术"@zh ;
    rdfs:label "Computer Vision Technique"@en .

ex:TeslaDrivers rdf:type airo:Stakeholder ;
    rdfs:label "特斯拉驾驶员"@zh ;
    rdfs:label "Tesla Drivers"@en .

ex:RoadSafetyConcern rdf:type airo:Impact ;
    rdfs:label "道路安全隐患影响"@zh ;
    rdfs:label "Road Safety Concern Impact"@en ;
    airo:hasImpactOnStakeholder ex:TeslaDrivers .