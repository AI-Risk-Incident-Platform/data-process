@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AIPoweredDriveThruSystem rdf:type airo:AISystem ;
    rdfs:label "AI-Powered Drive-Thru System"@en ;
    rdfs:label "AI驱动的免下车服务系统"@zh ;
    airo:hasRisk ex:MisrepresentationOfAICapabilities ;
    airo:hasStakeholder ex:FastFoodCustomers ;
    airo:isDeployedBy ex:PhilippinesHumanWorkers .

ex:MisrepresentationOfAICapabilities rdf:type airo:Risk ;
    rdfs:label "Misrepresentation of AI Capabilities Risk"@en ;
    rdfs:label "AI能力虚假陈述风险"@zh ;
    airo:hasConsequence ex:ConsumerDeception ;
    airo:isRiskSourceFor ex:FalseMarketingOfAISystems .

ex:FastFoodCustomers rdf:type airo:Stakeholder ;
    rdfs:label "Fast Food Customers"@en ;
    rdfs:label "快餐顾客"@zh .

ex:PhilippinesHumanWorkers rdf:type airo:AIOperator ;
    rdfs:label "Philippines Human Workers"@en ;
    rdfs:label "菲律宾人工操作员"@zh .

ex:FalseMarketingOfAISystems rdf:type airo:Misuse ;
    rdfs:label "False Marketing of AI Systems"@en ;
    rdfs:label "AI系统虚假营销"@zh .

ex:ConsumerDeception rdf:type airo:Consequence ;
    rdfs:label "Consumer Deception Consequence"@en ;
    rdfs:label "消费者欺骗后果"@zh ;
    airo:hasImpact ex:LossOfTrustInAITechnology .

ex:LossOfTrustInAITechnology rdf:type airo:Impact ;
    rdfs:label "Loss of Trust in AI Technology Impact"@en ;
    rdfs:label "AI技术信任丧失影响"@zh ;
    airo:hasImpactOnStakeholder ex:FastFoodCustomers .