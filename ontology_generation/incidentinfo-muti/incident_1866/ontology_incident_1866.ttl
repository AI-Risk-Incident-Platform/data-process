@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:GoogleBard rdf:type airo:AISystem ;
    rdfs:label "Google Bard"@en ;
    rdfs:label "谷歌Bard"@zh ;
    airo:hasRisk ex:RansomwareRisk ;
    airo:hasStakeholder ex:Researchers ;
    airo:hasVulnerability ex:SecurityFlaws .

ex:RansomwareRisk rdf:type airo:Risk ;
    rdfs:label "Ransomware Risk"@en ;
    rdfs:label "勒索软件风险"@zh ;
    airo:isRiskSourceFor ex:SecurityFlaws ;
    airo:hasImpact ex:ElectionInterference .

ex:Researchers rdf:type airo:Stakeholder ;
    rdfs:label "Researchers"@en ;
    rdfs:label "研究人员"@zh .

ex:SecurityFlaws rdf:type airo:RiskSource ;
    rdfs:label "Security Flaws"@en ;
    rdfs:label "安全漏洞"@zh .

ex:ElectionInterference rdf:type airo:Impact ;
    rdfs:label "Election Interference"@en ;
    rdfs:label "选举干扰"@zh ;
    airo:hasImpactOnStakeholder ex:Researchers .