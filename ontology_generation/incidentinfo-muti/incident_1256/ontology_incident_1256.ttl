@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:CaseWayAILegalAssistant rdf:type airo:AISystem ;
    rdfs:label "CaseWay AI Legal Assistant"@en ;
    rdfs:label "CaseWay AI法律助手"@zh ;
    airo:hasRisk ex:CopyrightInfringementRisk ;
    airo:hasStakeholder ex:CanadianLegalInformationInstitute ;
    airo:isAppliedWithinDomain ex:LegalDatabaseDomain ;
    airo:hasPurpose ex:LegalInformationProvisionPurpose ;
    airo:isDeployedBy ex:CaseWayCompany .

ex:CopyrightInfringementRisk rdf:type airo:Risk ;
    rdfs:label "Copyright Infringement Risk"@en ;
    rdfs:label "版权侵权风险"@zh ;
    airo:hasConsequence ex:LegalDisputeConsequence .

ex:CanadianLegalInformationInstitute rdf:type airo:Stakeholder ;
    rdfs:label "Canadian Legal Information Institute"@en ;
    rdfs:label "加拿大法律信息研究所"@zh .

ex:LegalDatabaseDomain rdf:type airo:Domain ;
    rdfs:label "Legal Database Domain"@en ;
    rdfs:label "法律数据库领域"@zh .

ex:LegalDisputeConsequence rdf:type airo:Consequence ;
    rdfs:label "Legal Dispute Consequence"@en ;
    rdfs:label "法律纠纷后果"@zh ;
    airo:hasImpact ex:IntellectualPropertyRightsViolationImpact .

ex:CaseWayCompany rdf:type airo:AIOperator ;
    rdfs:label "CaseWay Company"@en ;
    rdfs:label "CaseWay公司"@zh .

ex:LegalInformationProvisionPurpose rdf:type airo:Purpose ;
    rdfs:label "Legal Information Provision Purpose"@en ;
    rdfs:label "法律信息提供目的"@zh .

ex:IntellectualPropertyRightsViolationImpact rdf:type airo:Impact ;
    rdfs:label "Intellectual Property Rights Violation Impact"@en ;
    rdfs:label "知识产权侵权影响"@zh ;
    airo:hasImpactOnStakeholder ex:CanadianLegalInformationInstitute .