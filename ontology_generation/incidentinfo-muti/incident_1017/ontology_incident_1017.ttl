@base <https://w3id.org/airo#> .

ex:DeepfakeVideoGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Video Generation System"@en ;
    rdfs:label "深度伪造视频生成系统"@zh ;
    airo:hasRisk ex:MisinformationRisk ;
    airo:usesTechnique ex:DeepLearningBasedVideoSynthesis ;
    airo:hasPurpose ex:CreatingDeceptiveContent .

ex:MisinformationRisk rdf:type airo:Risk ;
    rdfs:label "Misinformation Risk"@en ;
    rdfs:label "虚假信息风险"@zh ;
    airo:hasImpact ex:PublicFigureReputationDamage .

ex:DeepLearningBasedVideoSynthesis rdf:type airo:AITechnique ;
    rdfs:label "Deep Learning Based Video Synthesis Technique"@en ;
    rdfs:label "基于深度学习的视频合成技术"@zh .

ex:PublicFigureReputationDamage rdf:type airo:Impact ;
    rdfs:label "Public Figure Reputation Damage Impact"@en ;
    rdfs:label "公众人物声誉损害影响"@zh .

ex:CreatingDeceptiveContent rdf:type airo:Purpose ;
    rdfs:label "Creating Deceptive Content Purpose"@en ;
    rdfs:label "创建欺骗性内容的目的"@zh .