@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:HireVueFacialAnalysisSystem rdf:type airo:AISystem ;
    rdfs:label "HireVue Facial Analysis System"@en ;
    rdfs:label "HireVue面部分析系统"@zh ;
    airo:hasRisk ex:FacialExpressionAnalysisBiasRisk ;
    airo:usesTechnique ex:FacialExpressionRecognition ;
    airo:hasPurpose ex:AutomatedCandidateScreening ;
    airo:isDeployedBy ex:HireVue ;
    airo:hasStakeholder ex:JobApplicants ;
    airo:isAppliedWithinDomain ex:HumanResources .

ex:FacialExpressionAnalysisBiasRisk rdf:type airo:Risk ;
    rdfs:label "Facial Expression Analysis Bias Risk"@en ;
    rdfs:label "面部表情分析偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminatoryHiringPractices ;
    airo:isRiskSourceFor ex:OverblownClaimsAboutEmotionAI .

ex:FacialExpressionRecognition rdf:type airo:AITechnique ;
    rdfs:label "Facial Expression Recognition Technique"@en ;
    rdfs:label "面部表情识别技术"@zh .

ex:JobApplicants rdf:type airo:Stakeholder ;
    rdfs:label "Job Applicants"@en ;
    rdfs:label "求职者"@zh .

ex:HireVue rdf:type airo:AIOperator ;
    rdfs:label "HireVue"@en ;
    rdfs:label "HireVue公司"@zh .

ex:OverblownClaimsAboutEmotionAI rdf:type airo:RiskSource ;
    rdfs:label "Overblown Claims About Emotion AI"@en ;
    rdfs:label "关于情感AI的夸大宣传"@zh .

ex:DiscriminatoryHiringPractices rdf:type airo:Consequence ;
    rdfs:label "Discriminatory Hiring Practices Consequence"@en ;
    rdfs:label "歧视性招聘行为后果"@zh ;
    airo:hasImpact ex:UnfairEmploymentOpportunities .

ex:UnfairEmploymentOpportunities rdf:type airo:Impact ;
    rdfs:label "Unfair Employment Opportunities Impact"@en ;
    rdfs:label "不公平就业机会影响"@zh ;
    airo:hasImpactOnStakeholder ex:JobApplicants .

ex:AutomatedCandidateScreening rdf:type airo:Purpose ;
    rdfs:label "Automated Candidate Screening Purpose"@en ;
    rdfs:label "自动化候选人筛选目的"@zh .

ex:HumanResources rdf:type airo:Domain ;
    rdfs:label "Human Resources Domain"@en ;
    rdfs:label "人力资源领域"@zh .