@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:DeepfakeGenerationSystem rdf:type airo:AISystem ;
    rdfs:label "Deepfake Generation System"@en ;
    rdfs:label "深度伪造生成系统"@zh ;
    airo:hasRisk ex:DeepfakeHarassmentRisk ;
    airo:usesTechnique ex:GenerativeAdversarialNetworks ;
    airo:hasPurpose ex:DigitalHarassment ;
    airo:hasStakeholder ex:PolicyMakers ;
    airo:hasAISubject ex:Congresswomen ;
    airo:isAppliedWithinDomain ex:PoliticalSphere .

ex:DeepfakeHarassmentRisk rdf:type airo:Risk ;
    rdfs:label "Deepfake Harassment Risk"@en ;
    rdfs:label "深度伪造骚扰风险"@zh ;
    airo:hasConsequence ex:ReputationDamage ;
    airo:isRiskSourceFor ex:MaliciousContentCreation .

ex:Congresswomen rdf:type airo:AISubject ;
    rdfs:label "Congresswomen"@en ;
    rdfs:label "国会女议员"@zh .

ex:ReputationDamage rdf:type airo:Consequence ;
    rdfs:label "Reputation Damage Consequence"@en ;
    rdfs:label "名誉损害后果"@zh ;
    airo:hasImpact ex:PsychologicalDistress .

ex:PsychologicalDistress rdf:type airo:Impact ;
    rdfs:label "Psychological Distress Impact"@en ;
    rdfs:label "心理困扰影响"@zh ;
    airo:hasImpactOnStakeholder ex:Congresswomen .

ex:GenerativeAdversarialNetworks rdf:type airo:AITechnique ;
    rdfs:label "Generative Adversarial Networks Technique"@en ;
    rdfs:label "生成对抗网络技术"@zh .

ex:PolicyMakers rdf:type airo:Stakeholder ;
    rdfs:label "Policy Makers"@en ;
    rdfs:label "政策制定者"@zh .

ex:MaliciousContentCreation rdf:type airo:RiskSource ;
    rdfs:label "Malicious Content Creation Risk Source"@en ;
    rdfs:label "恶意内容创作风险源"@zh .

ex:DigitalHarassment rdf:type airo:Purpose ;
    rdfs:label "Digital Harassment Purpose"@en ;
    rdfs:label "数字骚扰目的"@zh .

ex:PoliticalSphere rdf:type airo:Domain ;
    rdfs:label "Political Sphere Domain"@en ;
    rdfs:label "政治领域"@zh .