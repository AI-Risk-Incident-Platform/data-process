@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:AmazonRecruitmentAI rdf:type airo:AISystem ;
    rdfs:label "Amazon Recruitment AI"@en ;
    rdfs:label "亚马逊招聘AI"@zh ;
    airo:hasRisk ex:GenderBiasInHiring ;
    airo:usesTechnique ex:MachineLearningForResumeScreening .

ex:GenderBiasInHiring rdf:type airo:Risk ;
    rdfs:label "Gender Bias in Hiring Risk"@en ;
    rdfs:label "招聘中的性别偏见风险"@zh ;
    airo:hasConsequence ex:DiscriminationAgainstFemaleApplicants ;
    airo:isRiskSourceFor ex:BiasedTrainingData .

ex:DiscriminationAgainstFemaleApplicants rdf:type airo:Consequence ;
    rdfs:label "Discrimination Against Female Applicants Consequence"@en ;
    rdfs:label "对女性申请者的歧视后果"@zh ;
    airo:hasImpact ex:ReducedDiversityInWorkforce .

ex:MachineLearningForResumeScreening rdf:type airo:AITechnique ;
    rdfs:label "Machine Learning for Resume Screening Technique"@en ;
    rdfs:label "用于简历筛选的机器学习技术"@zh .

ex:BiasedTrainingData rdf:type airo:RiskSource ;
    rdfs:label "Biased Training Data Risk Source"@en ;
    rdfs:label "有偏见的训练数据风险源"@zh .

ex:ReducedDiversityInWorkforce rdf:type airo:Impact ;
    rdfs:label "Reduced Diversity in Workforce Impact"@en ;
    rdfs:label "劳动力多样性减少的影响"@zh .