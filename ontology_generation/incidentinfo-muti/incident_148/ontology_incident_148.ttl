ex:FacebookTwitterContentModerationSystem rdf:type airo:AISystem ;
    rdfs:label "Facebook Twitter Content Moderation System"@en ;
    rdfs:label "Facebook和Twitter内容审核系统"@zh ;
    airo:hasRisk ex:ContentViolationRisk ;
    airo:hasStakeholder ex:FacebookTwitterUsers ;
    airo:isAppliedWithinDomain ex:SocialMediaPlatforms .

ex:ContentViolationRisk rdf:type airo:Risk ;
    rdfs:label "Content Violation Risk"@en ;
    rdfs:label "内容违规风险"@zh .

ex:FacebookTwitterUsers rdf:type airo:Stakeholder ;
    rdfs:label "Facebook Twitter Users"@en ;
    rdfs:label "Facebook和Twitter用户"@zh .

ex:SocialMediaPlatforms rdf:type airo:Domain ;
    rdfs:label "Social Media Platforms"@en ;
    rdfs:label "社交媒体平台"@zh .