@prefix airo: <https://w3id.org/airo#> .
@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix ex: <https://example.com/aievents#> .
@base <https://w3id.org/airo#> .

ex:VisaApplicationAlgorithm rdf:type airo:AISystem ;
    rdfs:label "Visa Application Algorithm"@en ;
    rdfs:label "签证申请算法"@zh ;
    airo:hasRisk ex:DiscriminatoryAlgorithmRisk ;
    airo:hasStakeholder ex:HomeOffice ;
    airo:hasAISubject ex:VisaApplicants ;
    airo:isAppliedWithinDomain ex:ImmigrationSystem ;
    airo:hasStakeholder ex:HomeOfficeVisaDepartment .

ex:DiscriminatoryAlgorithmRisk rdf:type airo:Risk ;
    rdfs:label "Discriminatory Algorithm Risk"@en ;
    rdfs:label "歧视性算法风险"@zh ;
    airo:isRiskSourceFor ex:LackOfAlgorithmTransparency ;
    airo:hasConsequence ex:DiscriminatoryOutcomes .

ex:HomeOffice rdf:type airo:Stakeholder ;
    rdfs:label "Home Office"@en ;
    rdfs:label "内政部"@zh .

ex:HomeOfficeVisaDepartment rdf:type airo:AIOperator ;
    rdfs:label "Home Office Visa Department"@en ;
    rdfs:label "内政部签证部门"@zh .

ex:VisaApplicants rdf:type airo:AISubject ;
    rdfs:label "Visa Applicants"@en ;
    rdfs:label "签证申请人"@zh .

ex:DiscriminatoryOutcomes rdf:type airo:Impact ;
    rdfs:label "Discriminatory Outcomes"@en ;
    rdfs:label "歧视性结果"@zh ;
    airo:hasImpactOnStakeholder ex:VisaApplicants .

ex:ImmigrationSystem rdf:type airo:Domain ;
    rdfs:label "Immigration System"@en ;
    rdfs:label "移民系统"@zh .

ex:LackOfAlgorithmTransparency rdf:type airo:RiskSource ;
    rdfs:label "Lack of Algorithm Transparency"@en ;
    rdfs:label "算法缺乏透明度"@zh .